<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_barcode_labels_inherit" model="ir.ui.view">
        <field name="name">barcode.labels.inherit.view</field>
        <field name="model">barcode.labels</field>
        <field name="inherit_id" ref="dynamic_barcode_labels.view_barcode_labels"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_get_ids']/tree/field[@name='product_id']" position="attributes">
                <attribute name="domain">[('barcode', '!=', False)]</attribute>
            </xpath>
        </field>
    </record>

</odoo>
