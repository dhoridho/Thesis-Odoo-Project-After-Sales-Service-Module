<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_main_wizard_form" model="ir.ui.view">
        <field name="name">main.wizard.form</field>
        <field name="model">main.wizard</field>
        <field name="arch" type="xml">
            <form>
                <div class="alert alert-info" role="alert">
                    Wizard Change Sequence
                </div>

                <field name="main_model_id"/>

                <footer>
                    <button name="action_maju"
                            type="object"
                            string="Sequence Maju"
                            class="oe_highlight"/>
                    <button name="action_mundur"
                            type="object"
                            string="Sequence mundur"
                            class="oe_highlight"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>