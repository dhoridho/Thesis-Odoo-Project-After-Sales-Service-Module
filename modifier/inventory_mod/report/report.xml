<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="stock.action_report_lot_label" model="ir.actions.report">
            <field name="name">Lot/Serial Number (PDF)</field>
            <field name="model">stock.production.lot</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">stock.report_lot_label</field>
            <field name="report_file">stock.report_lot_label</field>
            <field name="print_report_name">'Lot-Serial - %s' % object.name</field>
            <field name="binding_model_id" ref="stock.model_stock_production_lot"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="inventory_mod.paperformat_ktp"/>
        </record>

        <record id="action_report_lot_label_move" model="ir.actions.report">
            <field name="name">Lot/Serial Number (PDF)</field>
            <field name="model">stock.move.line</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">inventory_mod.report_lot_label_move</field>
            <field name="report_file">inventory_mod.report_lot_label_move</field>
            <field name="print_report_name">'Lot-Serial - %s' % object.name</field>
            <field name="binding_model_id" ref="stock.model_stock_move_line"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="inventory_mod.paperformat_ktp"/>
        </record>


    </data>
</odoo>
