<odoo>
    <data>
        <template id="stock.report_lot_label">
            <t t-call="web.basic_layout">
                <t t-foreach="docs" t-as="o">
                    <t>
                        <div class="page data-container">
                            <div class="oe_structure" />

                            <style type="text/css">
                                .tg { border-collapse: collapse; padding-top: 10px ; width:100%;}
                                .tg td { border: none; font-family: Arial, sans-serif; font-size:
                                10px; padding: 2px; word-break: normal; }
                                .tr-normal { height:20px; }
                                .tg .tg-0lax { text-align: left; vertical-align: top; }
                                .tg-samadengan { width: 4px; font-weight: bold ; font-size: 12px ;text-align: center;}
                                .tg-field { white-space: nowrap; font-size: 12px !important; font-weight: bold;}
                                .tg-value { min-width: 150px; font-size: 12px !important; font-weight: bold;}
                                .tg-value-test { font-size: 12px !important; font-weight: bold;}
                                .tg-value-kecil { min-width: 90px; font-size: 12px !important; font-weight: bold;}
                                .tg-product-name { white-space: nowrap; font-size: 16px !important; font-weight: bold;}
                                .tg-product-value { font-size: 16px !important; font-weight: bold;}
                                .tg-product-samadengan { width: 4px; font-weight: bold ; font-size: 16px ;text-align: center;}
                                .data-container { page-break-inside: avoid; }
                                .tg-tgl { white-space: nowrap;font-size: 12px; text-align: right; vertical-align:
                                bottom; }
                                .tg-lot_barcode { text-align: center; font-size: 14px; }

                                .letter-spacing {
                                    letter-spacing: 0.5em; 
                                    font-size: 30px;
                                    font-weight: bold ;
                                }
                                .page-break {
                                    page-break-before: always;
                                }
                                .tg-qty {
                                   text-align: center; 
                                   vertical-align: bottom;
                                }
                               

                            </style>

                            <table class="tg">
                                <tbody>
                                    <tr class='tr-normal'>
                                        <td class="tg-field">KODE ITEM</td>
                                        <td class="tg-samadengan">:</td>
                                        <td class="tg-value">
                                            <span t-field="o.product_id.default_code" />
                                        </td>
                                        <td class="tg-tgl" style="font-size: 12px; text-align: right; vertical-align: bottom;">TGL.
                                            CETAK : <t t-esc="time.strftime('%Y-%m-%d')" />
                                        </td>
                                    </tr>
                                    <tr class='tr-normal'>
                                        <td class="tg-field">NO CONTAINER</td>
                                        <td class="tg-samadengan">:</td>
                                        <td class="tg-value-test" >
                                            <span t-field="o.no_container" />
                                        </td>
                                        <td></td>
                                    </tr>

                                    
                                    <tr class='tr-normal'>
                                        <td class="tg-field">JENIS PRINT</td>
                                        <td class="tg-samadengan"
                                            style="font-size: 12px;">:</td>
                                        <td class="tg-value" >
                                            <span t-esc="o.product_id.search_attribute('JENIS PRINT')"
                                                style="white-space: pre-wrap;" />
                                        </td>
                                        <td class="tg-qty" rowspan="3">
                                            <span t-esc="o.search_product_qty()"
                                                style="font-size: 50px; text-align: center; vertical-align:
                                bottom;" />
                                        </td>
                                       
                                        
                                        
                                    </tr>
                                    <tr class='tr-normal'>
                                        <td class="tg-field">PANJANG</td>
                                        <td class="tg-samadengan"
                                            style="font-size: 12px;">:</td>
                                    
                                        <td class="tg-value-kecil" >
                                            <span t-esc="o.product_id.search_attribute('PANJANG')"
                                                style="white-space: pre-wrap;" /> <span t-field="o.product_id.uom_id" />
                                        </td>

                                        
                                        
                                    </tr>
                                    <tr class='tr-normal'>
                                        <td class="tg-field">LEBAR</td>
                                        <td class="tg-samadengan"
                                            style="font-size: 12px;">:</td>
                                        <td class="tg-value-kecil" >
                                            <span t-esc="o.product_id.search_attribute('LEBAR')"
                                                style="white-space: pre-wrap;" /> <span>CM</span>
                                        </td>
                                        
                                    </tr>
                                    
                                    <tr >
                                        <td class="tg-product-name">NAMA ITEM</td>
                                        <td class="tg-product-samadengan">:</td>
                                        <td class="tg-product-value" colspan="2">
                                            <span t-field="o.product_id.name"
                                                style="white-space: pre-wrap;" />
                                        </td>
                                    </tr>

                                    <tr name="lot_name">
                                        <td class="tg-barcode" style="width: 100%"
                                            colspan="4">
                                            <img t-if="o.name"
                                                t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', o.name, 3000, 250)"
                                                style="width:165%;height:80px;margin-left: -150px;" />
                                        </td>
                                    </tr>
                                    <tr >
                                        <td class="tg-lot_barcode" colspan="4" style="text-align: center;">
                                            <span t-field="o.name" class='letter-spacing' style="white-space: pre-wrap; padding-top : 0px ; padding-bottom: 0px"/>

                                        </td>
                                    </tr>
                                </tbody>
                            </table>

                            <div class="page-break"></div>
                        </div>
                    </t>
                </t>
            </t>
        </template>


