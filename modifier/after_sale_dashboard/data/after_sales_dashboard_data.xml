<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

<!--        <function model="ks_dashboard_ninja.board" name="unlink" eval="[ref('ks_after_sales_dashboard')]"/>-->
<!--        <function model="ks_dashboard_ninja.board_template" name="unlink" eval="[ref('ks_after_sales')]"/>-->

        <record id="ks_after_sales" model="ks_dashboard_ninja.board_template">
            <field name="name">After Sales</field>
            <field name="ks_gridstack_config">
                [
                {"item_id": "after_sale_dashboard.ks_after_sales_item_1", "data": {"x": 0, "y": 0, "w": 3, "h": 2}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_2", "data": {"x": 3, "y": 0, "w": 3, "h": 2}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_3", "data": {"x": 6, "y": 0, "w": 3, "h": 2}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_4", "data": {"x": 9, "y": 0, "w": 3, "h": 2}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_5", "data": {"x": 0, "y": 2, "w": 2, "h": 2}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_6", "data": {"x": 0, "y": 4, "w": 2, "h": 2}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_7", "data": {"x": 2, "y": 2, "w": 6, "h": 4}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_8", "data": {"x": 9, "y": 2, "w": 4, "h": 4}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_9", "data": {"x": 0, "y": 6, "w": 6, "h": 4}},
                {"item_id": "after_sale_dashboard.ks_after_sales_item_10", "data": {"x": 6, "y": 6, "w": 6, "h": 4}}
                ]
            </field>
            <field name="ks_item_count">9</field>
        </record>

        <record id="ks_after_sales_item_1" model="ks_dashboard_ninja.item">
            <field name="name">Service Request</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('after_sales_service.model_service_request')"/>
            <field name="ks_domain">[["state","!=","done"]]</field>
            <field name="ks_default_icon">book</field>
            <field name="ks_dashboard_item_theme">blue</field>
            <field name="ks_background_color">#337ab7,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_company_id" eval="False"/>
        </record>

        <record id="ks_after_sales_item_2" model="ks_dashboard_ninja.item">
            <field name="name">Warranty Claim</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('after_sales_service.model_warranty_claim')"/>
            <field name="ks_domain">[["state","!=","done"]]</field>
            <field name="ks_default_icon">shopping-cart</field>
            <field name="ks_dashboard_item_theme">yellow</field>
            <field name="ks_background_color">#f0ad4e,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_company_id" eval="False"/>
        </record>

        <record id="ks_after_sales_item_3" model="ks_dashboard_ninja.item">
            <field name="name">Sale Return</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('dev_rma.model_dev_rma_rma')"/>
            <field name="ks_domain">[]</field>  <!-- Added empty domain instead of commented one -->
            <field name="ks_default_icon">address-book-o</field>
            <field name="ks_dashboard_item_theme">red</field>
            <field name="ks_background_color">#d9534f,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_company_id" eval="False"/>  <!-- Changed from 0 to False -->
        </record>

        <record id="ks_after_sales_item_4" model="ks_dashboard_ninja.item">
            <field name="name">Repair &amp; Maintenance</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('after_sales_service.model_repair_history')"/>
            <field name="ks_domain">[]</field>  <!-- Added empty domain instead of commented one -->
            <field name="ks_default_icon">address-book-o</field>
            <field name="ks_dashboard_item_theme">red</field>
            <field name="ks_background_color">#E1A563,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_company_id" eval="False"/>  <!-- Changed from 0 to False -->
        </record>

        <record id="ks_after_sales_item_5" model="ks_dashboard_ninja.item">
            <field name="name">Ongoing SR</field>
            <field name="ks_domain">["&amp;",["state","!=","completed"],["state","!=","cancel"]]</field>
            <field name="ks_model_id" eval="ref('after_sales_service.model_service_request')"/>
            <field name="ks_layout">layout5</field>
            <field name="ks_icon_select">Default</field>
            <field name="ks_default_icon" eval="False"/>
            <field name="ks_record_count_type">count</field>
            <field name="ks_dashboard_item_type">ks_kpi</field>
            <field name="ks_chart_item_color">default</field>
            <field name="ks_chart_data_count_type">count</field>
            <field name="ks_show_records">True</field>
            <field name="ks_hide_legend">True</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_auto_update_type">ks_update_interval</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_after_sales_item_6" model="ks_dashboard_ninja.item">
            <field name="name">Ongoing WC</field>
            <field name="ks_domain">["&amp;",["state","!=","completed"],["state","!=","cancel"]]</field>
            <field name="ks_model_id" eval="ref('after_sales_service.model_warranty_claim')"/>
            <field name="ks_layout">layout5</field>
            <field name="ks_icon_select">Default</field>
            <field name="ks_default_icon" eval="False"/>
            <field name="ks_record_count_type">count</field>
            <field name="ks_dashboard_item_type">ks_kpi</field>
            <field name="ks_chart_item_color">default</field>
            <field name="ks_chart_data_count_type">count</field>
            <field name="ks_show_records">True</field>
            <field name="ks_hide_legend">True</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_auto_update_type">ks_update_interval</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_after_sales_item_7" model="ks_dashboard_ninja.item">
            <field name="name">Last 3 Month After Sales Operation</field>
            <field name="ks_model_id" eval="ref('after_sale_dashboard.model_after_sales_monthly_report')"/>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_dashboard_item_type">ks_bar_chart</field>
            <field name="ks_chart_item_color">cool</field>
            <field name="ks_chart_relation_groupby" eval="ref('after_sale_dashboard.field_after_sales_monthly_report__month_year')"/>
            <field name="ks_chart_measure_field"
                   eval="[(6, 0, [
                       ref('after_sale_dashboard.field_after_sales_monthly_report__service_request_count'),
                       ref('after_sale_dashboard.field_after_sales_monthly_report__warranty_claim_count'),
                       ref('after_sale_dashboard.field_after_sales_monthly_report__sale_return_count')
                   ])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_sort_by_field" eval="ref('after_sale_dashboard.field_after_sales_monthly_report__month_year')"/>
            <field name="ks_show_data_value">True</field>
            <field name="ks_export_all_records">True</field>
            <field name="ks_show_records">True</field>
            <field name="ks_hide_legend">True</field>
            <field name="ks_record_data_limit_visibility">True</field>
            <field name="ks_record_data_limit">3</field>
            <field name="ks_auto_update_type">ks_update_interval</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_after_sales_item_8" model="ks_dashboard_ninja.item">
            <field name="name">Top 5 Requested Product</field>
            <field name="ks_model_id" eval="ref('after_sale_dashboard.model_after_sales_product_report')"/>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_dashboard_item_type">ks_pie_chart</field>
            <field name="ks_chart_item_color">neon</field>
            <field name="ks_chart_groupby_type">date_type</field>
            <field name="ks_chart_relation_groupby" eval="ref('after_sale_dashboard.field_after_sales_product_report__product_id')"/>
            <field name="ks_chart_measure_field"
                   eval="[(6, 0, [
                       ref('after_sale_dashboard.field_after_sales_product_report__total_count')
                   ])]"/>
            <field name="ks_sort_by_field" eval="False"/>
            <field name="ks_export_all_records">True</field>
            <field name="ks_show_records">False</field>
            <field name="ks_hide_legend">False</field>
            <field name="ks_record_data_limit_visibility">True</field>
            <field name="ks_record_data_limit">5</field>
            <field name="ks_auto_update_type">ks_update_interval</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_after_sales_item_9" model="ks_dashboard_ninja.item">
            <field name="name">Customer Service Statistic</field>
            <field name="ks_model_id" eval="ref('after_sale_dashboard.model_after_sales_cs_report')"/>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_dashboard_item_type">ks_horizontalBar_chart</field>
            <field name="ks_chart_item_color">warm</field>
            <field name="ks_chart_groupby_type">date_type</field>
            <field name="ks_chart_relation_groupby" eval="ref('after_sale_dashboard.field_after_sales_cs_report__responsible_id')"/>
            <field name="ks_chart_measure_field"
                   eval="[(6, 0, [
                       ref('after_sale_dashboard.field_after_sales_cs_report__total_count')
                   ])]"/>
            <field name="ks_sort_by_field" eval="False"/>
            <field name="ks_export_all_records">True</field>
            <field name="ks_show_records">False</field>
            <field name="ks_hide_legend">False</field>
            <field name="ks_auto_update_type">ks_update_interval</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_after_sales_item_10" model="ks_dashboard_ninja.item">
            <field name="name">Technician Statistic</field>
            <field name="ks_model_id" eval="ref('after_sales_service.model_repair_history')"/>
            <field name="ks_chart_data_count_type">count</field>
            <field name="ks_dashboard_item_type">ks_horizontalBar_chart</field>
            <field name="ks_chart_item_color">warm</field>
            <field name="ks_chart_relation_groupby" eval="ref('after_sales_service.field_repair_history__technician_id')"/>
            <field name="ks_sort_by_field" eval="False"/>
            <field name="ks_export_all_records">True</field>
            <field name="ks_show_records">False</field>
            <field name="ks_hide_legend">False</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_after_sales_dashboard" model="ks_dashboard_ninja.board">
            <field name="name">After Sales Dashboard</field>
            <field name="ks_dashboard_menu_name">After Sales Dashboard</field>
            <field name="ks_dashboard_top_menu_id" eval="ref('after_sales_service.menu_after_sales_root')"/>
            <field name="ks_dashboard_default_template" eval="ref('after_sale_dashboard.ks_after_sales')"/>
            <field name="ks_dashboard_active">True</field>
            <field name="ks_dashboard_group_access" eval="False"/>
            <field name="ks_dashboard_menu_sequence">1</field>
        </record>

    </data>
</odoo>