<odoo>
    <record id="warranty_notification_cron" model="ir.cron">
        <field name="name">Send Warranty Notifications</field>
        <field name="model_id" ref="model_warranty_notification"/>
        <field name="state">code</field>
        <field name="code">model.send_warranty_notifications()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active">True</field>
    </record>
</odoo>