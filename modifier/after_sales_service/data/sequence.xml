<odoo>
    <record id="seq_warranty_claim" model="ir.sequence">
        <field name="name">Warranty Claim Sequence</field>
        <field name="code">warranty.claim</field>
        <field name="prefix">WC/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_service_request" model="ir.sequence">
        <field name="name">Service Request Sequence</field>
        <field name="code">service.request</field>
        <field name="prefix">SR/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_repair_history" model="ir.sequence">
        <field name="name">Repair History Sequence</field>
        <field name="code">repair.history</field>
        <field name="prefix">RH/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>