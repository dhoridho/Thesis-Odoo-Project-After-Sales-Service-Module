<?xml version="1.0" encoding="UTF-8"?>
<odoo>
     <record id="view_after_sales_list" model="ir.ui.view">
        <field name="name">after.sales.list</field>
        <field name="model">after.sales</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

     <record id="view_after_sales_form" model="ir.ui.view">
        <field name="name">after.sales.form</field>
        <field name="model">after.sales</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_after_sales" model="ir.actions.act_window">
        <field name="name">After Sales</field>
        <field name="res_model">after.sales</field>
        <field name="view_mode">tree,form</field>
    </record>

<!--    <record id="action_after_sales" model="ir.actions.act_window">-->
<!--        <field name="name">After Sales</field>-->
<!--        <field name="res_model">after.sales</field>-->
<!--        <field name="view_mode">tree,form</field>-->
<!--    </record>-->

    <menuitem
            id="menu_after_sales_root"
            name="After Sales"
            web_icon="after_sales_service,static/description/menu_icon.png"/>

    <menuitem
            id="menu_after_sales"
            name="Dashboard"
            parent="menu_after_sales_root"
            action="action_after_sales"
            sequence="1"/>


</odoo>