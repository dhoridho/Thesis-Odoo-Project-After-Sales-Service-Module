<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Sequences for Material To Purchase -->
        <record id="seq_mrp_material_purchase" model="ir.sequence">
            <field name="name">Material to Purchase</field>
            <field name="code">mrp.material.purchase</field>
            <field name="prefix">MTP/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
