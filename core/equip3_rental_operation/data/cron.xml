<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record forcecreate="True" model="ir.cron" id="browseinfo_rental_management.rental_contract_renew_generate_invoice">
        <field name="interval_type">hours</field>
    </record>

    <record id="ir_cron_auto_expiry_rental" model="ir.cron">
		<field name="name">Auto Cancel Rental Order</field>
		<field name="model_id" ref="model_rental_order"/>
		<field name="state">code</field>
		<field name="code">model.auto_expiry_rental()</field>
		<field name="user_id" ref="base.user_root"/>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field eval="False" name="doall"/>
	</record>

	<record id="ir_cron_auto_close_rental" model="ir.cron">
		<field name="name">Auto Close Rental Order</field>
		<field name="model_id" ref="model_rental_order"/>
		<field name="state">code</field>
		<field name="code">model.auto_close_rental()</field>
		<field name="user_id" ref="base.user_root"/>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field eval="False" name="doall"/>
	</record>

</odoo>