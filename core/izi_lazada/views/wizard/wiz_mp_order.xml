<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="izi_lazada.form_wiz_mp_order" model="ir.ui.view">
            <field name="name">Wizard Marketplace Order Form View</field>
            <field name="model">wiz.mp.order</field>
            <field name="inherit_id" ref="izi_marketplace.form_wiz_mp_order"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='mp_get_order']" position="inside">
                    <field name="range_type" attrs="{'invisible': ['|',('marketplace', '!=', 'lazada'), ('params', '!=', 'by_date_range')]}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>