<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="izi_lazada.action_window_lz_shop" model="ir.actions.act_window">
      <field name="name">Shop</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.lazada.seller</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="izi_lazada.action_window_lz_logistic" model="ir.actions.act_window">
      <field name="name">Logistic</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.lazada.logistic</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="izi_lazada.action_window_lz_webhook_order" model="ir.actions.act_window">
      <field name="name">Webhook Order</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.webhook.order</field>
      <field name="view_mode">tree,form</field>
      <field name="domain">[('marketplace', '=', 'lazada')]</field>
      <field name="context">{
        'default_marketplace': 'lazada',
        "search_default_group_by_mp_account": 1,
      }</field>
    </record>
  </data>
</odoo>