<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="equip3_hr_dashboard_extend.module_category_hr_dashboard" model="ir.module.category">
        <field name="sequence">9</field>
    </record>

    <record id="group_hr_dashboard_user" model="res.groups">
        <field name="name">Employee</field>
        <field name="category_id" ref="base.module_category_human_resources_employees"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_hr_dashboard_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="equip3_hr_dashboard_extend.module_category_hr_dashboard"/>
        <field name="implied_ids" eval="[(4, ref('hr.group_hr_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>
