        <record id="ks_hr_dashboard_item_18" model="ks_dashboard_ninja.item">
            <field name="name">Training</field>
            <field name="ks_data_calculation_type">query</field>
            <field name="ks_custom_query">select c.name as training, count(a.employee_id) as peserta
from training_conduct_line a
join training_conduct b on a.conduct_id=b.id
join training_courses c on b.course_id=c.id
join training_stages d on b.stage_id=d.id
where d.name='On Progress'
group by c.name
order by c.name ASC
            </field>
            <field name="ks_show_data_value" eval="True"/>
            <field name="ks_chart_item_color">warm</field>
            <field name="ks_chart_data_count_type">count</field>
            <field name="ks_dashboard_item_type">ks_bar_chart</field>
            <field name="ks_xlabels">training</field>
            <field name="ks_ylabels">{"peserta":{"measure":"Peserta","chart_type":"bar","group":" "}}</field>
        </record>

