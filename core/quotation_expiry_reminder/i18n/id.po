#. module: quotation_expiry_reminder
#: model:mail.template,body_html:quotation_expiry_reminder.product_expiration_mail_inherit
msgid ""
"\n"
"\t\t \n"
"<p>Hi,</p>The following orders are going to expire in ${ctx.get('frequency')}.<br><br>\n"
"<p>Please take the necessary action.</p>\n"
"<table width=\"771\" cellspacing=\"1\" cellpadding=\"4\" border=\"1\" height=\"73\">\n"
"    <tbody>\n"
"        <tr style=\"font-weight:bold;\">\n"
"            <th>S.No</th>\n"
"            <th>SO Id</th>\n"
"            <th>Customer</th>\n"
"            <th>Order Price</th>\n"
"            <th>Expiry Date</th>\n"
"        </tr>\n"
"        % if ctx.get('sale_order_list'):\n"
"        \t% for values in ctx['sale_order_list']\n"
"\t\t        <tr>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['serial']}</td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['name']}</td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['partner_id']}<br></td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${format_amount(values['amount_total'], object.currency_id)}<br></td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['validity_date']}<br></td>\n"
"\t\t        </tr>\n"
"       \t \t% endfor\n"
"       \t % endif\n"
"    </tbody>\n"
"</table><br>\n"
"<p>Thank you</p>\n"
"\t\t\n"
msgstr ""
"\n"
"\t\t \n"
"<p>Hai,</p>Pesanan berikut akan kedaluwarsa dalam ${ctx.get('frequency')}.<br><br>\n"
"<p>Mohon ambil tindakan yang diperlukan.</p>\n"
"<table width=\"771\" cellspacing=\"1\" cellpadding=\"4\" border=\"1\" height=\"73\">\n"
"    <tbody>\n"
"        <tr style=\"font-weight:bold;\">\n"
"            <th>S.No</th>\n"
"            <th>SO Id</th>\n"
"            <th>Pelanggan</th>\n"
"            <th>Harga Pesanan</th>\n"
"            <th>Tanggal Kadaluarsa</th>\n"
"        </tr>\n"
"       % if ctx.get('daftar_pesanan_penjualan'):\n"
"        \t% fatau nilai dalam ctx['sale_order_list']\n"
"\t\t        <tr>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['serial']}</td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['name']}</td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['partner_id']}<br></td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${format_jumlah(values['jumlah_total'], object.currency_id)}<br></td>\n"
"\t\t            <td valign=\"middle\" align=\"center\">${values['validity_date']}<br></td>\n"
"\t\t        </tr>\n"
"       \t \t% enduntuk\n"
"       \t % endif\n"
"    </tbody>\n"
"</table><br>\n"
"<p>Terima kasih</p>\n"
"\t\t\n"

