<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1"> 
    <record id="sequence_sale_service" model="ir.sequence">
        <field name="name">Sale Service Sequence</field>
        <field name="code">sale.service</field>
        <field name="prefix">SRV/%(y)s/%(month)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="4" name="padding"/>
        <field eval="False" name="company_id"/>
    </record>
    <record id="sequence_goods_receive_service" model="ir.sequence">
        <field name="name">Goods Receive Service Sequence</field>
        <field name="code">goods.receive.service</field>
        <field name="prefix">GRS/%(y)s/%(month)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="4" name="padding"/>
        <field eval="False" name="company_id"/>
    </record>
    <record id="sequence_maintenance_service" model="ir.sequence">
        <field name="name">Maintenance Service Sequence</field>
        <field name="code">maintenance.service</field>
        <field name="prefix">MNT/%(y)s/%(month)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="4" name="padding"/>
        <field eval="False" name="company_id"/>
    </record>
</data>
</odoo>
