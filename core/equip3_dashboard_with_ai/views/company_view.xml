<!-- Inherit Res Company Form -->
<odoo>
    <data>
        <record id="view_res_company_form_inherit" model="ir.ui.view">
            <field name="name">res.company.form.inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="izi_dashboard.view_res_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[last()]" position="replace">
                <!-- <xpath expr="//page[@string='IZI Lab']" position="replace"> -->
                    <page string="AI API KEY">
                        <group>
                            <field name="izi_lab_api_key" string="AI API Key" readonly="0"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>