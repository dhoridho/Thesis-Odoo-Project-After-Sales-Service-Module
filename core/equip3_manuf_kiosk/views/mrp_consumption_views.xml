<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="mrp_consumption_form" model="ir.ui.view">
        <field name="name">mrp_consumption_form</field>
        <field name="model">mrp.consumption</field>
        <field name="inherit_id" ref="equip3_manuf_operations_contd.mrp_consumption_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_raw_ids']/tree/field[@name='product_uom_qty']" position="after">
                <field name="kiosk_qty" optional="hide"/>
            </xpath>
        </field>
    </record>

</odoo>