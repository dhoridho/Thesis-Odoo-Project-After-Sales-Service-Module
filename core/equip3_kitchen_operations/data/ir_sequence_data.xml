<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="seq_kitchen_production_record" model="ir.sequence">
            <field name="name">Kitchen Production Record</field>
            <field name="code">kitchen.production.record</field>
            <field name="prefix">KPR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_kitchen_outlet_order" model="ir.sequence">
            <field name="name">Outlet Order</field>
            <field name="code">internal.transfer.outlet</field>
            <field name="prefix">OTO/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

    </data>
</odoo>
