<odoo>
  <data>

    <record id="catering_graph" model="ir.ui.view">
      <field name="name">catering.graph</field>
      <field name="model">catering.order.line</field>
      <field name="arch"  type="xml">
        <graph string="Catering Pivot Analysis" type="line">
          <field name="creation_date" type="row" interval="day"/>
          <field name="qty_order" type="measure"/>
        </graph>
      </field>
    </record>

    <record id="catering_pivot" model="ir.ui.view">
      <field name="name">catering.pivot</field>
      <field name="model">catering.order.line</field>
      <field name="arch"  type="xml">
        <pivot string="Catering Pivot Analysis">
          <field name="package_id" type="row"/>
          <field name="warehouse_id" type="col"/>
          <field name="untaxed_total" type="measure"/>
        </pivot>
      </field>
    </record>

    <record id="action_catering_pivot_analysis" model="ir.actions.act_window">
      <field name="name">Catering Pivot Analysis</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">catering.order.line</field>
      <field name="view_mode">graph,pivot</field>
      <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'graph', 'view_id': ref('catering_graph')}),
                (0, 0, {'view_mode': 'pivot', 'view_id': ref('catering_pivot')})]"/>
    </record>

    <menuitem name="Report"
              id="menu_report"
              parent="equip3_catering_masterdata.catering_menu_root"
              sequence="1"/>
    <menuitem name="Catering Pivot Analysis"
              id="menu_catering_pivot_analysis"
              parent="menu_report"
              action="action_catering_pivot_analysis"
              sequence="2"/>
  </data>
</odoo>