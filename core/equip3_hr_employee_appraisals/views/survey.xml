<odoo>
<data>
    <record model="ir.actions.act_window" id="action_survey_form_survey_extend_appraisal">
        <field name="name">Feedback Questions</field>
        <field name="res_model">survey.survey</field>
        <field name="view_mode">kanban,tree,form,activity</field>
        <field name="context">{'search_default_groupby_state': 1,'default_survey_type':'peer_review'}</field>
        <field name="domain">[('survey_type','=','peer_review')]</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Add a new Feedback Questions
            </p>
        </field>
    </record>


    <menuitem id="menu_hr_employee_appraisals_survey" 
    parent="employee_performance.menu_performance_config"
    action="action_survey_form_survey_extend_appraisal" 
    name="Feedback Questions"
    sequence="13" />


    <record model='ir.ui.menu' id='menu_hr_employee_appraisals_survey'>
        <field name="groups_id" eval="[(6, 0, [ref('equip3_hr_employee_appraisals.group_hr_appraisal_manager')])]"/>
    </record>



</data>


</odoo>