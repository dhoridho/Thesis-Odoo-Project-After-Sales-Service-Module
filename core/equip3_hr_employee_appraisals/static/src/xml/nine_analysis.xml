<?xml version="1.0" encoding="utf-8"?>
<template xml:space="preserve">
    <t t-name="NineAnalysisTemplate">
        <div class="container">
            <br/>
            <div class="row" style="width:100%;">
                <div class="col-xs-12" style="width:100%;">
                    <table class="table-bordered" style="width:100%;">
                        <tr>
                            <td width="5%" style="border-color: black;background: #dbdee1;" rowspan="5">
                                <p class="text-center" style="color:black;-webkit-transform: rotate( 270deg); -moz-transform: rotate(270deg); -o-transform: rotate(270deg); -ms-transform: rotate(270deg); transform: rotate( 270deg);margin: 0px; font-size: 20px !important;position: absolute;margin-left: -21px;"><b>Potential</b></p>
                            </td>
                            <td width="2.5%" style="border-color: black;">
                                <p class="text-center" style="color:black;-webkit-transform: rotate( 270deg); -moz-transform: rotate(270deg); -o-transform: rotate(270deg); -ms-transform: rotate(270deg); transform: rotate( 270deg);position: absolute;margin-left: -4px;"><b>High</b></p>
                            </td>
                            <t t-foreach="widget.dataresult['high']" t-as="high">
                                <td width="31%" style="border-color: black;cursor:pointer;" class="box_nine_analysis" t-att-data-id="high.id">
                                    <div t-if="high.id" t-att-style='"background-color:"+high.color'>
                                        <br/>
                                        <p class="text-center" style="color:black;"><b><span t-esc="high.number_analysis"/>. <span t-esc="high.name"/></b></p>
                                        <div style="border: 1px solid; margin: 0 25px;">
                                            <br/>
                                            <p class="text-center" style="color:black;"><b><span t-esc="high.count"/></b></p>
                                            <p class="text-center" style="color:black;"><b>Employees</b></p>
                                        </div>
                                        <br/>
                                    </div>
                                </td>
                            </t>
                                
                        </tr>
                        <tr>
                            <td  style="border-color: black;">
                                <p class="text-center" style="color:black;-webkit-transform: rotate( 270deg); -moz-transform: rotate(270deg); -o-transform: rotate(270deg); -ms-transform: rotate(270deg); transform: rotate( 270deg);position: absolute;margin-left: -15px;"><b>Medium</b></p>
                            </td>
                            <t t-foreach="widget.dataresult['medium']" t-as="medium">
                                <td  style="border-color: black;cursor:pointer;" class="box_nine_analysis" t-att-data-id="medium.id">
                                    <div t-if="medium.id" t-att-style='"background-color:"+medium.color'>
                                        <br/>
                                        <p class="text-center" style="color:black;"><b><span t-esc="medium.number_analysis"/>. <span t-esc="medium.name"/></b></p>
                                        <div style="border: 1px solid; margin: 0 25px;">
                                            <br/>
                                            <p class="text-center" style="color:black;"><b><span t-esc="medium.count"/></b></p>
                                            <p class="text-center" style="color:black;"><b>Employees</b></p>
                                        </div>
                                        <br/>
                                    </div>
                                </td>
                            </t>
                        </tr>
                        <tr>
                            <td  style="border-color: black;">
                                <p class="text-center" style="color:black;-webkit-transform: rotate( 270deg); -moz-transform: rotate(270deg); -o-transform: rotate(270deg); -ms-transform: rotate(270deg); transform: rotate( 270deg);position: absolute;"><b>Low</b></p>
                            </td>
                            <t t-foreach="widget.dataresult['low']" t-as="low">
                                <td  style="border-color: black;cursor:pointer;" class="box_nine_analysis" t-att-data-id="low.id">
                                    <div t-if="low.id" t-att-style='"background-color:"+low.color'>
                                        <br/>
                                        <p class="text-center" style="color:black;"><b><span t-esc="low.number_analysis"/>. <span t-esc="low.name"/></b></p>
                                        <div style="border: 1px solid; margin: 0 25px;">
                                            <br/>
                                            <p class="text-center" style="color:black;"><b><span t-esc="low.count"/></b></p>
                                            <p class="text-center" style="color:black;"><b>Employees</b></p>
                                        </div>
                                        <br/>
                                    </div>
                                </td>
                            </t>
                        </tr>
                        <tr>
                            <td style="border-color: black;background: white;">
                                <div>
                                    
                                </div>
                            </td>
                            <td style="border-color: black;">
                                <div>
                           
                                    <p class="text-center" style="color:black;margin:0px;"><b>Low</b></p>
                                </div>
                            </td>
                            <td style="border-color: black;">
                                <div>
                             
                                    <p class="text-center" style="color:black;margin:0px;"><b>Medium</b></p>
                                </div>
                            </td>
                            <td style="border-color: black;">
                                <div>
                                    
                                    <p class="text-center" style="color:black;margin:0px;"><b>High</b></p>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4" style="border-color: black;background: #dbdee1;">
                                <div>
                           
                                    <p class="text-center" style="padding: 7px 0; color: black; margin: 0px; font-size: 20px !important;"><b>Performance</b></p>
                                </div>
                            </td>
                        </tr>
                    </table>
                </div>
                
            </div>
        </div>
    </t>
</template>
