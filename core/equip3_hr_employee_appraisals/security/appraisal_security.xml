    </record>

    <record id="equip3_hr_employee_appraisals.group_hr_appraisal_administrator" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="equip3_hr_employee_appraisals.module_category_appraisal"/>
        <field name="implied_ids" eval="[(4, ref('equip3_hr_employee_appraisals.group_hr_appraisal_manager'))]"/>
