<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <!-- Client Action -->
    <t t-name="kitchen_flow_wizard_action_one">
        <div class="kitchen_flow_action">
            <div class="kitchen_flow_action_content d-flex flex-column">
                <div class="content-header">
                    <div class="d-flex p-3 w-100">
                        <span>Kitchen Flow</span>
                    </div>
                </div>
                <div class="content-body">
                    <div class="container">
                        <div class="row h-100">
                            <div class="accordion">
                                <div class="accordion-item accordion-master-data">
                                    <h2 class="accordion-header" data-target="collapseMasterDataAsembly">
                                        <button class="accordion-button" type="button">
                                        Master Data
                                        </button>
                                    </h2>
                                    <div id="collapseMasterDataKitchen" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="product.product_template_action" type="action" class="btn btn-flow btn-default menu-button-product">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/product_icon.svg"/>
                                                    </div>
                                                    <div class="button-text">Product</div>
                                                </button>
                                                <button name="product.product_normal_action" type="action" class="btn btn-flow btn-default menu-button-product-variant">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/product_icon.svg"/>
                                                    </div>
                                                    <div class="button-text">Product Variant</div>
                                                </button>
                                                <button name="equip3_kitchen_masterdata.kitchen_bom_form_action" type="action" class="btn btn-flow btn-default menu-button-bill-of-material">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/bills_of_material.svg"/>
                                                    </div>
                                                    <div class="button-text">Bill of Material</div>
                                                </button>
                                                <button name="equip3_kitchen_operations.action_view_kitchen_safety_stock_management" type="action" class="btn btn-flow btn-default menu-button-safety-stock-management">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/safety_stock.svg"/>
                                                    </div>
                                                    <div class="button-text">Safety Stock Management</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-kitchen-flow">
                                    <h2 class="accordion-header" data-target="collapseKitchenFlow">
                                        <button class="accordion-button" type="button">
                                        Kitchen Flow
                                        </button>
                                    </h2>
                                    <div id="collapseKitchenFlow" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">
                                                <button name="equip3_kitchen_operations.action_internal_transfer_request_outlet_order" type="action" class="btn btn-flow btn-default menu-button-kitchen">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/outlet_order.svg"/>
                                                    </div>
                                                    <div class="button-text">Outlet Order</div>
                                                </button>
                                                <div class="nd_connector" style="width: 100px;left: 207px;top: 54px;">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <button name="equip3_kitchen_operations.action_view_kitchen_cooking_list" type="action" style="left: 300px;" class="btn btn-flow btn-default menu-button-kitchen-production-record">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/cooking_list.svg"/>
                                                    </div>
                                                    <div class="button-text">Cooking List</div>
                                                </button>
                                                <div class="nd_connector" style="width: 110px;left: 480px;top: 54px;">
                                                    <div class="nd_connector_line dotted"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <button name="equip3_kitchen_operations.action_view_dashboard_kitchen" type="action" style="left: 585px;" class="btn btn-flow btn-default menu-button-kitchen">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/dashboard.svg"/>
                                                    </div>
                                                    <div class="button-text">Dashboard</div>
                                                </button>
                                                <div class="nd_connector" style="width: 110px;left: 750px;top: 54px;">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <button name="equip3_kitchen_operations.action_view_kitchen_production_record" type="action" style="width: 240px;left: 855px; padding-right: 10px" class="btn btn-flow btn-default menu-button-kitchen-production-record">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/kitchen_product_record.svg"/>
                                                    </div>
                                                    <div class="button-text">Kitchen Production Record</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-reporting">
                                    <h2 class="accordion-header" data-target="collapseReportingAsembly">
                                        <button class="accordion-button" type="button">
                                        Reporting
                                        </button>
                                    </h2>
                                    <div id="collapseReportingAsembly" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">
                                                <button name="equip3_kitchen_reports.action_view_finished_product_kitchen" type="action" class="btn btn-flow btn-default menu-button-finished-product-report">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/finished_product_report.svg"/>
                                                    </div>
                                                    <div class="button-text">Finished Product Report</div>
                                                </button>
                                                <button name="equip3_kitchen_reports.action_view_material_consumed_kitchen" type="action" class="btn btn-flow btn-default menu-button-material-consumed">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_kitchen_reports/static/img/material_consumed_report.svg"/>
                                                    </div>
                                                    <div class="button-text">Material Consumed Report</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>