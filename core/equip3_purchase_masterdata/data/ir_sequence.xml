<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<record id="vendor_id_sequence" model="ir.sequence">
			<field name="name">Vendor ID</field>
			<field name="code">vendor.id.sequence</field>
			<field name="prefix">VE</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="target_kpi_sequence" model="ir.sequence">
			<field name="name">Purchase Target KPI</field>
			<field name="code">purchase.target.kpi</field>
			<field name="prefix">PKPI/%(y)s/%(month)s/%(day)s/</field>
			<field name="padding">3</field>
		</record>
</odoo>
