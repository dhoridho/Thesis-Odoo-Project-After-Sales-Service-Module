<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_pos" name="Assets POS" inherit_id="equip3_pos_masterdata.assets_pos">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/equip3_pos_cashbox/static/src/css/custom.css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/equip3_pos_cashbox/static/src/js/PopUps/ManageCashPopup.js"/>
            
            <script type="text/javascript" src="/equip3_pos_cashbox/static/src/js/ChromeWidgets/ManageCashButton.js"/>
        </xpath>
    </template>

    <template id="pos_cashbox_assets" inherit_id="web.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" href="/equip3_pos_cashbox/static/src/css/backend_custom.css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/equip3_pos_cashbox/static/src/js/form_controller.js"/>
        </xpath>
    </template>

</odoo>