<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	
    <record id="pos_cashbox_history_tree" model="ir.ui.view">
        <field name="name">pos.cashbox.history.tree</field>
        <field name="model">pos.cashbox.history</field>
        <field name="arch" type="xml">
            <tree create="0" edit="0" delete="0">
                <field name="start_date"/>
                <field name="closing_date"/>
                <field name="pos_config_id"/>
                <field name="pos_session_id"/>
                <field name="opened_by_user_id"/>
                <field name="start_balance"/>
                <field name="closing_balance"/>
            </tree>
        </field>
    </record>

    <record id="pos_cashbox_history_search" model="ir.ui.view">
        <field name="name">pos.cashbox.history.filter</field>
        <field name="model">pos.cashbox.history</field>
        <field name="arch" type="xml">
            <search>
                <field name="pos_session_id"/>
            </search>
        </field>
    </record>

</odoo>