<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Sequences for Cost Actualization -->
        <record id="seq_mrp_cost_actualization_day" model="ir.sequence">
            <field name="name">Cost Actualization</field>
            <field name="code">mrp.cost.actualization.day</field>
            <field name="prefix">PCA/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
