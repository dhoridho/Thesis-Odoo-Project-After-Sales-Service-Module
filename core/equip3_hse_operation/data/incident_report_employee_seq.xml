<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="incident_report_multiple_sequence" model="ir.sequence">
            <field name="name">Multiple Incident Report Sequence</field>
            <field name="code">incident.report.multiple.sequence</field>
            <field name="prefix">MIR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="incident_report_employee_sequence" model="ir.sequence">
            <field name="name">Employee Incident Report Sequence</field>
            <field name="code">incident.report.employee.sequence</field>
            <field name="prefix">EIR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="death_report_employee_sequence" model="ir.sequence">
            <field name="name">Employee Death Report Sequence</field>
            <field name="code">death.report.employee.sequence</field>
            <field name="prefix">EDR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="investigation_report_employee_sequence" model="ir.sequence">
            <field name="name">Employee Investigation Report Sequence</field>
            <field name="code">investigation.report.employee.sequence</field>
            <field name="prefix">IVR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="action_checkist_sequence" model="ir.sequence">
            <field name="name">Action Checklist Sequence</field>
            <field name="code">action.checklist.sequence</field>
            <field name="prefix">ACT/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>