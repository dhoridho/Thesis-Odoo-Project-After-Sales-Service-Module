<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="ist_scoring_iq_dominance_form_view" model="ir.ui.view">
            <field name="name">ist.scoring.iq.dominance.form</field>
            <field name="model">ist.scoring.iq.dominance</field>
            <field name="arch" type="xml">
                <form string="IST Parameter" create="0" delete="0">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
                            <field name="score_from"/>
                            <field name="score_to"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="ist_scoring_iq_dominance_tree_view" model="ir.ui.view">
            <field name="name">ist.scoring.iq.dominance.tree</field>
            <field name="model">ist.scoring.iq.dominance</field>
            <field name="arch" type="xml">
                <tree string="IQ" create="0" delete="0">
                    <field name="name"/>
                    <field name="description"/>
                    <field name="score_from"/>
                    <field name="score_to"/>
                </tree>
            </field>
        </record>

        <record id="ist_scoring_iq_dominance_act_window" model="ir.actions.act_window">
            <field name="name">IST IQ Dominance</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ist.scoring.iq.dominance</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here.
                </p>
            </field>
        </record>

    </data>
</odoo>