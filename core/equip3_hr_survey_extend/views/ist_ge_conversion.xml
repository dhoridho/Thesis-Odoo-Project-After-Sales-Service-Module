<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="ist_ge_conversion_form_view" model="ir.ui.view">
            <field name="name">ist.ge.conversion.form</field>
            <field name="model">ist.ge.conversion</field>
            <field name="arch" type="xml">
                <form string="Konversi GE" create="0" delete="0">
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="name" required="1"/>
                            </h1>
                        </div>
                        <notebook name="ist_ge_conversion_line">
                            <page name="matrix_line" string="Matrix Line">
                                <field name="ist_ge_conversion_line_ids">
                                    <tree editable="bottom">
                                        <field name="actual_score"/>
                                        <field name="converted_score"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="ist_sge_conversion_tree_view" model="ir.ui.view">
            <field name="name">ist.ge.conversion.tree</field>
            <field name="model">ist.ge.conversion</field>
            <field name="arch" type="xml">
                <tree string="Konversi GE" create="0" delete="0">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="ist_ge_conversion_act_window" model="ir.actions.act_window">
            <field name="name">IST Konversi GE</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ist.ge.conversion</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new Disc Personality.
                </p>
            </field>
        </record>

    </data>
</odoo>