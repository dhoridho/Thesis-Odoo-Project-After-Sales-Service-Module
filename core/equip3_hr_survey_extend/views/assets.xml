    <template id="inherit_assets_backend" inherit_id="web.assets_backend">
        <xpath expr="//script[last()]" position="after">
            <!--  Library  -->
            <script type="text/javascript" src="/equip3_hr_survey_extend/static/lib/plotly/plotly-latest.min.js"/>
            <!--  Widget  -->
            <script type="text/javascript" src="/equip3_hr_survey_extend/static/src/js/widget_plotly.js"/>
        </xpath>
    </template>

    <template id="survey_extend_asset" inherit_id="survey.survey_assets">
        <xpath expr="link[last()]" position="after">
            <link href="/equip3_hr_survey_extend/static/src/css/loader.css" rel="stylesheet" type="text/css"/>
        </xpath>
    </template>

    <template id="general_assets_backend" inherit_id="web.assets_backend">
            <xpath expr="link[last()]" position="after">
                <link rel="stylesheet" type="text/css" href="/equip3_hr_survey_extend/static/src/css/survey_flow.css"/>
                <script type="text/javascript" src="/equip3_hr_survey_extend/static/src/js/survey_flow.js"/>
            </xpath>
    </template>

    <template id="assets_wysiwyg_video_survey" inherit_id="web_editor.assets_wysiwyg">
        <xpath expr="//script[@src='/web_elearning_video/static/src/js/video_dialog.js']" position="replace">
            <script type="text/javascript" src="/equip3_hr_survey_extend/static/src/js/video_dialog.js"/>
        </xpath>
    </template>

</data>
</odoo>
