<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="vak_parameter_form_view" model="ir.ui.view">
            <field name="name">vak.parameter.form</field>
            <field name="model">vak.parameter</field>
            <field name="arch" type="xml">
                <form string="VAK Parameter" create="0" delete="1">
                    <sheet>
                        <group>
                            <group>
                                <field name="sequence" required="1" readonly="1" invisible="1"/>
                                <field name="name" required="1" readonly="1"/>
                                <field name="vak_code"/>
                                <field name="description"/>
                                <field name="strengths"/>
                                <field name="learning_preferences"/>
                                <field name="challenges"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="vak_parameter_tree_view" model="ir.ui.view">
            <field name="name">vak.parameter.tree</field>
            <field name="model">vak.parameter</field>
            <field name="arch" type="xml">
                <tree string="Parameter Line" create="0" delete="1">
                    <field name="sequence" widget="handle"/>
                    <field name="name" required="1"/>
                    <field name="vak_code" />
                    <field name="description"/>
                    <field name="strengths"/>
                    <field name="learning_preferences"/>
                    <field name="challenges"/>
                </tree>
            </field>
        </record>

        <record id="vak_parameter_act_window" model="ir.actions.act_window">
            <field name="name">VAK Parameter</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vak.parameter</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new VAK Parameter.
                </p>
            </field>
        </record>

    </data>
</odoo>