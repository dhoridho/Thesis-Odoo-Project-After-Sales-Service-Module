<odoo>
    <data>
        <record id="ir_cron_notifications_expiry_customer_bank_guarantee" model="ir.cron">
            <field name="name">Email Notification for Bank Guarantee</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model.cron_send_email_expiry_bank_guarantee()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
        </record>
        <record id="reset_sequence_fmcg_reimbursement" model="ir.cron">
            <field name="name">Reset Sequence FMCG Reimbursement</field>
            <field name="model_id" ref="model_fmcg_reimbursement"/>
            <field name="state">code</field>
            <field name="code">model.cron_reset_sequence_fmcg_reimbursement()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
        </record>
    </data>
</odoo>