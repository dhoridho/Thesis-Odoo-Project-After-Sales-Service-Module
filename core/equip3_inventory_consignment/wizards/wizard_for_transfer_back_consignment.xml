<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_wizard_for_transfer_back_consignment" model="ir.ui.view">
        <field name="name">view.wizard.for.transfer.back.consignment</field>
        <field name="model">transfer.back.consignment</field>
        <field name="arch" type="xml">
            <form>
                <group name="transfer_back">
                    <notebook>
                        <page string="Products" name="products">
                            <field name="transfer_line">
                                <tree string="Transfer Line" editable="bottom">
                                    <field name="product_id" force_save="1" readonly="1"/>
                                    <field name="warehouse_destination_id" />
                                    <field name="available_qty" />
                                    <field name="transfer_qty" />
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </group>
                <footer>
                    <button name="process" string="Confirm" type="object" class="btn-primary" />
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>