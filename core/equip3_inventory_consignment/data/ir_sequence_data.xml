<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="receiving_notes_consignment_sequence" model="ir.sequence">
            <field name="name">Receiving Notes Consignment Sequence</field>
            <field name="code">receiving.notes.consignment.sequence</field>
            <field name="padding">3</field>
            <field name="prefix">/%(y)s/%(month)s/%(day)s/</field>
            <field name="use_date_range">True</field>
            <field name="range_reset">yearly</field>
        </record>

         <!-- now using this -->
        <record id="consignment_agreement_new_sequence" model="ir.sequence">
            <field name="name">Sequence Consignment Agreement</field>
            <field name="code">purchase.requisition.consignment.new</field>
            <field name="prefix">CSG/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
            <field name="range_reset">yearly</field>
        </record>

    </data>

</odoo>
