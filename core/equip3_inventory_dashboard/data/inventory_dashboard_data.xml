<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ks_inventory" model="ks_dashboard_ninja.board_template">
            <field name="name">Inventory</field>
            <field name="ks_gridstack_config">[
                {"item_id":"equip3_inventory_dashboard.ks_inventory_item_12", "data": {"x": 0, "y": 0, "w": 4, "h":
                2}},
                {"item_id":"equip3_inventory_dashboard.ks_inventory_item_13", "data": {"x": 4, "y": 0, "w": 4, "h":
                2}},
                {"item_id":"equip3_inventory_dashboard.ks_inventory_item_14", "data": {"x": 8, "y": 0, "w": 4, "h":
                2}},
                {"item_id":"equip3_inventory_dashboard.ks_inventory_item_15", "data": {"x": 0, "y": 2, "w": 4, "h":
                2}},
                {"item_id":"equip3_inventory_dashboard.ks_inventory_item_16", "data": {"x": 4, "y": 2, "w": 4, "h": 2}},
                {"item_id":"equip3_inventory_dashboard.ks_inventory_item_17", "data": {"x": 8, "y": 2, "w": 4, "h": 2}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_3", "data": {"x": 0, "y": 8, "w": 6, "h":
                5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_4", "data": {"x": 6, "y": 8, "w": 6, "h":
                5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_1", "data": {"x": 0, "y": 12, "w": 6, "h":
                5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_2", "data": {"x": 6, "y": 12, "w": 6, "h":
                5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_5", "data": {"x": 0, "y": 16, "w": 4, "h": 5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_6", "data": {"x": 8, "y": 16, "w": 4, "h": 5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_8", "data": {"x": 4, "y": 16, "w": 4, "h": 5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_9", "data": {"x": 0, "y": 20, "w": 8, "h": 5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_10", "data": {"x": 8, "y": 20, "w": 4, "h": 5}},
                {"item_id":"ks_inventory_dashboard.ks_inventory_item_11", "data": {"x": 0, "y": 24, "w": 12, "h": 5}}
                ]
            </field>
            <field name="ks_item_count">16</field>
            <field name="ks_item_count">17</field>
        </record>

        <record id="ks_inventory_item_16" model="ks_dashboard_ninja.item">
            <field name="name">Low Stock Alert</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('stock.model_stock_warehouse_orderpoint')"/>
            <!-- <field name="ks_domain">[["is_low_stock","=",True]]</field> -->
            <field name="ks_domain">[["qty_to_order", "&gt;", 0.0], ["snoozed_until", "=", False], ["is_replenish_document_created", "=", False]]</field>
            <!-- <field name="ks_default_icon">star-half-o</field> -->
            <field name="ks_icon" type="base64" file="equip3_inventory_dashboard/static/src/description/images/low_stock.png"/>
            <field name="ks_dashboard_item_theme">white</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_default_icon_color">#ffc107,0.99</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_inventory_item_17" model="ks_dashboard_ninja.item">
            <field name="name">Material Request</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('equip3_inventory_operation.model_material_request')"/>
            <field name="ks_icon" type="base64" file="equip3_inventory_dashboard/static/src/description/images/mr.png"/>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_domain"></field>
            <!-- <field name="ks_default_icon">bar-chart</field> -->
            <field name="ks_dashboard_item_theme">white</field>
            <field name="ks_background_color">ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#cc1377,0.99</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_inventory_item_12" model="ks_dashboard_ninja.item">
            <field name="name">Internal Transfer</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('stock.model_stock_move_line')"/>
            <field name="ks_icon" type="base64" file="equip3_inventory_dashboard/static/src/description/images/internal_transfer.png"/>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_domain">[["location_id.usage","=","internal"],["location_dest_id.usage","=","internal"],["state","!=","cancel"]]</field>
            <field name="ks_date_filter_field" eval="ref('stock.field_stock_move_line__create_date')"/>
            <!-- <field name="ks_default_icon">home</field> -->
            <field name="ks_dashboard_item_theme">white</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#302dc4,0.99</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_inventory_item_13" model="ks_dashboard_ninja.item">
            <field name="name">Delivery Orders</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_actions" eval="ref('equip3_inventory_operation.action_delivery_order')"/>
            <field name="ks_model_id" eval="ref('stock.model_stock_picking')"/>
            <field name="ks_icon" type="base64" file="equip3_inventory_dashboard/static/src/description/images/delivery_order.png"/>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_domain">[["picking_type_id.code","=","outgoing"],["state","=","done"]]</field>
            <field name="ks_date_filter_field" eval="ref('stock.field_stock_picking__create_date')"/>
            <!-- <field name="ks_default_icon">truck</field> -->
            <field name="ks_dashboard_item_theme">white</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#d95551,0.99</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_inventory_item_14" model="ks_dashboard_ninja.item">
            <field name="name">Receipts</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_actions" eval="ref('equip3_inventory_operation.stock_picking_receiving_note')"/>
            <field name="ks_model_id" eval="ref('stock.model_stock_picking')"/>
            <field name="ks_icon" type="base64" file="equip3_inventory_dashboard/static/src/description/images/receiving.png"/>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_domain">[["picking_type_id.code","=","incoming"],["state","=","done"]]</field>
            <field name="ks_date_filter_field" eval="ref('stock.field_stock_picking__create_date')"/>
            <!-- <field name="ks_default_icon">check</field> -->
            <field name="ks_dashboard_item_theme">white</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#006641,0.99</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_inventory_item_15" model="ks_dashboard_ninja.item">
            <field name="name">Alert Expiry</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('stock.model_stock_production_lot')"/>
            <field name="ks_icon" type="base64" file="equip3_inventory_dashboard/static/src/description/images/expiry_alert.png"/>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_date_filter_field" eval="ref('product_expiry.field_stock_production_lot__alert_date')"/>
            <field name="ks_date_filter_selection">t_month</field>
            <!-- <field name="ks_default_icon">calendar-times-o</field> -->
            <field name="ks_dashboard_item_theme">white</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#d95551,0.99</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <!-- Single Inventory Demo Dashboard Templates-->

        <record id="inventory_dashboard_new" model="ks_dashboard_ninja.board">
            <field name="name">Inventory Dashboard</field>
            <field name="ks_dashboard_menu_name">Inventory Dashboard</field>
            <field name="ks_dashboard_top_menu_id" eval="ref('stock.menu_stock_root')"/>
            <field name="ks_dashboard_default_template" eval="ref('equip3_inventory_dashboard.ks_inventory')"/>
            <field name="ks_dashboard_active">1</field>
            <field name="ks_dashboard_group_access" eval="False"/>
            <field name="ks_dashboard_menu_sequence" eval="-5"/>
        </record>

        <record id="ks_inventory_dashboard.demo_inventory_dashboard" model="ks_dashboard_ninja.board">
            <field name="ks_dashboard_active">0</field>
        </record>
    </data>
</odoo>
