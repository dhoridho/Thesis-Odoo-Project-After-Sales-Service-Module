<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_employee_code" model="ir.sequence">
            <field name="name">Employee Sequence Code</field>
            <field name="code">hr.employee</field>
            <field name="prefix">EMP/</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_company_document" model="ir.sequence">
            <field name="name">Company Document Sequence</field>
            <field name="code">hr.company.document</field>
            <field name="prefix">CPD/%(day)s/%(month)s/%(range_y)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
