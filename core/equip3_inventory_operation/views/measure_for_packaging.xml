<odoo>
    <data>

        <record model="ir.ui.view" id="measure_for_packaging_tree_view">
            <field name="name">measure.for.packaging.tree</field>
            <field name="model">measure.for.packaging</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="measure"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="measure_for_packaging_form_view">
            <field name="name">measure.for.packaging.form</field>
            <field name="model">measure.for.packaging</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="measure"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="measure_for_packaging_action" model="ir.actions.act_window">
        <field name="name">Measure For Packaging</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">measure.for.packaging</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- <menuitem
        id="measure_for_packaging_menu"
        name="Measure Packaging"
        parent="equip3_inventory_masterdata.approval_matrix_material_configuration_menu"
        action="measure_for_packaging_action"
        sequence="500"/> -->

    <record model="ir.ui.menu" id="measure_for_packaging_menu">
        <field name="name">Measure For Packaging</field>
        <field name="parent_id" ref="stock.menu_warehouse_config"/>
        <field name="action" ref="measure_for_packaging_action"/>
        <field name="sequence" eval="17"/>
    </record>


    </data>
</odoo>