<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="seq_internal_transfer" model="ir.sequence">
            <field name="name">Internal Transfer</field>
            <field name="code">internal.transfer</field>
            <field name="prefix">ITR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range">True</field>
            <field name="range_reset">yearly</field>
        </record>

        <record id="seq_material_request" model="ir.sequence">
            <field name="name">Material Request</field>
            <field name="code">material.request</field>
            <field name="prefix">MR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
            <field name="range_reset">yearly</field>
        </record>

        <record id="seq_svl_line_mark_id" model="ir.sequence">
            <field name="name">SVL Line Mark ID</field>
            <field name="code">svl.line.mark.id</field>
            <field name="prefix"></field>
            <field name="padding">1</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
