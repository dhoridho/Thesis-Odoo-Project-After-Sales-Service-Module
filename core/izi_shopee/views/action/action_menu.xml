<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="izi_shopee.action_window_sp_shop" model="ir.actions.act_window">
      <field name="name">Shop</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.shopee.shop</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="izi_shopee.action_window_sp_logistic" model="ir.actions.act_window">
      <field name="name">Logistic</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.shopee.logistic</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="izi_shopee.action_window_sp_promotion" model="ir.actions.act_window">
      <field name="name">Promotion</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.promotion.program</field>
      <field name="view_mode">tree,form</field>
      <field name="domain">[('marketplace', '=', 'shopee')]</field>
      <field name="context">{
        'default_marketplace': 'shopee',
      }</field>  
    </record>
    <record id="izi_shopee.action_window_sp_webhook_order" model="ir.actions.act_window">
      <field name="name">Webhook Order</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.webhook.order</field>
      <field name="view_mode">tree,form</field>
      <field name="domain">[('marketplace', '=', 'shopee')]</field>
      <field name="context">{
        'default_marketplace': 'shopee',
        "search_default_group_by_mp_account": 1,
      }</field>
    </record>
    <record id="izi_shopee.action_window_mp_stock" model="ir.actions.act_window">
        <field name="name">Marketplace Stock</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">mp.stock</field>
        <field name="view_mode">tree</field>
        <field name="domain">[('marketplace', '=', 'shopee')]</field>
        <field name="context">{
          'create': False,
          'delete': False,
          'default_marketplace': 'shopee',
          "search_default_group_by_mp_account": 1,
        }</field>
    </record>
    <record id="izi_shopee.action_window_sp_category" model="ir.actions.act_window">
      <field name="name">Product Category</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">mp.shopee.category</field>
      <field name="view_mode">tree</field>
    </record>
  </data>
</odoo>