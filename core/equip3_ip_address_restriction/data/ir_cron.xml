<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record forcecreate="True" id="cron_auto_logout" model="ir.cron">
            <field name="name">Auto Logout</field>
            <field name="model_id" ref="equip3_ip_address_restriction.model_res_users"/>
            <field name="code">model._validate_sessions()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="True" name="doall" />
        </record>

        <record forcecreate="True" id="cron_remove_history" model="ir.cron">
            <field name="name">Remove Old Log Login</field>
            <field name="model_id" ref="equip3_ip_address_restriction.model_res_users"/>
            <field name="code">model._remove_log_login(200)</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="True" name="doall" />
        </record>
    </data>
</odoo>
