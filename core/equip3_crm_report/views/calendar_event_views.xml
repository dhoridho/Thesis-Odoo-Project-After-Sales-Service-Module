<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_calendar_event_form" model="ir.ui.view">
        <field name="model">calendar.event</field>
        <field name="inherit_id" ref="calendar.view_calendar_event_form"/>
        <field name="arch" type="xml">
            <field name="start" position="after">
                <field name="meeting_count" invisible="1"/>
                <field name="duration_count" invisible="1"/>
                <field name="reschedule_count" invisible="1"/>
                <field name="done_count" invisible="1"/>
                <field name="cancel_count" invisible="1"/>
            </field>
        </field>
    </record>

</odoo>