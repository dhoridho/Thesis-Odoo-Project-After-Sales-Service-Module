<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="izi_marketplace.search_mp_stock" model="ir.ui.view">
      <field name="name">Marketplace Stock Search View</field>
      <field name="model">mp.stock</field>
      <field name="arch" type="xml">
        <search string="Marketplace Stock">
          <group name="filter_per_mp"/>
          <separator/>
          <filter name="group_by_marketplace" string="Marketplace" context="{'group_by': 'marketplace'}"/>
          <filter name="group_by_mp_account" string="MP Account" context="{'group_by': 'mp_account_id'}"/>
        </search>
      </field>
    </record>
    <record id="izi_marketplace.tree_mp_stock" model="ir.ui.view">
      <field name="name">Marketplace Stock Tree View</field>
      <field name="model">mp.stock</field>
      <field name="arch" type="xml">
        <tree string="Marketplace Stock" nocreate="1" create="false">
          <field name="product_id" readonly="1"/>
          <field name="marketplace" readonly="1"/>
          <field name="mp_account_id" readonly="1"/>
          <field name="warehouse_id" readonly="1"/>
          <field name="stock" readonly="1"/>
        </tree>
      </field>
    </record>
  </data>
</odoo>