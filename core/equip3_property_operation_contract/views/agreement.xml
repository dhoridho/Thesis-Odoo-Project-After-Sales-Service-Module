<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- agreement tree view -->
        <record id="property_agreement_tree_view" model="ir.ui.view">
            <field name="name">agreement.view.tree</field>
            <field name="model">agreement</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code" string="Reference" />
                    <field name="name"/>
                    <field name="stage_id"/>
                    <field name="start_date"/>
                    <field name="end_date"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>