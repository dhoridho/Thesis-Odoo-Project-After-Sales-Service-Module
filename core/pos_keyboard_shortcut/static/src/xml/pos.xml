<templates id="template" xml:space="preserve">
    <t t-name="CashierName" t-inherit="point_of_sale.CashierName" t-inherit-mode="extension" owl="1">
        <xpath expr="//span" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>

    <t t-name="SyncNotification" t-inherit="point_of_sale.SyncNotification" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('oe_status')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>

    <t t-name="HeaderButton" t-inherit="point_of_sale.HeaderButton" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('header-button') and hasclass('close_button')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>
    
    <t t-name="ClientListScreen" t-inherit="point_of_sale.ClientListScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('button') and hasclass('back')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
        <xpath expr="//div[hasclass('button') and hasclass('new-customer')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>

    <t t-name="PaymentScreen" t-inherit="point_of_sale.PaymentScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('button') and hasclass('back')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
        <!-- <xpath expr="//div[hasclass('button') and hasclass('js_invoice')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath> -->
        <!-- <xpath expr="//div[hasclass('customer-button')]//div[hasclass('button')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath> -->
        <xpath expr="//div[hasclass('button') and hasclass('next')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>

    <t t-name="PaymentMethodButton" t-inherit="point_of_sale.PaymentMethodButton" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('button') and hasclass('paymentmethod')]" position="inside">
            <span class="hidden_tags"></span>
            <span class="payment_id" t-att-data-payment-id="props.paymentMethod.id"></span>
        </xpath>
    </t>

    <t t-name="ReceiptScreen" t-inherit="point_of_sale.ReceiptScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('button') and hasclass('next')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
        <xpath expr="//div[hasclass('button') and hasclass('print')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>
    
    <t t-name="ActionpadWidget" t-inherit="point_of_sale.ActionpadWidget" t-inherit-mode="extension" owl="1">
        <xpath expr="//button[hasclass('button') and hasclass('set-customer')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
        <xpath expr="//button[hasclass('button') and hasclass('pay')]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>

    <t t-name="NumpadWidget" t-inherit="point_of_sale.NumpadWidget" t-inherit-mode="extension" owl="1">
        <xpath expr="//button[@class='mode-button'][1]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
        <xpath expr="//button[@class='mode-button'][2]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
        <xpath expr="//button[@class='mode-button'][3]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>

    <t t-name="TicketButton" t-inherit="point_of_sale.TicketButton" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('ticket-button')]//div[2]" position="inside">
            <span class="hidden_tags"></span>
        </xpath>
    </t>
</templates>