<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_pos" name="Assets POS" inherit_id="equip3_pos_masterdata.assets_pos">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/equip3_pos_emenu/static/src/css/pos_custom.css"/>
            <link rel="stylesheet" href="/equip3_pos_emenu/static/src/css/pos_responsive.css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Core/BigData.js"/>
            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Chrome.js"/>
            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/pos_order.js"/>

            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/ChromeWidgets/TicketButton.js"/>

            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Screens/Report/EmenuQRCodeLabel.js"/>
            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Popups/EmenuQrCodePopup.js"/>

            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Screens/ProductScreen/EmenuQrOrderButton.js"/>
            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Screens/ProductScreen/ControlButtons/SubmitOrderButton.js"/>
            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Screens/ProductScreen/ControlButtons/TakeAwayButton.js"/>

            <script type="text/javascript" src="/equip3_pos_emenu/static/src/js/Screens/Ticket/TicketScreen.js"/>
        </xpath>
    </template>

</odoo>