<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="EmenuQrCodePopup" owl="1">
        <div role="dialog" class="modal-dialog emenu_qrcode_popups">
            <Draggable>
                <div class="popup popup-textarea" >
                    <header class="title drag-handle">
                        <t t-esc="props.title" />

                        <div class="close" t-on-click="cancel">
                            <i class="fa fa-times"/>
                        </div>
                    </header>
                    <div class="emenu_qrcode_content">
                        <EmenuQRCodeLabel 
                            printed_date="props.printed_date" 
                            emenu_url="props.emenu_url"/>
                    </div>
                    <footer class="footer">
                        <!-- <div class="button cancel" t-on-click="cancel">
                            <t t-esc="props.cancelText" />
                        </div> -->
                        <div class="button print_qrcode" t-on-click="print_qrcode">
                            <t t-esc="props.confirmText" />
                        </div>
                    </footer>
                </div>
            </Draggable>
        </div>
    </t>

</templates>