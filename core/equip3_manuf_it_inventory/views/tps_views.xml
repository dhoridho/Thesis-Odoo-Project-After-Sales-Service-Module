<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="ceisa_storehouse_location_view_search" model="ir.ui.view">
		<field name="name">ceisa.storehouse.location.search</field>
		<field name="model">ceisa.storehouse.location</field>
		<field name="arch" type="xml">
			<search string="TPS">
				<field name="name"/>
			</search>
		</field>
	</record>
	<record id="view_storehouse_location_documents_tree" model="ir.ui.view">
		<field name="name">Tempat Penimbunan Sementara (TPS)</field>
		<field name="model">ceisa.storehouse.location</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Tempat Penimbunan Sementara (TPS)" editable="bottom">
				<field name="code" required="1"/>
				<field name="name" required="1"/>
			</tree>
		</field>
	</record>
    <record id="action_tps_master_data" model="ir.actions.act_window">
		<field name="name">Tempat Penimbunan Sementara (TPS)</field>
		<field name="res_model">ceisa.storehouse.location</field>
		<field name="view_mode">tree,form</field>
		<field name="search_view_id" ref="equip3_manuf_it_inventory.ceisa_storehouse_location_view_search"/>
		<field name="view_id" eval="False"/>
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
			Create a new Tempat Penimbunan Sementara (TPS) datas
			</p><p>
			Notes: All the datas must be syncronize with beacukai data...
			</p>
		</field>
	</record>
</odoo>