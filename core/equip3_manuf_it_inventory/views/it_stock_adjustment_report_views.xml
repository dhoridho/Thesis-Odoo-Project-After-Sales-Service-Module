<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_it_stock_adjustment_report_tree" model="ir.ui.view">
        <field name="name">view.it.stock.adjustment.report.tree</field>
        <field name="model">it.stock.adjustment.report</field>
        <field name="arch" type="xml">
            <tree string="Laporan Stock Adjustment" js_class="it_list_view_report">
                <field name="sequence" invisible="1"/>
                <field name="date_done"/>
                <field name="product_code"/>
                <field name="product_id"/>
                <field name="product_type"/>
                <field name="quantity"/>
                <field name="uom_id"/>
                <field name="stock_on_hand"/>
                <field name="first_balance"/>
                <field name="income"/>
                <field name="outcome"/>
                <field name="adjustment"/>
                <field name="last_balance"/>
                <field name="stock_enumeration"/>
                <field name="difference"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="action_view_it_stock_adjustment_report_set_default" model="ir.actions.server">
        <field name="name">Stock Adjustment Reports Default Context</field>
        <field name="model_id" ref="equip3_manuf_it_inventory.model_it_stock_adjustment_report"/>
        <field name="state">code</field>
        <field name="code">action = model.get_default_action('action_view_it_stock_adjustment_report')</field>
    </record>

    <record id="action_view_it_stock_adjustment_report" model="ir.actions.act_window">
        <field name="name">Laporan Data Adjustment</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">it.stock.adjustment.report</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
			<p class="o_view_nocontent_empty_folder">
				No data to display
			</p>
			<p>
				Some description about this report.
			</p>
		</field>
    </record>

</odoo>