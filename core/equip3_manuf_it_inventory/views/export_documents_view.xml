<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="form_export_ceisa_documents" model="ir.ui.view">
            <field name="name">Export Documents Form View</field>
            <field name="model">ceisa.export.documents</field>
            <field name="arch" type="xml">
                <form string="Export Documents" nocreate="1" create="false">
                    <sheet>
                        <widget name="web_ribbon" title="Sent" attrs="{'invisible': [('sent_state', '=', False)]}"/>
                        <div class="oe_title">
                            <label class="oe_edit_only" for="name"/>
                            <h1>
                                <field name="name" placeholder="Document Name" readonly="1"/>
                            </h1>
                        </div>
                        <group>
                            <field name="sent_state" invisible="1"/>
<!--                            <field name="picking_id" invisible="1"/>-->
                            <field name="document_type_id" invisible="1" default="30"/>
                            <group>
                                <label for="picking_id" string="Delivery Order"/>
                                <div class="o_row" name="picking_id">
                                    <field name="picking_id" required="1" domain="['&amp;', ('state', 'in', ['confirmed', 'assigned']), ('picking_type_code', '=', 'outgoing')]"/>
                                </div>
                                <label for="no_aju"/>
                                <div class="o_row" name="no_aju">
                                    <field name="no_aju"/>
                                    <field name="aju_date"/>
                                </div>
                                <label for="origin_beacukai_office"/>
                                <div class="o_row" name="origin_beacukai_office">
                                    <field name="origin_beacukai_office" options="{'no_create': True}" required="1"/>
                                </div>
                                <label for="origin_port_export"/>
                                <div class="o_row" name="origin_port_export">
                                    <field name="origin_port_export" options="{'no_create': True}"/>
                                </div>
                                <label for="origin_pabean_export_office"/>
                                <div class="o_row" name="origin_pabean_export_office">
                                    <field name="origin_pabean_export_office" options="{'no_create': True}"/>
                                </div>
                                <label for="komoditi"/>
                                <div class="o_row" name="komoditi">
                                    <field name="komoditi"/>
                                </div>
                                <label for="curah"/>
                                <div class="o_row" name="curah">
                                    <field name="curah"/>
                                </div>
                                <label for="disclaimer"/>
                                <div class="o_row" name="disclaimer">
                                    <field name="disclaimer"/>
                                </div>
                            </group>
                            <group>
                                <label for="export_type"/>
                                <div class="o_row" name="export_type">
                                    <field name="export_type" options="{'no_create': True}"/>
                                </div>
                                <label for="export_category"/>
                                <div class="o_row" name="export_category">
                                    <field name="export_category" options="{'no_create': True}"/>
                                </div>
                                <label for="trade_way_id"/>
                                <div class="o_row" name="trade_way_id">
                                    <field name="trade_way_id" options="{'no_create': True}"/>
                                </div>
                                <label for="payment_term"/>
                                <div class="o_row" name="payment_term">
                                    <field name="payment_term"/>
                                    <field name="payment_code" attrs="{'required': [('payment_term','=','9')], 'readonly': [('payment_term', '!=', '9')]}"/>
                                </div>
                            </group>
                        </group>
                        <notebook>
                            <page name="pkb_information" string="PKB">
                                <group>
                                    <group name="facilities">
                                        <field name="facility_kite"/>
                                        <field name="zoning_kite"/>
                                        <field name="pkb_date"/>
                                        <field name="pic_address"/>
                                        <field name="pic_phone"/>
                                        <field name="pic_name"/>
                                        <field name="product_type"/>
                                        <field name="warehouse_type"/>
                                    </group>
                                    <group name="stuffing_information">
                                        <field name="container_20"/>
                                        <field name="container_40"/>
                                        <field name="container_way"/>
                                        <field name="partof_type"/>
                                        <field name="investigate_date"/>
                                        <field name="pic_location"/>
                                    </group>
                                </group>
                            </page>
                            <page name="entitas_information" string="Entitas">
                                <group>
                                    <group string="Exportir" name="exim_information" priority="5">
                                        <label for="exim_identity_type" string="Jenis Identitas"/>
                                        <div class="o_row" name="exim_identity_type">
                                            <field name="exim_identity_type" options="{'no_create': True}"/>
                                            <field name="exim_identity_number"/>
                                        </div>
                                        <field name="exim_partner_id" string="Nama Partner" options="{'no_create': True}"/>
                                        <field name="exim_nib_entitas" string="NIB Entitas"/>
                                        <field name="exim_address" string="Alamat"/>
                                        <field name="exim_country" string="Kode Negara"/>
                                    </group>
                                    <group string="Pemilik" name="owner_entitas_information" priority="5">
                                        <label for="owner_identity_type" string="Jenis Identitas"/>
                                        <div class="o_row" name="owner_identity_type">
                                            <field name="owner_identity_type" options="{'no_create': True}"/>
                                            <field name="owner_identity_number"/>
                                        </div>
                                        <field name="owner_partner_id" string="Nama Partner" options="{'no_create': True}"/>
                                        <field name="owner_address" string="Alamat"/>
                                        <field name="owner_country" string="Kode Negara"/>
                                    </group>
                                    <group string="Penerima" name="recipient_entitas_information" priority="5">
                                        <label for="recipient_identity_type" string="Jenis Identitas"/>
                                        <div class="o_row" name="recipient_identity_type">
                                            <field name="recipient_identity_type" options="{'no_create': True}"/>
                                            <field name="recipient_identity_number"/>
                                        </div>
                                        <field name="recipient_partner_id" string="Nama Partner" options="{'no_create': True}"/>
                                        <field name="recipient_address" string="Alamat"/>
                                        <field name="recipient_country" string="Kode Negara"/>
                                    </group>
                                    <group string="Pembeli" name="buyer_entitas_information" priority="5">
                                        <label for="buyer_identity_type" string="Jenis Identitas"/>
                                        <div class="o_row" name="buyer_identity_type">
                                            <field name="buyer_identity_type" options="{'no_create': True}"/>
                                            <field name="buyer_identity_number"/>
                                        </div>
                                        <field name="buyer_partner_id" string="Nama Partner" options="{'no_create': True}"/>
                                        <field name="buyer_address" string="Alamat"/>
                                        <field name="buyer_country" string="Kode Negara"/>
                                    </group>
                                </group>
                                <field name="entitas_line_id">
                                    <tree string="Entitas Information" editable="bottom">
                                        <field name="code" required="True" domain="[('code', 'not in', ['1', '2', '6', '7', '8'])]"/>
                                        <field name="identity_type" attrs="{'required': [('code','in',['4', '7', '11'])]}"/>
                                        <field name="number" attrs="{'required': [('code','in',['4', '7', '11'])]}"/>
                                        <field name="name" required="True" />
                                        <field name="address" required="True"/>
                                        <field name="country_id" required="True"/>
                                    </tree>
                                </field>
                            </page>
                            <page name="document" string="Document">
                                <group>
                                    <field name="document_line_id">
                                        <tree string="Dokumen Lampiran" editable="bottom">
                                            <field name="type"/>
                                            <field name="number"/>
                                            <field name="doc_date"/>
                                            <field name="facility"/>
                                            <field name="izin"/>
                                            <field name="office"/>
                                            <field name="file"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                            <page name="pengangkut" string="Pengangkut">
                                <group>
                                    <group name="penimbunan">
                                        <label for="storehouse_location"/>
                                        <div name="storehouse_location" class="o_row">
                                            <field name="storehouse_location" domain="[('beacukai_office_id', '=', origin_beacukai_office)]" options="{'no_create': True}"/>
                                        </div>
                                        <label for="origin_port_office"/>
                                        <div name="origin_port_office" class="o_row">
                                            <field name="origin_port_office" options="{'no_create': True}"/>
                                        </div>
                                        <field name="origin_port_export" options="{'no_create': True}"/>
                                        <label for="destination_unloading_port"/>
                                        <div name="destination_unloading_port" class="o_row">
                                            <field name="destination_unloading_port" options="{'no_create': True}"/>
                                        </div>
                                        <field name="destination_port_office" options="{'no_create': True}"/>
                                        <field name="destination_country" options="{'no_create': True}"/>
                                        <field name="exim_date_estimation"/>
                                    </group>
                                    <group name="pemeriksaan">
                                        <field name="inspection_location"/>
                                        <field name="inspection_date"/>
                                        <field name="inspection_office"/>
                                    </group>
                                </group>
                                <field name="transportation_line_id">
                                    <tree string="Sarana Angkut" editable="bottom">
                                        <field name="name"/>
                                        <field name="number"/>
                                        <field name="transport_type"/>
                                        <field name="country_id"/>
                                    </tree>
                                </field>
                            </page>
                            <page name="kemasan_peti_kemas" string="Kemasan dan Peti Kemas">
                                <group>
                                    <group name="kemasan">
                                        <field name="package_line_id">
                                            <tree string="Kemasan" editable="bottom">
                                                <field name="value"/>
                                                <field name="package_id"/>
                                                <field name="merek"/>
                                            </tree>
                                        </field>
                                    </group>
                                    <group name="petikemas">
                                        <field name="container_line_id">
                                            <tree string="Peti Kemas" editable="bottom">
                                                <field name="number"/>
                                                <field name="size"/>
                                                <field name="category"/>
                                                <field name="type"/>
                                            </tree>
                                        </field>
                                    </group>
                                </group>
                            </page>
                            <page name="transaction" string="Transaksi">
                                <group>
                                    <group name="transaction_information">
                                        <field name="valuta_id"/>
                                        <label for="ndpbm"/>
                                        <div name="ndpbm" class="o_row">
                                            <field name="ndpbm" default="1.00"/>
                                        </div>
                                        <field name="cara_penyerahan"/>
                                        <field name="nilai_export" default="0.01"/>
                                        <field name="freight" default="0.01"/>
                                        <field name="insurance_type"/>
                                        <field name="insurance_value" default="0.01"/>
                                        <field name="weight_bruto_kgm" default="0.01"/>
                                        <field name="weight_netto_kg" default="0.01"/>
                                    </group>
                                    <group name="transaction_information_right">
                                        <field name="maklon_value" default="0.01"/>
                                        <field name="bea_out_value" default="0.01"/>
                                        <field name="pph_bea_keluar" default="0.01"/>
                                        <field name="sawit_tax_value" default="0.01"/>
                                    </group>
                                    <group name="bank_devisa">
                                        <field name="bank_payment_id"/>
                                    </group>
                                </group>
                            </page>
                            <page string="barang" name="Barang">
                                <button name="action_add_new_product" string="Add Product" class="oe_highlight" type="object"/>
                                <group name="product_information">
                                    <field name="product_line_ids" mode="tree" options="{'reload_whole_on_button': true}">
                                        <tree string="Barang" create="0" no_create_edit="1" delete="1" edit="false">
                                            <field name="product_hs"/>
                                            <field name="product_id"/>
                                            <field name="fob_price" default="0.01"/>
                                            <field name="product_qty"/>
                                            <field name="product_uom"/>
                                        </tree>
                                        <form string="Add New Products" create="false">
                                            <group>
                                                <group>
                                                    <field name="product_hs"/>
                                                    <field name="code"/>
                                                    <field name="product_id"/>
                                                    <field name="name"/>
                                                    <field name="merk"/>
                                                    <field name="product_type"/>
                                                    <field name="product_size"/>
                                                    <field name="origin_country"/>
                                                    <field name="origin_city"/>
                                                </group>
                                                <group>
                                                    <field name="product_qty"/>
                                                    <field name="product_uom"/>
                                                    <field name="package_qty"/>
                                                    <field name="package_type"/>
                                                    <field name="fob_price" default="0.01"/>
                                                    <field name="volume" default="0.01"/>
                                                    <field name="netto_weight" default="0.01"/>
                                                    <field name="fob_uom_price" default="0.01"/>
                                                </group>
                                            </group>
                                            <field name="export_document_line_id">
                                                <tree string="Dokumen Lampiran" editable="bottom">
                                                    <field name="type"/>
                                                    <field name="number"/>
                                                    <field name="doc_date"/>
                                                    <field name="facility"/>
                                                    <field name="izin"/>
                                                    <field name="office"/>
                                                    <field name="file"/>
                                                </tree>
                                            </field>
                                        </form>
                                    </field>
                                </group>
                            </page>
                            <page string="pungutan" name="Pungutan">
                                <group>
                                    <group name="tax_information">
                                        <field name="tax_line_id">
                                            <tree string="Pungutan" editable="bottom">
                                                <field name="tax_value"/>
                                                <field name="payment_tax"/>
                                                <field name="government_tax"/>
                                                <field name="pending_tax"/>
                                                <field name="untax_value"/>
                                                <field name="free_tax"/>
                                                <field name="paid_tax"/>
                                            </tree>
                                        </field>
                                    </group>
                                </group>
                            </page>
                            <page string="pernyataan" name="Pernyataan">
                                <group>
                                    <group name="dateplace">
                                        <field name="place_statement"/>
                                        <field name="date_statement"/>
                                    </group>
                                    <group name="jobname">
                                        <field name="name_statement"/>
                                        <field name="job_statement"/>
                                    </group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="ceisa_export_documents_view_search" model="ir.ui.view">
            <field name="name">ceisa.export.documents.search</field>
            <field name="model">ceisa.export.documents</field>
            <field name="arch" type="xml">
                <search string="Blog">
                    <field name="name"/>
                    <filter string="Type" name="type" domain="[('type','=','export')]"/>
                </search>
            </field>
        </record>
        <record id="view_ceisa_export_documents_tree" model="ir.ui.view">
            <field name="name">Export Documents</field>
            <field name="model">ceisa.export.documents</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Dokumen Ekspor">
                    <field name="name"/>
                    <field name="no_aju"/>
                    <field name="aju_date"/>
                    <field name="export_category"/>
                    <field name="export_type"/>
                    <field name="origin_beacukai_office"/>
                </tree>
            </field>
        </record>
        <record id="action_export_documents_view" model="ir.actions.act_window">
            <field name="name">Dokumen Ekspor</field>
            <field name="res_model">ceisa.export.documents</field>
            <field name="view_mode">form</field>
<!--            <field name="domain">[('type', '=', 'export')]</field>-->
            <field name="context">{'default_picking_id': active_id}</field>
            <field name="view_id" ref="equip3_manuf_it_inventory.form_export_ceisa_documents" />
        </record>
        <record id="action_ceisa_export_documents_mainview" model="ir.actions.act_window">
            <field name="name">Dokumen Ekspor</field>
            <field name="res_model">ceisa.export.documents</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="equip3_manuf_it_inventory.ceisa_export_documents_view_search"/>
<!--            <field name="domain">[('type', '=', 'export')]</field>-->
            <field name="view_id" eval="False"/> <!-- Force empty -->
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                Create a new export documents
                </p>
		    </field>
        </record>
    </data>
</odoo>