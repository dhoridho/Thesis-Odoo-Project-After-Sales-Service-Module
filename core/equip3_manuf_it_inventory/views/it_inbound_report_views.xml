<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_it_inbound_report_tree" model="ir.ui.view">
        <field name="name">view.it.inbound.report.tree</field>
        <field name="model">it.inbound.report</field>
        <field name="arch" type="xml">
            <tree string="Laporan Pemasukan Barang" js_class="it_list_view_report_bound">
                <field name="sequence" invisible="1"/>
                <field name="doc_type_id"/>
                <field name="registration_number"/>
                <field name="registration_date"/>
                <field name="request_number"/>
                <field name="date_done"/>
                <field name="partner_id"/>
                <field name="product_code"/>
                <field name="product_id"/>
                <field name="uom_id"/>
                <field name="quantity"/>
                <field name="value"/>
                <field name="warehouse_id" invisible="1"/>
            </tree>
        </field>
    </record>

    <record id="action_view_it_inbound_report_set_default" model="ir.actions.server">
        <field name="name">Inbound Reports Default Context</field>
        <field name="model_id" ref="equip3_manuf_it_inventory.model_it_inbound_report"/>
        <field name="state">code</field>
        <field name="code">action = model.get_default_action('action_view_it_inbound_report')</field>
    </record>

    <record id="action_view_it_inbound_report" model="ir.actions.act_window">
<!--        <field name="name">Inbound Reports</field>-->
        <field name="name">Laporan Pemasukan Barang</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">it.inbound.report</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
			<p class="o_view_nocontent_empty_folder">
				No data to display
			</p>
			<p>
				Some description about this report.
			</p>
		</field>
    </record>

</odoo>