<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_it_raw_material_mutation_tree" model="ir.ui.view">
        <field name="name">view.it.raw.material.mutation.tree</field>
        <field name="model">it.raw.material.mutation</field>
        <field name="arch" type="xml">
            <tree string="Mutation of Raw Materials" js_class="it_list_view_report">
                <field name="sequence" invisible="1"/>
                <field name="product_code"/>
                <field name="product_id"/>
                <field name="uom_id"/>
                <field name="stock_on_hand"/>
                <field name="first_balance" />
                <field name="income"/>
                <field name="outcome"/>
                <field name="adjustment"/>
                <field name="last_balance"/>
                <field name="stock_opname"/>
                <field name="difference"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="action_view_it_raw_material_mutation_set_default" model="ir.actions.server">
        <field name="name">Mutation of Raw Materials Default Context</field>
        <field name="model_id" ref="equip3_manuf_it_inventory.model_it_raw_material_mutation"/>
        <field name="state">code</field>
        <field name="code">action = model.get_default_action('action_view_it_raw_material_mutation')</field>
    </record>

    <record id="action_view_it_raw_material_mutation" model="ir.actions.act_window">
        <field name="name">Mutation of Raw Materials</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">it.raw.material.mutation</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
			<p class="o_view_nocontent_empty_folder">
				No data to display
			</p>
			<p>
				Some description about this report.
			</p>
		</field>
    </record>

</odoo>