<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="paperformat_it_inventory_list_report" model="report.paperformat">
			<field name="name">It Inventory Reports</field>
			<field name="format">A4</field>
			<field name="orientation">Landscape</field>
			<field name="margin_top">35</field>
			<field name="margin_bottom">20</field>
            <field name="margin_left">20</field>
			<field name="margin_right">20</field>
			<field name="header_line" eval="True"/>
			<field name="header_spacing">25</field>
			<field name="dpi">90</field>
		</record>

        <record id="action_it_inventory_list_report" model="ir.actions.report">
            <field name="name">IT Inventory Report</field>
            <field name="model">it.inventory.list.report</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">equip3_manuf_it_inventory.it_inventory_list_report</field>
            <field name="report_file">equip3_manuf_it_inventory.it_inventory_list_report</field>
            <field name="print_report_name">'IT Inventory Report'</field>
            <field name="binding_model_id" ref="model_it_inventory_list_report"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="equip3_manuf_it_inventory.paperformat_it_inventory_list_report"/>
        </record>

    </data>
</odoo>