<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- account.move inherit form view -->
        <record id="account_move_sale_consignment_view_form_inherit" model="ir.ui.view">
            <field name="name">account_move_sale_consignment_view_form_inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="sale_consignment_id" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>