<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="EmployeeMealListWidget" owl="1">
        <div class="custom_column btn-employee-meal" t-on-click="onClick">
            <span style="border-radius: 3px">
                <span>Employee List</span>
            </span>
        </div>
    </t>

    <t t-name="EmployeeMealProductScreenSection1Ext" t-inherit="equip3_pos_general.ProductScreenSection1" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('custom_row3')]" position="after">
            <div class="custom_row custom_row4 employee-meal-row" style="display:none;"> 
                <EmployeeMealListWidget/>
            </div>
        </xpath>
        <xpath expr="//div[hasclass('custom_row_mobile')]" position="inside">
            <div class="employee-meal-row mobile-employee-meal-row" style="display:none;"> 
                <EmployeeMealListWidget/>
            </div>
        </xpath>
    </t>

</templates>
