<?xml version="1.0" encoding="UTF-8" ?>
<templates id="template" xml:space="preserve">

    <t t-name="InsightWidget">
        <div class="o_box_wrapper">
            <div class="o_box_header">
                <div t-attf-class="o_box_title {{ !widget.isDebug &amp;&amp; widget.titleAlign === 'center' ? 'm-auto' : '' }}">
                    <t t-esc="widget.label"/>
                </div>
                <div class="o_box_debug" t-if="widget.isDebug">
                    <div class="o_box_config btn-group" title="Configuration">
                        <button type="button" class="btn o_config_toggler" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="fa fa-lg fa-cog"/>
                        </button>
                        <div class="dropdown-menu dropdown-menu-right o_config_dropdown">
                            <t t-foreach="widget.editors" t-as="cfg">
                                <button class="o_config dropdown-item" type="button" t-att-data-editor="cfg" t-if="cfg_value.visible">
                                    <t t-esc="cfg_value.label"/>
                                </button>
                            </t>
                        </div>
                    </div>
                    <div class="o_box_demo" title="Demo Data">
                        <div class="o_demo_checkbox o_field_boolean o_boolean_toggle o_field_widget custom-control custom-checkbox">
                            <input type="checkbox" class="o_demo_input custom-control-input"/>
                            <label class="custom-control-label"/>
                        </div>
                    </div>
                </div>
            </div>
            <div class="o_box_content"/>
        </div>
    </t>

    <t t-name="InsightEditor">
        <div class="o_box_editor d-none" t-att-data-editor="configName">
            <div class="o_editor"/>
            <div class="o_buttons">
                <button class="btn btn-sm btn-primary o_btn_editor mr-2" data-action="save">Save</button>
                <button class="btn btn-sm btn-primary o_btn_editor mr-2" data-action="reset">Reset</button>
                <button class="btn btn-sm btn-secondary o_btn_editor" data-action="discard">Discard</button>
            </div>
        </div>
    </t>

    <t t-name="InsightTotalClosingContent">
        <div class="o_content_wrapper">
            <t t-set="value" t-value="widget.data['this_' + range_type].value"/>
            <t t-set="percentage" t-value="widget.data['this_' + range_type].percentage"/>
            <t t-set="status" t-value="percentage > 0 ? 'up' : 'down'"/>

            <div t-attf-class="o_icon o_{{ status }}">
