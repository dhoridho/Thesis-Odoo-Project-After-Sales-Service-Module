            <div class="o_info">
                <div class="o_info_title">This <t t-esc="range_type === 'month' ? 'Month' : 'Quarter'"/></div>
                <div class="o_info_value"><t t-esc="widget._formatCurrency(value)"/></div>
                <div class="o_info_percentage">
                    <span t-attf-class="o_{{ status }}">
                        <span t-attf-class="fa fa-arrow-{{ status }}"/>
                        <b><t t-esc="Math.abs(percentage)"/>%</b>
                    </span> than last <t t-esc="range_type"/>.
                </div>
            </div>
        </div>
    </t>

    <t t-name="InsightTotalClosing">
        <div class="o_total_closing_content">
            <div class="o_content_section">
                <t t-call="InsightTotalClosingContent">
                    <t t-set="range_type" t-value="'month'"/>
                </t>
            </div>
            <div class="o_separator">
                <div class="o_line"/>
            </div>
            <div class="o_content_section">
                <t t-call="InsightTotalClosingContent">
                    <t t-set="range_type" t-value="'quarter'"/>
                </t>
            </div>
        </div>
    </t>

    <t t-name="InsightTrending">
        <div class="o_trending_content">
            <t t-foreach="widget.data" t-as="text">
                <div class="o_item">
                    <div class="o_dot"/>
                    <div class="o_text">
                        <t t-esc="text"/>
                    </div>
                </div>
            </t>
        </div>
    </t>

