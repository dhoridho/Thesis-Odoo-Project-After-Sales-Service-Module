            <div class="o_circle_info">
                <div class="o_info o_left">
                    <t t-set="left_status" t-value="widget.data.left.percentage > 0 ? 'up' : 'down'"/>
                    <div class="o_value"><t t-esc="widget.data.left.this_value"/></div>
                    <div t-attf-class="o_percentage o_{{ left_status }}">
                        <t t-if="widget.data.left.percentage !== undefined">
                            <span t-attf-class="fa fa-arrow-{{ left_status }}"/>
                            <b><t t-esc="Math.abs(widget.data.left.percentage)"/>%</b>
                        </t>
                    </div>
                    <div class="o_text"><b><t t-esc="widget.data.left.text"/></b></div>
                </div>
                <div class="o_separator">
                    <div class="o_line"/>
                </div>
                <div class="o_info o_right">
                    <t t-set="right_status" t-value="widget.data.right.percentage > 0 ? 'up' : 'down'"/>
                    <div class="o_value"><t t-esc="widget.data.right.this_value"/></div>
                    <div t-attf-class="o_percentage o_{{ right_status }}">
                        <t t-if="widget.data.right.percentage !== undefined">
                            <span t-attf-class="fa fa-arrow-{{ right_status }}"/>
                            <b><t t-esc="Math.abs(widget.data.right.percentage)"/>%</b>
                        </t>
                    </div>
                    <div class="o_text"><b><t t-esc="widget.data.right.text"/></b></div>
                </div>
            </div>
        </div>
    </t>

    <t t-name="InsightChartContainer">
        <div class="o_chart">
            <div t-attf-class="o_{{ widget.name }}_legend" t-if="widget.customLegend &amp;&amp; widget.legendPosition === 'top'"/>
            <canvas t-attf-id="o_{{ widget.name }}_chart"/>
            <div t-attf-class="o_{{ widget.name }}_legend" t-if="widget.customLegend &amp;&amp; widget.legendPosition === 'bottom'"/>
        </div>
    </t>

    <t t-name="InsightCustomLegend">
        <ul class="o_custom_legend">
            <t t-foreach="labels" t-as="label">
                <li class="o_legend" t-att-data-index="label_index">
                    <span class="o_legend_icon mr-2" t-attf-style="color: {{ colors[label_index] }};"><t t-raw="icon"/></span>
                    <span class="o_legend_label"><t t-esc="label"/></span>
                </li>
            </t>
        </ul>
    </t>

    <t t-name="CRMInsight">
        <div class="o_crm_insight">
            <div class="o_section">
                <div class="o_box o_total_closing"></div>
                <div class="o_box o_trending"></div>
                <div class="o_box o_generated_account"></div>
                <div class="o_box o_opportunities"></div>
                <div class="o_box o_won_revenue"></div>
                <div class="o_box o_revenue_per_lead_type"></div>
                <div class="o_box o_top_source"></div>
                <div class="o_box o_best_team"></div>
                <div class="o_box o_reason_of_lost"></div>
            </div>
        </div>
    </t>

</templates>