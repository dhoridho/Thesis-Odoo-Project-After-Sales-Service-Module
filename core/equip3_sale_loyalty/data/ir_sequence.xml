<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- Add you code here -->
        <record id="seq_customer_target" model="ir.sequence">
            <field name="name">Customer Target</field>
            <field name="code">customer.target</field>
            <field name="prefix">CTG/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="active" eval="True"/>
            <field name="implementation">standard</field>
        </record>
        <record id="seq_customer_voucher" model="ir.sequence">
            <field name="name">Customer Voucher</field>
            <field name="code">customer.voucher</field>
            <field name="prefix">VC/NOCUSTOMERTARGET-</field>
            <field name="padding">3</field>
            <field name="active" eval="True"/>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>