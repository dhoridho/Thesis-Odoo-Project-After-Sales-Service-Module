<odoo>
    <data>
        <record id="ir_cron_notifications_expiry_voucher" model="ir.cron">
            <field name="name">Email Notification for expiry voucher</field>
            <field name="model_id" ref="model_customer_voucher"/>
            <field name="state">code</field>
            <field name="code">model.get_expiry_voucher()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
        </record>
        <record id="ir_cron_expiry_cust_target" model="ir.cron">
            <field name="name">Auto expiry customer target</field>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 23:00:00')" />
            <field name="model_id" ref="model_customer_target"/>
            <field name="state">code</field>
            <field name="code">model.get_expiry_cust_target()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
        </record>
    </data>
</odoo>