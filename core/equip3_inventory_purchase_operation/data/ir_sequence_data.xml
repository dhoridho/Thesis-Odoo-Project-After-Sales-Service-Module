<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="procurement_planning_model" model="ir.sequence">
            <field name="name">Procurement Planning</field>
            <field name="code">procurement.planning.model</field>
            <field name="prefix">PRP/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
            <field name="range_reset">yearly</field>
        </record>
    </data>
</odoo>