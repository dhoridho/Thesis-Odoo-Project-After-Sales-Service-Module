<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<record id="direct_purchase_sequence_dp_new" model="ir.sequence">
			<field name="name">Direct Purchase</field>
			<field name="code">direct.purchase.sequence.dp.new</field>
			<field name="prefix">DP/%(y)s/%(month)s/%(day)s/</field>
			<field name="padding">3</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="direct_purchase_sequence_dp_new_goods" model="ir.sequence">
			<field name="name">Direct Purchase</field>
			<field name="code">direct.purchase.sequence.dp.new.goods</field>
			<field name="prefix">DP/G/%(y)s/%(month)s/%(day)s/</field>
			<field name="padding">3</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="direct_purchase_sequence_dp_new_services" model="ir.sequence">
			<field name="name">Direct Purchase</field>
			<field name="code">direct.purchase.sequence.dp.new.services</field>
			<field name="prefix">DP/S/%(y)s/%(month)s/%(day)s/</field>
			<field name="padding">3</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="direct_purchase_sequence_rfq" model="ir.sequence">
			<field name="name">Direct Purchase</field>
			<field name="code">direct.purchase.sequence.rfq</field>
			<field name="prefix">RFQ</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>
	</data>
</odoo>
