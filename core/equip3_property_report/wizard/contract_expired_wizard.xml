<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- contract.expired.form inherit form view -->
        <record id="contract_expired_form_inherit_reports" model="ir.ui.view">
            <field name="name">contract.expired.form.inherit.view.form.reports</field>
            <field name="model">contract.expired</field>
            <field name="inherit_id" ref="property_rental_mgt_app.contract_expired_wizard"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='get_pdf_report']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//button[@name='get_pdf_report']" position="after">
                    <button name="get_xls_report" type="object" string="Print Contract Report" icon="fa-print" class="oe_highlight" default_focus="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>