<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- agreement tree view -->
        <record id="agreement_expired_view_tree_report" model="ir.ui.view">
            <field name="name">agreement.view.tree</field>
            <field name="model">agreement</field>
            <field name="arch" type="xml">
                <tree create="0">
                    <field name="code" string="Code"/>
                    <field name="name"/>
                    <field name="start_date"/>
                    <field name="expired_date" string="Expired Date"/>
                    <field name="property_id"/>
                    <field name="property_book_for"/>
                    <field name="payment_type"/>
                    <field name="expected_revenue" string="Price"/>
                    <field name="expected_revenue" string="Deposite"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>