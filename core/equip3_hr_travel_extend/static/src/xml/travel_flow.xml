<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <!-- Client Action -->
    <t t-name="travel_configuration_action">
        <div class="travel_flow_action">
            <div class="travel_action_content d-flex flex-column">
                <div class="content-header">
                    <div class="d-flex p-3 w-100">
                        <span>Travel Flow</span>
                    </div>
                </div>
                <div class="content-body">
                    <div class="container">
                        <div class="row h-100">
                            <div class="accordion">
                                <div class="accordion-item accordion-master-data">
                                    <h2 class="accordion-header" data-target="collapseMasterData">
                                        <button class="accordion-button" type="button">
                                        Master Data
                                        </button>
                                    </h2>
                                    <div id="collapseMasterData" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="equip3_hr_travel_extend.travel_approval_matrix_act_window" type="action" class="btn btn-flow btn-default menu-button-competency-area">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/travel-approval-matrix.svg"/>
                                                    </div>
                                                    <div class="button-text">Travel Approval Matrix</div>
                                                </button>


                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-travel-flow">
                                    <h2 class="accordion-header" data-target="collapseTravelFlow">
                                        <button class="accordion-button" type="button">
                                        Travel Flow
                                        </button>
                                    </h2>
                                    <div id="collapseTravelFlow" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">

                                                <button name="equip3_hr_travel_extend.action_travel_request_custom" type="action" style=" top: 100px;" class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/travel-request.svg"/>
                                                    </div>
                                                    <div class="button-text">Travel Request</div>

                                                </button>

                                                <button name="bi_employee_travel_managment.action_travel_req_hr" type="action" style=" left: 265px; top: 100px;" class="btn btn-flow btn-default btn-approval">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/approval.svg"/>
                                                    </div>
                                                    <div class="button-text">Approval</div>
                                                </button>

                                                <button name="action_none" type="object" style=" left: 265px;" class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/return.svg"/>
                                                    </div>
                                                    <div class="button-text">Return</div>
                                                    <div class="menu-arrow-up-right">
                                                        <span class="arrow is-triangle arrow-bar is-up-right"></span>
                                                    </div>
                                                </button>


                                                <button name="equip3_hr_travel_extend.action_travel_cancel_custom" type="action" style=" left: 265px; top: 200px;" class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/travel-cancelation.svg"/>
                                                    </div>
                                                    <div class="button-text">Travel Cancellation</div>
                                                </button>
                                                <button name="equip3_hr_travel_extend.action_travel_cancel_approve" type="action" style=" left: 265px; top: 300px;" class="btn btn-flow btn-default btn-approval2">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/travel-cancelation-approved.svg"/>
                                                    </div>
                                                    <div class="button-text">Approval</div>
                                                </button>
                                                <button name="equip3_hr_expense_extend.action_hr_expense_sheet_my_all_custom" type="action" style=" left: 520px;" class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/expense-created.svg"/>
                                                    </div>
                                                    <div class="button-text">Expense Created</div>
                                                </button>
                                                <button name="equip3_hr_cash_advance.action_account_cash_advance_approve_custom" type="action" style=" left: 520px; top: 100px;" class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/cash-advance.svg"/>
                                                    </div>
                                                    <div class="button-text">Cash Advance Created</div>
                                                </button>


                                                <div class="nd_connector" style="width: 150px;left: 120px;top: 125px;">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" style="width: 115px;left: 410px;top: 125px;">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" style="width: 115px;left: 410px;top: 25px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>

                                                <div class="nd_connector" style="transform:rotate(-90deg); width: 50px;left: 315px;top: 78px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" style="transform:rotate(90deg);width: 50px;left: 315px;top: 178px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" style="transform:rotate(90deg);width: 50px;left: 315px;top: 278px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-reporting">
                                    <h2 class="accordion-header" data-target="collapseReporting">
                                        <button class="accordion-button" type="button">
                                        Reporting
                                        </button>
                                    </h2>
                                    <div id="collapseReporting" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="equip3_hr_travel_extend.action_hr_travel_view_view_report" type="action" class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_travel_extend/static/src/img/travel-analysis.svg"/>
                                                    </div>
                                                    <div class="button-text">Travel Analysis</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>