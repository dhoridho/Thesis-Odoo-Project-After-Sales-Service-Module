<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <!-- Client Action -->
    <t t-name="expense_configuration_action">
        <div class="expense_flow_action">
            <div class="expense_action_content d-flex flex-column">
                <div class="content-header">
                    <div class="d-flex p-3 w-100">
                        <span>Expense Flow</span>
                    </div>
                </div>
                <div class="content-body">
                    <div class="container">
                        <div class="row h-100">
                            <div class="accordion">
                                <div class="accordion-item accordion-master-data">
                                    <h2 class="accordion-header" data-target="collapseMasterData">
                                        <button class="accordion-button" type="button">
                                        Master Data
                                        </button>
                                    </h2>
                                    <div id="collapseMasterData" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="hr_expense.hr_expense_product" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/products.svg"/>
                                                    </div>
                                                    <div class="button-text">Expense Products</div>
                                                </button>
                                                <button name="equip3_hr_expense_extend.hr_expense_cycle_action" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/expense-cycle.svg"/>
                                                    </div>
                                                    <div class="button-text">Expense Cycle</div>
                                                </button>
                                                <button name="equip3_hr_expense_extend.expense_approval_matrix_act_window" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/expense-approval-matrix.svg"/>
                                                    </div>
                                                    <div class="button-text">Expense Approval Matrix</div>
                                                </button>


                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-expense-flow">
                                    <h2 class="accordion-header" data-target="collapseExpenseFlow">
                                        <button class="accordion-button" type="button">
                                        Expense Flow
                                        </button>
                                    </h2>
                                    <div id="collapseExpenseFlow" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">

                                                <button name="hr_expense.hr_expense_actions_my_all" 
                                                    type="action"
                                                    style="top: 100px"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/my-expense-line.svg"/>
                                                    </div>
                                                    <div class="button-text">My Expense Line</div>
                                                    <div class="menu-arrow">
                                                        <span class="arrow is-triangle arrow-bar is-right"></span>
                                                    </div> 
                                                </button>
                                                <button name="equip3_hr_expense_extend.action_hr_expense_sheet_my_all_custom" 
                                                    type="action"
                                                    style="top: 100px; left:210px"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/my-expense.svg"/>
                                                    </div>
                                                    <div class="button-text">My Expense</div>
                                                    <div class="menu-arrow">
                                                        <span class="arrow is-triangle arrow-bar is-right"></span>
                                                    </div> 
                                                </button>
                                                <button name="hr_expense.action_hr_expense_sheet_all_to_post" 
                                                    type="action"
                                                    style="top: 100px; left:400px"
                                                    class="btn btn-flow btn-default expense-to-post">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/expense-to-post.svg"/>
                                                    </div>
                                                    <div class="button-text">Expense to Post</div>
                                                </button>
                                                <button name="hr_expense.action_hr_expense_sheet_all_to_pay" 
                                                    type="action"
                                                    style="top: 100px; left:600px"
                                                    class="btn btn-flow btn-default expense-to-pay">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/expense-to-pay.svg"/>
                                                    </div>
                                                    <div class="button-text">Expense to Pay</div>
                                                </button>
                                               <button name="action_none" 
                                                    type="object"
                                                    style=" left:100px"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/cash-advance.svg"/>
                                                    </div>
                                                    <div class="button-text">Cash Advance</div>
                                                </button>
                                                <button name="action_none" 
                                                    style="left:300px"
                                                    type="object"
                                                    class="btn btn-flow btn-default"
                                                    id="expense_travel_btn">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/travel.svg"/>
                                                    </div>
                                                    <div class="button-text">Travel</div>
                                                </button>
                                                
                                                <div class="nd_connector" style="width: 50px;left: 165px;top: 125px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" style="width: 50px;left: 355px;top: 125px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" style="width: 50px;left: 555px;top: 125px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" 
                                                style="width:80px;left: 250px;top: 25px;">
                                                    <div class="nd_connector_line"></div>
                                                </div>
                                                <div class="nd_connector" 
                                                style="transform: rotate(90deg);width: 80px;left: 240px;top: 65px;">
                                                    <div class="nd_connector_line"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-reporting">
                                    <h2 class="accordion-header" data-target="collapseReporting">
                                        <button class="accordion-button" type="button">
                                        Reporting
                                        </button>
                                    </h2>
                                    <div id="collapseReporting" class="accordion-collapse show ">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="hr_expense.hr_expense_actions_all" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_expense_extend/static/src/img/expense-analysis.svg"/>
                                                    </div>
                                                    <div class="button-text">Expense Analysis</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>