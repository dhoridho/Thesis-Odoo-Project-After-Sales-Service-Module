        <!-- Tooth Surface -->
        <record id="acs_tooth_surface_distal" model="acs.tooth.surface">
            <field name="name">Distal</field>
            <field name="description">The surface that is away from the midline of the face.</field>
        </record>

        <record id="acs_tooth_surface_facial" model="acs.tooth.surface">
            <field name="name">Facial</field>
            <field name="description">The surface that faces the cheeks or lips</field>
        </record>

        <record id="acs_tooth_surface_labial" model="acs.tooth.surface">
            <field name="name">Labial</field>
            <field name="description">The surface towards the lips.</field>
        </record>

        <record id="acs_tooth_surface_buccal" model="acs.tooth.surface">
            <field name="name">Buccal</field>
            <field name="description">The surface towards the cheeks.</field>
        </record>

        <record id="acs_tooth_surface_incisal" model="acs.tooth.surface">
            <field name="name">Incisal</field>
            <field name="description">The biting edge of an anterior tooth.</field>
        </record>

        <record id="acs_tooth_surface_lingual" model="acs.tooth.surface">
            <field name="name">Lingual</field>
            <field name="description">The surface that faces the tongue.</field>
        </record>

        <record id="acs_tooth_surface_mesial" model="acs.tooth.surface">
            <field name="name">Mesial</field>
            <field name="description">The surface that is closest to the midline of the face.</field>
        </record>

        <record id="acs_tooth_surface_occlusal" model="acs.tooth.surface">
            <field name="name">Occlusal</field>
            <field name="description">The chewing surface of posterior teeth.</field>
        </record>

        <record id="acs_tooth_surface_proximal" model="acs.tooth.surface">
            <field name="name">Proximal</field>
            <field name="description">Tooth surfaces that are next to each other (i.e., distal of lateral incisor and mesial of canine).</field>
        </record>

