<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <!-- Scheduler for Auto Follow Up Overtime Approver-->
        <record forcecreate="True" id="ir_cron_overtime_auto_follow_up_approver" model="ir.cron">
            <field name="name">Auto Follow Up Overtime Approver</field>
            <field name="model_id" ref="model_hr_overtime_request"/>
            <field name="state">code</field>
            <field name="code">model.get_auto_follow_up_approver()</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>
        <!-- Scheduler for Auto Follow Up Actual Overtime Approver-->
        <record forcecreate="True" id="ir_cron_actual_overtime_auto_follow_up_approver" model="ir.cron">
            <field name="name">Auto Follow Up Actual Overtime Approver</field>
            <field name="model_id" ref="model_hr_overtime_actual"/>
            <field name="state">code</field>
            <field name="code">model.get_auto_follow_up_approver()</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>
    </data>
</odoo>