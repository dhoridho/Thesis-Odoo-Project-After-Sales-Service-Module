"                    <td style=\"border:1px solid black;text-align:center;\">${line.end_time}</td>\n"
"                    % endif\n"
"                    % if object.actual_based_on == 'overtime_request':\n"
"                    <td style=\"border:1px solid black;text-align:center;\">${line.hours}</td>\n"
"                    % endif\n"
"                    <td style=\"border:1px solid black;text-align:center;\">${line.actual_start_time}</td>\n"
