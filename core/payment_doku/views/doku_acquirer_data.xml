<odoo>

  <template id="payment_doku_error" name="Doku Error">
        	<t t-call="website.layout">
        			<div class="cold-md-12 text-center">
        				<img  src='/payment_Doku/static/src/img/odoo-payment-error-2.jpg' alt="odoo-payment-error"  height='350px' width='100%'/>
        			</div>
              <div class="hero-unit text-center">
                  <t t-raw="error_msg"/>
                  <p>
                    <a class="btn btn-primary btn-large" href='/shop/checkout'>
                     Back to Checkout
                    </a>
                  </p>
              </div>
			</t>
        </template>

</odoo>
