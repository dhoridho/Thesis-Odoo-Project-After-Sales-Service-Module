<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_payment_acquirer_view" model="ir.actions.server">
            <field name="name">Payment Acquirer</field>
            <field name="model_id" ref="payment.model_payment_acquirer"/>
            <field name="state">code</field>
            <field name="code">action = model.recompute_and_go()</field>
        </record>

      <menuitem
            action='action_payment_acquirer_view'
            id='payment.payment_acquirer_menu'
            parent='account.root_payment_menu'
            sequence='10' />

    <menuitem
            action='payment.payment_token_action'
            id='payment.payment_token_menu'
            parent='account.root_payment_menu'
            groups='base.group_no_one'
            active='0'
            />

    <menuitem
            action="payment.action_payment_icon"
            id="payment.payment_icon_menu"
            parent="account.root_payment_menu"
            groups="base.group_no_one"
            active="0"
            />
</odoo>