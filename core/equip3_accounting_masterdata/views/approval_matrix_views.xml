<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_form_approval_matrix_accounting" model="ir.ui.view">
        <field name="name">approval.matrix.accounting.form</field>
        <field name="model">approval.matrix.accounting</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            
                            <field name="name"/>
                            <field name="company_id" force_save="1"/>
                            <field name="branch_id" options="{'no_create': True, 'no_create_edit': True}"/>
                            <field name="filter_branch_ids" invisible="1" widget="many2many_tags"/>
                            <field name="create_uid"/>
                            <field name="create_date"/>
                            <field name="approval_matrix_type" invisible="1" readonly="1" force_save="1"
                                   string="Approval Matrix"/>
                        </group>
                        <group>
                            <field name="min_amount" required="1" attrs="{'invisible': [('approval_matrix_type', 'in' , ('budget', 'purchase_budget','budget_change_request_approval','purchase_budget_change_request_approval'))]}" string="Minimum Amount"/>
                            <field name="min_amount" required="1" attrs="{'invisible': [('approval_matrix_type', 'not in' , ('budget_change_request_approval','purchase_budget_change_request_approval'))]}" string="Min Change Amount"/>
                            <field name="max_amount" required="1" attrs="{'invisible': [('approval_matrix_type', 'in' , ('budget', 'purchase_budget','budget_change_request_approval','purchase_budget_change_request_approval'))]}" string="Maximum Amount"/>
                            <field name="max_amount" required="1" attrs="{'invisible': [('approval_matrix_type', 'not in' , ('budget_change_request_approval','purchase_budget_change_request_approval'))]}" string="Max Change Amount"/>
                            <field name="change_amount" required="1"
                                   attrs="{'invisible': [('approval_matrix_type', 'in', ('budget', 'purchase_budget', 'invoice', 'bill', 'other_income', 'other_expense', 'payment_voucher', 'credit_note', 'refund_approval_matrix', 'customer_deposit_approval_matrix', 'vendor_deposit_approval_matrix', 'customer_multi_receipt_approval_matrix', 'vendor_multi_receipt_approval_matrix', 'receipt_giro_approval_matrix', 'payment_giro_approval_matrix', 'receipt_approval_matrix', 'payment_approval_matrix', 'inter_bank_cash_approval_matrix', 'purchase_currency_approval_matrix', 'cash_advance_approval_matrix', 'budget_change_request_approval','purchase_budget_change_request_approval'))]}"/> 
                        </group>
                    </group>
                    <notebook>
                        <page string="Approving Matrix">
                            <field name="approval_matrix_line_ids"
                                   context="{'approval_matrix_line_ids': approval_matrix_line_ids}">
                                <tree editable="bottom" class="class_container_approver_matrix">
                                    <field name="sequence" widget="handle"/>
                                    <field name="sequence2" force_save="1" readonly="1"
                                           class="approver_matrix_sequence"/>
                                    <field name="user_ids" widget="many2many_tags"/>
                                    <field name="minimum_approver"/>
                                    <field name="approved_users" invisible="1"/>
                                    <field name="last_approved" invisible="1"/>
                                    <field name="approved" invisible="1"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" groups="base.group_user"/>
                    <field name="activity_ids"/>
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
    </record>

    <record id="view_tree_approval_matrix_accounting" model="ir.ui.view">
        <field name="name">approval.matrix.accounting.tree</field>
        <field name="model">approval.matrix.accounting</field>
        <field name="arch" type="xml">
            <tree string="Approval Matrix Accounting">
                <field name="name"/>
                <field name="company_id"/>
                <field name="branch_id"/>
                <field name="approval_matrix_type" invisible="1" readonly="1"/>
                <field name="min_amount" attrs="{'column_invisible': [('approval_matrix_type', 'in' , ('budget','purchase_budget', 'budget_change_request_approval','purchase_budget_change_request_approval'))]}"/>
                <field name="max_amount" attrs="{'column_invisible': [('approval_matrix_type', 'in' , ('budget', 'purchase_budget', 'budget_change_request_approval', 'purchase_budget_change_request_approval'))]}"/>
            </tree>
        </field>
    </record>

