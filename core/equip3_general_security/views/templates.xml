<odoo>
    <data>
        <template id="assets_backend" name="My assets" inherit_id="web.assets_common">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="equip3_general_security/static/src/js/crash_manager.js"></script>
            </xpath>
        </template>

        <template id="page_404_inherit" inherit_id="website.page_404">
            <xpath expr="//div[contains(@class, 'alert alert-info mt32')]" position="attributes">
                <attribute name="style">visibility:hidden;</attribute>
            </xpath>
      </template>



    </data>
</odoo>