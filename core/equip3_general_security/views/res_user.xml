<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

            <record id="res_users_view_form_hide_alert" model="ir.ui.view">
            <field name="name">res.users.form.hide.alert</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="auth_signup.res_users_view_form"/> <!-- Replace with the actual module reference -->
            <field name="arch" type="xml">
                <xpath expr="//field[@name='signup_url']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='signup_url']" position="attributes">
                    <attribute name="signup_valid">1</attribute>
                </xpath>
            </field>
        </record>

    
        
    
    </data>
    

</odoo>
