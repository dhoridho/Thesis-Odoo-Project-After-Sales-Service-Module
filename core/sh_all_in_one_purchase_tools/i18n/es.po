#. module: sh_all_in_one_purchase_tools
#: model:mail.template,body_html:sh_all_in_one_purchase_tools.email_template_edi_purchase_custom
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <ul style=\"list-style-type: none;\">\n"
"                            % if object.company_id.purchase_order_information_in_message:\n"
"\t\t\t\t\t\t\t\tDear ${object.partner_id.name} ,%0A%0A\n"
"\t\t\t\t\t\t\t\tHere is the your Order *${object.name}*\n"
"\t\t\t\t\t\t\t\tamounting in \n"
"\t\t\t\t\t\t\t\t${object.amount_total} ${object.currency_id.symbol}\n"
"\t\t\t\t\t\t\t\tfrom ${object.company_id.name} %0A%0A \n"
"\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t% endif \n"
"\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t    % if object.company_id.purchase_product_detail_in_message:\n"
"\t                            <li style=\"list-style-type: none;\">\n"
"\t\t\t\t\t\t\t\t    Following is your order details. %0A%0A\n"
"\t\t\t\t\t\t\t\t</li>\n"
"\t                            %for line in object.order_line:\n"
"\t                            <li style=\"list-style-type:none;\"> *${line.name}* %0A *Qty:* ${line.product_uom_qty} %0A *Price:* ${line.price_unit}$  %0A ________________________  %0A%0A </li>\n"
"\t                            % endfor\n"
"\t                            <br/>\n"
"\t                            *Total Amount:* %20 ${object.amount_total} ${object.currency_id.symbol} %0A%0A\n"
"\t\t\t\t\t    \t% endif\n"
"\t                            <br/>\n"
"\t                            % if object.company_id.po_send_pdf_in_message:\n"
"\t                            <li style=\"list-style-type: none;\">*Click here to download Report* %20 :${object.purchase_url} %0A%0A</li>\n"
"\t                            % endif\n"
"\t                            <br/>\n"
"\t                            % if object.company_id.purchase_signature and object.env.user.sign :\n"
"\t                            <li style=\"list-style-type: none;\">%20 ${object.env.user.sign} %0A%0A</li>\n"
"\t                            % endif\n"
"                        </ul>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        <ul style=\"list-style-type: none;\">\n"
"                            % if object.company_id.purchase_order_information_in_message:\n"
"\t\t\t\t\t\t\t\tQuerida ${object.partner_id.name} ,%0A%0A\n"
"\t\t\t\t\t\t\t\tAquí está su orden *${object.name}*\n"
"\t\t\t\t\t\t\t\tascendiendo en \n"
"\t\t\t\t\t\t\t\t${object.amount_total} ${object.currency_id.symbol}\n"
"\t\t\t\t\t\t\t\tde ${object.company_id.name} %0A%0A \n"
"\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t\t% endif \n"
"\t\t\t\t\t\t\t\n"
"\t\t\t\t\t\t    % if object.company_id.purchase_product_detail_in_message:\n"
"\t                            <li style=\"list-style-type: none;\">\n"
"\t\t\t\t\t\t\t\t    A continuación se muestran los detalles de su pedido. %0A%0A\n"
"\t\t\t\t\t\t\t\t</li>\n"
"\t                            %for line in object.order_line:\n"
"\t                            <li style=\"list-style-type:none;\"> *${line.name}* %0A *Cant .:* ${line.product_uom_qty} %0A *Precio:* ${line.price_unit}$  %0A ________________________  %0A%0A </li>\n"
"\t                            % endfor\n"
"\t                            <br/>\n"
"\t                            *Cantidad total:* %20 ${object.amount_total} ${object.currency_id.symbol} %0A%0A\n"
"\t\t\t\t\t    \t% endif\n"
"\t                            <br/>\n"
"\t                            % if object.company_id.po_send_pdf_in_message:\n"
"\t                            <li style=\"list-style-type: none;\">*Haga clic aquí para descargar el informe* %20 :${object.purchase_url} %0A%0A</li>\n"
"\t                            % endif\n"
"\t                            <br/>\n"
"\t                            % if object.company_id.purchase_signature and object.env.user.sign :\n"
"\t                            <li style=\"list-style-type: none;\">%20 ${object.env.user.sign} %0A%0A</li>\n"
"\t                            % endif\n"
"                        </ul>\n"
"                    </p>\n"
"                </div>\n"
"            "

