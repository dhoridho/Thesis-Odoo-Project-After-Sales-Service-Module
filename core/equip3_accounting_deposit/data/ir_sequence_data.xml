<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_customer_deposit" model="ir.sequence">
            <field name="name">Customer Desposit</field>
            <field name="code">customer.deposit</field>
            <field name="prefix">CSD/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_vendor_deposit" model="ir.sequence">
            <field name="name">Vendor Desposit</field>
            <field name="code">vendor.deposit</field>
            <field name="prefix">VRD/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
