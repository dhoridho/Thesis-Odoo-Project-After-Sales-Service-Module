<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="pos_online_outlet_form_inherit" model="ir.ui.view">
        <field name="name">pos.online.outlet.form.view.inherit</field>
        <field name="model">pos.online.outlet</field>
        <field name="inherit_id" ref="equip3_pos_online_outlet.pos_online_outlet_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside"> 
                <page string="GrabFood">
                    <group>
                        <group>
                            <field name="grabfood_merchant_id" string="StoreID"/>
                            <field name="grabfood_partner_merchant_id" string="Partner StoreID"/>
                        </group>
                    </group>
                    <group string="Log Info" groups="base.group_no_one">
                        <field name="grabfood_update_menu_error_msg" readonly="1"/>
                        <field name="grabfood_change_state_error_msg" readonly="1"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>