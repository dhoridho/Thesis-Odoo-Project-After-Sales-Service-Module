<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="rental_account_id" model="account.account">
            <field name="code">RO100100</field>
            <field name="name">Rental Order Account</field>
            <field name="user_type_id" ref="account.data_account_type_direct_costs"/>
        </record>

        <record id="ir_cron_feedback_1" model="ir.cron">
            <field name="name">Rental Order Feedback</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_aspl_feedback_template"/>
            <field name="code">model.send_mail_to_rental_feedback()</field>
            <field name="state">code</field>
        </record>

        <record id="send_mail_product_feedback" model="mail.template">
            <field name="name">Rental Order Feedback Mail</field>
            <field name="subject">Rental Order Feedback</field>
            <field name="auto_delete" eval="False"/>
            <field name="model_id" ref="model_aspl_feedback_template"/>
        </record>

        <record forcecreate="True" id="ir_cron_rental_contract_costs" model="ir.cron">
            <field name="name">Generate Rental contracts costs</field>
            <field name="model_id" ref="model_fleet_vehicle_contract"/>
            <field name="state">code</field>
            <field name="code">model.run_scheduler()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>

        <record id="email_template_vehicle_rental" model="mail.template">
            <field name="name">Vehicle Order- Send by Email</field>
            <field name="email_from">
                ${(object.create_uid.email and '%s &lt;%s&gt;' % (object.create_uid.name, object.create_uid.email)or
                '')|safe}>
            </field>
            <field name="subject">Order (Ref ${object.res_number or 'n/a' })</field>
            <field name="partner_to">${object.customer_name.id}</field>
            <field name="model_id" ref="aspl_vehicle_rental.model_fleet_vehicle_order"/>
            <field name="report_template" ref="aspl_vehicle_rental.vehicle_rental_order_report"/>
            <field name="report_name">${(object.res_number)}</field>
            <field name="lang">${object.customer_name.lang}</field>         
            <field name="body_html">
                <![CDATA[ <p>Dear ${object.customer_name.name},<br/>
                <br/><br/> 
                <p>You can reply to this email if you have any questions.</p>
                <p>Thank you,</p>

                <p style="color:#888888;">
                    % if object.user_id and object.user_id.signature:
                         ${object.user_id.signature| safe}
                    % endif
                </p> 
                ]]>
           </field>
        </record>

        <record id="email_template_vehicle_contract" model="mail.template">
            <field name="name">Vehicle Coontract- Send by Email</field>
            <field name="email_from">
                ${(object.create_uid.email and '%s &lt;%s&gt;' % (object.create_uid.name, object.create_uid.email)or
                '')|safe}>
            </field>
            <field name="subject">Order (Ref ${object.name or 'n/a' })</field>
            <field name="partner_to">${object.partner_id.id}</field>
            <field name="model_id" ref="aspl_vehicle_rental.model_fleet_vehicle_contract"/>
            <field name="report_template" ref="aspl_vehicle_rental.vehicle_rental_contract_report"/>
            <field name="report_name">${(object.name)}</field>
            <field name="lang">${object.partner_id.lang}</field>         
            <field name="body_html">
                <![CDATA[ <p>Dear ${object.partner_id.name},<br/><br/>
                  <p>Your Rental contract has been created,Here we attached contract detail</p>                   
                <br/><br/> 
                <p>You can reply to this email if you have any questions.</p>
                <p>Thank you,</p>

                <p style="color:#888888;">
                    % if object.user_id and object.user_id.signature_contractor:
                         ${object.user_id.signature_contractor | safe}
                    % endif
                </p> 
                ]]>
           </field>
        </record>
    </data>

    <data>
        <record id="vehicle_type_class_1" model="vehicle.class">
            <field name="vehicle_class">Passenger</field>
        </record>
        <record id="vehicle_type_class_2" model="vehicle.class">
            <field name="vehicle_class">Cargo</field>
        </record>
        <record id="vehicle_type_class_3" model="vehicle.class">
            <field name="vehicle_class">Heavy</field>
        </record>
        <record id="vehicle_type_class_7" model="vehicle.class">
            <field name="vehicle_class">2Tire - Bike</field>
        </record>
        <record id="vehicle_type_class_4" model="vehicle.class">
            <field name="vehicle_class">4Tire - SingleUnit</field>
        </record>
        <record id="vehicle_type_class_5" model="vehicle.class">
            <field name="vehicle_class">Busses</field>
        </record>
        <record id="vehicle_type_class_6" model="vehicle.class">
            <field name="vehicle_class">Trailer</field>
        </record>
    </data>

    <data>
        <record id="vehicle_type_1" model="fleet.vehicle.type">
            <field name="name">Sedan</field>
        </record>
        <record id="vehicle_type_2" model="fleet.vehicle.type">
            <field name="name">Coupe</field>
        </record>
        <record id="vehicle_type_3" model="fleet.vehicle.type">
            <field name="name">Convertible</field>
        </record>
        <record id="vehicle_type_4" model="fleet.vehicle.type">
            <field name="name">Minivan</field>
        </record>
        <record id="vehicle_type_5" model="fleet.vehicle.type">
            <field name="name">Truck</field>
        </record>
        <record id="vehicle_type_6" model="fleet.vehicle.type">
            <field name="name">SUV</field>
        </record>
        <record id="vehicle_type_7" model="fleet.vehicle.type">
            <field name="name">Certified Pre-Owned</field>
        </record>
        <record id="vehicle_type_8" model="fleet.vehicle.type">
            <field name="name">Green Driving</field>
        </record>
        <record id="vehicle_type_9" model="fleet.vehicle.type">
            <field name="name">Collector Car</field>
        </record>
        <record id="vehicle_type_10" model="fleet.vehicle.type">
            <field name="name">Motor Cycle</field>
        </record>
    </data>

    <data>
        <record id="fuel_type_1" model="fuel.type">
            <field name="name">Gasoline</field>
        </record>
        <record id="fuel_type_2" model="fuel.type">
            <field name="name">Diesel</field>
        </record>
        <record id="fuel_type_3" model="fuel.type">
            <field name="name">Electric</field>
        </record>
        <record id="fuel_type_4" model="fuel.type">
            <field name="name">Petrol</field>
        </record>
        <record id="fuel_type_5" model="fuel.type">
            <field name="name">Hybrid</field>
        </record>
    </data>

</odoo>