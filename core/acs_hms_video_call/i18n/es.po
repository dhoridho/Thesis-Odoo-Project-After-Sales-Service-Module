#. module: acs_hms_video_call
#: model:mail.template,body_html:acs_hms_video_call.acs_video_call_invitaion
msgid ""
"<div style=\"font-size:13px;font-family:&quot;Lucida Grande&quot;, "
"Helvetica, Verdana, Arial, sans-serif;padding:0px;margin:auto;background: "
"#FFFFFF repeat top /100%;color:#777777\">\n"
"    % set call_date = format_datetime(object.date, tz=(object.physician_id."
"tz or \"UTC\"), dt_format=(object.physician_id.lang.date_format))\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Hello Dear ${object."
"patient_id.name},</p><br>\n"
"\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Your Video "
"Consultation with ${object.physician_id.name} have been Scheduled with "
"following details.</p>\n"
"    <ul style=\"margin:0px 0 1rem 0;\">\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Appointment Number: "
"${object.name}</p><br>\n"
"        </li>\n"
"        % if object.video_call_id:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Meeting Link: <a href="
"\"${object.video_call_id.meeting_link}\" style=\"background-color:"
"transparent;text-decoration-thickness:initial;color:rgb(0, 160, 157);\">"
"${object.video_call_id.meeting_link}</a></p><br>\n"
"        </li>\n"
"        % endif\n"
"        % if object.video_call_id.password:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Meeting Password: "
"${object.video_call_id.password}</p><br>\n"
"        </li>\n"
"        % endif\n"
"        % if object.date:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Date: ${call_date} "
"(Timezone: ${object.physician_id.tz or \"UTC\"})</p><br>\n"
"        </li>\n"
"        % endif\n"
"        % if object.physician_id:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Physician: ${object."
"physician_id.name}</p><br>\n"
"        </li>\n"
"        % endif\n"
"    </ul>\n"
"\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Please feel free to "
"call anytime for further information or any query.</p>\n"
"\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Best regards.</"
"p><br>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"font-size:13px;font-family:&quot;Lucida Grande&quot;, "
"Helvetica, Verdana, Arial, sans-serif;padding:0px;margin:auto;background: "
"#FFFFFF repeat top /100%;color:#777777\">\n"
"    % set call_date = format_datetime(object.date, tz=(object.physician_id."
"tz or \"UTC\"), dt_format=(object.physician_id.lang.date_format))\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Hola ${object."
"patient_id.name} ,</p><br>\n"
"\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Su consulta de video "
"con ${object.physician_id.name} se ha programado con los siguientes detalles."
"</p>\n"
"    <ul style=\"margin:0px 0 1rem 0;\">\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Número de cita: "
"${object.name}</p><br>\n"
"        </li>\n"
"        % if object.video_call_id:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Enlace de reunión: <a "
"href=\"${object.video_call_id.meeting_link}\" style=\"background-color:"
"transparent;text-decoration-thickness:initial;color:rgb(0, 160, 157);\">"
"${object.video_call_id.meeting_link}</a></p><br>\n"
"        </li>\n"
"        % endif\n"
"        % if object.video_call_id.password:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Contraseña de la "
"reunión: ${object.video_call_id.password}</p><br>\n"
"        </li>\n"
"        % endif\n"
"        % if object.date:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Fecha: ${call_date} "
"(Zona horaria: ${object.physician_id.tz or \"UTC\"})</p><br>\n"
"        </li>\n"
"        % endif\n"
"        % if object.physician_id:\n"
"        <li>\n"
"            <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Profesional: ${object."
"physician_id.name}</p><br>\n"
"        </li>\n"
"        % endif\n"
"    </ul>\n"
"\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">No dude en llamar en "
"cualquier momento para obtener más información o cualquier consulta.</p>\n"
"\n"
"    <p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida "
"Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Saludos.</p><br>\n"
"</div>\n"
"            "

