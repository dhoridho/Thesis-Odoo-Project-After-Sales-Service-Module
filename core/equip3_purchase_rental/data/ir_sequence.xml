<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="purchase_order_rental" model="ir.sequence">
		<field name="name">Purchase Order Sequence</field>
		<field name="code">purchase.order.seqs.r</field>
		<field name="prefix">RFQ/R/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>

    <record id="purchase_request_rental_seq" model="ir.sequence">
		<field name="name">Purchase Request Sequence</field>
		<field name="code">purchase.request.seqs.r</field>
		<field name="prefix">PR/R/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>

	<record id="purchase_order_new_seqs_rental" model="ir.sequence">
		<field name="name">Purchase Order</field>
		<field name="code">purchase.order.seqs.rental</field>
		<field name="prefix">PO/R/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
		<field name="company_id" eval="False"/>
	</record>

    <record id="sh_purchase_agreement_seq3_rental" model="ir.sequence">
		<field name="name">Purchase Agreement Sequence</field>
		<field name="code">purchase.agreement.seqs.r</field>
		<field name="prefix">TE/R/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>
    <record id="sh_open_purchase_agreement_seq3_rental" model="ir.sequence">
		<field name="name">Open Purchase Agreement Sequence</field>
		<field name="code">purchase.agreement.seqs.r.open</field>
		<field name="prefix">OTE/R/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>

	<record id="seq_blanket_orders_seq_3_rental" model="ir.sequence">
		<field name="name">Blanket Order</field>
		<field name="code">purchase.requisition.blanket.order.new.r</field>
		<field name="prefix">BO/R/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
		<field name="company_id" eval="False"></field>
	</record>

    <record id="direct_purchase_rental" model="ir.sequence">
		<field name="name">Direct Purchase Sequence</field>
		<field name="code">direct.purchase.seqs.r</field>
		<field name="prefix">DP/R/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>


</odoo>
