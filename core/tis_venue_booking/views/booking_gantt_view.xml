<odoo>
    <data>
        <record id="booking_gantt_view" model="ir.ui.view">
            <field name="name">booking.gantt.view</field>
            <field name="model">booking.booking</field>
            <field name="arch" type="xml">
                <gantt date_stop="to_date" date_start="from_date" string="Venue Bookings" default_group_by="venue_id">
                </gantt>
            </field>
        </record>
    </data>
</odoo>
