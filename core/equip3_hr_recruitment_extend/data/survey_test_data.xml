<odoo>
    <data noupdate="1">
        <record id="survey_root_test_master" model="survey.survey">
            <field name="title">ngetest</field>
            <field name="survey_type">general</field>
            <field name="questions_layout">one_page</field>
            <field name="is_time_limited" eval="True"/>
            <field name="time_limit">60</field>
            <field name="scoring_type">scoring_without_answers</field>
            <field name="scoring_success_min">80.0</field>
            <field name="state">open</field>
            <field name="category_id" ref="equip3_hr_recruitment_extend.survey_category_non_technical"/>
        </record>



        <record id="survey_root_test_master_1" model="survey.question">
            <field name="title">1. In PowerPoint, the header and footer button can be found on the insert tab in what group?</field>
            <field name="question_type">simple_choice</field>
            <field name="survey_id" ref="survey_root_test_master"/>
        </record>
        <record id="survey_question_test_master_1_col_1" model="survey.question.answer">
            <field name="value">A. Tables group</field>
            <field name="is_correct" eval="False"/>
            <field name="answer_score">0.0</field>
            <field name="question_id" ref="survey_root_test_master_1"/>
        </record>
        <record id="survey_question_test_master_1_col_2" model="survey.question.answer">
            <field name="value">B. Text group</field>
            <field name="is_correct" eval="True"/>
            <field name="answer_score">3.33</field>
            <field name="question_id" ref="survey_root_test_master_1"/>
        </record>
        <record id="survey_question_test_master_1_col_3" model="survey.question.answer">
            <field name="value">C. Object group</field>
            <field name="is_correct" eval="False"/>
            <field name="answer_score">0.0</field>
            <field name="question_id" ref="survey_root_test_master_1"/>
        </record>
        <record id="survey_question_test_master_1_col_4" model="survey.question.answer">
            <field name="value">D. Illustrations group</field>
            <field name="is_correct" eval="False"/>
            <field name="answer_score">0.0</field>
            <field name="question_id" ref="survey_root_test_master_1"/>
        </record>
    </data>
</odoo>