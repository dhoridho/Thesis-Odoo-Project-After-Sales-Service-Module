<odoo>
    <data noupdate="0">


    <record id="mpp_sequence" model="ir.sequence">
        <field name="name">MPP Sequence</field>
        <field name="code">manpower.planning</field>
        <!-- <field name="prefix">MPP/</field>
        <field name="padding">4</field> -->
        <field name="prefix">MPP/%(range_y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field eval="True" name="use_date_range"/>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field name="company_id" eval="False"/>
    </record>

    <record id="mpp_requisition_sequence" model="ir.sequence">
        <field name="name">MPP Requisition Sequence</field>
        <field name="code">manpower.requisition</field>
        <field name="prefix">MPR/%(range_y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field eval="True" name="use_date_range"/>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field name="company_id" eval="False"/>
    </record>

    <record id="applicant_sequence" model="ir.sequence">
        <field name="name">Applicant Sequence</field>
        <field name="code">applicant.sequence</field>
        <field name="prefix">APP-</field>
        <field name="padding">9</field>
    </record>

    <record id="schedulling_sequence" model="ir.sequence">
        <field name="name">Schedulling Sequence</field>
        <field name="code">schedulling.sequence</field>
        <field name="prefix">sch-</field>
        <field name="padding">5</field>
    </record>

    <record id="hr_offering_request_sequence" model="ir.sequence">
        <field name="name">Offering Letter Request</field>
        <field name="code">hr.offering.request</field>
        <field name="prefix">OLR/%(range_y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field eval="True" name="use_date_range"/>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field name="company_id" eval="False"/>
    </record>

    </data>
</odoo>