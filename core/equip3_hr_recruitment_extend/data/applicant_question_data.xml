<?xml version="1.0"?>
<odoo>
	<data noupdate="0">
		<record id="job_question_1" model="applicant.question">
			<field name="question">What is your name?</field>
			<field name="type">text</field>
			<field name="is_readonly">True</field>
			<field name="global_question">True</field>
			<field name="is_name">True</field>

		</record>
		<record id="job_question_2" model="applicant.question">
			<field name="question">What is your email?</field>
			<field name="type">text</field>
