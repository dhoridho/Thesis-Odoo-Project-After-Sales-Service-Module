			<field name="choices">Yes,No</field>
			<field name="is_readonly">True</field>
			<field name="global_question">True</field>
		</record> -->


		<record id="job_question_16" model="applicant.question">
			<field name="question">Past Experience</field>
			<field name="is_past_experience">True</field>
			<field name="type"></field>
			<field name="is_readonly">True</field>
			<field name="global_question">False</field>
		</record>


		<record id="job_question_17" model="applicant.question">
			<field name="question">Employee Skills</field>
			<field name="is_employee_skill">True</field>
			<field name="type"></field>
			<field name="is_readonly">True</field>

			<field name="global_question">False</field>
		</record>
		<record id="job_question_18" model="applicant.question">
			<field name="question">When are you ready to start work if you are accepted in this company?</field>
			<field name="type"></field>
			<field name="is_readonly">True</field>
			<field name="type">date</field>
			<field name="global_question">False</field>
		</record>


		<record id="job_question_19" model="applicant.question">
			<field name="question">Source of Information</field>
			<field name="type">many2one</field>
			<field name="model_id" ref="utm.model_utm_source"/>
			<field name="is_readonly">True</field>
			<field name="global_question">False</field>
