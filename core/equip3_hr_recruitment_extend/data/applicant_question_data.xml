			<field name="global_question">True</field>
			<field name="is_cv">True</field>
		</record>


		<record id="job_question_7" model="applicant.question">
			<field name="question">What is your expected salary?</field>
			<field name="type">round</field>
			<field name="is_readonly">True</field>
			<field name="global_question">False</field>
		</record>

