			<field name="question">What is your degree?</field>
			<field name="type">many2one</field>
			<field name="model_id" ref="hr_recruitment.model_hr_recruitment_degree"/>
			<field name="is_readonly">True</field>
			<field name="global_question">True</field>
		</record>

		<record id="job_question_6" model="applicant.question">
			<field name="question">Upload your CV</field>
