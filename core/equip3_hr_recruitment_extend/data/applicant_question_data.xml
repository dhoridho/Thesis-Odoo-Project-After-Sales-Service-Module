			<field name="is_email_question">True</field>

		</record>
		<record id="job_question_3" model="applicant.question">
			<field name="question">What is your phone number?</field>
			<field name="type">round</field>
			<field name="is_readonly">True</field>
			<field name="global_question">False</field>
			
			
		</record>

		<record id="job_question_4" model="applicant.question">
			<field name="question">What is your mobile number?</field>
