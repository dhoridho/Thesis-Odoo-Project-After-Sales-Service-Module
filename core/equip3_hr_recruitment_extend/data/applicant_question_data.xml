			<field name="question">What is your date of birth?</field>
			<field name="type">date</field>
			<field name="is_readonly">True</field>
			<field name="global_question">True</field>
		</record>



		<record id="job_question_11" model="applicant.question">
			<field name="question">What is your address?</field>
			<field name="type">text_area</field>
			<field name="is_readonly">True</field>
			<field name="global_question">True</field>
		</record>


		<record id="job_question_12" model="applicant.question">
			<field name="question">What is your marital status?</field>
			<field name="type">many2one</field>
			<field name="model_id" ref="equip3_hr_masterdata_employee.model_employee_marital_status" />
			<field name="is_readonly">True</field>
			<field name="global_question">False</field>
		</record>


		<record id="job_question_13" model="applicant.question">
			<field name="question">What is your religion?</field>
			<field name="type">many2one</field>
			<field name="model_id" ref="equip3_hr_masterdata_employee.model_employee_religion" />
			<field name="is_readonly">True</field>
			<field name="global_question">False</field>
		</record>


		<record id="job_question_14" model="applicant.question">
			<field name="question">What is your last drawn salary?</field>
			<field name="type">round</field>
			<field name="is_readonly">True</field>
			<field name="global_question">False</field>
		</record>


