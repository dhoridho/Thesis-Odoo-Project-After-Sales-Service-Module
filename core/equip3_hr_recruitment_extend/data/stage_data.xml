<?xml version="1.0"?>
<odoo>
	<data noupdate="0">
		
		<record model="hr.recruitment.stage" id="hr_recruitment.stage_job1">
			<field name="name">Initial Qualification</field>
			<field name="sequence">1</field>
			<field name="is_global">True</field>
			<field name="is_first_stage">True</field>
			<field name="is_masterdata">True</field>
			<!-- <field name="template_wa_id" ref="equip3_hr_recruitment_extend.wa_template_9"/> -->
			<field name="template_id" ref="equip3_hr_recruitment_extend.mail_reminder_invite_acknowledgement_indonesia"/>
		</record>
		<record model="hr.recruitment.stage" id="hr_recruitment.stage_job2">
			<field name="name">First Interview</field>
			<field name="sequence">4</field>
			<field name="is_global">True</field>
			<field name="is_masterdata">True</field>
			<!-- <field name="template_wa_id" ref="equip3_hr_recruitment_extend.wa_template_12"/> -->
			<field name="template_id" ref="equip3_hr_recruitment_extend.email_template_digital_interview_invitation_id"/>
		</record>
		<record model="hr.recruitment.stage" id="hr_recruitment.stage_job3">
			<field name="name">Second Interview</field>
			<field name="sequence">5</field>
			<field name="is_global">True</field>
			<field name="is_masterdata">True</field>
			<!-- <field name="template_wa_id" ref="equip3_hr_recruitment_extend.wa_template_13"/> -->
			<field name="template_id" ref="equip3_hr_recruitment_extend.interview_template_applicant_extend_id"/>
		</record>
		<record model="hr.recruitment.stage" id="hr_recruitment.stage_job4">
			<field name="name">Contract Proposal</field>
			<field name="sequence">4</field>
			<field name="is_global">False</field>
			<field name="is_masterdata">True</field>
		</record>
		<record model="hr.recruitment.stage" id="hr_recruitment.stage_job5">
			<field name="name">Contract Signed</field>
			<field name="sequence">8</field>
			<field name="fold" eval="True"/>
			<field name="is_global">True</field>
			<field name="is_masterdata">True</field>
			<field name="is_final_stage">True</field>
			<!-- <field name="template_wa_id" ref="equip3_hr_recruitment_extend.wa_template_15"/> -->
			<field name="template_id" ref="equip3_hr_recruitment_extend.contract_template_id"/>
		</record>



		<record id="shortlist_qualified_candidates" model="hr.recruitment.stage">
			<field name="name">Shortlist Qualified Candidates</field>
			<field name="sequence">2</field>
			<field name="is_global">True</field>
			<field name="is_masterdata">True</field>
			<!-- <field name="template_wa_id" ref="equip3_hr_recruitment_extend.wa_template_10"/> -->
			<field name="template_id" ref="equip3_hr_recruitment_extend.mail_applicant_interest_extend_indonesia"/>
		</record>

		<record id="technical_test" model="hr.recruitment.stage">
			<field name="name">Technical Test</field>
			<field name="sequence">3</field>
			<field name="is_global">True</field>
			<field name="is_masterdata">True</field>
			<!-- <field name="template_wa_id" ref="equip3_hr_recruitment_extend.wa_template_11"/> -->
			<field name="template_id" ref="equip3_hr_recruitment_extend.mail_template_applicant_invite_id_test"/>
		</record>

		<record id="background_check" model="hr.recruitment.stage">
			<field name="name">Background Check</field>
			<field name="sequence">6</field>
			<field name="is_global">False</field>
			<field name="is_masterdata">True</field>
		</record>

		<record id="offrering_letter" model="hr.recruitment.stage">
			<field name="name">Offering Letter</field>
			<field name="sequence">7</field>
			<field name="is_global">True</field>
			<field name="is_masterdata">True</field>
		</record>


		<record id="stage_reject" model="hr.recruitment.stage">
			<field name="name">Rejected</field>
			<field name="template_id" ref="hr_recruitment.email_template_data_applicant_refuse" />
			<field name="is_global">False</field>
			<field name="is_masterdata">True</field>
			<field name="sequence">10</field>
		</record>

		<record id="stage_blacklist" model="hr.recruitment.stage">
			<field name="name">Blacklisted</field>
			<field name="is_global">False</field>
			<field name="is_masterdata">True</field>
			<field name="fold">True</field>
			<field name="sequence">20</field>
		</record>
		<record id="stage_re_evaluation" model="hr.recruitment.stage">
			<field name="name">Re-Evaluation</field>
			
		</record>


		<record id="not_suitable" model="hr.recruitment.stage">
			<field name="name">Not Suitable</field>
			<field name="is_global">False</field>
			<field name="is_masterdata">True</field>
			<field name="sequence">9</field>

		</record>
	</data>
</odoo>