<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="stock_scrap_request_seq" model="ir.sequence">
            <field name="name">Stock Scrap Request</field>
            <field name="code">stock.scrap.request</field>
            <field name="prefix">SCR/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="stock_product_usage_seq" model="ir.sequence">
            <field name="name">Stock Scrap Request</field>
            <field name="code">stock.product.usage</field>
            <field name="prefix">PRU/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="inventory_adjust_sequence" model="ir.sequence">
            <field name="name">Inventory Adjust Sequence</field>
            <field name="code">inv.adj.seq</field>
            <field name="prefix">SCT/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="inventory_adjust_value_sequence" model="ir.sequence">
            <field name="name">Inventory Adjust Value Sequence</field>
            <field name="code">inv.adj.value.seq</field>
            <field name="prefix">INV/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="stock_warehouse_orderpoint_sequence" model="ir.sequence">
            <field name="name">Sequence Number Format</field>
            <field name="code">sequence.stock.warehouse.orderpoint</field>
            <field name="prefix">OP/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="stock_day_average_sequence" model="ir.sequence">
            <field name="name">Sequence Stock Average Day</field>
            <field name="code">stock.day.average</field>
            <field name="prefix">ASD/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

    </data>
</odoo>
