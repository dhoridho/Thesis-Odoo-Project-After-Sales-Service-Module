<odoo noupdate="1">

    <!-- Sequence for agreement -->
    <record id="seq_contract" model="ir.sequence">
        <field name="name">Contract</field>
        <field name="code">contract</field>
        <field name="prefix">CR</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

    <!-- Sequence for agreement template-->
    <record id="seq_contract_template" model="ir.sequence">
        <field name="name">Contract Templates</field>
        <field name="code">contract.template</field>
        <field name="prefix">CRT</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

</odoo>
