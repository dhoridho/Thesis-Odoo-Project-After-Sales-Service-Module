<odoo>
    <data>
        
        <record id="menu_c_view_tree" model="ir.ui.view">
            <field name="name">Form C</field>
            <field name="model">menu.accounting.c</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="sctp_no"/>
                    <field name="contact_person"/>
                    <field name="designation"/>
                    <field name="email_address"/>
                    <field name="assessment_year"/>
                    <field name="due_date"/>
                </tree>
            </field>
        </record>

        <record id="menu_c_view_form" model="ir.ui.view">
            <field name="name">Form C</field>
            <field name="model">menu.accounting.c</field>
            <field name="arch" type="xml">
                <form string="Contact Information">
                    <sheet>
                    <group>
                        <group>
                            <field name="contact_person"/>
                            <field name="designation"/>
                            <field name="email_address"/>
                            <field name="assessment_year"/>
                        </group>
                        <group>
                            <field name="mobile"/>
                            <field name="Office"/>
                            <field name="due_date"/>
                        </group>
                    </group>
                        <notebook>
                            <page string="GENERAL INFO ">
                            <group>
                                <group>
                                    <separator string="Particular of person who riviewed the return" />
                                    <field name="name"/>
                                    <field name="sctp_no"/>
                                    <field name="sctp_radio" default="option1" widget="radio" options="{'horizontal': true}" string="The return has been reviewed by a person who is a singapore chartered tax professionals limited (SCTP) Accredited tax advisor or accredited tax practitioner for income tax"/>
                                </group>
                                <group>
                                    <separator string="The company declares the following"/>
                                    <field name="audited_radio" default="option1" widget="radio" options="{'horizontal': true}" string="Which type of financial statements is the company submitting"/>
                                    <field name="radio_button_a" default="option1" widget="radio" options="{'horizontal': true}" string="Did the auditors of the company make a qualification on the audited financial statements for the financial period?"/>
                                    <field name="radio_button_b" default="option1" widget="radio" options="{'horizontal': true}" string="Is the company claiming for tax exemption scheme for new start up companies?"/>
                                    <field name="radio_button_c" default="option1" widget="radio" options="{'horizontal': true}" string="Has the company statisfied all conditions to qualify for tax exemption scheme for new Start-Up Companies?"/>

                                    <label for="start_date" string="The financial period for this return based on the company's financial statement is?"/>
                                            <field name="start_date" widget="date"/>
                                            <field name="end_date" widget="date"/>
                                </group>
                            </group>
                            </page>
                            <page string="Assesment 1">
                                <notebook>
                                    <page string="Tab 1">
                                        <group>
                                            <group>
                                                <label for="trade_business_income" string="1.  Income Accuring in/ Derived from singapore (Before Exempt Amount)"/>
                                                <div style="white-space: nowrap;">
                                                    <label for="trade_business_income" string="1a. Trade/Bussiness Income/Loss – Section 10 (1)(a)"/>
                                                    <div style="white-space: nowrap;">
                                                        <field name="trade_business_income" class="o_form_label oe_inline" style="max-width:48%"/>
                                                        <span>  </span>
                                                        <field name="trade_business_lose" class="o_form_label oe_inline" style="max-width:47%"/>
                                                    </div>
                                                    <label for="other_income_not_falling_under_items_1a" string="1b. Other income not falling under items 1a to 1e – Section 10 (1)(g)"/>
                                                    <div style="white-space: nowrap;">
                                                        <field name="other_income_not_falling_under_items_1a" class="o_form_label oe_inline" style="max-width:48%"/>
                                                        <span>  </span>
                                                        <field name="other_income_not_falling_under_items_1e" class="o_form_label oe_inline" style="max-width:47%"/>
                                                    </div>
                                                </div>
                                                <label for="total_income_before_donation" string="2. Total income /loss (Before Donation)"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="total_income_before_donation" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="total_loss_before_donation" class="o_form_label oe_inline" style="max-width:47%"/>
                                                </div>
                                                <label for="chargeable_income" string="3. Chargeable income after group relief (before exempt amount)"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="chargeable_income" class="o_form_label oe_inline" style="max-width:80%"/>
                                                </div>
                                            </group>
                                            <group>
                                                <label for="loss_claimed_from_transferor_company_from" string="4. Loss Claimed from transferor company"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="loss_claimed_from_transferor_company_from" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="loss_claimed_from_transferor_company_to" class="o_form_label oe_inline" style="max-width:47%"/>
                                                </div>
                                                <label for="chargeable_income_after_group_relief_from" string="5. Chargeable income after group relief (Before exempt amount)"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="chargeable_income_after_group_relief_from" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="chargeable_income_after_group_relief_to" class="o_form_label oe_inline" style="max-width:47%"/>
                                                </div>
                                                <label for="tax_to_be_remitted_under_section_92_1" string="6. Tax to be remitted under Section 92(1) and 92(2) (Do not include corporate income tax rebate"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="tax_to_be_remitted_under_section_92_1" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="tax_to_be_remitted_under_section_92_2" class="o_form_label oe_inline" style="max-width:47%"/>
                                                </div>
                                            </group>
                                        </group>
                                        <separator string="4. Unutilised Donations b/f &amp; Current year donation"/>
                                        <group>
                                            <group>
                                                <label for="unutilisted_donation_normal_rate1" string="Unutilisted donation b/f @ normal rate (IRAS' Record) "/>
                                                <div style="white-space: nowrap;">
                                                    <field name="unutilisted_donation_normal_rate1" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="unutilisted_donation_normal_rate2" class="o_form_label oe_inline" style="max-width:47%; margin-left:23%;"/>
                                                </div>
                                                <label for="company_declaration_unutilisted_donation_normal_rate1" string="Company’s Declaration"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="company_declaration_unutilisted_donation_normal_rate1" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="company_declaration_unutilisted_donation_normal_rate2" class="o_form_label oe_inline" style="max-width:47%"/>
                                                </div>
                                                <label for="unutilisted_donation_concessionary_rate1" string="Unutilisted Donations b/f @Concessionary Rate (IRAS Record)"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="unutilisted_donation_concessionary_rate1" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="unutilisted_donation_concessionary_rate2" class="o_form_label oe_inline" style="max-width:47%; margin-left:23%;"/>
                                                </div>
                                                <label for="company_declaration_unutilisted_donation_concessionary_rate1" string="Company’s Declaration"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="company_declaration_unutilisted_donation_concessionary_rate1" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="company_declaration_unutilisted_donation_concessionary_rate2" class="o_form_label oe_inline" style="max-width:47%"/>
                                                </div>
                                            </group>
                                            <group>
                                                <label for="current_year_donation_1" string="Current year donations (IRAS' Record)"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="current_year_donation_1" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="current_year_donation_2" class="o_form_label oe_inline" style="max-width:47%; margin-left:23%;"/>
                                                </div>
                                                <label for="company_declaration_current_year_donation_1" string="Company’s Declaration"/>
                                                <div style="white-space: nowrap;">
                                                    <field name="company_declaration_current_year_donation_1" class="o_form_label oe_inline" style="max-width:48%"/>
                                                    <span>  </span>
                                                    <field name="company_declaration_current_year_donation_2" class="o_form_label oe_inline" style="max-width:47%"/>
                                                </div>
                                            </group>
                                        </group>
                                    </page>
                                    <page string="Tab 2">
                                        <field name="foreign_ids">
                                            <tree editable="bottom">
                                                <field name="nature_of_income"/>
                                                <field name="country"/>
                                                <field name="amount_1"/>
                                                <field name="amount_2"/>
                                            </tree>
                                        </field>
                                    </page>
                                </notebook>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="menu_account_form_c_action" model="ir.actions.act_window">
            <field name="name">Form C</field>
            <field name="res_model">menu.accounting.c</field>
            <field name="view_mode">tree,form</field> 
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                </p>
            </field>
        </record>

        <menuitem
        id="menu_account_report_csc"
        name="Form C-S/C"
        parent="account.menu_finance_reports"
        sequence="11"/>

        <menuitem 
        id="menu_account_report_form_c"
        name="Form C"
        parent="menu_account_report_csc"
        action="menu_account_form_c_action"
        sequence="1"/>
    
    
    </data>
</odoo>