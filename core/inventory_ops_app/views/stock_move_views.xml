<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_move_tree_storereserved_qty_app" model="ir.ui.view">
        <field name="name">stock picking move tree store reserved quantity For Inventory APP</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_picking_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='reserved_availability']" position="after">
                <!-- Add your fields or attributes here -->
                <field name="reserved_availability_stored"/>
                <field name="temp_barcode" invisible="1" />
            </xpath>
        </field>
    </record>

    <record id="view_stock_quant_tree_store_availably_qty_app" model="ir.ui.view">
        <field name="name">Stock Quant Tree Store Available Qty For Inventory App</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='available_quantity']" position="after">
                <field name="available_quantity_stored" invisible="0" />
            </xpath>
        </field>
    </record>
</odoo>