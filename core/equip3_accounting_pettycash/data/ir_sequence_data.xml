<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="account_pettycash_sequence" model="ir.sequence">
            <field name="name">Account Pettycash Sequence</field>
            <field name="code">account.pettycash.sequence</field>
            <field name="prefix">PCS/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    
        <record id="account_pettycash_voucher_sequence" model="ir.sequence">
            <field name="name">Account Pettycash Sequence</field>
            <field name="code">account.pettycash.voucher.wizard.seq</field>
            <field name="prefix">PCV/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    
    </data>
</odoo>
