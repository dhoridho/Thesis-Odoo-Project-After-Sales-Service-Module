<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_report_petty_cash_voucher" model="ir.actions.report">
        <field name="name">Print Petty Cash Voucher</field>
        <field name="model">account.pettycash.voucher.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">equip3_accounting_pettycash.report_petty_voucher</field>
        <field name="report_file">equip3_accounting_pettycash.report_petty_voucher</field>
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>
    </record>
    
    <record id="action_report_multipayment_voucher" model="ir.actions.report">
        <field name="name">Vouchers</field>
        <field name="model">account.pettycash.voucher.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">equip3_accounting_pettycash.report_petty_voucher</field>
        <field name="report_file">equip3_accounting_pettycash.report_petty_voucher</field>
        <field name="binding_model_id" ref="equip3_accounting_operation.model_account_multipayment"/>
        <field name="binding_type">report</field>
    </record>

    <record id="action_report_petty_cash_vouchers" model="ir.actions.report">
        <field name="name">Print Petty Cash Vouchers</field>
        <field name="model">account.pettycash</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">equip3_accounting_pettycash.report_petty_vouchers</field>
        <field name="report_file">equip3_accounting_pettycash.report_petty_vouchers</field>
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>
    </record>

    <record id="action_report_petty_cash_history" model="ir.actions.report">
        <field name="name">Petty Cash Voucher History</field>
        <field name="model">account.pettycash</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">equip3_accounting_pettycash.report_petty_history</field>
        <field name="report_file">equip3_accounting_pettycash.report_petty_history</field>
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>
    </record>
    <record id="action_report_petty_cash_activity" model="ir.actions.report">
        <field name="name">Petty Cash Cash Activity</field>
        <field name="model">account.pettycash</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">equip3_accounting_pettycash.report_petty_activity</field>
        <field name="report_file">equip3_accounting_pettycash.report_petty_activity</field>
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>
    </record>

    <record id="export_dowload_petty_action" model="ir.actions.server">
        <field name="name">Print Petty Cash Voucher</field>
        <field name="model_id" ref="model_account_pettycash_voucher_wizard"/>
        <field name="binding_model_id" ref="model_account_pettycash_voucher_wizard"/>
        <field name="state">code</field>
        <field name="code">action = records.download_petty()</field>
    </record>
    <record id="export_dowload_petty_vouchers_action" model="ir.actions.server">
        <field name="name">Print Petty Cash Vouchers</field>
        <field name="model_id" ref="model_account_pettycash"/>
        <field name="binding_model_id" ref="model_account_pettycash"/>
        <field name="state">code</field>
        <field name="code">action = records.download_petty_vouchers()</field>
    </record>
    <record id="export_dowload_petty_history_action" model="ir.actions.server">
        <field name="name">Petty Cash Voucher History</field>
        <field name="model_id" ref="model_account_pettycash"/>
        <field name="binding_model_id" ref="model_account_pettycash"/>
        <field name="state">code</field>
        <field name="code">action = records.download_petty_history()</field>
    </record>
    <record id="export_dowload_petty_activity_action" model="ir.actions.server">
        <field name="name">Petty Cash Cash Activity</field>
        <field name="model_id" ref="model_account_pettycash"/>
        <field name="binding_model_id" ref="model_account_pettycash"/>
        <field name="state">code</field>
        <field name="code">action = records.download_petty_activity()</field>
    </record>

</odoo>
