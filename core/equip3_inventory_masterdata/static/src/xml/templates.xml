<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <div t-extend="3DView">
        <t t-jquery="#location_info" t-operation="after">
            <div id="location_full_info" data-current="0">
                <span id="close_location_all_info_icon" class="fa fa-times" aria-hidden="false" title="Close"></span>
                <div class="ml-4" style="margin-top: -15px;">
                    <span id="location_name"></span>
                </div>
                <div class="ml-4 mt-2">
                    <span class="occupied"><b>Occupied (%): </b></span>
                    <span id="occupied_percent"></span>
                </div>
                <div id="products_stock_data" class="ml-4 mt-2">
                    <span class="current_stock"><b>Current Stock: </b></span>
                </div>
            </div>
        </t>
    </div>
</templates>