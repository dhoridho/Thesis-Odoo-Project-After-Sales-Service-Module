                                   t-att-class="{'oe_hidden': !hasProductPackaging,}"
                                   aria-label="Set Packaging"
                                   role="img"
                                   title="Set Packaging"
                                   style="font-size:15px;"
                                />
                                <i t-on-click.stop="updateStockEachLocation"
