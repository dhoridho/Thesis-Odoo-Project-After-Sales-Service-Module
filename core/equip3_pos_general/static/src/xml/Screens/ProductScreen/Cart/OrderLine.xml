<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
     <t t-name="RetailOrderline2" owl="1">
        <tr t-on-click="selectLine" class="orderline receipt-line" t-att-class="addedClasses"
