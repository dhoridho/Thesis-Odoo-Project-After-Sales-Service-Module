                        <span style="color:white;">--</span>
                        <span class="line-product_name" t-att-pos_coupon_line="props.line.pos_coupon_reward_description?true:false">
                            <span><t t-esc="props.line.get_orderline_product_name()"/></span>
                        </span> 
                    </p>
                 <!--    <t t-if="env.pos.config.display_image_orderline and !env.isMobile and state.screen == 'Products'">
                        <img class='orderline_image' t-att-src="props.line.get_line_image()"/>
                    </t> -->
                    <t t-if="props.line.get_lot_lines()">
                        <ul class="info-list">
                            <t t-foreach="props.line.get_lot_lines()" t-as="lot" t-key="lot.cid">
                                <li>
                                    SN <t t-esc="lot.attributes['lot_name']"/>
                                </li>
                            </t>
                        </ul>
                        <br/>
                    </t>
                    <p class="order-line-discount" style="font-size: 13px;line-height: 22px;margin: unset;font-weight: 100;padding-left: 25px;" t-if="props.line.get_all_total_discount()">
                        <span>
                            Discount : <t t-esc="env.pos.format_currency(props.line.get_all_total_discount(),false,env.pos.get_order().currency)"/>
                            <span t-if="props.line.pos_coupon_reward_discount" class="order-line-discount-coupon">
                                <t t-esc="props.line.pos_coupon_reward_discount"></t>
                            </span>
                        </span>
                    </p>
                    <p t-if="env.pos.config.display_tax_orderline and props.line.get_tax()" style="font-size: 13px;line-height: 22px;margin: unset;font-weight: 100;padding-left: 25px;" > Taxes :
                        <ul style=" display: inline-flex; position: absolute; float: left;min-width:100%;width:100%;" >
                                <t t-set="taxes" t-value="props.line.get_taxes()"/>
                                <t t-foreach="taxes || []" t-as="tax" t-key="tax.id">
                                    <li class="info" style="margin-right: 3px; margin-left: 3px;">
                                        <t t-esc="tax.name"/>
                                    </li>
                                </t>
                        </ul>
                    </p>
                    <div style="display:block;position:relative;width:100%;">
