                        <ul style="display: inline-block; position: relative; width: 100%; list-style-type: none; float: none; vertical-align: top;" class="pos_price_ul_screen">
                            <li t-if="env.pos.config.allow_remove_line and state.screen == 'Products'" style="font-size: 11px; font-weight: bold;" class="li_allow_remove_line">
                                <i t-on-click.stop="removeLine"
                                   class="oe_link_icon fa fa-times oe_icon line-icon"
                                   style="padding: 0px;margin: 0px;color: white;font-size: 11px;background: red;border-radius: 5px;position: relative;top: -3px;min-width: unset;min-height: unset;width: 21px;height: 24px; "
                                   aria-label="Remove Line"
                                   role="img"
                                   t-if="env.pos.config.allow_remove_line"
                                   title="Remove Line"
                                />
                            </li>
                            <li class="info info-subtotal" style=" font-size: 11px; font-weight: bold;width:unset !important;">
                                <span class="price" style="font-size: 11px; font-weight: bold;color:black;">
                                    <t t-esc="env.pos.format_currency(props.line.get_display_price(),false,env.pos.get_order().currency)"/>
                                </span>
                            </li>
                            <li t-on-click.stop="sendInput('-')"
                               class="oe_link_icon fa fa-minus oe_icon line-icon"
                               aria-label="Down Qty 1"
                                style="font-size: 11px;width:unset !important;position:relative;margin-top:-5px;"
                               role="img"
                               t-if="!env.pos.config.validate_quantity_change and env.pos.config.allow_qty and props.line.selected and state.screen == 'Products'"
                               title="Down Qty 1"
                            />


                            <li class="info price" style="margin-right:unset !important;font-size: 11px; font-weight: bold;width:unset !important;"  t-if="!env.pos.config.validate_quantity_change and env.pos.config.allow_qty and props.line.selected and state.screen == 'Products'">
                                <t t-esc="props.line.quantity"/>
                                <span> </span>
                                <t t-if="props.line.get_quantity_str() !== '1' || props.line.selected ">
                                    <t t-esc="props.line.get_unit().name" />
                                </t>    
                            </li>

                            <li class="info price" style=" font-size: 11px; font-weight: bold;width:unset !important;"  t-if="env.pos.config.validate_quantity_change or !env.pos.config.allow_qty or !props.line.selected">
                                <t t-esc="props.line.quantity"/>
                                <span> </span>
                                <t t-if="props.line.get_quantity_str() !== '1' || props.line.selected ">
                                    <t t-esc="props.line.get_unit().name" />
                                </t>    
                            </li>




                            <li t-on-click.stop="sendInput('+')"
                               class="oe_link_icon fa fa-plus oe_icon line-icon"
                               aria-label="Up Qty 1"
                                style="font-size: 11px;width:unset !important;position:relative;margin-top:-5px;margin-right:unset !important;"
                               role="img"
                               title="Up Qty 1"
                               t-if="!env.pos.config.validate_quantity_change and env.pos.config.allow_qty and props.line.selected and state.screen == 'Products'"
                            />
                            <t  t-if="state.screen == 'Products'">
                                <li class="info price info-base-price" style="font-size: 11px;width:unset !important;">
                                    <t t-esc="env.pos.format_currency(props.line.get_price_with_pricelist(),false,env.pos.get_order().currency)"/>
                                </li>
                            </t>
                            <t t-else="">
                                <li class="info price" style="font-size: 11px;width:unset !important;">
                                    <t t-esc="env.pos.format_currency_no_symbol(props.line.price)"/>
                                </li>
                            </t>
                            <i t-on-click.stop="setNotes"
                               t-if="env.pos.config.module_pos_restaurant"
                               class="oe_link_icon fa fa-edit oe_icon line-icon"
                               aria-label="Set Notes"
                               role="img"
                               style="font-size:15px;"
                               title="Set Notes"/>
                               <span class="product-name2" style="font-size: 11px;">
                                    <t t-esc="props.line.get_line_note()"/>
                                </span>
                        </ul>
                    </div>
   
                

                  
                </div>
                
            </td>
        </tr>
    </t>

