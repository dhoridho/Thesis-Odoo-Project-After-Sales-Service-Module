<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <template id="assets_backend"  inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/backend_views.js" />
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/form_controller.js"/>
        </xpath>
    </template>

    <!-- After closing the PoS, open the dashboard menu -->
    <record id="action_client_pos_menu" model="ir.actions.client">
        <field name="name">Open POS Menu</field>
        <field name="tag">reload</field>
        <field name="params" eval="{'menu_id': ref('point_of_sale.menu_point_root')}"/>
    </record>
    
    <template id="custom_pos_basic" name="custom pos basic" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">

            <script type="text/javascript" src="/equip3_pos_general/static/src/js/ChromeWidgets/CartPaymentButton.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/ChromeWidgets/CardGroupImg.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/ChromeWidgets/CardPaymentImg.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/ChromeWidgets/GroupCardPaymentWidegt.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/ChromeWidgets/PopUpCardPayment.js"/>

            <script type="text/javascript" src="/equip3_pos_general/static/src/js/PopUps/CashierSelectionPopup.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/PopUps/CrossSalePopUps.js"/>

            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Core/models.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Core/Database.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/PosOrdersListScreen.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/pos_order.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/CashierName.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/LoginScreen.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/ProductsWidgetControlPanel.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/PosOrderDetail.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Chrome.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/SaleOrderDetail.js"/>
            
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Screens/ProductScreen/ProductScreenSection1.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Screens/ProductScreen/Cart/NumpadWidget.js"/>
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Screens/ProductScreen/Cart/OrderLine.js"/>

            <script type="text/javascript" src="/equip3_pos_general/static/src/js/pos_order_return.js"></script>
        </xpath>
        <xpath expr="//script[@src='/point_of_sale/static/src/js/ChromeWidgets/HeaderButton.js']" position="replace">
            <script type="text/javascript" src="/equip3_pos_general/static/src/basic/js/HeaderButton.js"/>
        </xpath>
        <xpath expr="//script[@src='/point_of_sale/static/src/js/Popups/ErrorTracebackPopup.js']" position="replace">
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/PopUps/POSErrorTracebackPopup.js"/>
        </xpath>
        <xpath expr="//script[@src='/point_of_sale/static/src/js/Popups/ErrorPopup.js']" position="after">
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/PopUps/PosErrorMessagePopup.js"/>
        </xpath>
        <!-- <xpath expr="//link[@href='/point_of_sale/static/src/css/pos.css']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/basic/static/src/css/pos.css"/>
        </xpath> -->
    
    </template>


    <template id="custom_pos" name="custom pos inherit" inherit_id="equip3_pos_masterdata.assets_pos">
        <xpath expr="//link[@href='/equip3_pos_masterdata/static/src/css/768.css']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/768.css"/>
        </xpath>
        <xpath expr="//link[@href='/equip3_pos_masterdata/static/src/css/1200.css']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/1200.css"/>
        </xpath>
        <xpath expr="//link[@href='/equip3_pos_masterdata/static/src/css/Custom.css']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/Custom.css"/>
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/responsive.css"/>
        </xpath>
        <xpath expr="//link[@href='/equip3_pos_masterdata/static/src/css/GiftCard.css']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/GiftCard.css"/>
        </xpath>
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/TableWidget.css"/>
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/ReservationPopup.css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
        </xpath> 
        <xpath expr="//link[@href='/equip3_pos_masterdata/static/src/css/Core.scss']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/Core.scss"/>
        </xpath>
        <xpath expr="//link[@href='/equip3_pos_masterdata/static/src/css/Navigation.css']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/Navigation.css"/>
        </xpath>
        <xpath expr="//link[@href='/equip3_pos_masterdata/static/src/css/Ribbon.css']" position="replace">
            <link rel="stylesheet" type="text/css" href="/equip3_pos_general/static/src/css/Ribbon.css"/>
        </xpath>
        <xpath expr="//script[@src='/equip3_pos_masterdata/static/src/js/Screens/ProductScreen/ProductScreen.js']" position="after">
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Screens/ProductScreen/ProductScreen.js"/>
        </xpath>
        <xpath expr="//script[@src='/equip3_pos_masterdata/static/src/js/Core/BigData.js']" position="after">
            <script type="text/javascript" src="/equip3_pos_general/static/src/js/Core/BigData.js"/>
        </xpath>
    </template>

</odoo>
