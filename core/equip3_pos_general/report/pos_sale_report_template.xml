<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="pos_sale_report_template">
            <t t-call="web.html_container">
                <t t-call="web.basic_layout">

                    <t t-set="report_data" t-value="env['pos.sale.report']._report_data(docs)"/>

                    <div class="page">

                        <t t-foreach="docs" t-as="o">
                            <table border="0" cellpadding="0" cellspacing="0" style="line-height: 15px;" width="100%">
                                <tr>
                                    <td align="left" style="color:#4D4D4F; padding:0px 15px;width:40%; vertical-align: middle!important;" class="text-center">
                                        <img style="max-height:100px; display:inline-block;"  t-att-src="'data:image/png;base64,%s' % to_text(o.company_id.logo)"/> 
                                    </td>
                                </tr>
                            </table> 
                            <div class="header_company text-center" t-att-style="'font-family:'+user_id.company_id.font_id.family+';font-size:'+str(user_id.company_id.font_size)+'px;'">
                                <div style="color: #51565b;">
                                  <div id="report_title_super_top" style="margin-top:10px;">
                                      <div><strong>Address:</strong></div>
                                      <div t-field="user_id.company_id.partner_id"/>
                                  </div>
                                  <t t-if="user_id.company_id.partner_id.street">
                                      <div>
                                          <span t-esc="user_id.company_id.partner_id.street"/>
                                          <br/>
                                      </div>
                                  </t>
                                  <t t-if="user_id.company_id.partner_id.mobile">
                                      <strong>Mobile:</strong>
                                      <span t-field="user_id.company_id.partner_id.mobile"/>
                                      <br/>
                                  </t>
                                  <t t-if="user_id.company_id.partner_id.email">
                                      <strong>Email:</strong>
                                      <span t-field="user_id.company_id.partner_id.email"/> 
                                  </t>
                                </div>
                                <div id="ph_report" t-att-style="'font-family:'+user_id.company_id.font_id.family+';font-size:'+str(user_id.company_id.font_size)+'px;'"/>
                            </div>
                            <br/>
                            <table width="100%">
                                <tr>
                                    <td style="padding-top:1px;border:1px solid black;color:#ffffff; background-color: #333333;" class='text-center'>
                                        <span>
                                            <h2 t-if="o.state!='opened'">Z Report</h2>
                                             <h2 t-if="o.state=='opened'">X Report</h2>
                                        </span>
                                    </td>
                                </tr>
                            </table>
                            <br/>
                            <!--HEADER1-->
                            <table style="width:100%; color:#3A3B3C;">
                                <tr>
                                    <td width="35%" class="text-left">
                                        Print Date
                                    </td>
                                    <td width="5%" class="text-left">
                                        :
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="o.get_current_date()"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="35%" class="text-left">
                                        Print Time
                                    </td>
                                    <td width="5%" class="text-left">
                                        :
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="o.get_current_time()"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="35%" class="text-left">
                                        Branch
                                    </td>
                                    <td width="5%" class="text-left">
                                        :
                                    </td>
                                    <td class="text-left">
                                        <t t-if="o.pos_branch_id">
                                            <span t-esc="o.pos_branch_id.name"/>
                                        </t>
                                        <t t-else="">
                                            <span>-</span>
                                        </t>
                                        
                                    </td>
                                </tr>
                                <tr>
                                    <td width="35%" class="text-left">
                                        Cashier
                                    </td>
                                    <td width="5%" class="text-left">
                                        :
                                    </td>
                                    <td class="text-left">
                                        <t t-if="o.cashier_id">
                                            <span t-esc="o.cashier_id.name"/>
                                        </t>
                                        <t t-else="">
                                            <span>-</span>
                                        </t>
                                        
                                    </td>
                                </tr>
                            </table>
                            <br/>

                            <!--HEADER 2-->
                            <table style="width:100%; color:#3A3B3C;">
                                <tr>
                                    <td width="35%" class="text-left">
                                        Session Name
                                    </td>
                                    <td width="5%" class="text-left">
                                        :
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="o.name"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="35%" class="text-left">
                                        Session State
                                    </td>
                                    <td width="5%" class="text-left">
                                        :
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="o.state"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="35%" class="text-left">
                                        Started at
                                    </td>
                                    <td width="5%" class="text-left">
                                        :
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="o.get_session_date(o.start_at)"/>
                                    </td>
                                </tr>
                                <t t-if="o.stop_at">
                                    <tr>
                                        <td width="35%" class="text-left">
                                            Ended at
                                        </td>
                                        <td width="5%" class="text-left">
                                            :
                                        </td>
                                        <td class="text-left">
                                            <span t-esc="o.get_session_date(o.stop_at)"/>
                                        </td>
                                    </tr>
                                </t>
                            </table>
                            <br/>

                            <!--SALE SUMMARY-->
                            <table width="100%">
                                <tr>
                                    <td colspan="2" style="padding-top:1px;border:1px solid black;color:#ffffff; background-color: #333333;" class='text-center'><span><h3>Sales Summary</h3></span></td>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        Opening Total
                                    </td>
                                    <td class="text-right">
                                        <span t-esc="o.cash_opening_balance" class="text-right" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        Closing Total
                                    </td>
                                    <td class="text-right">
                                        <span t-esc="o.cash_register_balance_end_real" class="text-right" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        Gross Sales
                                    </td>
                                    <td class="text-right">
                                        <t t-set="t_gross_sales" t-value="o.get_total_first() + o.get_total_discount()"/>
                                        <span t-esc="t_gross_sales" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        Net Sales
                                    </td>
                                    <td class="text-right">
                                        <t t-set="t_net_sales" t-value="(o.get_total_first() + o.get_total_discount()) - o.get_total_tax()"/>
                                        <span t-esc="t_net_sales" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        Tax
                                    </td>
                                    <td class="text-right">
                                        <span t-esc="o.get_total_tax()" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        Discount
                                    </td>
                                    <td class="text-right">
                                        <span t-esc="o.get_total_discount()" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>


                                <t t-if="report_data and 'receivable_by_session_id' in report_data">
                                    <tr class="tr-sales-summary_receivable">
                                        <td class="text-left">
                                            Receivable
                                        </td>
                                        <td class="text-right">
                                            <span t-esc="report_data['receivable_by_session_id'].get(o.id, 0.0)" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                        </td>
                                    </tr>
                                </t>

                                <tr>
                                    <td class="text-left">
                                        Grand Total
                                    </td>
                                    <td class="text-right">
                                        <span t-esc="o.get_total_first()" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>
                            </table>
                            <br/>
                        

                            <!--PAYMENT-->
                            <table width="100%">
                                <tr>
                                    <td colspan="2" style="padding-top:1px;border:1px solid black;color:#ffffff; background-color: #333333;" class='text-center'><span><h3>Payments</h3></span></td>
                                </tr>
                                <t t-set="pos_payment_total" t-value="0.0" />
                                <tr t-foreach="o.get_payments_amount()" t-as="payment_amount">
                                    <t t-if="payment_amount.get('name')">
                                        <td class="text-left">
                                            <span t-esc="payment_amount.get('name')"/>
                                        </td>
                                    </t>
                                    <td class="text-right">
                                        <span t-esc="payment_amount.get('amount')" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                    <t t-set="pos_payment_total" t-value="pos_payment_total + payment_amount.get('amount')"/>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        <b><span>Total</span></b>
                                    </td>
                                    <td style="text-align:right;">
                                        <t t-esc="pos_payment_total" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                </tr>
                            </table>
                            <br/>

                            <!--PRODUCT DETAILS-->
                            <t t-if="o.get_product_category()">
                                <table width="100%">
                                    <tr>
                                        <td colspan="3" style="padding-top:1px;border:1px solid black;color:#ffffff; background-color: #333333;" class='text-center'><span><h3>Product Details</h3></span></td>
                                    </tr>
                                    <t t-set="pos_product_count" t-value="0.0"/>
                                    <t t-set="pos_product_total" t-value="0.0"/>

                                    <tr t-foreach="o.get_product_category()" t-as="l">
                                        <td width="55%" class="text-left">
                                            <span t-esc="o.get_product_name(l.get('pos_categ_id')) or 'Others'"/>
                                        </td>
                                        <td width="5%" class="text-left">
                                        <span t-esc="l.get('price')"/>
                                        </td>
                                        <td width="40%" class="text-right">
                                            <span t-esc="l.get('price_total')" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                        </td>
                                        <t t-set="pos_product_count" t-value="pos_product_count + l.get('price')"/>
                                        <t t-set="pos_product_total" t-value="pos_product_total + l.get('price_total')"/>
                                    </tr>
                                    <tr>
                                        <td class="text-left">
                                          <b><span>Total</span></b>
                                        </td>
                                        <td style="text-align:right;">
                                            <b><t t-esc="pos_product_count"/></b>
                                        </td>
                                        <td style="text-align:right;">
                                        <b><t t-esc="pos_product_total" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/></b>
                                    </td>
                                    </tr>
                                </table>
                            </t>
                            <br/>

                            <!--DISCOUNT-->
                            <table width="100%" class="table-discount_summary">
                                <tr>
                                    <td colspan="3" style="padding-top:1px;border:1px solid black;color:#ffffff; background-color: #333333;" class='text-center'><span><h3>Discount Summary</h3></span></td>
                                </tr>
                                <t t-set="pos_promotion_count" t-value="0.0" />
                                <t t-set="pos_promotion_total" t-value="0.0" />
                              
                                <tr t-foreach="o.get_discount_list()" t-as="l">
                                    <td width="55%" class="text-left">
                                        <span t-esc="o.get_promotion_name(l.get('promotion_id')) or 'Others'"/>
                                    </td>
                                    <td width="5%" class="text-left">
                                        <span t-esc="l.get('promo_count')"/>
                                    </td>
                                    <td width="40%" class="text-right">
                                        <span t-esc="l.get('promo_val')" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                    </td>
                                    <t t-set="pos_promotion_count" t-value="pos_promotion_count + l.get('promo_count')"/>
                                    <t t-set="pos_promotion_total" t-value="pos_promotion_total + l.get('promo_val')"/>
                                </tr>
                                <tr>
                                    <td class="text-left">
                                        <b><span>Total</span></b>
                                    </td>
                                    <td style="text-align:right;">
                                        <b><t t-esc="pos_promotion_count"/></b>
                                    </td>
                                    <td style="text-align:right;">
                                        <b><t t-esc="pos_promotion_total" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/></b>
                                    </td>
                                </tr>
                            </table>
                            <br/>


                            

                            <table width="100%" class="table-receivable_payment" t-if="report_data and 'payment_receivable_by_session_id' in report_data" id="id-table-receivable_payment">
                                <t t-set="receivable_payments" t-value="report_data['payment_receivable_by_session_id'].get(o.id, [])"/>
                                <t t-set="receivable_payment_total" t-value="0.0" />
                                <tr>
                                    <td colspan="2" style="padding-top:1px;border:1px solid black;color:#ffffff; background-color: #333333;" class='text-center'>
                                        <span><h3>Receivable Payment</h3></span>
                                    </td>
                                </tr>
                              
                                <tr t-foreach="receivable_payments" t-as="l">
                                    <td class="text-left">
                                        <span t-esc="l.get('payment_name') or ' - '"/>
                                    </td>
                                    <td class="text-right">
                                        <span>
                                            <t t-esc="l.get('total_amount')" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                                        </span>
                                    </td>
                                    <t t-set="receivable_payment_total" t-value="receivable_payment_total + l.get('total_amount')"/>
                                </tr>

                                <tr>
                                    <td class="text-left">
                                        <b><span>Total</span></b>
                                    </td>
                                    <td class="text-right">
                                        <b><t t-esc="receivable_payment_total" t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/></b>
                                    </td>
                                </tr>
                            </table>
                            <br/>
                            


                            <p style="page-break-after: always;"></p>
                        </t>
                    </div>

                    <div class="footer" t-att-style="'font-family:'+user_id.company_id.font_id.family+';font-size:'+str(user_id.company_id.font_size)+'px;'" t-if="user_id.company_id.report_footer_selection == 'standard'">
                        <div class="text-center" style="border-top: 1px solid black;">
                            <ul class="list-inline mb4">
                                <li t-if="user_id.company_id.phone">Phone: 
                                    <span t-field="user_id.company_id.phone"/></li>
                                <li t-if="user_id.company_id.email">Email: 
                                    <span t-field="user_id.company_id.email"/></li>
                                <li t-if="user_id.company_id.website">Web: 
                                    <span t-field="user_id.company_id.website"/></li>
                                <li t-if="user_id.company_id.vat">
                                    <t t-esc="user_id.company_id.country_id.vat_label or 'TIN'"/>: 
                                    <span t-field="user_id.company_id.vat"/></li>
                            </ul>
                            <div name="financial_infos">
                                <span t-field="user_id.company_id.report_footer"/>
                            </div>
                            <div class="text-muted">Page: 
                                <span class="page"/>
                                / 
                                <span class="topage"/></div>
                        </div>
                    </div>
                </t>
            </t>
        </template>

