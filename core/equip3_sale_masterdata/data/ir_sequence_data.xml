<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="sequence_customer_sequence" model="ir.sequence">
        <field name="name">Customer Sequence</field>
        <field name="code">res.partner.customer.sequence</field>
        <field name="prefix">CS</field>
        <field name="padding">4</field>
    </record>

    <record id="sequence_product_pricelist_request" model="ir.sequence">
        <field name="name">Pricelists Request</field>
        <field name="code">product.pricelist.request</field>
        <field name="prefix">PLR/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field name="use_date_range" eval="True"/>
        <field name="company_id" eval="False"/>
    </record>

</odoo>