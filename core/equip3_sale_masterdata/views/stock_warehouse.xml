<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- stock.warehouse inherit form view -->
        <record id="view_warehouse_inherit" model="ir.ui.view">
            <field name="name">stock.warehouse.inherit.view.form</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <!-- Add your xpath here -->
                <field name="lot_stock_id" position="after">
                    <field name="default_delivery_location_id"/>
                </field>
            </field>
        </record>
    </data>
</odoo>