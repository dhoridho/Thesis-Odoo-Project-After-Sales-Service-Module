<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <menuitem
        id="menu_expired_product"
        name="Expired Products"
        sequence="5"
        parent="stock.menu_stock_root"
    />

    <record id="menu_expired_product" model="ir.ui.menu">
        <field name="equip_icon_class">icon_expired</field>
    </record>

    <menuitem
        id="aspl_product_expiry_alert.stock_picking_type_menu1"
        name="Product Expiry Dashboard"
        sequence="1"
        parent="menu_expired_product"
    />

    <menuitem name="Expired Product Notes"
        id="menu_expried_product_notes"
        parent="menu_expired_product"
        action="action_expired_product_notes"
        sequence="3" />

    <menuitem
        id="aspl_product_expiry_alert.menu_product_expiry_config"
        parent="stock.menu_stock_config_settings"
        action="aspl_product_expiry_alert.action_product_expiry_config" />

    <menuitem
        id="aspl_product_expiry_alert.stock_picking_type_menu1"
        parent="stock.menu_stock_root"
        name="Product Expiry Dashboard"
        active="False"
    />

    <record id="aspl_product_expiry_alert.stock_picking_type_menu1" model="ir.ui.menu">
        <field name="name">Product Expiry Dashboard Configuration</field>
    </record>

</odoo>
