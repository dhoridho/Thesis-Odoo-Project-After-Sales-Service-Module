#. module: equip3_inventory_tracking
#: model:mail.template,body_html:equip3_inventory_tracking.email_template_expired_alert_stock_production_lot
msgid ""
"\n"
"                        <p>Dear ${ctx.get('responsible_users')}, these are "
"the following products that have reached it’s alert date and will be "
"expiring soon.</p>\n"
"                        <table cellspacing=\"0\" cellpadding=\"0\" "
"style=\"width:100%;border-collapse:collapse;background:inherit;color:"
"inherit;border:1px solid black;\">\n"
"                            <thead style=\"border:1px solid black;\">\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">No</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Location</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Lot/Serial Number</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Product</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Expiry Date</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Quantity</th>\n"
"                                <th></th>\n"
"                            </thead>\n"
"                            <tbody style=\"border:1px solid black;\">\n"
"                                % for quant in ctx.get('quant'):\n"
"                                    % set lot_url = quant.lot_id."
"get_lot_url()\n"
"                                    <tr style=\"border:1px solid black;"
"\">\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${loop.index}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.location_id."
"display_name}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.lot_id.name}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.product_id.name}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.lot_id.expiration_date}"
"</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.quantity}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">\n"
"                                            <a href=${lot_url} "
"style=\"background-color:#875A7B; padding: 10px; text-decoration: none; "
"color: #fff; border-radius: 5px;\">\n"
"                                                    View ${quant.lot_id."
"name}\n"
"                                            </a>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                % endfor\n"
"                            </tbody>\n"
"                        </table>\n"
"                        <p>Thank you.</p>\n"
"                        \n"
"            "
msgstr ""
"\n"
"                        <p>Kepada ${ctx.get('responsible_users')}, berikut "
"ini adalah produk-produk yang telah mencapai tanggal peringatan dan akan "
"segera kedaluwarsa.</p>\n"
"                        <table cellspacing=\"0\" cellpadding=\"0\" "
"style=\"width:100%;border-collapse:collapse;background:inherit;color:"
"inherit;border:1px solid black;\">\n"
"                            <thead style=\"border:1px solid black;\">\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Nomor</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Lokasi</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Lot/Nomor Seri</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Produk</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Tanggal Kadaluarsa</th>\n"
"                                <th class=\"text-center\" "
"style=\"border:1px solid black;\">Jumlah</th>\n"
"                                <th></th>\n"
"                            </thead>\n"
"                            <tbody style=\"border:1px solid black;\">\n"
"                               % Untuk quant di ctx.get('quant'):\n"
"                                   % set lot_url = quant.lot_id."
"get_lot_url()\n"
"                                    <tr style=\"border:1px solid black;"
"\">\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${loop.index}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.location_id."
"display_name}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.lot_id.name}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.product_id.name}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.lot_id.expiration_date}"
"</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">${quant.quantity}</td>\n"
"                                        <td style=\"border:1px solid black;"
"text-align:center;padding:10px 0 10px 0;\">\n"
"                                            &lt;a href=${lot_url} style = "
"\"background-color:#875A7B; padding: 10px; text-decoration: none; color: "
"#fff; border-radius: 5px;\">\n"
"                                                    Lihat ${quant.lot_id."
"name}\n"
"                                            </a>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                               % endfor\n"
"                            </tbody>\n"
"                        </table>\n"
"                        <p>Terima kasih.</p>\n"
"                        \n"
"            "

