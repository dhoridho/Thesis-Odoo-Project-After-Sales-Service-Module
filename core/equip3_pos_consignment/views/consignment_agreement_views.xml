<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_consignment_agreement_new_form_inherit" model="ir.ui.view">
        <field name="name">view.consignment.agreement.new.form.inherit</field>
        <field name="model">consignment.agreement</field>
        <field name="inherit_id" ref="equip3_inventory_consignment.view_consignment_agreement_new_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='button_box_consignment']" position="inside"> 
                <button name="action_view_pos_order_line" type="object" class="oe_stat_button" icon="fa-shopping-cart o-hm-icon o-hm-smart-pos-orders">
                    <field name="pos_order_line_count" widget="statinfo" string="POS Order"/>
                </button>
            </xpath>
        </field>
    </record>

</odoo>