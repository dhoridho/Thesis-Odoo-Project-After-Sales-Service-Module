<odoo>
      <data noupdate="0">

        <record id="qiscus_wa_template_applicant_cron" model="ir.cron">
            <field name="name">Syncronize Qiscus Template</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 23:00:00')" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_qiscus_wa_template"/>
            <field name="state">code</field>
            <field name="doall" eval="True"/>
            <field name="code">model.ir_cron_syncronize_template()</field>
        </record>


      </data>


</odoo>