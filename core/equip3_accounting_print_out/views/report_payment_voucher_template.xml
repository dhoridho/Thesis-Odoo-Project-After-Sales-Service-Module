<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


		<record id="aos_account_voucher.action_report_account_voucher" model="ir.actions.report">
		    <field name="paperformat_id" ref="base.paperformat_euro" />
		</record>
    	
    	
        <template id="aos_account_voucher.report_account_voucher">
            <t t-call="web.html_container">
			<t t-call="web.external_layout">
			<t t-set="o" t-value="o.with_context(lang=lang)" />
                <t t-set="address">
                    <address t-field="o.partner_id" t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />
                    <div t-if="o.partner_id.vat" class="mt16">
                        <t t-if="o.company_id.country_id.vat_label" t-esc="o.company_id.country_id.vat_label" id="inv_tax_id_label"/>
                        <t t-else="">Tax ID</t>: <span t-field="o.partner_id.vat"/></div>
                </t>
				 <h2>	

                        <span t-if="o.voucher_type == 'purchase'">Other Expense</span>
                        <span t-if="o.voucher_type == 'sale'">Other Income</span>
                        
                    <span  t-field="o.number"/>
                </h2>
				<div style="margin-top:18px;">
				        <table style="border:0;">
                			<!-- <tr style="border:0;">
				        		<td>Number Test</td> 
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td>
				        		<td style="width:320px;"><span t-field="o.number" /></td>
				        		<td>Account ID</td> 
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td> 
				        		<td><span t-field="o.account_id.name" /></td>
				        	</tr> -->
				        	<tr>
				        		<td>Date</td> 
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td>
				        		<td style="width:320px;"><span t-field="o.account_date" /></td>
				        		<td style="padding-left: 100px">Account</td>
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td> 
				        		<td><span t-field="o.account_id.name" /></td>
				        	
				        	</tr>
				        	<tr>
							
				        		<td>Cash In/Out</td> 
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td>
				        		<td style="width:320px;"><span t-field="o.voucher_type" /></td>
								<td style="padding-left: 100px">Account Code</td>
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td> 
				        		<td><span t-field="o.account_id.code" /></td>
				        		
				        	</tr>
				        	<tr style="border:0;">
								<td>Responsible</td> 
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td>
				        		<td><span t-field="o.user_id.name" /></td>
				        		<td style="width: 250px;padding-left: 100px">Journal Entry</td> 
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td>
				        		<td style="width:320px;"><span t-field="o.move_id.name" /></td>
				        		<!-- <td>Hal</td> 
				        		<td style="padding-left: 5px; padding-right: 5px"> : </td>
				        		<td><span t-field="o.name" /></td> -->
				        	</tr>
			            </table>
			   		</div>
                    <div class="page" style="margin-top:10px;">
                        <table class="table">
                            <thead>
                                <tr style="text-align:center; margin: auto;">
                                	<!-- <th>Number</th> -->
                                	<th>Account</th>
                                	<th>Description</th>
                                    <th>Quantity</th>
                                    <th>Unit Price</th>
                                    <th>Tax</th>
                                    <th>SubTotal</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr t-foreach="o.line_ids" t-as="line_id">
									<!-- <td style="text-align:center;">
							            <span t-esc="line_id_index+1"/>
									</td> -->
									<td style="text-align:center;">
										<t t-if="line_id.account_id.name">
									        <span t-esc="line_id.account_id.name"/>
									    </t>
									</td>
									<td style="text-align:center;">
										<t t-if="line_id.name">
									        <span t-esc="line_id.name"/>
									    </t>
									</td>
									<td style="text-align:center;" class="text-right">
										<t t-if="line_id.quantity">
									        <span t-field="line_id.quantity"/>
									    </t>
									</td>
									<td style="text-align:center;" class="text-right">
										<t t-if="line_id.price_unit">
									        <span t-field="line_id.price_unit"/>
									    </t>
									</td>
									<td style="text-align:center;" class="text-right">
										<t t-if="line_id.tax_ids">
									        <span t-esc="line_id.get_tax_string()"/>
									    </t>
									</td>
									<td style="text-align:center;" class="text-right">
										<t t-if="line_id.price_subtotal">
									        <span t-field="line_id.price_subtotal"/>
									    </t>
									</td>
                                </tr>
                            </tbody>
                        </table>
                            
                     	<div class="clearfix">
		                   <div id="total" class="row">
		                       <div t-attf-class="col-6  ml-auto">
		                           <table class="table table-sm">
		                               <tr class="border-black o_total">
		                                   <td><strong>Untaxed Amount</strong></td>
		                                   <td class="text-right">
		                                       <span style="padding-right:7px;" t-field="o.untax_amount"/>
		                                   </td>
		                               </tr>
		                               <tr style="border:0;">
		                                   <td><strong>Tax Amount</strong></td>
		                                   <td class="text-right">
		                                       <span style="padding-right:7px;" t-field="o.tax_amount"/>
		                                   </td>
		                               </tr>
		                               <tr class="border-black o_total">
		                                   <td><strong>Total</strong></td>
		                                   <td class="text-right">
		                                       <span style="padding-right:7px;" t-field="o.amount"/>
		                                   </td>
		                               </tr>
		                           </table>
		                       </div>
		                   </div>
		               </div>
                  </div>

			<div  style="margin-top:100px;">
			   <div class="text-left">
		           <div class="last-page">
		              <table class="table table-sm" style="border:1px solid black; width: 100%;" name="form_ceklist">
		                  <thead>
		                      <tr style="text-align:center; margin: auto;">
		                          <th style="border:1px solid black; width: 31%;">Created by :</th>
		                          <th style="border:1px solid black; width: 31%;">Received by :</th>
		                          <th style="border:1px solid black; width: 31%;">Approved by :</th>
		                      </tr>
		                  </thead>
		                  <tbody>
		                      <tr style="height: 80px;">
		                          <td style="border:1px solid black;"></td>
		                          <td style="border:1px solid black;"></td>
		                          <td style="border:1px solid black;"></td>
		                      </tr>
		                  </tbody>
		              </table>
		           </div>
	           </div>
		    </div>
				

