<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="0">

        <function name="write" model = "ir.model.data">
            <function name="search" model = "ir.model.data">
                <value eval="[('module', '=', 'school_fees'), ('name', '=', 'seq_student_payslip')]"/>
            </function>
            <value eval="{'noupdate': False}"/>
        </function>
        
        <record id="school_fees.seq_student_payslip" model="ir.sequence">
            <field name="name">student payslip</field>
            <field name="code">student.payslip</field>
            <field name="prefix">REC/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

        <function name="write" model = "ir.model.data">
            <function name="search" model = "ir.model.data">
                <value eval="[('module', '=', 'school_fees'), ('name', '=', 'seq_student_fees_register')]"/>
            </function>
            <value eval="{'noupdate': False}"/>
        </function>

        <record id="school_fees.seq_student_fees_register" model="ir.sequence">
            <field name="name">Student Fees Register</field>
            <field name="code">student.fees.register</field>
            <field name="prefix">FRG/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>


