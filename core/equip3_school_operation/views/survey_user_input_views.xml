<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="astudent_assignment_survey_user_input_form" model="ir.ui.view">
        <field name="name">hashmicro.survey.user_input.form</field>
        <field name="model">survey.user_input</field>
        <field name="inherit_id" ref="survey.survey_user_input_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='access_token']" position="after">
                <field name="assignment_id" invisible="1" />
                <field name="additional_id" invisible="1" />
                <field name="exam_id" invisible="1" />
            </xpath>
        </field>
    </record>
</odoo>