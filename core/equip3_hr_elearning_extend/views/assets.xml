<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>


    <template id="inherit_assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/css" href="/equip3_hr_elearning_extend/static/src/css/style.css"/>
            </xpath>
    </template>

    <template inherit_id="website_slides.slide_embed_assets" id="slide_embed_assets">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/css" href="/equip3_hr_elearning_extend/static/src/css/style.css"/>
            </xpath>
        </template>

    <template id="inherit_assets_backend" name="Assets Backend" inherit_id="web.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/equip3_hr_elearning_extend/static/src/css/elearning_flow.css"/>
            <script type="text/javascript" src="/equip3_hr_elearning_extend/static/src/js/elearning_flow.js"/>
        </xpath>
    </template>

</data>
</odoo>
