<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="elearning_configuration_action">
        <div class="elearning_flow_action">
            <div class="elearning_flow_action_content d-flex flex-column">
                <div class="content-header">
                    <div class="d-flex p-3 w-100">
                        <span>E-Learning Flow</span>
                    </div>
                </div>
                <div class="content-body">
                    <div class="container">
                        <div class="row h-100">
                            <div class="accordion">
                                <div class="accordion-item accordion-master-data">
                                    <h2 class="accordion-header" data-target="collapseMasterData">
                                        <button class="accordion-button" type="button">
                                        Master Data
                                        </button>
                                    </h2>
                                    <div id="collapseMasterData" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="website_slides.slide_channel_tag_group_action"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/course-group.svg"/>
                                                    </div>
                                                    <div class="button-text">Course Groups</div>
                                                </button>
                                                <button name="website_slides.website_slides_action_settings"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/settings.svg"/>
                                                    </div>
                                                    <div class="button-text">Settings</div>
                                                </button>
                                                <button name="website_slides.action_slide_tag"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/content-tags.svg"/>
                                                    </div>
                                                    <div class="button-text">Content Tags</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="accordion-item accordion-elearning-flow">
                                    <h2 class="accordion-header" data-target="collapsePayrollFlow">
                                        <button class="accordion-button" type="button">
                                        E-Learning Flow
                                        </button>
                                    </h2>
                                    <div id="collapsePayrollFlow" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">
                                                <button name="website_slides.slide_channel_action_overview"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-course">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/courses.svg"/>
                                                    </div>
                                                    <div class="button-text">Courses</div>
                                                </button>
                                                <div class="nd_connector connector_line_right_course">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>

                                                <button name="equip3_hr_elearning_extend.action_open_lms_equip3"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-lms-forum">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/lms-forum.svg"/>
                                                    </div>
                                                    <div class="button-text">LMS &#38; Forum</div>
                                                </button>

                                                <div class="nd_connector connector_line_up_contents">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <button name="website_slides.slide_slide_action"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-contents">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/contents-1.svg"/>
                                                    </div>
                                                    <div class="button-text">Contents</div>
                                                </button>
                                                <div class="nd_connector connector_line_right_contents">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector connector_line_down_contents">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>

                                                <button name="website_slides_survey.survey_survey_action_slides_report"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-certifications">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/certificate-1.svg"/>
                                                    </div>
                                                    <div class="button-text">Certifications</div>
                                                </button>

                                                <button name="website_slides.rating_rating_action_slide_channel"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-reviews">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/reviews-1.svg"/>
                                                    </div>
                                                    <div class="button-text">Reviews</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="accordion-item accordion-reporting">
                                    <h2 class="accordion-header" data-target="collapseReporting">
                                        <button name="reporting" class="accordion-button" type="button">
                                        Reporting
                                        </button>
                                    </h2>
                                    <div id="collapseReporting" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="website_slides.slide_channel_action_report"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/courses-1.svg"/>
                                                    </div>
                                                    <div class="button-text">Course</div>
                                                </button>

                                                <button name="website_slides.slide_slide_action_report"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/contents.svg"/>
                                                    </div>
                                                    <div class="button-text">Contents</div>
                                                </button>

                                                <button name="website_slides.rating_rating_action_slide_channel_report"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/reviews.svg"/>
                                                    </div>
                                                    <div class="button-text">Reviews</div>
                                                </button>

                                                <button name="website_slides.slide_question_action_report"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/quizzes.svg"/>
                                                    </div>
                                                    <div class="button-text">Quizzes</div>
                                                </button>

                                                <button name="website_slides_survey.survey_survey_action_slides_report"
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_elearning_extend/static/src/img/certificate.svg"/>
                                                    </div>
                                                    <div class="button-text">Certifications</div>
                                                </button>
                                                <!-- <div class="d-flex" style="width: 100%; gap:15px; ">
                                                    
                                                </div> -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>