<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
        <record id="job_estimate_new" model="ir.sequence">
            <field name="name">BOQ New</field>
            <field name="code">job.sequence.new</field>
            <field name="prefix">BOQ/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="job_estimate_vo" model="ir.sequence">
            <field name="name">BOQ VO</field>
            <field name="code">job.sequence.vo</field>
            <field name="prefix">BOQ/VO/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>