<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="sequence_sale_quotation_const" model="ir.sequence">
            <field name="name">Quotation Contruction</field>
            <field name="code">sale.order.quotation.const</field>
            <field name="prefix">SQ/CONS/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_sale_quotation_order_const" model="ir.sequence">
            <field name="name">Sale Order Construction</field>
            <field name="code">sale.order.quotation.order.const</field>
            <field name="prefix">SO/CONS/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_payment" model="ir.sequence">
            <field name="name">Payment Sequence</field>
            <field name="code">payment.sequence</field>
            <field name="prefix">PAY/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
