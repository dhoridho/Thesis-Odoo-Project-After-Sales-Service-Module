<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="purchase_order_assest" model="ir.sequence">
		<field name="name">Purchase Order Sequence</field>
		<field name="code">purchase.order.seqs.a</field>
		<field name="prefix">RFQ/A/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>

    <record id="purchase_request_assest_seq" model="ir.sequence">
		<field name="name">Purchase Request Sequence</field>
		<field name="code">purchase.request.seqs.a</field>
		<field name="prefix">PR/A/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>

	<record id="purchase_order_new_seqs_assest" model="ir.sequence">
		<field name="name">Purchase Order</field>
		<field name="code">purchase.order.seqs.assests</field>
		<field name="prefix">PO/A/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
		<field name="company_id" eval="False"/>
	</record>

    <record id="sh_purchase_agreement_seq3_assest" model="ir.sequence">
		<field name="name">Purchase Agreement Sequence</field>
		<field name="code">purchase.agreement.seqs.a</field>
		<field name="prefix">TE/A/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>
    <record id="sh_open_purchase_agreement_seq3_assest" model="ir.sequence">
		<field name="name">Open Purchase Agreement Sequence</field>
		<field name="code">purchase.agreement.seqs.a.open</field>
		<field name="prefix">OTE/A/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>

	<record id="seq_blanket_orders_seq_3_assest" model="ir.sequence">
		<field name="name">Blanket Order</field>
		<field name="code">purchase.requisition.blanket.order.new.a</field>
		<field name="prefix">BO/A/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
		<field name="company_id" eval="False"></field>
	</record>

    <record id="direct_purchase_assest" model="ir.sequence">
		<field name="name">Direct Purchase Sequence</field>
		<field name="code">direct.purchase.seqs.a</field>
		<field name="prefix">DP/A/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>


</odoo>
