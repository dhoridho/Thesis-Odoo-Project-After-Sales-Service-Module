<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/BluetoothPrinterReceiptScreen.js"></script>
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/CategoryReceipt.js"></script>
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/KitchenReceiptScreen.js"></script>
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/LabelReceipt.js"></script>
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/Models.js"></script>
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/ReprintReceiptScreen.js"></script>
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/PaymentScreen.js"></script>
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/SaleDetailsButton.js"></script>
        </xpath>
        <xpath expr="//link[@id='pos-stylesheet']" position="after">
            <link rel="stylesheet" href="/equip3_pos_printer_bluetooth/static/src/css/BluetoothPrinterReceiptScreen.css"/>
        </xpath>
    </template>

    <template id="inherit_equip3_pos_masterdata_assets_pos" inherit_id="equip3_pos_masterdata.assets_pos">
        <xpath expr="//script[@src='/equip3_pos_masterdata/static/src/js/Screens/Report/ReportScreen.js']" position="replace">
            <script type="text/javascript" src="/equip3_pos_printer_bluetooth/static/src/js/ReportScreen1.js"></script>
        </xpath>
    </template>

</odoo>
