<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

<t t-name="CategoryReceipt" owl="1">
        <div class="pos-receipt">
                <t t-set='count_v' t-value='0' />
                <t t-foreach="computeChanges_orderlines.all" t-as="currentOrderline">
                <t t-if="currentOrderline.category == props.category_receipt">
                    <t t-if="count_v == 0">
                    <span style=" visibility: hidden;">
                           <b><t t-esc="currentOrderline.category"/></b>
                    </span>
                    <t t-set='count_v' t-value='1' />
                    </t>
                </t>
                </t>

            <div style="text-align: center; margin-bottom: 10px;"><t t-esc="changes.name" /></div>
            <t t-if="changes.floor || changes.table">
                <br />
                <div class="pos-receipt-title">
                    <t t-esc="changes.floor" /> / <t t-esc="changes.table"/>
                </div>
            </t>
            <div class="order-date" style="margin-bottom: 10px;"><t t-esc="changes.date1"/><span t-esc="changes.date2" class="pos-receipt-right-align"/></div>
                <t t-if="env.pos.get_client()">
                    <div class="table-info" style="padding-top: 0px !important;margin-bottom: 10px;">
                        <div> <span>Client Name: </span><span><t t-esc="receipt.client.name"/></span></div>
                        <t t-if="receipt.client.phone">
                            <div> <span>Client Phone: </span><span><t t-esc="receipt.client.phone"/></span></div>
                        </t>
                    </div>
                </t>
            <t t-set='count_v' t-value='0' />
            <t t-foreach="computeChanges_orderlines.all" t-as="currentOrderline">
                <t t-if="currentOrderline.category == props.category_receipt">
                    <t t-if="count_v == 0">
                    <div style="font-size: 150% !important; display: grid; justify-content: center;">
                        <tr>
                            <td><b><t t-esc="currentOrderline.categoryName"/></b></td>
                        </tr>
                    </div>
                    <t t-set='count_v' t-value='1' />
                    </t>
                </t>
            </t>

            <t t-if="changes.cancelled.length > 0">
                <t t-set='count_v' t-value='0' />
                <div class="pos-order-receipt-cancel">
                    <t t-foreach="changes.cancelled" t-as="orderline">
                        <t t-if="orderline.category == props.category_receipt">
                            <t t-if="count_v == 0">
                                <div class="pos-receipt-title" style="margin-top: 10px;">
                                    CANCELLED
                                    <t t-esc='changes.time.hours'/>:<t t-esc='changes.time.minutes'/>
                                </div>
                                <t t-set='count_v' t-value='1' />
                            </t>
                        </t>
                    </t>
                    <div class="orderlines">
                        <table class="mine-table">
                            <t t-set='count_v' t-value='0' />
                            <t t-foreach="changes.cancelled" t-as="orderline">
                                <t t-if="orderline.category == props.category_receipt">
                                    <t t-if="count_v == 0">
                                        <colgroup>
                                            <col width='10%'/>
                                            <col width='80%'/>
                                        </colgroup>
                                        <thead>
                                            <tr>
                                                <th > <span class="start">Qty</span></th>
                                                <th > <span >Item</span></th>
                                            </tr>
                                        </thead>
                                        <t t-set='count_v' t-value='1' />
                                    </t>
                                </t>
                            </t>
                            <t t-foreach="changes.cancelled" t-as="orderline">
                                <t t-if="orderline.category == props.category_receipt">
                                    <tbody>
                                        <t t-if="orderline.category == props.category_receipt">
                                            <tr>
                                                <td style='text-align:center;float:none'>
                                                    <t t-esc="orderline.qty"/>
                                                </td>
                                                <td>
                                                    <span t-esc="orderline.name_wrapped[0]"/>
                                                    <t t-foreach="orderline.name_wrapped.slice(1)" t-as="wrapped_line">
                                                        <span style="text-align: right">
                                                            <span t-esc="wrapped_line"/>
                                                        </span>
                                                    </t>
                                                    <t t-if="orderline.note">
                                                        <div>
                                                            NOTE
                                                            <span class="pos-receipt-right-align">...</span>
                                                        </div>
                                                        <div><span class="pos-receipt-left-padding">--- <t t-esc="orderline.note" /></span></div>
                                                        <br/>
                                                    </t>
                                                </td>
                                            </tr>
                                        </t>
                                    </tbody>
                                </t>
                            </t>
                        </table>
                    </div>
                </div>
            </t>
            <t t-if="changes.new.length > 0">
                <t t-set='count_v' t-value='0' />
                <t t-foreach="changes.new" t-as="orderline">
                    <t t-if="orderline.category == props.category_receipt">
                        <t t-if="count_v == 0">
                            <div class="pos-receipt-title" style="margin-top: 10px;">
                                NEW
                                <t t-esc='changes.time.hours'/>:<t t-esc='changes.time.minutes'/>
                            </div>
                            <t t-set='count_v' t-value='1' />
                        </t>
                    </t>
                </t>
                <div class="orderlines">
                    <table class="mine-table">
                        <t t-set='count_v' t-value='0' />
                            <t t-foreach="changes.new" t-as="orderline">
                                <t t-if="orderline.category == props.category_receipt">
                                <t t-if="count_v == 0">
                                <colgroup>
                                <col width='10%'/>
                                <col width='80%'/>
                                </colgroup>
                                <thead>
                                    <tr>
                                        <th > <span class="start">Qty</span></th>
                                        <th > <span >Item</span></th>
                                    </tr>
                                </thead>
                                <t t-set='count_v' t-value='1' />
                                </t>
                            </t>
                        </t>
                        <t t-foreach="changes.new" t-as="orderline">
                        <t t-if="orderline.category == props.category_receipt">
                            <tbody>
                            <t t-if="orderline.category == props.category_receipt">
                                <tr>
                                    <td style='text-align:center;float:none'>
                                        <t t-esc="orderline.qty"/>
                                    </td>
                                    <td>
                                        <span t-esc="orderline.name_wrapped[0]"/>
                                        <t t-foreach="orderline.name_wrapped.slice(1)" t-as="wrapped_line">
                                            <span style="text-align: right">
                                                <span t-esc="wrapped_line"/>
                                            </span>
                                        </t>
                                        <t t-if="orderline.note">
                                            <div>
                                                NOTE
                                                <span class="pos-receipt-right-align">...</span>
                                            </div>
                                            <div><span class="pos-receipt-left-padding">--- <t t-esc="orderline.note" /></span></div>
                                            <br/>
                                        </t>
                                    </td>
                                </tr>
                            </t>
                            </tbody>
                            </t>
                        </t>
                    </table>
                </div>
            </t>
            <div class="order-date" style="margin-bottom: 10px;">

            </div>
            <br />
            <br />
        </div>
    </t>
