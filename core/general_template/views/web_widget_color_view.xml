<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="assets_backend" name="general_template assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/general_template/static/src/css/colpick.css"/>
                <link rel="stylesheet" href="/general_template/static/src/css/widget.css"/>
                <script type="text/javascript" src="/general_template/static/lib/colpick/colpick.js"></script>
                <script type="text/javascript" src="/general_template/static/src/js/widget.js"></script>
            </xpath>
        </template>
        <template id="report_assets_common_general_template" inherit_id="web.report_assets_common" name="General Template common assets">
            <xpath expr="link[last()]" position="after">
                <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet"/>
            </xpath>
        </template>
    </data>
</odoo>
