<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <!-- Client Action -->
    <t t-name="timesheet_configuration_action">
        <div class="cash_advance_flow_action">
            <div class="timesheet_action_content d-flex flex-column">
                <div class="content-header">
                    <div class="d-flex p-3 w-100">
                        <span>Timesheet Flow</span>
                    </div>
                </div>
                <div class="content-body">
                    <div class="container">
                        <div class="row h-100">
                            <div class="accordion">
                                <div class="accordion-item accordion-master-data">
                                    <h2 class="accordion-header" data-target="collapseMasterData">
                                        <button class="accordion-button" type="button">
                                        Master Data
                                        </button>
                                    </h2>
                                    <div id="collapseMasterData" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="project.open_view_project_all" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/project.svg"/>
                                                    </div>
                                                    <div class="button-text">Project</div>  
                                                </button>
                                                <button name="project.action_view_all_task" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/task.svg"/>
                                                    </div>
                                                    <div class="button-text">Task</div>  
                                                </button>
                                                <button name="equip3_hr_timesheet_extend.hr_timesheet_approval_matrix_act_window" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/timesheet-approval-matrix.svg"/>
                                                    </div>
                                                    <div class="button-text">Timesheet Approval Matrix</div>  
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-cash-advance-flow">
                                    <h2 class="accordion-header" data-target="collapseTimesheetFlow">
                                        <button class="accordion-button" type="button">
                                        Timesheet Flow
                                        </button>
                                    </h2>
                                    <div id="collapseTimesheetFlow" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">                                             
                                            
                                                <button name="equip3_hr_timesheet_extend.hr_manage_timesheet_act_window" 
                                                    type="action"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/manage-timesheets.svg"/>
                                                    </div>
                                                    <div class="button-text">Manage Timesheet</div>
                                                </button>   
                                                <button name="equip3_hr_timesheet_extend.hr_timesheet_act_window" 
                                                    type="action"
                                                    style="top:200px"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/my-timesheets.svg"/>
                                                    </div>
                                                    <div class="button-text">My Timesheet</div>  
                                                </button>     
                                                <button name="hr_timesheet.act_hr_timesheet_line" 
                                                    type="action"
                                                    style="top:100px;left:280px"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/my-timesheets-lines.svg"/>
                                                    </div>
                                                    <div class="button-text">My Timesheet Lines</div>
                                                </button>
                                                <button name="equip3_hr_timesheet_extend.hr_timesheet_approve_act_window" 
                                                    type="action"
                                                    style="top:100px;left:550px"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/timesheet-to-approve.svg"/>
                                                    </div>
                                                    <div class="button-text">Timesheet To Approve</div>
                                                </button>
                                                <button name="hr_timesheet.timesheet_action_all" 
                                                    type="action"
                                                    style="top:100px;left:840px"
                                                    class="btn btn-flow btn-default">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/all-timesheet.svg"/>
                                                    </div>
                                                    <div class="button-text">All Timesheet</div>
                                                </button>
                                                
                                                <div class="nd_connector" style="width: 175px;left: 200px;top: 25px">
                                                    <div class="nd_connector_line"></div>
                                                </div>

                                                <div class="nd_connector" style="width: 225px;left: 150px;top: 225px">
                                                    <div class="nd_connector_line"></div>
                                                </div>

                                                <div class="nd_connector" style="width: 80px;left: 475px;top: 125px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>

                                                <div class="nd_connector" style="width: 95px;left: 750px;top: 125px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                
                                                <div class="nd_connector" style="transform: rotate(90deg);width: 80px;left: 335px;top: 64px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>

                                                <div class="nd_connector" style="transform: rotate(-90deg);width: 80px;left: 335px;top: 185px">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                               
                                               
                                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-reporting">
                                    <h2 class="accordion-header" data-target="collapseReporting">
                                        <button class="accordion-button" type="button">
                                        Reporting
                                        </button>
                                    </h2>
                                    <div id="collapseReporting" class="accordion-collapse show">
                                        <button name="hr_timesheet.act_hr_timesheet_report" 
                                            type="action"
                                            class="btn btn-flow btn-default">
                                            <div class="button-img">
                                                <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/reporting-by-employee.svg"/>
                                            </div>
                                            <div class="button-text">Reporting By Employee</div>
                                        </button>
                                        <button name="hr_timesheet.timesheet_action_report_by_project" 
                                            type="action"
                                            class="btn btn-flow btn-default">
                                            <div class="button-img">
                                                <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/reporting-by-project.svg"/>
                                            </div>
                                            <div class="button-text">Reporting By Project</div>  
                                        </button>        
                                        <button name="hr_timesheet.timesheet_action_report_by_task" 
                                            type="action"
                                            class="btn btn-flow btn-default">
                                            <div class="button-img">
                                                <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/reporting-by-task.svg"/>
                                            </div>
                                            <div class="button-text">Reporting By Task</div>
                                             
                                        </button>
                                        <button name="sale_timesheet.timesheet_action_billing_report" 
                                            type="action"
                                            class="btn btn-flow btn-default">
                                            <div class="button-img">
                                                <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/reporting-by-billing-type.svg"/>
                                            </div>
                                            <div class="button-text">Reporting By Billing Type</div>  
                                        </button>        
                                        <button name="hr_timesheet_attendance.action_hr_timesheet_attendance_report" 
                                            type="action"
                                            class="btn btn-flow btn-default">
                                            <div class="button-img">
                                                <img class="img-fluid" src="/equip3_hr_timesheet_extend/static/src/img/attendance.svg"/>
                                            </div>
                                            <div class="button-text">Timesheet / Attendance</div>
                                             
                                        </button>
                                            
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>