<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="make_invisible" model="res.groups">
            <field name="name">Invisible</field>
        </record>
        <record model="ir.ui.menu" id="agreement_legal.agreement_dashboard">
            <field name="groups_id" eval="[(6,0,[ref('make_invisible')])]"/>
        </record>

        <record forcecreate="True" id="contract_ks_dashboard_ninja" model="ks_dashboard_ninja.board">
            <field name="name">Contract Management</field>
            <field name="ks_dashboard_menu_name">Dashboard</field>
            <field name="ks_dashboard_top_menu_id" eval="ref('agreement.agreement_menu')"/>
            <field name="ks_dashboard_active">1</field>
            <field name="ks_dashboard_menu_sequence">0</field>
            <field name="ks_dashboard_group_access" eval="False"/>
        </record>

        <record id="ks_contracts_item_1" model="ks_dashboard_ninja.item">
            <field name="name">Draft Contract</field>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_domain">[("is_template", "=", False), ("stage_id.name", "=", "Draft")]</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_date_filter_field" ref="agreement.field_agreement__create_date"/>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_date_filter_selection">l_none</field>
            <field name="ks_data_format">global</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_dashboard_item_theme">red</field>
            <field name="ks_background_color">#534a61,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_icon_select">Default</field>
            <field name="ks_default_icon">bar-chart</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_button_color">#000000,0.99</field>
        </record>

        <record id="ks_cproperty_item_2" model="ks_dashboard_ninja.item">
            <field name="name">Negotiated Contract</field>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_domain">[("is_template", "=", False), ("stage_id.name", "=", "Negotiation")]</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_date_filter_field" ref="agreement.field_agreement__create_date"/>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_date_filter_selection">l_none</field>
            <field name="ks_data_format">global</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_dashboard_item_theme">red</field>
            <field name="ks_background_color">#376361,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_icon_select">Default</field>
            <field name="ks_default_icon">bar-chart</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_button_color">#000000,0.99</field>
        </record>

        <record id="ks_maintenance_item_3" model="ks_dashboard_ninja.item">
            <field name="name">Active Contract</field>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_domain">[("is_template", "=", False), ("stage_id.name", "=", "Active")]</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_date_filter_field" ref="agreement.field_agreement__create_date"/>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_date_filter_selection">l_none</field>
            <field name="ks_data_format">global</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_dashboard_item_theme">red</field>
            <field name="ks_background_color">#630d0a,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_icon_select">Default</field>
            <field name="ks_default_icon">bar-chart</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_button_color">#000000,0.99</field>
        </record>

        <record id="ks_rec_maintenance_item_4" model="ks_dashboard_ninja.item">
            <field name="name">Expired Contract</field>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_domain">[("is_template", "=", False), ("stage_id.name", "=", "Expired")]</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_date_filter_field" ref="agreement.field_agreement__create_date"/>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_date_filter_selection">l_none</field>
            <field name="ks_data_format">global</field>
            <field name="ks_layout">layout3</field>
            <field name="ks_dashboard_item_theme">red</field>
            <field name="ks_background_color">#525659,0.99</field>
            <field name="ks_font_color">#ffffff,0.99</field>
            <field name="ks_icon_select">Default</field>
            <field name="ks_default_icon">bar-chart</field>
            <field name="ks_default_icon_color">#ffffff,0.99</field>
            <field name="ks_button_color">#000000,0.99</field>
        </record>

        <record id="ks_expected_cost_item_5" model="ks_dashboard_ninja.item">
            <field name="name">Top 10 Contract by Highest Expected Cost</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_domain">[("is_template", "=", False)]</field>
            <field name="ks_list_view_fields" eval="[(6, 0, [ref('agreement.field_agreement__code'),ref('agreement.field_agreement__name'),ref('agreement.field_agreement__agreement_type_id'),ref('equip3_contract_masterdata.field_agreement__expected_cost')])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_sort_by_field" eval="ref('equip3_contract_masterdata.field_agreement__expected_cost')"/>
            <field name="ks_record_data_limit">10</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_contract_stage_item_6" model="ks_dashboard_ninja.item">
            <field name="name">Contract</field>
            <field name="ks_chart_data_count_type">count</field>
            <field name="ks_chart_groupby_type">relational_type</field>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_domain">[("is_template", "=", False)]</field>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_chart_relation_groupby"  eval="ref('agreement_legal.field_agreement__stage_id')"/>
            <field name="ks_show_data_value">1</field>
            <field name="ks_chart_item_color">cool</field>
            <field name="ks_dashboard_item_type">ks_pie_chart</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_expected_revenue_item_7" model="ks_dashboard_ninja.item">
            <field name="name">Top 10 Contract by Highest Revenue Cost</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_domain">[("is_template", "=", False)]</field>
            <field name="ks_list_view_fields" eval="[(6, 0, [ref('agreement.field_agreement__code'),ref('agreement.field_agreement__name'),ref('agreement.field_agreement__agreement_type_id'),ref('equip3_contract_masterdata.field_agreement__expected_revenue')])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_sort_by_field" eval="ref('equip3_contract_masterdata.field_agreement__expected_revenue')"/>
            <field name="ks_record_data_limit">10</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_customer_expected_cost_item_8" model="ks_dashboard_ninja.item">
            <field name="name">Top 10 Customers by Highest Total Contract Expected Cost</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_list_view_type">grouped</field>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_domain">[("is_template", "=", False)]</field>
            <field name="ks_chart_relation_groupby" eval="ref('agreement.field_agreement__partner_id')"/>
            <field name="ks_list_view_group_fields" eval="[(6, 0, [ref('equip3_contract_masterdata.field_agreement__expected_cost')])]"/>
            <field name="ks_sort_by_order">ASC</field>
            <field name="ks_sort_by_field" eval="ref('agreement.field_agreement__partner_id')"/>
            <field name="ks_record_data_limit">10</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_customer_expected_revenue_item_9" model="ks_dashboard_ninja.item">
            <field name="name">Top 10 Customers by Highest Total Contract Expected Revenue</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_list_view_type">grouped</field>
            <field name="ks_dashboard_ninja_board_id" eval="ref('equip3_contract_dashboard.contract_ks_dashboard_ninja')"/>
            <field name="ks_model_id" eval="ref('agreement.model_agreement')"/>
            <field name="ks_domain">[("is_template", "=", False)]</field>
            <field name="ks_chart_relation_groupby" eval="ref('agreement.field_agreement__partner_id')"/>
            <field name="ks_list_view_group_fields" eval="[(6, 0, [ref('equip3_contract_masterdata.field_agreement__expected_revenue')])]"/>
            <field name="ks_sort_by_order">ASC</field>
            <field name="ks_sort_by_field" eval="ref('agreement.field_agreement__partner_id')"/>
