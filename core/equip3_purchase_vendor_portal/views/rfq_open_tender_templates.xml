<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- Add you code here -->
        <template id="rfq_open_tender_submission_portal_template" name="Portal: My RFQ Open Tender Submission's">
            <t t-call="portal.portal_layout">
                <t t-set="breadcrumbs_searchbar" t-value="True"/>

                <t t-call="portal.portal_searchbar">
                    <t t-set="title">Open Tender Submission</t>
                </t>
                <t t-if="rfq_open_tenders" t-call="portal.portal_table">
                    <div>
                        <t t-set="is_price_rating_config" t-value="request.env['ir.config_parameter'].sudo().get_param('is_price_ratting_rfq_tender')"/>
                        <style>
                            td {
                            white-space:normal !important;
                            box-sizing:content-box !important;
                            }
                        </style>
                        <t t-foreach="grouped_rfq" t-as="g_rfq">
                            <thead>
                                <tr class="thead-light" t-if="groupby and groupby!='none'">
                                    <th colspan="8">
                                        <t t-if="groupby == 'price_rating'">
                                            <em class="font-weight-normal text-muted"></em>
                                            <span t-field="g_rfq[0].price_rating"/>
                                        </t>
                                        <t t-if="groupby == 'period_by_day'">
                                            <em class="font-weight-normal text-muted"></em>
                                            <span t-field="g_rfq[0].date_order" t-options='{"widget": "date","format": "d MMMM Y"}'/>
                                        </t>
                                        <t t-if="groupby == 'period_by_month'">
                                            <em class="font-weight-normal text-muted"></em>
                                            <span t-field="g_rfq[0].date_order" t-options='{"widget": "date","format": "MMMM Y"}'/>
                                        </t>
                                        <t t-if="groupby == 'period_by_year'">
                                            <em class="font-weight-normal text-muted"></em>
                                            <span t-field="g_rfq[0].date_order" t-options='{"widget": "date","format": "Y"}'/>
                                        </t>
                                    </th>
                                </tr>
                                <tr class="active">
                                    <th>Request For Quotation</th>
                                    <th>Tender</th>
                                    <th>Order Date</th>
                                    <th>Total</th>
                                    <t t-if="is_price_rating_config">
                                        <th>Price Rating</th>
                                    </t>
                                </tr>
                            </thead>
                            
                            <tbody>
                                <t t-foreach="g_rfq" t-as="order">
                                    <tr>
                                        <td>
                                            <a t-attf-href="/my/rfq-open-tender/#{order.id}?#{keep_query()}"><t t-esc="order.name" /></a>
                                        </td>
                                        <td>
                                            <a t-attf-href="/open_tender/#{order.agreement_id.id}?#{keep_query()}"><t t-esc="order.agreement_id.name" /></a>
                                        </td>
                                        <td><span t-field="order.date_order" /></td>
                                        <td><span t-field="order.amount_total" t-options='{"widget": "monetary", "display_currency": order.currency_id}' /></td>
                                        <t t-if="is_price_rating_config">
                                            <t t-if="order.price_rating == 'competitive'">
                                                <t t-set="icon_rating" t-value="'check'"/>
                                                <t t-set="rating_color" t-value="'green'"/>
                                            </t>
                                            <t t-elif="order.price_rating == 'highest'">
                                                <t t-set="icon_rating" t-value="'cross'"/>
                                                <t t-set="rating_color" t-value="'red'"/>
                                            </t>
                                            <t t-elif="order.price_rating == 'beaten'">
                                                <t t-set="icon_rating" t-value="'circle'"/>
                                                <t t-set="rating_color" t-value="'orange'"/>
                                            </t>
                                            <t t-else="">
                                                <t t-set="icon_rating" t-value="'cross_grey'"/>
                                                <t t-set="rating_color" t-value="'gray'"/>
                                            </t>
                                            <td t-attf-style="color:{{rating_color}}">
                                                <t t-if="order.price_rating == 'empty'">
                                                    <span class="text-muted text-right" style="text-align:right" t-field="order.price_rating" />
                                                </t>
                                                <t t-else="">
                                                    <img t-attf-src="/equip3_purchase_vendor_portal/static/src/img/{{icon_rating}}.svg" alt="check_harian" style="height:20px;weight:20px;"/>
                                                    <span t-field="order.price_rating"/>
                                                </t>
                                            </td>
                                        </t>
                                        <t t-else="">
                                            <td>
                                            </td>
                                        </t>
                                    </tr>
                                </t>
                            </tbody>
                        </t>
                    </div>
                </t>
            </t>
        </template>

        <template id="portal_rfq_open_tender_form_templates" name="RFQ Tender Portal Template" inherit_id="portal.portal_sidebar"
                primary="True">
            <xpath expr="//div[hasclass('o_portal_sidebar')]" position="inside">
                <div class="row mt16 o_portal_sale_sidebar">
                    <!-- Sidebar -->
                    <t t-call="portal.portal_record_sidebar">
                        <t t-set="classes" t-value="'col-lg-auto d-print-none'"/>
                        <t t-set="entries">
                            <ul class="list-group list-group-flush flex-wrap flex-row flex-lg-column">
                                <li class="list-group-item flex-grow-1">
                                    <div class="o_download_pdf btn-toolbar flex-sm-nowrap">
                                        <div class="btn-group flex-grow-1 mr-1 mb-1">
                                            <a class="btn btn-secondary btn-block o_download_btn"
                                            t-att-href="rfq_open_tender.get_portal_url(report_type='pdf', download=True)"
                                            title="Download"><i class="fa fa-download"/> Download</a>
                                        </div>
                                    </div>
                                </li>

                                <li class="navspy list-group-item pl-0 flex-grow-1" t-ignore="true" role="complementary">
                                    <ul class="nav flex-column bs-sidenav"></ul>
                                </li>
                                <li class="list-group-item flex-grow-1">
                                    <div class="small mb-1"><strong class="text-muted">Responsible</strong></div>
                                    <div class="row flex-nowrap">
                                        <div class="col flex-grow-0 pr-2">
                                            <img class="rounded-circle mr4 float-left o_portal_contact_img"
                                                t-if="rfq_open_tender.create_uid.image_1024"
                                                t-att-src="image_data_uri(rfq_open_tender.create_uid.image_1024)" alt="Contact"/>
                                            <img class="rounded-circle mr4 float-left o_portal_contact_img"
                                                t-if="not rfq_open_tender.create_uid.image_1024"
                                                src="/web/static/src/img/placeholder.png" alt="Contact"/>
                                        </div>
                                        <div class="col pl-0" style="min-width: 150px;">
                                            <span t-field="rfq_open_tender.create_uid"
                                                t-options='{"widget": "contact", "fields": ["name", "phone"], "no_marker": True}'/>
                                            <a href="#discussion" class="small"><i class="fa fa-comment"></i> Send
                                                message</a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </t>
                    </t>

                    <!-- Page content -->
                    <div id="rfq_tender_content" class="col-12 col-lg-9 justify-content-end p-lg-0 ml-lg-2">
                        <div t-attf-class="card #{'pb-5' if report_type == 'html' else ''}">
                            <div t-call="equip3_purchase_vendor_portal.rfq_open_tender_portal_contents_new"/>
                        </div>
                        <div id="rfq_communication" class="mt-4">
                            <h2>History</h2>
                            <t t-call="portal.message_thread">
                                <t t-set="object" t-value="rfq_open_tender"/>
                            </t>
                        </div>
                    </div>
                </div>
            </xpath>
        </template>
        <template id="rfq_open_tender_portal_contents_new" name="Tender Portal Content">
            <!-- Intro -->
            <div id="introduction" t-attf-class="#{'card-header bg-white' if report_type == 'html' else ''}">
                <h2 class="my-0">
                    <em t-esc="rfq_open_tender.name"/>
                </h2>
            </div>
            <div class="card-body-parent">
                <div t-attf-class="#{'card-body' if report_type == 'html' else ''}">
                    <!-- Informations -->
                    <div id="informations">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="col-5"><strong>Date:</strong></td>
                                    <td><span t-field="rfq_open_tender.date_order" t-options='{"widget": "date"}'/></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <section id="details" style="page-break-inside: auto;">
                        <div class="container">
                            <style>
                                table {
                                border-collapse: collapse;
                                border-spacing: 0;
                                width: 120%;
                                }

                                th, td {
                                text-align: left;
                                padding: 8px;
                                }

                            </style>
                            <form action="/rfq/update?is_rfq_open_tender=True" method="post">
                                <input type="hidden" name="order_id" t-att-value="rfq_open_tender.id" id="order_id"/>
                                <div class="mb-3 col-6 vendor-payment-term">
                                    <strong>Vendor Payment Terms:</strong>
                                    <span class="ml-1">
                                        <div class="form-group">
                                            <div class="input-group ">
                                                <input type="text" class="form-control" id="vendor_payment_terms"
                                                    t-att-name="rfq_open_tender.vendor_payment_terms"
                                                    t-attf-name="vendor_payment_terms"
                                                    t-att-disabled="rfq_open_tender.state == 'retreat'"
                                                    t-att-value="(rfq_open_tender.vendor_payment_terms)"
                                                    t-attf-value=" "
                                                />
                                            </div>
                                        </div>
                                    </span>
                                </div>
                                <h3 id="details" class="mt-5">Products</h3>
                                <div style="overflow-x:auto;">
                                    <table>
                                        <thead>
                                            <tr>
                                                <th colspan="2">Product</th>
                                                <th>Description</th>
                                                <th>Receipt Date</th>
                                                <th>Destination</th>
                                                <th>Discount</th>
                                                <th>Quantity</th>
                                                <th>UOM</th>
                                                <th nowrap="nowrap" style="width:200px;">Unit Price</th>
                                                <th>Taxes</th>
                                                <th>Subtotal</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <t t-foreach="rfq_open_tender.order_line" t-as="ol">
                                                <tr>
                                                    <td>
                                                        <img t-if="ol.product_id.image_128"
                                                            class="rounded-circle mt-1 o_portal_contact_img"
                                                            t-att-src="image_data_uri(ol.product_id.image_128)" alt="Product"/>
                                                        <img t-if="not ol.product_id.image_128"
                                                            class="rounded-circle mt-1 o_portal_contact_img"
                                                            src="/web/static/src/img/placeholder.png" alt="Product"/>
                                                    </td>
                                                    <td><span t-field="ol.product_id"/></td>
                                                    <td><span t-field="ol.name"/></td>
                                                    <td><span t-field="ol.date_planned"/></td>
                                                    <td><span t-field="ol.destination_warehouse_id"/></td>
                                                    <td>
                                                        <t t-if="ol.discount_method == 'fix'">
                                                            <span t-field="ol.discount_amount" t-options='{"widget": "monetary", "display_currency": rfq_open_tender.currency_id}'/>
                                                        </t>
                                                        <t t-if="ol.discount_method == 'per'">
                                                            <span t-esc="'%.0f'% ol.discount_amount"/><span>%</span>
                                                        </t>
                                                        <t t-if="not ol.discount_method">
                                                            <span t-field="ol.discount_amount"/>
                                                        </t>
                                                    </td>
                                                    <td><input t-att-disabled="rfq_open_tender.state == 'retreat'" type="text" pattern="[0-9]+([\.,][0-9]+)?" class="form-control"
                                                            id="quantity" t-attf-name="#{ol.id}_qty"
                                                            t-att-value="str(ol.product_qty)"/></td>
                                                    <td><span t-field="ol.product_uom"/></td>
                                                    <td>
                                                        <t t-set="price_unit" t-value="ol.price_unit % 1 and ol.price_unit or int(ol.price_unit)"/>
                                                        <input t-att-disabled="rfq_open_tender.state == 'retreat'" type="text" class="form-control po_unitprice" id="unit_price"
                                                                t-att-name="ol.id" t-att-value="str(price_unit)"
                                                                nowrap="nowrap"/>
                                                    </td>
                                                    <td><span t-field="ol.taxes_id"/></td>
                                                    <td><span t-field="ol.price_total"
                                                            t-options='{"widget": "monetary", "display_currency": rfq_open_tender.currency_id}'/></td>
                                                </tr>
                                            </t>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="row">
                                    <div class="col-12 mt-4">
                                        <label for="rfq_note" style="margin-top: 10px;">Terms and Conditions</label>
                                        <t t-set="notes" t-value=""/>
                                        <t t-if="rfq_open_tender.term_condition_box">
                                            <t t-set="notes" t-value="rfq_open_tender.set_notes()"/>
                                        </t>
                                        <!--                                Textarea hide because editing not required more. Logic change -->
                                        <textarea t-att-data-readonly="'1' if rfq_open_tender.state == 'retreat' else None" type="text" name="rfq_note"
                                                class="form-control o_survey_question_text_box bg-transparent text-dark rounded-0 p-0"
                                                placeholder="Terms and conditions......" rows="10" cols="50"><t
                                                t-raw="notes"/></textarea>
                                        <br/>
                                        <!--                                Add below lines to show Terms and Conditions in HTML view-->
                                        <!--                                <t t-raw="rfq_open_tender.term_condition_box"/>-->
                                        <!--                                <style>p {margin-bottom: 0;}</style>-->
                                    </div>
                                    <div class="col-12 mt-4">
                                        <label for="agreement_note" style="margin-top: 10px;">Service Level Agreement</label>
                                        <t t-if="rfq_open_tender.service_level_agreement_box">
                                            <t t-set="service_level_agreement_box" t-value="rfq_open_tender.set_notes_service()"/>
                                        </t>
                                        <textarea t-att-data-readonly="'1' if rfq_open_tender.state == 'retreat' else None" type="text" name="agreement_note"
                                                class="form-control o_survey_question_text_box bg-transparent text-dark rounded-0 p-0"
                                                placeholder="Agreements......" rows="10" cols="50"><t
                                                    t-raw="service_level_agreement_box"/></textarea>
                                        <!--                                Add below lines to show Terms and Conditions in HTML view-->
                                        <!--                                <t t-raw="rfq_open_tender.term_condition_box"/>-->
                                        <!--                                <style>p {margin-bottom: 0;}</style>-->
                                    </div>
                                </div>
                                <button type="submit" t-if="rfq_open_tender.state != 'retreat'" id="btn_update_bid" class="btn btn-secondary mt-4" title="Update Bid">Update
                                    Bid</button>
                                <button type="submit" t-if="rfq_open_tender.state != 'retreat'" id="btn_retreat_rfq_open_tender" class="btn btn-secondary mt-4 ml-4" title="Retreat">Retreat</button>
                                <hr/>
                            </form>
                            <div class="row mt-4" id="amount_div">
                                <div class="col-lg-12 text-right">
                                    <div class="row">
                                        <div class="col-lg-9 text-right">
                                            Untaxed Amount:
                                        </div>
                                        <div class="col-lg-3 text-right">
                                            <span t-field="rfq_open_tender.amount_untaxed"
                                                t-options='{"widget": "monetary", "display_currency": rfq_open_tender.currency_id}'/>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-9 text-right">
                                            - Discount:
                                        </div>
                                        <div class="col-lg-3 text-right">
                                            <span t-field="rfq_open_tender.discount_amt"
                                                t-options='{"widget": "monetary", "display_currency": rfq_open_tender.currency_id}'/>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-9 text-right">
                                            Taxes:
                                        </div>
                                        <div class="col-lg-3 text-right">
                                            <span t-field="rfq_open_tender.amount_tax"
                                                t-options='{"widget": "monetary", "display_currency": rfq_open_tender.currency_id}'/>
                                        </div>
                                    </div>
                                    <div class="row"
                                        style="position:absolute;right:25px;border-top:2px solid black;width:325px;"></div>
                                    <div class="row">
                                        <div class="col-lg-9 text-right" style="{'}">
                                            <strong>Total:</strong>
                                        </div>
                                        <div class="col-lg-3 text-right">
                                            <strong><span t-field="rfq_open_tender.amount_total"
                                                        t-options='{"widget": "monetary", "display_currency": rfq_open_tender.currency_id}'/></strong>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </template>

