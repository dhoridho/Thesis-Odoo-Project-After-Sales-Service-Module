<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <template id="portal_open_tenders" name="My Tenders">
            <!-- <style>
                td {
                white-space:normal !important;
                box-sizing:content-box !important;
                }
            </style> -->
            <t t-call="portal.portal_layout">
                <t t-set="breadcrumbs_searchbar" t-value="True" />

                <t t-call="portal.portal_searchbar">
                    <t t-set="title">Open Tenders</t>
                </t>
                <t t-if="not tenders">
                    <p>There are currently no Tenders for your account.</p>
                </t>
                <t t-if="tenders">
                    <div class="open_tender_list table-responsive mt-4 mb-4">
                        <table class="bg-white table">
                            <t t-foreach="grouped_tenders" t-as="g_tender">
                                <thead>
                                    <tr class="thead-light" t-if="groupby and groupby!='none'">
                                        <th colspan="8">
                                            <t t-if="groupby == 'status'">
                                                <em class="font-weight-normal text-muted">Status:</em>
                                                <span t-field="g_tender[0].state2"/>
                                            </t>
                                            <t t-if="groupby == 'period_by_day'">
                                                <em class="font-weight-normal text-muted"></em>
                                                <span t-field="g_tender[0].create_date" t-options='{"widget": "date","format": "d MMMM Y"}'/>
                                            </t>
                                            <t t-if="groupby == 'period_by_month'">
                                                <em class="font-weight-normal text-muted"></em>
                                                <span t-field="g_tender[0].create_date" t-options='{"widget": "date","format": "MMMM Y"}'/>
                                            </t>
                                            <t t-if="groupby == 'period_by_year'">
                                                <em class="font-weight-normal text-muted"></em>
                                                <span t-field="g_tender[0].create_date" t-options='{"widget": "date","format": "Y"}'/>
                                            </t>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th>Tender Reference</th>
                                        <th class="text-right">Tender Name</th>
                                        <th class="text-right">Created Date</th>
                                        <th class="text-right">Status</th>
                                         
                                        <th class="text-right">Expiry Date</th>
                                        <th style="display:none;"/>
                                    </tr>
                                </thead>
                                <tbody>
                                    <t t-foreach="g_tender" t-as="tender">
                                        <tr>
                                            <td>
                                                <a t-att-href="'/open_tender/'+str(tender.id)+'?tender_dashboard_selection='+str(tender_dashboard_selection)">
                                                    <t t-esc="tender.name"/>
                                                </a>
                                            </td>
                                            <td class="text-right">
                                                <span t-field="tender.tender_name"/>
                                            </td>
                                            <td class="text-right">
                                                <span t-field="tender.create_date" t-options="{'widget':'date'}"/>
                                            </td>
                                            <t t-if="tender.state2 == 'bid_submission'">
                                                <td class="text-right">
                                                    <span style="background-color: #0500FF !important" class="badge badge-secondary">Bid Submission</span>
                                                </td>
                                                <t t-if="tender.sh_bid_agreement_deadline">
                                                    <td class="text-right">
                                                        <span t-field="tender.sh_bid_agreement_deadline"/>
                                                    </td>
                                                </t>
                                                <t t-if="not tender.sh_bid_agreement_deadline">
                                                    <td class="text-right"/>
                                                </t>
                                            </t>
                                            <t t-elif="tender.state2 == 'bid_selection'">
                                                <td class="text-right">
                                                    <span style="background-color:#00B012 !important" class="badge badge-secondary">Bid Selection</span>
                                                </td>
                                                <t t-if="tender.sh_bid_selection_agreement_deadline">
                                                    <td class="text-right">
                                                        <span t-field="tender.sh_bid_selection_agreement_deadline"/>
                                                    </td>
                                                </t>
                                                <t t-if="not tender.sh_bid_selection_agreement_deadline">
                                                    <td class="text-right"/>
                                                </t>
                                            </t>
                                            <t t-else="">
                                                <td class="text-right">
                                                    <span t-field="tender.state2"/>
                                                </td>
                                                <t t-if="tender.sh_agreement_deadline">
                                                    <td class="text-right">
                                                        <span t-field="tender.sh_agreement_deadline"/>
                                                    </td>
                                                </t>
                                                <t t-if="not tender.sh_agreement_deadline">
                                                    <td class="text-right"/>
                                                </t>
                                            </t>
                                            <td style="display:none;">
                                                <input type="hidden" id="tender_id" name="tender_id" t-att-value="tender.id" t-att-tender_id="tender.id"/>
                                            </td>
                                        </tr>
                                    </t>
                                </tbody>
                            </t>
                        </table>
                    </div>
                    <div t-if="pager" class="o_portal_pager text-center">
                        <t t-call="portal.pager"/>
                    </div>
                </t>
            </t>
        </template>

        <template id="information">
            <div style="">
                <table class="table">
                    <tr>
                        <td>
                            <strong>Tender Reference</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Name</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.tender_name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Ordering Date</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.sh_order_date" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Status</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.state2" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Type</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.sh_agreement_type.name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Amount</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.amount_total" t-options='{"widget": "monetary", "display_currency": open_tender.currency_id}'/>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Participant</strong>
                        </td>
                        <td>
                            <span t-esc="jumlah_peserta"/> Participant
                        </td>
                    </tr>
                </table>
            </div>
            <div style="overflow-x:auto;">
                <table class="table">
                    <thead>
                        <tr>
                            <th colspan="2">Product</th>
                            <th>Description</th>
                            <th>Quantity</th>
                            <th>UOM</th>
                            <th nowrap="nowrap" style="width:200px;">Reference Price</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="open_tender.sh_purchase_agreement_line_ids" t-as="ol">
                            <tr>
                                <td>
                                    <img t-if="ol.sh_product_id.image_128"
                                        class="rounded-circle mt-1 o_portal_contact_img"
                                        t-att-src="image_data_uri(ol.sh_product_id.image_128)" alt="Product"/>
                                    <img t-if="not ol.sh_product_id.image_128"
                                        class="rounded-circle mt-1 o_portal_contact_img"
                                        src="/web/static/src/img/placeholder.png" alt="Product"/>
                                </td>
                                <td><span t-field="ol.sh_product_id"/></td>
                                <td><span t-field="ol.sh_product_description"/></td>
                                <td><span t-field="ol.sh_qty"/></td>
                                <td><span t-field="ol.sh_product_uom_id"/></td>
                                <td><span t-field="ol.sh_price_unit"/></td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </div>
        </template>
        <template id="participant">
            <table class="table">
                    <thead class="thead-light">
                        <tr>
                            <th>
                                No
                            </th>
                            <th>
                                Vendor
                            </th>
                            <th>
                                Quotation Amount
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-set="no" t-value="1"/>
                        <t t-foreach="sorted(participants, key=lambda p: p.partner_id.name.lower())" t-as="participant">
                            <tr>
                                <td>
                                    <t t-esc="no"/>
                                </td>
                                <td>
                                    <t t-esc="participant.partner_id.name"/>
                                </td>
                                <td>
                                    <t t-esc="participant.amount_total" t-options='{"widget": "monetary", "display_currency": participant.currency_id}'/>
                                </td>
                            </tr>
                            <t t-set="no" t-value="no+1"/>
                        </t>
                    </tbody>
                </table>
        </template>
        <template id="tender_evaluation">
            <table class="table">
                    <thead class="thead-light">
                        <tr>
                            <th>
                                No
                            </th>
                            <th>
                                Vendor
                            </th>
                            <th>
                                Quotation Amount
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-set="no" t-value="1"/>
                        <t t-foreach="participants.sorted(key=lambda p: p.amount_total)" t-as="tender">
                            <tr>
                                <td>
                                    <t t-esc="no"/>
                                </td>
                                <td>
                                    <t t-esc="tender.partner_id.name"/>
                                </td>
                                <td>
                                    <t t-esc="tender.amount_total" t-options='{"widget": "monetary", "display_currency": tender.currency_id}'/>
                                </td>
                            </tr>
                            <t t-set="no" t-value="no+1"/>
                        </t>
                    </tbody>
                </table>
        </template>
        <template id="tender_winner">
            <div>
                <table class="table">
                    <tr>
                        <td>
                            <strong>Tender Reference</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Name</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.tender_name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Type</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.sh_agreement_type.name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Tender Amount</strong>
                        </td>
                        <td>
                            <span t-field="open_tender.amount_total" t-options='{"widget": "monetary", "display_currency": open_tender.currency_id}'/>
                        </td>
                    </tr>
                </table>
                <table class="table">
                    <thead class="thead-light">
                        <tr>
                            <th>
                                Vendor
                            </th>
                            <th>
                                Address
                            </th>
                            <th>
                                Quotation Amount
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="winners" t-as="winner">
                            <tr>
                                <td>
                                    <t t-esc="winner.partner_id.name"/>
                                </td>
                                <td>
                                    <t t-esc="winner.partner_id.street"/>
                                </td>
                                <td>
                                    <t t-esc="winner.amount_total" t-options='{"widget": "monetary", "display_currency": winner.currency_id}'/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>

            </div>
        </template>
        <template id="tender_quotation">
            <div>
                <table class="table">
                    <tr>
                        <td>
                            <strong>Reference</strong>
                        </td>
                        <td>
                            <span t-field="purchase_order_id.name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Source Document</strong>
                        </td>
                        <td>
                            <span t-field="purchase_order_id.origin" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>RFQ Expiry Date</strong>
                        </td>
                        <td>
                            <span t-field="purchase_order_id.date_order" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Vendor</strong>
                        </td>
                        <td>
                            <span t-field="purchase_order_id.partner_id"/>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <strong>Vendor Bill Address</strong>
                        </td>
                        <td>
                            <span t-field="purchase_order_id.partner_invoice_id"/>
                        </td>
                    </tr>
                </table>
            </div>
            <div style="overflow-x:auto;">
                <table class="table">
                    <thead>
                        <tr>
                            <th colspan="2">Product</th>
                            <th>Description</th>
                            <th>Quantity</th>
                            <th>Subtotal</th>
                            <th nowrap="nowrap" style="width:200px;">Price Rating</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="purchase_order_id.order_line" t-as="ol">
                            <tr>
                                <td>
                                    <img t-if="ol.product_id.image_128"
                                        class="rounded-circle mt-1 o_portal_contact_img"
                                        t-att-src="image_data_uri(ol.product_id.image_128)" alt="Product"/>
                                    <img t-if="not ol.product_id.image_128"
                                        class="rounded-circle mt-1 o_portal_contact_img"
                                        src="/web/static/src/img/placeholder.png" alt="Product"/>
                                </td>
                                <td><span t-field="ol.product_id"/></td>
                                <td><span t-field="ol.name"/></td>
                                <td><span t-field="ol.product_qty"/></td>
                                <td><span t-field="ol.price_total" t-options='{"widget": "monetary", "display_currency": purchase_order_id.currency_id}'/></td>
                                <td><span t-field="purchase_order_id.price_rating"/></td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </div>
        </template>

        <template id="portal_record_sidebar" inherit_id="portal.portal_record_sidebar">
            <xpath expr="//div[hasclass('card-footer')]" position="replace">
                <div class="card-footer small text-center text-muted border-top-0 pt-1 pb-1 d-none d-lg-block">
                    Powered by <a target="_blank" href="http://www.hashmicro.com?utm_source=db&amp;utm_medium=portal" title="hashmicro"><img src="/equip3_purchase_vendor_portal/static/src/img/logo.png" alt="Odoo Logo" height="15"/>HashMicro</a>
                </div>
            </xpath>
        </template>

        <template id="portal_open_tender_form_template" name="Open Tender Portal Template" inherit_id="portal.portal_sidebar" primary="True">
            <xpath expr="//div[hasclass('o_portal_sidebar')]" position="inside">
                <div class="row mt-4 o_portal_open_tender">
                    <!-- Sidebar -->
                    <t t-call="portal.portal_record_sidebar">
                        <t t-set="classes" t-value="'col-lg-auto d-print-none'" />
                        <t t-set="entries">
                            <ul class="list-group list-group-flush flex-wrap flex-row flex-lg-column">
                                <li class="list-group-item flex-grow-1">
                                    <div class="o_download_pdf btn-toolbar flex-sm-nowrap">
                                        <div class="btn-group flex-grow-1 mr-1 mb-1">
                                            <a class="btn btn-secondary btn-block o_download_btn" t-att-href="open_tender.get_portal_url(report_type='pdf', download=True)" title="Print"><i class="fa fa-print" /> Print</a>
                                        </div>
                                    </div>
<!--                                    <t t-foreach="attachments" t-as="attachment">-->
<!--                                        <a t-att-href="'/attachment/download?attachment_id=%i' % attachment.id">-->
<!--                                            <span t-esc="attachment.name" class="fa fa-download"/>-->
<!--                                        </a><br/>-->
<!--                                    </t>-->
                                    <t t-set="user_document" t-value="request.env['ir.attachment'].sudo().search([('res_model','=','purchase.agreement'),('res_id','=',open_tender.id)], limit=1)"/>
                                    <t t-if="user_document">
                                        <div class="btn-toolbar flex-sm-nowrap">
                                            <div class="btn-group flex-grow-1 mr-1 mb-1">
                                                <a style="font-size:15px" class="btn btn-secondary btn-block o_download_btn" t-att-href="'/attachment/download?attachment_id=%s' % user_document.id" title="Attachments"><i class="fa fa-paperclip" /> Download Attachments</a>
                                            </div>
                                        </div>
                                    </t>
                                </li>
                                <t t-if="open_tender.state2 == 'bid_submission'">
                                    <!-- Button trigger modal -->
                                    <li class="list-group-item flex-grow-1">
                                        <div class="o_download_pdf btn-toolbar flex-sm-nowrap">
                                            <div class="btn-group flex-grow-1 mr-1 mb-1">
                                                <t t-if="jenis_user == 'public'">
                                                    <button type="button" class="btn btn-secondary" data-toggle="modal" data-target="#loginOrRegisterModal">
                                                        Submit Quotation
                                                    </button>
                                                </t>
                                                <t t-elif="jenis_user == 'is_vendor'">
                                                    <button type="button" class="btn btn-secondary" data-toggle="modal" data-target="#exampleModal">
                                                        Submit Quotation
                                                    </button>
                                                </t>
                                                <t t-else="">
                                                    <button type="button" class="btn btn-secondary" data-toggle="modal" data-target="#bukanVendorModal">
                                                        Submit Quotation
                                                    </button>
                                                </t>
                                            </div>
                                        </div>
                                    </li>
                                </t>
                                <li class="navspy list-group-item pl-0 flex-grow-1" t-ignore="true" role="complementary">
                                    <ul class="nav flex-column bs-sidenav"></ul>
                                </li>
                                <li class="list-group-item flex-grow-1">
                                    <div class="small mb-1"><strong class="text-muted">Responsible</strong></div>
                                    <div class="row flex-nowrap">
                                        <div class="col flex-grow-0 pr-2">
                                            <img class="rounded-circle mr4 float-left o_portal_contact_img" t-if="open_tender.create_uid.image_1024" t-att-src="image_data_uri(open_tender.create_uid.image_1024)" alt="Contact" />
                                            <img class="rounded-circle mr4 float-left o_portal_contact_img" t-if="not open_tender.create_uid.image_1024" src="/web/static/src/img/placeholder.png" alt="Contact" />
                                        </div>
                                        <div class="col pl-0" style="min-width: 150px;">
                                            <span t-field="open_tender.create_uid" t-options='{"widget": "contact", "fields": ["name", "phone"], "no_marker": True}' />
                                            <a href="#discussion" class="small"><i class="fa fa-comment"></i> Send message</a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </t>
                    </t>
                    <!-- Page content -->
                    <div id="tender_content" class="col-12 col-lg justify-content-end">
                        <div t-attf-class="card">
                            <div t-call="equip3_purchase_vendor_portal.open_tender_portal_content" />
                        </div>

                        <!-- chatter -->
                        <div id="tender_communication" class="mt-4" t-if="request.env.user.has_group('sh_po_tender_management.sh_purchase_tender_user')">
                            <h2>History</h2>
                            <t t-call="portal.message_thread">
                                <t t-set="object" t-value="open_tender" />
                            </t>
                        </div>
                    </div>
                </div>
            </xpath>
        </template>
        <template id="open_tender_portal_content" name="Tender Portal Content">
            <!-- Intro -->
            <div id="introduction" t-attf-class="#{'card-header bg-white' if report_type == 'html' else ''}">
                <h2 class="my-0">
                    <em t-esc="open_tender.name" />
                </h2>
            </div>
            <br />
            <div class="row">
                <div class="mb-3 col-12">
                    <t t-if="message">
                        <div class="alert alert-success">
                            <h4><strong><span t-esc="message"/></strong></h4>
                        </div>
                    </t>
                </div>

            </div>
            <!-- Modal -->
            <div class="modal fade" style="margin-top:100px;" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document" style="height:3000px;">
                    <div class="modal-content">
                        <form action="/open_tender/rfq/create" method="post">
                            <div class="modal-header">
                                <h5 class="modal-title" id="exampleModalLabel">Create RFQ</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&#215;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <div class="mb-3 col-6">
                                    <strong>Vendor Payment Terms:</strong>
                                    <span class="ml-1">


                                        <div class="form-group">
                                            <div class="input-group ">
                                                <input type="text" class="form-control" id="vendor_payment_terms"
                                                />
                                            </div>
                                        </div>


                                    </span>
                                </div>
                                <div style="overflow-x:auto;">
                                    <table class="table">
                                        <tr>
                                            <th colspan="2">Product</th>
                                            <th>Description</th>
                                            <th>Quantity</th>
                                            <th>UOM</th>
                                            <th nowrap="nowrap" style="width:200px;">Unit Price</th>
                                        </tr>
                                        <t t-foreach="open_tender.sh_purchase_agreement_line_ids" t-as="ol">
                                            <tr>
                                                <td>
                                                    <img t-if="ol.sh_product_id.image_128"
                                                        class="rounded-circle mt-1 o_portal_contact_img"
                                                        t-att-src="image_data_uri(ol.sh_product_id.image_128)" alt="Product"/>
                                                    <img t-if="not ol.sh_product_id.image_128"
                                                        class="rounded-circle mt-1 o_portal_contact_img"
                                                        src="/web/static/src/img/placeholder.png" alt="Product"/>
                                                </td>
                                                <td><span t-field="ol.sh_product_id"/></td>
                                                <td><span t-field="ol.sh_product_description"/></td>
                                                <td><span t-field="ol.sh_qty"/></td>
                                                <td><span t-field="ol.sh_product_uom_id"/></td>
                                                <td>
                                                    <t t-set="price_unit" t-value="ol.sh_price_unit % 1 and ol.sh_price_unit or int(ol.sh_price_unit)"/>
                                                    <input type="text" class="form-control po_unitprice" id="unit_price"
                                                            t-att-name="ol.id" t-att-value="str(price_unit)"
                                                            nowrap="nowrap"/>
                                                </td>
                                            </tr>
                                        </t>
                                    </table>
                                </div>
                                <div class="row">
                                    <div class="col-12">
                                        <label for="rfq_note" style="margin-top: 10px;">Terms and Conditions</label>
                                        <textarea type="text" name="rfq_note"
                                                class="form-control o_survey_question_text_box bg-transparent text-dark rounded-0 p-0"
                                                placeholder="Terms and conditions......"><t
                                                t-raw="notes"/></textarea>
                                    </div>
                                    <div class="col-12">
                                        <label for="agreement_note" style="margin-top: 10px;">Service Level Agreement</label>
                                        <textarea type="text" name="agreement_note"
                                                class="form-control o_survey_question_text_box bg-transparent text-dark rounded-0 p-0"
                                                placeholder="Agreements......"><t
                                                    t-raw="service_level_agreement_box"/></textarea>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <input type="hidden" name="tender_id" t-att-value="open_tender.id"></input>
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                <button type="submit" style="margin-left: 20px;" id="btn_add_bid_form_open_tender" class="btn btn-secondary" t-att-data-id="open_tender.id" t-att-data-name="open_tender.id" t-att-data-value="open_tender.id">Submit</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="modal fade" style="margin-top:100px;" id="loginOrRegisterModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Create RFQ</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&#215;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <h5>Please Sign in or Register Your Vendor before submit quotation</h5>
                        </div>
                        <div class="modal-footer">
                            <a href="/web/login" style="margin-left: 20px;" class="btn btn-secondary">Sign In</a>
                            <a href="/vendor_sign_up" style="margin-left: 20px;" class="btn btn-secondary">Register</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal fade" style="margin-top:100px;" id="bukanVendorModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Create RFQ</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&#215;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <h3>Please Register or Sign In as a Vendor!</h3>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>

            <div t-attf-class="#{'card-body' if report_type == 'html' else ''}">
                <section id="details" style="page-break-inside: auto;">
                    <div class="row">
                        <ul class="nav nav-tabs col-md-12" id="myTab" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="information-tab" data-toggle="tab" href="#information" role="tab" aria-controls="information" aria-selected="true">Details</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="participant-tab" data-toggle="tab" href="#participant" role="tab" aria-controls="participant" aria-selected="true">Participant</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="tender_evaluation-tab" data-toggle="tab" href="#tender_evaluation" role="tab" aria-controls="tender_evaluation" aria-selected="false">Tender Evaluation</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="tender_winner-tab" data-toggle="tab" href="#tender_winner" role="tab" aria-controls="tender_winner" aria-selected="false">Tender Winner</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" t-if="purchase_order_id" id="quotation-tab" data-toggle="tab" href="#quotation" role="tab" aria-controls="quotation" aria-selected="false">Your Quotation</a>
                            </li>
                        </ul>
                        <div class="container">
                            <div class="tab-content mt-1" id="myTabContent">
                                <div class="tab-pane fade show active" id="information" role="tabpanel" aria-labelledby="information-tab">
                                    <t t-call="equip3_purchase_vendor_portal.information"/>
                                </div>
                                <div class="tab-pane fade" id="participant" role="tabpanel" aria-labelledby="participant-tab">
                                    <t t-call="equip3_purchase_vendor_portal.participant"/>
                                </div>
                                <div class="tab-pane fade" id="tender_evaluation" role="tabpanel" aria-labelledby="tender_evaluation-tab">
                                    <!-- <t t-call="equip3_purchase_vendor_portal.participant"/> -->
                                    <t t-call="equip3_purchase_vendor_portal.tender_evaluation"/>
                                </div>
                                <div class="tab-pane fade" id="tender_winner" role="tabpanel" aria-labelledby="tender_winner-tab">
                                    <t t-call="equip3_purchase_vendor_portal.tender_winner"/>
                                </div>
                                <div class="tab-pane fade" t-if="purchase_order_id" id="quotation" role="tabpanel" aria-labelledby="quotation-tab">
                                    <t t-set="partner_id" t-value="request.env.user.partner_id"/>
                                    <t t-if="purchase_order_id.partner_id != partner_id">
                                    </t>
                                    <t t-else="">
                                        <t t-call="equip3_purchase_vendor_portal.tender_quotation"/>
                                    </t>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br/>
                    
                </section>
            </div>
        </template>
        <template id="portal_my_home_menu_open_tender" name="Portal layout : Open Tender menu entries" inherit_id="portal.portal_breadcrumbs" priority="30">
            <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
                <li t-if="(page_name == 'open_tender' or open_tender) and not tender_dashboard_selection" t-attf-class="breadcrumb-item #{'active ' if not open_tender else ''}">
                    <a t-if="open_tender" t-attf-href="/open_tender?{{ keep_query() }}">Open Tender</a>
                    <t t-else="">Open Tender</t>
                </li>
                <li t-if="(page_name == 'open_tender' or open_tender) and tender_dashboard_selection=='active_tender'" t-attf-class="breadcrumb-item #{'active ' if not open_tender else ''}">
                    <a t-if="open_tender" t-attf-href="/open_tender?{{ keep_query() }}">Active Tender</a>
                    <t t-else="">Active Tender</t>
                </li>
                <li t-if="(page_name == 'open_tender' or open_tender) and tender_dashboard_selection=='doc_submit_tender'" t-attf-class="breadcrumb-item #{'active ' if not open_tender else ''}">
                    <a t-if="open_tender" t-attf-href="/open_tender?{{ keep_query() }}">Document Submitted</a>
                    <t t-else="">Document Submitted</t>
                </li>
                <li t-if="(page_name == 'open_tender' or open_tender) and tender_dashboard_selection=='total_tender'" t-attf-class="breadcrumb-item #{'active ' if not open_tender else ''}">
                    <a t-if="open_tender" t-attf-href="/open_tender?{{ keep_query() }}">Total Tender</a>
                    <t t-else="">Total Tender</t>
                </li>
                <li t-if="(page_name == 'open_tender' or open_tender) and tender_dashboard_selection=='won_tender'" t-attf-class="breadcrumb-item #{'active ' if not open_tender else ''}">
                    <a t-if="open_tender" t-attf-href="/open_tender?{{ keep_query() }}">Won Tender</a>
                    <t t-else="">Won Tender</t>
                </li>
                <li t-if="(page_name == 'open_tender' or open_tender) and tender_dashboard_selection=='Active Tenders ending this week'" t-attf-class="breadcrumb-item #{'active ' if not open_tender else ''}">
                    <a t-if="open_tender" t-attf-href="/open_tender?{{ keep_query() }}">Active Tenders ending this week</a>
                    <t t-else="">Active Tenders ending this week</t>
                </li>
                <li t-if="(page_name == 'open_tender' or open_tender) and tender_dashboard_selection=='Overdue Tender'" t-attf-class="breadcrumb-item #{'active ' if not open_tender else ''}">
                    <a t-if="open_tender" t-attf-href="/open_tender?{{ keep_query() }}">Overdue Tender</a>
                    <t t-else="">Overdue Tender</t>
                </li>
                <li t-if="open_tender" class="breadcrumb-item active">
                    <span t-field="open_tender.name" />
                </li>
            </xpath>
        </template>
    </data>
</odoo>