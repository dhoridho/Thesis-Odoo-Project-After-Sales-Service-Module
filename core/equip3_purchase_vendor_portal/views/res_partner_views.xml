<odoo>
	<data>
		 <template id="vendor_sign_up_form_view_inherit" inherit_id="sh_vendor_signup.vendor_sign_up_form_view">
			<xpath expr="//div[@class='row js_cls_sh_vendor_signup_country_state_wrapper']" position="after">
				<div class='row'>
					<div class='col-lg-6'>
						<div class="form-group">
						  <label for="capital_revenue"><b>Capital Revenue :</b></label>
						  <input type="text" inputmode="numeric" pattern="[0-9]+([\.,][0-9]+)?" class="form-control" id="capital_revenue" name="capital_revenue" options="{'currency_field': 'company_currency'}"/>
						</div>
					</div>
					<div class='col-lg-6'>
						<div class="form-group">
						  	<label for="company_size"><b>Company Size :</b></label>
							<div class='row'>
								<div class='col-lg-4'>
									<input type="text" class="form-control" id="company_size" name="company_size"/>
								</div>
								&#160;&#160;-&#160;&#160;
								<div class='col-lg-4'>
									<input type="text" class="form-control" id="company_size2" name="company_size2"/>
								</div>
							</div>
						</div>
					</div>
				</div>
			</xpath>
		</template>
	</data>
</odoo>