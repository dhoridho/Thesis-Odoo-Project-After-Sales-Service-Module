<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="qiscus_conversation_sync" model="ir.cron">
            <field name="name">Qiscus Conversation Syncronize Scheduler</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_acrux_chat_conversation"/>
            <field name="code">model.sync_conversation()</field>
            <field name="state">code</field>
            <field name="active" eval="False"/>
        </record>
    </data>
</odoo>