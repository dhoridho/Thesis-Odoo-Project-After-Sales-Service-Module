<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sequence_menu_planner" model="ir.sequence">
        <field name="name">Menu Planner</field>
        <field name="code">menu.planner</field>
        <field name="prefix">MPL/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="sequence_catering_menu_planner" model="ir.sequence">
        <field name="name">Menu Planner</field>
        <field name="code">catering.menu.planner</field>
        <field name="prefix">CMP/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>