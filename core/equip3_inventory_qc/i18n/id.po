#. module: equip3_inventory_qc
#: model:mail.template,body_html:equip3_inventory_qc.email_template_quality_alert
msgid ""
"\n"
"            <p>Dear ${ctx.get('user_name')}, There's product that need to "
"be Quality Checked</p>\n"
"            <table style=\"width:100%\">\n"
"                <thead style=\"border:1px solid black;\">\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Product</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Quantity to Check</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Partner</th>\n"
"                </thead>\n"
"                <tbody style=\"border:1px solid black;\">\n"
"                    <tr style=\"border:1px solid black;\">\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${object.product_id.name}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${ctx.get('checked_qty')}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${object.partner_id.name}</td>\n"
"                    </tr>\n"
"                </tbody>\n"
"            </table>\n"
"            <p>Thank you.</p>\n"
"            <p style=\"margin-top: 24px; margin-bottom: 16px;\">\n"
"                    <a href=${ctx.get('url')} style=\"background-color:"
"#875A7B; padding: 10px; text-decoration: none; color: #fff; border-"
"radius: 5px;\">\n"
"                            View ${object.piking_id.name}\n"
"                    </a>\n"
"                </p>\n"
"        \n"
"        "
msgstr ""
"\n"
"            <p>Kepada ${ctx.get('user_name')}, Terdapat produk yang "
"qualitasnya butuh dicek</p>\n"
"            <table style=\"width:100%\">\n"
"                <thead style=\"border:1px solid black;\">\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Produk</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Jumlah Untuk Dicek</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Partner</th>\n"
"                </thead>\n"
"                <tbody style=\"border:1px solid black;\">\n"
"                    <tr style=\"border:1px solid black;\">\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${object.product_id.name}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${ctx.get('checked_qty')}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${object.partner_id.name}</td>\n"
"                    </tr>\n"
"                </tbody>\n"
"            </table>\n"
"            <p>Terima Kasih.</p>\n"
"            <p style=\"margin-top: 24px; margin-bottom: 16px;\">\n"
"                    <a href=${ctx.get('url')} style=\"background-color:"
"#875A7B; padding: 10px; text-decoration: none; color: #fff; border-"
"radius: 5px;\">\n"
"                            Lihat ${object.piking_id.name}\n"
"                    </a>\n"
"                </p>\n"
"        \n"
"        "

