<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- internal.transfer inherit form view -->
    <record id="view_internal_transfer_form_inherit_qc" model="ir.ui.view">
        <field name="name">internal.transfer.inherit.view.form</field>
        <field name="model">internal.transfer</field>
        <field name="inherit_id" ref="equip3_inventory_operation.view_form_internal_transfer"/>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='create_date']" position="after">
                <field name="quality_check_id" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>
