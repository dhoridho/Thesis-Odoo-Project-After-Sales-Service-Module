    <t t-name="TaxReportTable">
        <t t-foreach="report_lines" t-as="record">
            <h3 class="mt-4 ml-3 " t-if="record.type == 'sale'">Sales</h3>
            <h3 class="mt-4 ml-3 " t-if="record.type == 'purchase'">Purchase</h3>
            <h3 class="mt-4 ml-3 " t-if="record.type == 'summary'">Tax Summary</h3>
            <h3 class="mt-4 ml-3 " t-if="record.type == 'none'">Others</h3>
            <div>
                <div class="table_main_view">
                    <table cellspacing="0" width="100%">
                        <thead class="tbl_report_title">
                            <tr width="100%">
                                <th width="20%">Number <i class="fa fa-sort btn btn-sort" data-sort="reference" style="color: darkgrey;" /></th>
                                <t t-if="record.type == 'summary'">
                                    <th width="10%" style="text-align:right;"></th>
                                    <th width="10%" style="text-align:right;"></th>
                                    <th width="10%" style="text-align:right;"></th>
                                    <th width="10%" style="text-align:right;"></th>
                                </t>
                                <t t-else="">
                                    <th width="10%" style="text-align:right;">Date <i class="fa fa-sort btn btn-sort" data-sort="date" style="color: darkgrey;" /></th>
                                    <th width="10%" style="text-align:right;">Account <i class="fa fa-sort btn btn-sort" data-sort="account_name" style="color: darkgrey;" /></th>
                                    <th width="10%" style="text-align:right;">Ref No <i class="fa fa-sort btn btn-sort" data-sort="ref_no" style="color: darkgrey;" /></th>
                                    <th width="10%" style="text-align:right;">Partner <i class="fa fa-sort btn btn-sort" data-sort="partner" style="color: darkgrey;" /></th>
                                </t>
                                <th width="10%" style="text-align:right;">Untaxed Amount <i class="fa fa-sort btn btn-sort" data-sort="untaxed_amount" style="color: darkgrey;" /></th>
                                <th width="10%" style="text-align:right;">Tax Amount <i class="fa fa-sort btn btn-sort" data-sort="tax_amount" style="color: darkgrey;" /></th>
                                <th width="10%" style="text-align:right;">Total <i class="fa fa-sort btn btn-sort" data-sort="amount_total" style="color: darkgrey;" /></th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-set="none_value" t-value="_"/>
                            <t t-foreach="record.record_lines" t-as="tax_line">
                                <t t-set="common_id" t-value="0"/>
                                <tr style="border: 0.5px solid #deb9b9;" t-attf-class="tr-line collapsed c{{tax_line['tax_id']}}"
                                    data-toggle="collapse"
                                    t-att-data-tax-id="tax_line['tax_id']"
                                    t-attf-data-target=".a{{tax_line['tax_id']}}">
                                    <td width="20%" style="border: 0px solid black;">
                                        <i class="fa fa-caret-down" role="img" aria-label="Unfolded" title="Unfolded"></i>
                                        <span>
                                            <t t-esc="tax_line['tax_name']"/>
                                        </span>
                                    </td>
                                    <td width="10%"></td>
                                    <td width="10%"></td>
                                    <td width="10%"></td>
                                    <td width="10%"></td>

                                    <t t-if="currency[1] == 'before'">
                                        <td  width="10%" style="text-align:right;">
                                            <t t-esc="currency[0]"/>
                                            <t t-raw="tax_line['untaxed_amount_total']"/>
                                        </td>
                                        <td  width="10%" style="text-align:right;">
                                            <t t-esc="currency[0]"/>
                                            <t t-raw="tax_line['gst_total']"/>
                                        </td>
                                        <td  width="10%" style="text-align:right;">
                                            <t t-esc="currency[0]"/>
                                            <t t-raw="tax_line['amount_total']"/>
                                        </td>
                                    </t>
                                    <t t-else="">
                                        <td  width="10%" style="text-align:right;">
                                            <t t-raw="tax_line['untaxed_amount_total']"/>
                                            <t t-esc="currency[0]"/>
                                        </td>
                                        <td  width="10%" style="text-align:right;">
                                            <t t-raw="tax_line['gst_total']"/>
                                            <t t-esc="currency[0]"/>
                                        </td>
                                        <td  width="10%" style="text-align:right;">
                                            <t t-raw="tax_line['amount_total']"/>
                                            <t t-esc="currency[0]"/>
                                        </td>

                                    </t>
                                </tr>
                                <tr t-attf-class="collapse a{{tax_line['tax_id']}}">
                                    <td colspan="10">
                                        <ul>
                                        </ul>
                                        <!-- <t t-call="TaxReportLines"/> -->
                                    </td>
                                </tr>
                            </t>
                            <tr width="100%">
                                <td width="20%">
                                    <t t-if="record.type == 'summary'">
                                        <span class="ml-4"><strong><t t-esc="record['name_tag_summary']"/></strong></span>
                                    </t>
                                    <t t-else="">
                                        <span class="ml-4"><strong>Total</strong></span>
                                    </t>
                                </td>
                                <td width="10%" class="text-right">
                                </td>
                                <td width="10%" class="text-right">
                                </td>
                                <td width="10%" class="text-right">
                                </td>
                                <td width="10%" class="text-right">
                                </td>
                                <td width="10%" class="text-right">
                                    <t t-if="currency[1] == 'before'">
                                        <strong><span><t t-esc="currency[0]"/></span></strong>
                                        <strong><span t-esc="record.tax_final_untaxed_amount_total"></span></strong>
                                    </t>
                                    <t t-else="">
                                        <strong><span t-esc="record.tax_final_untaxed_amount_total"></span></strong>
                                        <strong><span><t t-esc="currency[0]"/></span></strong>
                                    </t>
                                </td>
                                <td width="10%" class="text-right">
                                    <t t-if="currency[1] == 'before'">
                                        <strong><span><t t-esc="currency[0]"/></span></strong>
                                        <strong><span t-esc="record.tax_final_gst_total"></span></strong>
                                    </t>
                                    <t t-else="">
                                        <strong><span t-esc="record.tax_final_gst_total"></span></strong>
