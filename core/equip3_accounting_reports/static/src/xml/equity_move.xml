<templates>
    <t t-name="EquityMoveTemp">
        <div class="">
            <div class="">
                <h1 style="padding:10px">
                    Equity Movement Report
                </h1>

            </div>

            <div>
                <div class="filter_view_tb" style="overflow: unset;"></div>
            </div>
            <br></br>
            <div>
                <div class="table_view_tb" style="right:20px;"></div>
            </div>
        </div>
    </t>
    <t t-name="EquityMoveTable">
        <div>
            <div class="table_main_view">
                <table cellspacing="0" width="100%">
                    <thead class="tbl_report_title">
                        <tr>
                            <th colspan="6">Account</th>
                            <th class="mon_fld">Debit</th>
                            <th class="mon_fld">Credit</th>
                            <th class="mon_fld">Balance</th>
                        </tr>
                    </thead>

                    <tbody>
                        <t t-set="none_value" t-value="_"/>
                        <t t-foreach="account_equity_filtered" t-as="account">
                            <t t-set="common_id" t-value="0"/>
                            <tr style="border: 0.5px solid #deb9b9;" class="gl-line collapsed"
                                data-toggle="collapse">
                                <td colspan="6" style="border: 0px solid black;">
                                    <span>
                                        Authorized capital
                                    </span>
                                </td>
                                <t t-if="currency[1] == 'before'">
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['debit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['credit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['balance']"/>
                                    </td>

                                </t>
                                <t t-else="">
                                    <td class="mon_fld">
                                        <t t-raw="account['debit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['credit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['balance']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                </t>
                                <t t-set="common_id" t-value="'a'+account['id']"/>
                            </tr>
                        </t>
                        <t t-foreach="account_equity" t-as="account">
                            <t t-set="common_id" t-value="0"/>
                            <tr style="border: 0.5px solid #deb9b9;" class="gl-line collapsed"
                                data-toggle="collapse">
                                <td colspan="6" style="border: 0px solid black;">
                                    <span>
                                        Additional paid in capital
                                    </span>
                                </td>
                                <t t-if="currency[1] == 'before'">
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['debit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['credit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['balance']"/>
                                    </td>

                                </t>
                                <t t-else="">
                                    <td class="mon_fld">
                                        <t t-raw="account['debit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['credit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['balance']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                </t>
                                <t t-set="common_id" t-value="'a'+account['id']"/>
                            </tr>
                        </t>
                        <t t-foreach="account_retained_earnings" t-as="account">
                            <t t-set="common_id" t-value="0"/>
                            <tr style="border: 0.5px solid #deb9b9;" class="gl-line collapsed"
                                data-toggle="collapse">
                                <td colspan="6" style="border: 0px solid black;">
                                    <span>
                                        Retained Earning
                                    </span>
                                </td>
                                <t t-if="currency[1] == 'before'">
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['debit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['credit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['balance']"/>
                                    </td>

                                </t>
                                <t t-else="">
                                    <td class="mon_fld">
                                        <t t-raw="account['debit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['credit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['balance']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                </t>
                                <t t-set="common_id" t-value="'a'+account['id']"/>
                            </tr>
                        </t>
                        <t t-foreach="account_current_earnings" t-as="account">
                            <t t-set="common_id" t-value="0"/>
                            <tr style="border: 0.5px solid #deb9b9;" class="gl-line collapsed"
                                data-toggle="collapse">
                                <td colspan="6" style="border: 0px solid black;">
                                    <span>
                                        Profit And Loss
                                    </span>
                                </td>
                                <t t-if="currency[1] == 'before'">
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['debit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['credit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['balance']"/>
                                    </td>

                                </t>
                                <t t-else="">
                                    <td class="mon_fld">
                                        <t t-raw="account['debit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['credit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['balance']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                </t>
                                <t t-set="common_id" t-value="'a'+account['id']"/>
                            </tr>
                        </t>
                        <t t-foreach="account_prive" t-as="account">
                            <t t-set="common_id" t-value="0"/>
                            <tr style="border: 0.5px solid #deb9b9;" class="gl-line collapsed"
                                data-toggle="collapse">
                                <td colspan="6" style="border: 0px solid black;">
                                    <span>
                                        Prive
                                    </span>
                                </td>
                                <t t-if="currency[1] == 'before'">
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['debit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['credit']"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-esc="currency[0]"/>
                                        <t t-raw="account['balance']"/>
                                    </td>

                                </t>
                                <t t-else="">
                                    <td class="mon_fld">
                                        <t t-raw="account['debit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['credit']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                    <td class="mon_fld">
                                        <t t-raw="account['balance']"/>
                                        <t t-esc="currency[0]"/>
                                    </td>
                                </t>
                                <t t-set="common_id" t-value="'a'+account['id']"/>
                            </tr>
                        </t>
                        <td colspan="6" style="" class="">
                            <strong>Total</strong>
                        </td>
                        <t t-if="currency[1] == 'before'">
                            <td class="mon_fld" style="">
                                <strong>
                                    <t t-esc="currency[0]"/>
                                    <t t-esc="debit_total"/>
                                </strong>
                            </td>
                            <td class="mon_fld" style="">
                                <strong>
                                    <t t-esc="currency[0]"/>
                                    <t t-esc="credit_total"/>
                                </strong>
                            </td>
                            <td class="mon_fld" style="">
                                <strong>
                                    <t t-esc="currency[0]"/>
                                    <t t-esc="balance_total"/>
                                </strong>
                            </td>
                        </t>
                        <t t-else="">
                            <td class="mon_fld" style="">
                                <strong>
                                    <t t-raw="Math.round(debit_total * Math.pow(10, 2)) / Math.pow(10, 2)"/>
                                    <t t-esc="currency[0]"/>
                                </strong>
                            </td>
                            <td class="mon_fld" style="">
                                <strong>
                                    <t t-raw="Math.round(credit_total * Math.pow(10, 2)) / Math.pow(10, 2)"/>
                                    <t t-esc="currency[0]"/>
                                </strong>
                            </td>
                            <td class="mon_fld" style="">
                                <strong>
                                    <t t-raw="Math.round(balance_total * Math.pow(10, 2)) / Math.pow(10, 2)"/>
                                    <t t-esc="currency[0]"/>
                                </strong>
                            </td>
                        </t>
                    </tbody>
                </table>
            </div>
        </div>
    </t>


