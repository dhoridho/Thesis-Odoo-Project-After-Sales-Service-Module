<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <!-- Invoice Exchange -->
        <record id="seq_invoice_exchange" model="ir.sequence">
            <field name="name">Invoice Exchange</field>
            <field name="code">invoice.exchange</field>
            <field name="prefix">CIE/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_bill_exchange" model="ir.sequence">
            <field name="name">Bill Exchange</field>
            <field name="code">bill.exchange</field>
            <field name="prefix">VBE/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>