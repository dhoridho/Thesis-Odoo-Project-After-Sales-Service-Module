        <record id="ks_debt_coll_cq_row1" model="ks_dashboard_ninja.item">
            <field name="name">List Total Collection</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_data_calculation_type">query</field>
            <field name="ks_custom_query">
                    WITH date_ranges AS (
                        SELECT
                            'Today' AS type_of_debt,
                            1 AS sort_order,
                            date_trunc( 'day', now( ) ) AS start_date,
                            date_trunc( 'day', now( ) + INTERVAL '1 day' ) AS end_date UNION ALL
                        SELECT
                            'This Week' AS type_of_debt,
                            2 AS sort_order,
                            date_trunc( 'week', now( ) ) AS start_date,
                            date_trunc( 'week', now( ) + INTERVAL '1 week' ) AS end_date UNION ALL
                        SELECT
                            'This Month' AS type_of_debt,
                            3 AS sort_order,
                            date_trunc( 'month', now( ) ) AS start_date,
                            date_trunc( 'month', now( ) + INTERVAL '1 month' ) AS end_date UNION ALL
                        SELECT
                            'This Year' AS type_of_debt,
                            4 AS sort_order,
                            date_trunc( 'year', now( ) ) AS start_date,
                            date_trunc( 'year', now( ) + INTERVAL '1 year' ) AS end_date 
                        ) SELECT
                        dr.type_of_debt,
                        COALESCE ( q_adc.total_amount, 0 ) AS total_amount,
                        COALESCE ( q_adc.collection_amount, 0 ) AS collection_amount,
                        COALESCE ( q_adc.amount_due, 0 ) AS amount_due,
                        ( ( COALESCE ( q_adc.collection_amount, 0 ) / COALESCE ( q_adc.amount_due, 1 ) ) * 100 ) AS target_percentage 
                    FROM
                        date_ranges dr
                        LEFT JOIN (
                        SELECT
                            'Today' AS type_of_debt,
                            date_trunc( 'day', adc.DATE ) AS INTERVAL,
                            COUNT ( * ) AS debt_count,
                            SUM ( adc.amount_invoice ) AS total_amount,
                            SUM ( adc.amount ) AS collection_amount,
                            SUM ( adc.amount_residual ) AS amount_due 
                        FROM
                            account_debt_collection_line adc 
                        GROUP BY
                            INTERVAL,
                            type_of_debt UNION ALL
                        SELECT
                            'This Week' AS type_of_debt,
                            date_trunc( 'week', adc.DATE ) AS INTERVAL,
                            COUNT ( * ) AS debt_count,
                            SUM ( adc.amount_invoice ) AS total_amount,
                            SUM ( adc.amount ) AS collection_amount,
                            SUM ( adc.amount_residual ) AS amount_due 
                        FROM
                            account_debt_collection_line adc 
                        GROUP BY
                            INTERVAL,
                            type_of_debt UNION ALL
                        SELECT
                            'This Month' AS type_of_debt,
                            date_trunc( 'month', adc.DATE ) AS INTERVAL,
                            COUNT ( * ) AS debt_count,
                            SUM ( adc.amount_invoice ) AS total_amount,
                            SUM ( adc.amount ) AS collection_amount,
                            SUM ( adc.amount_residual ) AS amount_due 
                        FROM
                            account_debt_collection_line adc 
                        GROUP BY
                            INTERVAL,
                            type_of_debt UNION ALL
                        SELECT
                            'This Year' AS type_of_debt,
                            date_trunc( 'year', adc.DATE ) AS INTERVAL,
                            COUNT ( * ) AS debt_count,
                            SUM ( adc.amount_invoice ) AS total_amount,
                            SUM ( adc.amount ) AS collection_amount,
                            SUM ( adc.amount_residual ) AS amount_due 
                        FROM
                            account_debt_collection_line adc 
                        GROUP BY
                            INTERVAL,
                            type_of_debt 
                        ) AS q_adc ON dr.type_of_debt = q_adc.type_of_debt 
                        AND dr.start_date &lt;= q_adc.INTERVAL 
                        AND dr.end_date &gt; q_adc.INTERVAL 
                    ORDER BY
                        dr.sort_order 
            </field>
        </record>

