<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-inherit="mrp.button" t-inherit-mode="extension">
        <xpath expr="//button[hasclass('o_mrp_bom_print')]" position="replace">
            <button type="button" class="btn btn-primary o_mrp_bom_print">Print PDF</button>
        </xpath>
        <xpath expr="//button[hasclass('o_mrp_bom_print_unfolded')]" position="after">
            <button type="button" class="btn btn-primary o_mrp_bom_print_excel">Print Excel</button>
        </xpath>
    </t>
</templates>