<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    	<!-- Sequences for Manufacturing Production -->
        <record model="ir.sequence" id="seq_mrp_consumption2">
            <field name="name">Production Consumption</field>
            <field name="code">mrp.consumption</field>
            <field name="prefix">PRC/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>