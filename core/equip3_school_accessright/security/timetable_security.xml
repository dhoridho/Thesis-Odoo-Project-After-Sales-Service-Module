<?xml version="1.0" ?>
<odoo>

    <record model="ir.rule" id="rule_time_table">
        <field name="name">Teacher Timetable Rule</field>
        <field name="model_id" ref="timetable.model_time_table"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('school.group_school_teacher'))]"/>
    </record>

    <record model="ir.rule" id="rule_time_table_line">
        <field name="name">Teacher Timetable Line Rule</field>
        <field name="model_id" ref="timetable.model_time_table_line"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('school.group_school_teacher'))]"/>
    </record>

</odoo>
