<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_search_res_users_access_report_record" model="ir.ui.view">
        <field name="name">res.users.access.report.record.search</field>
        <field name="model">res.users.access.report.record</field>
        <field name="arch" type="xml">
            <search>
                <field name="user_id" string="User" filter_domain="['|', ('user_id', 'ilike', self), ('login', 'ilike', self)]"/>
            </search>
        </field>
    </record>

    <!--Tree-->
    <record id="view_tree_res_users_access_report_record" model="ir.ui.view">
        <field name="name">res.users.access.report.record.view.tree</field>
        <field name="model">res.users.access.report.record</field>
        <field name="arch" type="xml">
            <tree create="0" delete="0" edit="0">
                <field name="sl_no"/>
                <field name="user_id"/>
                <field name="column1" invisible="1"/>
                <field name="column2" invisible="1"/>
                <field name="column3" invisible="1"/>
                <field name="column4" invisible="1"/>
                <field name="column5" invisible="1"/>
                <field name="column6" invisible="1"/>
                <field name="column7" invisible="1"/>
                <field name="column8" invisible="1"/>
                <field name="column9" invisible="1"/>
                <field name="column10" invisible="1"/>
                <field name="column11" invisible="1"/>
                <field name="column12" invisible="1"/>
                <field name="column13" invisible="1"/>
                <field name="column14" invisible="1"/>
                <field name="column15" invisible="1"/>
            </tree>
        </field>
    </record>

</odoo>