<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="alert_stage_0" model="sh.qc.alert.stage">
        <field name="sequence">0</field>
        <field name="name">NEW</field>
    </record>
    <record id="alert_stage_1" model="sh.qc.alert.stage">
        <field name="sequence">1</field>
        <field name="name">WAITING</field>
    </record>
    <record id="alert_stage_2" model="sh.qc.alert.stage">
        <field name="sequence">2</field>
        <field name="name">DONE</field>
    </record>
    <record id="quality_alert_seq" model="ir.sequence">
        <field name="name">Quality Alert Sequence</field>
        <field name="code">sh.quality.alert</field>
        <field name="prefix">QA</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>
    <record id="mrp_quality_alert_seq" model="ir.sequence">
        <field name="name">Quality Alert Sequence</field>
        <field name="code">sh.mrp.quality.alert</field>
        <field name="prefix">QA</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>
    <record id="mail_alias_quality_alert" model="mail.alias">
        <field name="alias_name">alert</field>
        <field name="alias_model_id" ref="model_sh_quality_alert" />
        <field name="alias_user_id" ref="base.user_admin" />
        <field name="alias_parent_model_id" ref="sh_inventory_mrp_qc.model_sh_qc_team" />
    </record>
    <record id="quality_alert_team0" model="sh.qc.team">
        <field name="name">Main Quality Team</field>
        <field name="alias_id" ref="mail_alias_quality_alert" />
    </record>
    
