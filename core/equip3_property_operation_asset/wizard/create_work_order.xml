<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- model.name inherit form view -->
        <record id="create_work_order_view_inherit_property" model="ir.ui.view">
            <field name="name">model.name.inherit.view.form</field>
            <field name="model">work.order.wizard</field>
            <field name="inherit_id" ref="equip3_asset_fms_operation.create_work_order_views"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='asset_id']" position="after">
                    <field name="property_id" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>