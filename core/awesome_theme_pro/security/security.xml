<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="awesome_default_mode_access" model="ir.rule">
            <field name="name">Default Theme Mode Access</field>
            <field name="model_id" ref="model_awesome_theme_pro_theme_mode"/>
            <field name="domain_force">[('name', 'in', ['normal', 'dark', 'blue'])]</field>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
            <field name="perm_read" eval="False"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_unlink" eval="True"/>
        </record>

    </data>
</odoo>
