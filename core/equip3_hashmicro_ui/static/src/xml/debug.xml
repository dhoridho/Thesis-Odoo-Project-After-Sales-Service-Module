<?xml version="1.0" encoding="utf-8"?>
<templates id="equip3_hashmicro_ui_template" xml:space="preserve">
    <t t-extend="WebClient.DebugManager">
        <t t-jquery="span.fa-bug" t-operation="replace">
            <span class="o-hm-icon o-hm-view-debug o-hm-f18"/>
        </t>
    </t>

    <t t-inherit="sh_backmate_theme_adv.GlobalSearch" t-inherit-mode="extension">
        <xpath expr="//span[@id='topbar_search_icon']" position="attributes">
            <attribute name="class">o-hm-icon o-hm-view-search o-hm-f18</attribute>
        </xpath>
        <xpath expr="//span[@id='topbar_full_search_icon']" position="attributes">
            <attribute name="class">o-hm-icon o-hm-view-search o-hm-f18</attribute>
        </xpath>
    </t>

    <t t-inherit="oi_login_as.LoginAs" t-inherit-mode="extension">
        <xpath expr="//li[1]" position="replace">
            <li class="o_login_as" title="Login as">
                <a href="javascript:void(0)" id="login_as">
                    <i class="o-hm-icon o-hm-view-switch-user o-hm-f18"/>
                </a>
            </li>
        </xpath>
    </t>

    <t t-extend="ksDashboardNinjaHeader">
        <t t-jquery="span.fa-calendar" t-operation="replace">
            <span class="fa fa-calendar"/>
        </t>
        <t t-jquery="span.fa-play" t-operation="replace">
            <span class="fa fa-play mr-2"/>
        </t>
        <t t-jquery="span.fa-print" t-operation="replace">
            <span class="fa fa-print mr-2"/>
        </t>
        <t t-jquery="span.fa-plus-circle" t-operation="replace">
            <span class="fa fa-plus-circle mr-2"/>
        </t>
        <t t-jquery="span.fa-pencil" t-operation="replace">
            <span class="fa fa-pencil mr-2"/>
        </t>
    </t>
</templates>