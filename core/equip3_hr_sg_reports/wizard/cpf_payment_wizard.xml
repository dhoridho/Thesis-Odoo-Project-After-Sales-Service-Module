<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="cpf_payment_wizard_form_inherit_sg_hr_report" model="ir.ui.view">
        <field name="name">cpf.payment.wizard.view.form.inherit</field>
        <field name="model">cpf.payment.wizard</field>
        <field name="inherit_id" ref="sg_hr_report.cpf_payment_wizard_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/separator[2]" position="after">
                <group name="export" col="4">
                    <field name="export_type" string="Export"/>
                    <field name="export_file_name" invisible="1" />
                    <field name="export_file" widget="binary" filename="export_file_name" options="{'filename_field': 'export_file_name'}" nolabel="1" invisible="1" />
                </group>
            </xpath>
        </field>
    </record>    

</odoo>
