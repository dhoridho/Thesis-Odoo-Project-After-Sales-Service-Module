<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="cpf_return_checking_view_form" model="ir.ui.view">
        <field name="name">cpf_return_checking_wizard.view.form</field>
        <field name="model">cpf.return.checking.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <group name="employees">
                    <field name="employee_ids" />
                    <field name="end_date" />

                    <footer>
                        <button name="action_print_report" string="Print" type="object" class="oe_highlight"/>
                        or
                        <button special="cancel" string="Cancel" class="oe_link"/>
                    </footer>
                </group>
            </form>
        </field>
    </record>

    <record id="action_cpf_return_checking_wizard" model="ir.actions.act_window">
        <field name="name">CPF Return Checking</field>
        <field name="res_model">cpf.return.checking.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
   </record>

   <menuitem action="action_cpf_return_checking_wizard"
            id="menu_cpf_return_checking_wizard"
            parent="sg_hr_report.menu_report_payroll1_cpf"
            sequence="3"/>

</odoo>
