<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_attendance_change" model="ir.sequence">
            <field name="name">Attendance Change Sequence</field>
            <field name="code">hr.attendance.change</field>
            <field name="prefix">ATC/%(range_y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_schedule_exchange" model="ir.sequence">
            <field name="name">Attendance Schedule Exchange</field>
            <field name="code">schedule.exchange</field>
            <field name="prefix">WSC/%(range_y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
