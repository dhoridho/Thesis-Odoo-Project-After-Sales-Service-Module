<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_salary_increment" model="ir.sequence">
            <field name="name">Salary Increment Sequence</field>
            <field name="code">hr.salary.increment</field>
            <field name="prefix">SLI/%(day)s/%(month)s/%(range_year)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
