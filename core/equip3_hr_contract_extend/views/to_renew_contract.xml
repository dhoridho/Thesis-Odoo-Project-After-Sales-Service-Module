<odoo>
    <data>

        <record id="to_renew_view" model="ir.ui.view">
            <field name="name">to.renew.contract.form</field>
            <field name="model">to.renew.contract</field>
            <field name="arch" type="xml">
                <form string="To Renew Contract" create="0" delete="0" edit="0">
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="name" readonly="1"/>
                            </h1>
                        </div>
                        <group>
                              <button name="get_list" string="List To Renew Contract" class="oe_highlight" type="object"/>

                        </group>


                    </sheet>
                </form>
            </field>
        </record>

        <record id="contract_renew_action" model="ir.actions.act_window">
            <field name="name">Contract Renew</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">to.renew.contract</field>
            <field name="view_mode">form</field>

        </record>

    </data>

</odoo>