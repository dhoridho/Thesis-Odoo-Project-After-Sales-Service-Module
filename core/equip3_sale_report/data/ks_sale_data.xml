                   eval="[(6, 0, [ref('account.field_account_move__name'),ref('account.field_account_move__partner_id'),ref('account.field_account_move__invoice_date_due'),ref('account.field_account_move__company_id'),ref('account.field_account_move__amount_total_signed')])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_domain">[["payment_state","=","not_paid"],["move_type", "=", "out_invoice"]]</field>
            <field name="ks_sort_by_field" eval="ref('account.field_account_move__invoice_date_due')"/>
            <field name="ks_company_id" eval="0"/>
        </record>

