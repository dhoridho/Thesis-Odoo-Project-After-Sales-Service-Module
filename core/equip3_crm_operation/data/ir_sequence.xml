<odoo>
    <data noupdate="1">
        <record id="target_activity_seq" model="ir.sequence">
            <field name="name">Target Activity</field>
            <field name="code">target.activity.seq</field>
            <field name="prefix">TAC/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>
        <record id="sequence_lead_sequence" model="ir.sequence">
            <field name="name">Lead Sequence</field>
            <field name="code">res.partner.lead.sequence</field>
            <field name="prefix">LP</field>
            <field name="padding">4</field>
        </record>
        <record id="crm_target_seq" model="ir.sequence">
            <field name="name">CRM Target</field>
            <field name="code">crm.target.seq</field>
            <field name="prefix">MYT/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
    </data>
</odoo>