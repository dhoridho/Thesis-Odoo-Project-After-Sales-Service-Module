<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- Add you code here -->
        <!-- menu.planner inherit form view -->
        <record id="menu_planner_form" model="ir.ui.view">
            <field name="name">menu.planner.inherit.view.form</field>
            <field name="model">menu.planner</field>
            <field name="inherit_id" ref="equip3_catering_masterdata.menu_planner_form"/>
            <field name="arch" type="xml">
                <!-- Add your xpath here -->
                <xpath expr="//field[@name='customer_ids']/tree/field[@name='partner_id']" position="after">
                    <!-- Add your fields here -->
                    <field name="catering_id"/>
                </xpath>
                <xpath expr="//field[@name='customer_ids']/tree" position="inside">
                    <!-- Add your fields here -->
                    <button string="Change Menu" name="%(act_open_change_menu_wizard)d" type="action" attrs="{'invisible':[('is_button_visible','=',False)]}" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>