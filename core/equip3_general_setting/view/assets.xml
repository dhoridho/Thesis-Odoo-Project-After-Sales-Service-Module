<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="equip3_general_setting_assets_backend" name="equip3_general_setting assets" inherit_id="web.assets_backend">
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/equip3_general_setting/static/src/js/switch_branch_menu.js"></script>
                <script type="text/javascript" src="/equip3_general_setting/static/src/js/basic_model.js"></script>
                <script type="text/javascript" src="/equip3_general_setting/static/src/js/session.js"></script>
                <script type="text/javascript" src="/equip3_general_setting/static/src/js/abstract_web_client.js"></script>
                <script type="text/javascript" src="/equip3_general_setting/static/src/js/user_form_view.js"></script>
            </xpath>
        </template>

        <!-- branch assets -->
        <template id="branch_assets_replace" name="equip3_general_setting assets_replace" inherit_id="branch.assets_backend_bi_multi_branch">
            <xpath expr="//script[@src='/branch/static/src/js/default_branch.js']" position="replace"/>
            <xpath expr="//script[@src='/branch/static/src/js/session.js']" position="replace"/>
            <xpath expr="//script[@src='/branch/static/src/js/abstract_web_client.js']" position="replace"/>
        </template>
    </data>
</odoo>