        <record id="ks_construction_item_7" model="ks_dashboard_ninja.item">
            <field name="name">Cost Sheet</field>
            <field name="ks_model_id" ref="equip3_construction_operation.model_job_cost_sheet"/>
            <field name="ks_dashboard_item_type">ks_bar_chart</field>
            <field name="ks_data_calculation_type">custom</field>
            <field name="ks_chart_measure_field"
