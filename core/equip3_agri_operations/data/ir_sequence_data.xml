            <field name="name">Agriculture Cost Actualization</field>
            <field name="code">agri.cost.actualization</field>
            <field name="prefix">ACT/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
        </record>

    </data>
</odoo>
