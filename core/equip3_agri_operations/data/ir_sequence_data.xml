<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Plantation -->
        <record id="seq_agriculture_daily_activity" model="ir.sequence">
            <field name="name">Plantation Plan</field>
            <field name="code">agriculture.daily.activity</field>
            <field name="prefix">ADA/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_agriculture_daily_activity_line" model="ir.sequence">
            <field name="name">Plantation Lines</field>
            <field name="code">agriculture.daily.activity.line</field>
            <field name="prefix">ACL/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_agriculture_daily_activity_record" model="ir.sequence">
            <field name="name">Plantation Record</field>
            <field name="code">agriculture.daily.activity.record</field>
            <field name="prefix">ACR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

