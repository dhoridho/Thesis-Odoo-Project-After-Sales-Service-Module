<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sequence_sale_quotation" model="ir.sequence">
        <field name="name">Sale Order</field>
        <field name="code">sale.order.quotation</field>
        <field name="prefix">SQ/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="sequence_sale_order" model="ir.sequence">
        <field name="name">Sale Order</field>
        <field name="code">sale.quotation.order</field>
        <field name="prefix">SO/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="sequence_sale_quotation_order" model="ir.sequence">
        <field name="name">Sale Order</field>
        <field name="code">sale.order.quotation.order</field>
        <field name="prefix">SO/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
