<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="asset_backend_equip3_agri_reports" name="equip3_agri_reports assets" inherit_id="web.assets_backend">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/equip3_agri_reports/static/src/js/harvesting_analysis_report.js"/>
            <script type="text/javascript" src="/equip3_agri_reports/static/src/js/harvesting_monthly_report.js"/>
            <script type="text/javascript" src="/equip3_agri_reports/static/src/js/budget_analysis_report.js"/>
            <script type="text/javascript" src="/equip3_agri_reports/static/src/js/list_view_report.js"/>
            <script type="text/javascript" src="/equip3_agri_reports/static/src/js/agriculture_flow.js"/>
        </xpath>
        
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/equip3_agri_reports/static/src/css/analysis_report.css"/>
            <link rel="stylesheet" type="text/css" href="/equip3_agri_reports/static/src/css/agriculture_flow.css"/>
        </xpath>
   </template>

   <template id="report_assets_pdf" inherit_id="web.report_assets_pdf">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/equip3_agri_reports/static/src/css/analysis_report.css" type="text/css"/>
            <link rel="stylesheet" href="/equip3_agri_reports/static/src/css/budget_planning_report.css" type="text/css"/>
            <link rel="stylesheet" href="/equip3_agri_reports/static/src/css/budget_planning_block_report.css" type="text/css"/>
        </xpath>
    </template>
</odoo>