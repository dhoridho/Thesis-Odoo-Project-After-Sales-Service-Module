<odoo>
    <data noupdate="1">

      <record id="hashmicro_action_import" model="ir.cron">
          <field name="name">Hashmicro Action Import</field>
          <field eval="True" name="active"/>
          <field name="user_id" ref="base.user_root"/>
          <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 14:00:00')" />
          <field name="interval_number">1</field>
          <field name="interval_type">hours</field>
          <field name="numbercall">-1</field>
          <field name="model_id" ref="model_import_log"/>
          <field name="state">code</field>
          <field name="doall" eval="False"/>
          <field name="code">model.acion_import()</field>
      </record>


      <record id="cron_update_execution_import_logs" model="ir.cron">
          <field name="name">Update Execution Import Logs</field>
          <field eval="True" name="active"/>
          <field name="user_id" ref="base.user_root"/>
          <field name="interval_number">5</field>
          <field name="interval_type">minutes</field>
          <field name="numbercall">-1</field>
          <field name="model_id" ref="model_import_log"/>
          <field name="state">code</field>
          <field name="doall" eval="False"/>
          <field name="code">model.update_execution_import_logs()</field>
      </record>


    </data>


</odoo>