<odoo>

    <data>

        <record id="res_config_settings_view_form_import_log" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.import.log</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="40"/>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[hasclass('settings')]" position="inside">
                    
                    <div class="app_settings_block" data-string="Import Logs" string="Import Logs" data-key="equip3_base_import_extend" groups="">
                        <div id="base_import">
                            <h2>Test Import Rows Limit</h2>
                            <div class="row mt16 o_settings_container">
                                <div class="col-12 col-lg-6 o_setting_box">
                                        <div class="content-group">
                                            <label name="Test Import Rows Limit" for="test_import_batch_limit" string="Test Import Rows Limit"/>
                                            <div class="text-muted">
                                                Number of rows tested on import
                                            </div>
                                            <div class="mt16">
                                                <field name="test_import_batch_limit" class="o_light_label"/>
                                            </div>
                                        </div>
                                </div>
                            </div>

                            <h2>Split Processing</h2>
                            <div class="row mt16 o_settings_container">
                                <div class="col-12 col-lg-6 o_setting_box">
                                        <div class="content-group">
                                            <label name="split_generate_log" for="limit_split_line_import" string="Split Generate Log"/>
                                            <div class="text-muted">
                                                split generate log per batch
                                            </div>
                                            <div class="mt16">
                                                <field name="limit_split_line_import" class="o_light_label"/>
                                            </div>
                                        </div>
                                </div>
                                <div class="col-12 col-lg-6 o_setting_box">
                                        <div class="content-group">
                                            <label name="split_processing" for="is_split_processing" string="Split Processing"/>
                                            <div class="text-muted">
                                                split process of generate excel
                                            </div>
                                            <div class="mt16">
                                                <field name="is_split_processing" class="o_light_label"/>
                                            </div>
                                        </div>
                                </div>
                            </div>

                            <h2>Last Progress Data Pick</h2>
                            <div class="row mt16 o_settings_container">
                                <div class="col-12 col-lg-6 o_setting_box">
                                        <div class="content-group">
                                            <label name="last_proggress_data" for="last_proggress_data" string="Last Progress data Pick"/>
                                            <div class="text-muted">
                                                number the data will get if cron restart
                                            </div>
                                            <div class="mt16">
                                                <field name="last_proggress_data" class="o_light_label"/>
                                            </div>
                                        </div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </xpath>
            </field>
        </record>


        <record id="action_import_log_config" model="ir.actions.act_window">
            <field name="name">Settings</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.config.settings</field>
            <field name="view_mode">form</field>
            <field name="target">inline</field>
            <field name="context">{'module' : 'import_log', 'bin_size': False}</field>
        </record>

        <menuitem
            id="inmport_config_menu"
            name="Configuration"
            parent="import_log_root_menu"
            sequence="20"
            />


        <menuitem
            id="import_log_setting_menu"
            name="Settings"
            
            action="action_import_log_config"
            parent="inmport_config_menu"
            sequence="10"
            />


    </data>
</odoo>