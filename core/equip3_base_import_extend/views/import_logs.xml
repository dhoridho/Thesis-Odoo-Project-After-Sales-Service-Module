<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="import_log_view_search" model="ir.ui.view">
            <field name="name">import.log.view.search</field>
            <field name="model">import.log</field>
            <field name="arch" type="xml">
                <search string="Import Logs">
                    <field name="name" />
                    <field name="model_id" />
                    <group name="group_by" string="Group By">
                       <filter name="filter_by_name" string="Name" domain="[]" context="{'group_by' : 'name'}" />
                   </group>
                    
                </search>
            </field>
        </record>

        
        <record id="import_logs_view_form" model="ir.ui.view">
            <field name="name">import.log.view.form</field>
            <field name="model">import.log</field>
            <field name="arch" type="xml">
                <form string="Import Log" create="0" edit="0">
                    <header modifiers="{}">
                        <field name="state" widget="statusbar"/>
                        <button name="acion_import" type="object" string="Import" class="oe_highlight" groups="base.group_no_one"/>
                    </header>
                    <sheet>
                        <group>
                            <group >
                                <field name="name"/>
                                <field name="model_id"/>
                                <field name="excel_data" invisible="1"/>
                                <field name="to_delete" invisible="1"/>
                                <field name="excel_fields" invisible="1"/>
                                <field name="inserted_record" string="Inserted Record"/>
                                <field name="error_record" string="Error Record"/>
                                <field name="list_message_error" invisible="1"/>
                                <field name="skipped_record" string="Skipped Record" invisible="1"/>
                                <field name="list_data" invisible="1"/>
                                <field name="total_record" string="Record Total"/>
                                <field name="total_progress" invisible="1"/>
                                <field name="total_progress_char"  invisible="1"/>
                                <field name="total_line_char"/>

                                
                            </group>
                            <group>
                                <field name="import_datetime" />
                                <field name="completed_date" />
                                <field name="cron_running" />
                                <field name="file" filename="file_name" widget="binary" invisible="1"/>
                                <field name="file_name" invisible="1"/>
                                <field name="options" invisible="1"/>
                                <field name="field" invisible="1"/>
                                <field name="column" invisible="1"/>
    
                                <field name="file_error_name" invisible="1"/>
                                <field name="file_error_records"  widget="binary" filename="file_error_name" string="File Error Record"/>


                            </group>
                            
                        </group>
                        <notebook>
                        <page name="error_log" string="Error Log">
                            <field name="error_log"/>
                        </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>



        <record id="import_log_view_tree" model="ir.ui.view">
            <field name="name">import.log.view.tree</field>
            <field name="model">import.log</field>
            <field name="arch" type="xml">
                <tree string="Import Log" create="0">
                    <field name="name" />
                    <field name="batch_count" string="Batch"/>
                    <field name="model_id" />
                    <field name="import_datetime" />
                    <field name="cron_running" />
                    <field name="completed_date" />
                    <field name="inserted_record" />
                    <field name="error_record" />
                    <field name="total_progress_char" />
                    <field name="total_line_char" />
                    <field name="skipped_record" invisible="1"/>
                    <field name="state" />
                    <field name="file_error_name" invisible="1"/>
                    <field name="file_error_records"  widget="binary" filename="file_error_name" string="File Error Record"/>
                    <!-- <field name="list_data" invisible="1"/> -->
                    <button type="object" string="View Records" name="act_view_records" class="oe_highlight"/>
                </tree>
            </field>
        </record>

        <record id="import_log_action" model="ir.actions.act_window">
            <field name="name">Import Logs</field>
            <field name="res_model">import.log</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <!-- <field name="context">{'search_default_filter_by_name':1}</field> -->
            <field name="context">{}</field>
            <field name="help" type="html">
                <p></p>
                
            </field>
        </record>


        <menuitem
            id="import_log_root_menu"
            name="Import Logs"
            web_icon="equip3_base_import_extend,static/description/Import_Logs.png"
            />

        <menuitem
            id="import_log_menu"
            name="Import Logs"
            action="import_log_action"
            parent="import_log_root_menu"
            sequence="11"
            />

            <!-- <record id="import_log_menu" model="ir.ui.menu">
                <field name="equip_category_id" ref="equip3_hashmicro_ui.category_utilities"/> 
            </record> -->
        
    
    </data>
    

</odoo>
