        <record id="rec_draft_stage" model="order.stage">
            <field name="name">Draft</field>
            <field name="action_type">draft</field>
            <field name="sequence" type="int">1</field>
        </record>

        <record id="rec_sale_stage" model="order.stage">
            <field name="name">Confirmed</field>
            <field name="action_type">sale</field>
            <field name="sequence" type="int">3</field>
