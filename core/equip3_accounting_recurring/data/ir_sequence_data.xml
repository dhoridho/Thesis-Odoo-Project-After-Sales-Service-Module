<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="recurring_in_invoice_seq" model="ir.sequence">
            <field name="name">Recurring OutInvoice Sequence</field>
            <field name="code">recurring.in.invoice.seq</field>
            <field name="prefix">RCI/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding" eval="3" ></field>
        </record>
        
        <record id="recurring_out_invoice_seq" model="ir.sequence">
            <field name="name">Recurring InInvoice Sequence</field>
            <field name="code">recurring.out.invoice.seq</field>
            <field name="prefix">RVB/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding" eval="3" ></field>
        </record>

        <record id="recurring_entry_seq" model="ir.sequence">
            <field name="name">Recurring Entry Sequence</field>
            <field name="code">recurring.entry.seq</field>
            <field name="prefix">RJE/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding" eval="3" ></field>
        </record>

        <record id="recurring_customer_prepayment_seq" model="ir.sequence">
            <field name="name">Customer Prepayment Detail</field>
            <field name="code">recurring.customer.prepayment.seq</field>
            <field name="prefix">CPR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding" eval="3" ></field>
        </record>

        <record id="recurring_vendor_prepayment_seq" model="ir.sequence">
            <field name="name">Vendor Prepayment Detail</field>
            <field name="code">recurring.vendor.prepayment.seq</field>
            <field name="prefix">VPR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding" eval="3" ></field>
        </record>

    </data>
</odoo>

