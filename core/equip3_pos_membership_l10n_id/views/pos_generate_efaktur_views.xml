<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="pos_generate_efaktur_form_inherit" model="ir.ui.view">
        <field name="name">pos.generate.efaktur.form.inherit</field>
        <field name="model">pos.generate.efaktur</field>
        <field name="inherit_id" ref="equip3_pos_membership.pos_generate_efaktur_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_ids']/tree/field[@name='partner_id']" position="after">
                <field name="l10n_id_tax_number"/>
            </xpath>
            <xpath expr="//field[@name='digunggung_ids']/tree/field[@name='amount_total']" position="after">
                <field name="l10n_id_tax_number"/>
            </xpath>
            <xpath expr="//field[@name='gabungan_ids']/tree/field[@name='amount_total']" position="after">
                <field name="l10n_id_tax_number"/>
            </xpath>
            <xpath expr="//field[@name='other_ids']/tree/field[@name='partner_id']" position="after">
                <field name="l10n_id_tax_number"/>
            </xpath>
        </field>
    </record>

</odoo>