<odoo>
    <data>
        <record id="view_restaurant_table_form" model="ir.ui.view">
            <field name="name">Restaurant Table customer name and date reserve</field>
            <field name="model">restaurant.table</field>
            <field name="inherit_id" ref="pos_retail.view_restaurant_table_form"/>
            <field name="arch" type="xml">
                <field name="locked" position="after">
                    <field name="customer_name" groups="point_of_sale.group_pos_manager"/>
                    <field name="date_reserve" groups="point_of_sale.group_pos_manager"/>
                </field>
                <field name="user_ids" position="after">
                    <field name="tbl_moved_from"/>
                </field>
            </field>
        </record>
    </data>
</odoo>