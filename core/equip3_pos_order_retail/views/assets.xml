<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="pos product cancel retail assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/CancelReasonPopup.js" />
            <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/OrderLine.js" />
        </xpath>

    </template>

    <template id="remove_license" name="remove license pop up" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">

            <link rel="stylesheet" href="/equip3_pos_order_retail/static/src/css/reservation_screen.css"/>
            
            <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/TakeAwayButton.js"/>
            <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/ReservationScreen.js"/>
            <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/ReservationList.js"/>
        </xpath>
        <xpath expr="//script[@src='/pos_retail/static/src/libs/js/jquery-license.js']" position="replace">
            <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/jquery-license.js"/>
        </xpath>
    </template>

    <template id="asset_pos" inherit_id="pos_retail.assets_pos" name="pos assets retail">
        <xpath expr="//script[@src='/pos_retail/static/src/js/Chrome.js']" position="after">
            <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/Chrome.js"/>
             
        </xpath>
        <xpath expr="." position="inside">
             <link rel="stylesheet" href="/equip3_pos_order_retail/static/src/css/custom.css"/>
             <link rel="stylesheet" href="/equip3_pos_order_retail/static/src/css/complementarynotepopup.scss"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/DB.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/models.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/ComplementaryButton.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/FeatureButtons.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/combo_popup.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/combo_options.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/table_reservation_popup.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/PopUpCardComplementaryNote.js"/>
             <script type="text/javascript" src="/equip3_pos_order_retail/static/src/js/TableWidget.js"/>
        </xpath>
    </template>
</odoo>