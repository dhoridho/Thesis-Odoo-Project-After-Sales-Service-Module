<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- product.warranty inherit form view -->
        <record id="product_warranty_inherit_view_form" model="ir.ui.view">
            <field name="name">product.warranty.inherit.view.form</field>
            <field name="model">product.warranty</field>
            <field name="inherit_id" ref="bi_warranty_registration.warranty_details_form_view"/>
            <field name="arch" type="xml">
                <!-- Add your xpath here -->
                <xpath expr="//field[@name='warranty_end_date']" position="after">
                    <field name="remaining_warranty_days"/>
                </xpath>
                <xpath expr="//field[@name='renew_no']" position="after">
                    <field name="picking_id" invisible="1" readonly="1"/>
                    <field name="qr_code" widget='image' class="custom_wh" nolabel="1" attrs="{'invisible':[('qr_code', '=', False)]}" readonly="1"/>
                </xpath>
   
            </field>
        </record>
    </data>
</odoo>