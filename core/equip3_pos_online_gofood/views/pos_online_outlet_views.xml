<?xml version="1.0" encoding="UTF-8"?>
<odoo>
 
    <record id="pos_online_outlet_form_inherit" model="ir.ui.view">
        <field name="name">pos.online.outlet.form.view.inherit</field>
        <field name="model">pos.online.outlet</field>
        <field name="inherit_id" ref="equip3_pos_online_outlet.pos_online_outlet_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside"> 
                <page string="GoFood">
                    <group>
                        <field name="gofood_merchant_id" string="StoreID"/>
                        <field name="gofood_partner_merchant_id" 
                            string="Partner StoreID"/>
                        <field name="country_code"/>
                    </group>
                    <group string="Log Info" groups="base.group_no_one">
                        <field name="gofood_update_menu_error_msg" readonly="1"/>
                        <field name="gofood_change_state_error_msg" readonly="1"/>
                        <button name="gobiz_check_menu_structure" string="Check Menu Structure" type="object" 
                        class="oe_highlight oe_read_only"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>