<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="view_asset_allocation_menu_form" model="ir.ui.view">
            <field name="name">allocation.asset.form</field>
            <field name="model">allocation.asset</field>
            <field name="arch" type="xml">
                <form string="Asset Allocation">
                    <header>
                        <button string="Confirm" name="button_confirm" type="object" class="btn-primary"
                            attrs="{'invisible':[('state', '!=', 'draft')]}"/>
                        <button string="Request for Approval" name="button_request_for_approval" type="object" class="btn-primary"
                            invisible="1"/>
                        <button string="In Progress" name="button_start" type="object" class="btn-primary"
                            attrs="{'invisible':[('state', '!=', 'approved')]}"/>
                        <button string="Done" name="button_done" type="object" class="btn-primary"
                            attrs="{'invisible':[('state', 'in', ('draft', 'request', 'approved', 'rejected', 'done', 'canceled'))]}"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,in_progress,done" readonly="1"/>
                    </header>
                    <sheet>
                        <div>
                            <div class="oe_title">
                                <h1>
                                    <field name="number" nolabel="1" readonly="1"/>
                                </h1>
                            </div>
                            <group>
                                <group>
                                    <field name="company_id"/>
                                    <field name="branch_id" required="1" readonly="1" force_save="1"/>
                                    <field name="project" required="1" options="{'no_create': True, 'no_quick_create':True,'no_create_edit':True}"/>
                                    <field name="job_cost_sheet" readonly="1" force_save="1"/>
                                    <field name="budgeting_period" invisible="1"/>
                                    <field name="project_budget" string="Periodical Budget"
                                           attrs="{'invisible': [('budgeting_period', 'not in', ['custom', 'monthly'])],
                                                   'required': [('budgeting_period', 'in', ['custom', 'monthly'])]}"/>
                                    <field name="job_order" required="1"/>
                                    <field name="analytic_tag_ids" widget="many2many_tags"/>
                                    <field name="department_type" invisible="1"/>
                                </group>
                                <group>
                                    <field name="schedule_method" widget="radio" invisible="1"/>
                                    <field name="start_date" required="1"/>
                                    <field name="end_date" required="1"/>
                                    <field name="actual_start_date" invisible="1"/>   
                                    <field name="actual_end_date" invisible="1"/>
                                    <field name="duration" widget="asset_duration" invisible="1"/>
                                    <field name="create_uid" string='Created By'/>
                                    <field name="create_date" string='Creation Date'/>
                                    <field name="asset_allocation_option" invisible="1"/>
                                    <field name="is_asset_allocation_approval_matrix" invisible="1"/>
                                </group>
                            </group>
                        </div> 
                        <notebook>
                            <page string="Asset Allocation Line">
                                <field name="allocation_asset_ids" attrs="{'readonly':['|', ('state', '=', 'draft'), ('state', '=', 'done')]}">
                                    <tree editable='bottom' delete="0">
                                        <control>
                                            <create name="add_product_control" string="Add asset"/>
                                        </control>
                                        <field name="sequence" invisible="1"/>
                                        <field name="sr_no"/>
                                        <field name="allocation_id" invisible="1"/>
                                        <field name="asset_id" required="1"/>
                                        <field name="cs_internal_asset_id"
                                               attrs="{'column_invisible':[('parent.budgeting_period', '!=', 'project')],
                                                       'required':[('parent.budgeting_period', '=', 'project')]}"/>
                                        <field name="bd_internal_asset_id"
                                               attrs="{'column_invisible':[('parent.budgeting_period', '=', 'project')],
                                                       'required':[('parent.budgeting_period', 'in', ['monthly', 'custom'])]}"/>
                                        <field name="serial_number" invisible="1" readonly="1" force_save="1"/>
                                        <field name="start_date" force_save="1"/>
                                        <field name="end_date" force_save="1"/>
                                        <field name="actual_start_date" force_save="1" readonly="1"/>   
                                        <field name="actual_end_date"  force_save="1" readonly="1"/> 
                                        <field name="duration" widget="asset_duration" readonly="1"/>
                                        <field name="project" invisible="1"/>
                                        <field name="asset_allocation_option" invisible="1"/>
                                        <field name="job_cost_sheet" invisible="1"/>
                                        <field name="job_order" invisible="1"/> 
                                        <field name="project_budget" invisible="1"/>
                                        <field name="project_asset_ids" invisible="1"/>
                                        <field name="budgeting_period" invisible="1"/>
                                        <field name="project_scope"  invisible="1"/>  
                                        <field name="section" invisible="1"/>    
                                        <field name="variable" invisible="1"/>
                                        <field name="is_vehicle" invisible="1"/>
                                        <!-- <button string="Confirm" name="button_confirm" type="object" class="btn-primary oe_read_only"
                                            attrs="{'invisible':[('state', '!=', 'draft')]}"/>
                                        <button string="Start" name="start" type="object" class="btn-primary"
                                            attrs="{'invisible':['|',('state', '!=', 'approved'),('parent.asset_allocation_option', '!=', 'live_count')]}"/>
                                        <button string="Pause" name="pause" type="object" class="btn-primary"
                                            attrs="{'invisible':['|',('state', '!=', 'in_progress'),('parent.asset_allocation_option', '!=', 'live_count')]}"/>
                                        <button string="Continue" name="start_again" type="object" class="btn-primary"
                                            attrs="{'invisible':['|',('state', '!=', 'paused'),('parent.asset_allocation_option', '!=', 'live_count')]}"/>
                                        <button string="Done" name="done" type="object" class="btn-primary"
                                            attrs="{'invisible':[('state', 'in', ('draft', 'request', 'rejected', 'done', 'canceled'))]}"/> -->
                                        <field name="state" string="Status"
                                               widget="badge" readonly="1"
                                               decoration-secondary="state == 'draft'"
                                               decoration-warning3="state == 'to_approve'"
                                               decoration-success2="state == 'approved'"
                                               decoration-danger="state == 'rejected'"
                                               decoration-success="state == 'done'"
                                            />
                                        <button name="button_delete" confirm="Deleting this data will also delete any related data, are you sure?" type="object" class="btn fa fa-trash-o oe_edit_only"/>
                                    </tree>
                                </field>                                   
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="activity_ids"/>
                        <field name="message_ids"/>
                    </div>
                </form>
            </field>
        </record>

        <record id="view_asset_allocation_line_menu_form" model="ir.ui.view">
            <field name="name">allocation.asset.line.form</field>
            <field name="model">allocation.asset.line</field>
            <field name="arch" type="xml">
                <form string="Asset Allocation">
                    <header>
                        <button string="Confirm" name="button_confirm" type="object" class="btn-primary"
                            attrs="{'invisible':['|', ('state', '!=', 'draft'), ('allocation_id', '=', False)]}"/>
                        <button string="Request for Approval" name="request_approval" type="object" class="btn-primary"
                                attrs="{'invisible': ['|', ('state', '!=', 'in_progress'), ('is_asset_allocation_approval_matrix','=',False)]}"/>
                        <button name="btn_approve" string="Approve" type="object" class="oe_highlight"
                                attrs="{'invisible': ['|','|',('state', '!=', 'to_approve'), ('is_asset_allocation_approval_matrix','=',False), ('is_approver', '=', False)]}"/>
                        <button string="Reject" name="action_reject" type="object"
                                attrs="{'invisible': ['|','|',('state', '!=', 'to_approve'), ('is_asset_allocation_approval_matrix','=',False), ('is_approver', '=', False)]}"/>
                        <button string="Start" name="start" type="object" class="btn-primary"
                            attrs="{'invisible':['|',('state', '!=', 'approved'),('asset_allocation_option', '!=', 'live_count')]}"/>
                        <button string="Pause" name="pause" type="object" class="btn-primary"
                            attrs="{'invisible':['|',('state', '!=', 'in_progress'),('asset_allocation_option', '!=', 'live_count')]}"/>
                        <button string="Continue" name="start_again" type="object" class="btn-primary"
                            attrs="{'invisible':['|',('state', '!=', 'paused'),('asset_allocation_option', '!=', 'live_count')]}"/>
                        <button string="Done" name="done" id='done1' type="object" class="btn-primary"
                            attrs="{'invisible':['|', ('state', 'in', ('draft', 'done', 'canceled')), ('is_asset_allocation_approval_matrix','=',True)]}"/>
                        <button string="Done" name="done" id='done2' type="object" class="btn-primary"
                            attrs="{'invisible':['|', ('state', 'in', ('draft', 'in_progress', 'to_approve', 'rejected', 'done', 'canceled')), ('is_asset_allocation_approval_matrix','=',False)]}"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,in_progress,done" attrs="{'invisible': ['|', ('is_asset_allocation_approval_matrix', '=', True), ('state', 'in', ['rejected','canceled'])]}"/>
                        <field name="state1" widget="statusbar" statusbar_visible="draft,in_progress,approved,done" attrs="{'invisible': ['|', ('is_asset_allocation_approval_matrix', '=', False), ('state', 'in', ['rejected','canceled'])]}"/>
                        <field name="state2" widget="statusbar" statusbar_visible="draft,rejected" attrs="{'invisible': ['|', ('is_asset_allocation_approval_matrix', '=', False), ('state', '!=', 'rejected')]}"/>
                        <field name="state3" widget="statusbar" statusbar_visible="draft,in_progress,canceled" attrs="{'invisible': [('state', '!=', 'canceled')]}"/>
                    </header>
                    <sheet>
                        <div class="oe_button_box">
                            <button name="action_asset_usage"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="o-hm-smart-construction-product-usage"
                                    attrs="{'invisible': ['|',('state', 'in', ('draft')),('count_asset_usage', '=', 0)]}">
                                <field name="count_asset_usage" widget="statinfo" string="Asset Usage"/>
                            </button>
                            <button name="action_asset_fuel_log"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="fa-list"
                                    attrs="{'invisible': ['|',('state', 'in', ('draft')),('count_asset_fuel_log', '=', 0)]}">
                                <field name="count_asset_fuel_log" widget="statinfo" string="Fuel Logs"/>
                            </button>
                            <button name="action_asset_odometer"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="fa-dashboard"
                                    attrs="{'invisible': ['|',('state', 'in', ('draft')),('count_asset_odometer', '=', 0)]}">
                                <field name="count_asset_odometer" widget="statinfo" string="Odometer"/>
                            </button>
                            <button name="action_asset_hour_meter"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="fa-history"
                                    attrs="{'invisible': ['|',('state', 'in', ('draft')),('count_asset_hour_meter', '=', 0)]}">
                                <field name="count_asset_hour_meter" widget="statinfo" string="Hour Meter"/>
                            </button>
                            <button name="action_show_delivery_order"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="fa-truck"
                                    attrs="{'invisible': ['|',('state', 'in', ('draft')),('delivery_order_count', '=', 0)]}">
                                <field name="delivery_order_count" widget="statinfo" string="Delivery Order"/>
                            </button>
                        </div>
                        <div>
                            <div class="oe_title">
                                <h1>
                                    <field name="name" nolabel="1" readonly="1"/>
                                </h1>
                            </div>
                            <group>
                                <group>
                                    <field name="allocation_id" required="1"
                                           attrs="{'readonly':[('state', '!=', 'draft')]}"/>
                                    <field name="project" required="1"/>
                                    <field name="asset_allocation_option" invisible="1"/>
                                    <field name="job_cost_sheet" required="1"/>
                                    <field name="project_budget" string="Periodical Budget"
                                           attrs="{'invisible':[('budgeting_period', '=', 'project')],
                                                   'required':[('budgeting_period', 'in', ['monthly', 'custom'])]}"/>
                                    <field name="job_order" required="1"/>
                                    <field name="asset_id" required="1" 
                                           attrs="{'readonly':[('state', '!=', 'draft')]}"/>
                                    <field name="bd_internal_asset_id" force_save="1"
                                           attrs="{'invisible':[('budgeting_period', '=', 'project')],
                                                   'required':[('budgeting_period', 'in', ['monthly', 'custom'])],
                                                   'readonly':[('state', '!=', 'draft')]}"/>
                                    <field name="cs_internal_asset_id" force_save="1"
                                           attrs="{'invisible':[('budgeting_period', '!=', 'project')],
                                                   'required':[('budgeting_period', '=', 'project')],
                                                   'readonly':[('state', '!=', 'draft')]}"/>
                                    <field name="project_scope" invisible="1" force_save="1"/>
                                    <field name="section" invisible="1" force_save="1"/>
                                    <field name="variable" invisible="1" force_save="1"/>
                                    <field name="serial_number" readonly="1" force_save="1" invisible="1"/>
                                    <field name="warehouse_location_ids" invisible="1"/>
                                    <field name="fuel_type_id" attrs="{'invisible':[('is_vehicle', '=', False)]}"/>
                                    <field name="source_location_id" domain="[('id', 'in', warehouse_location_ids)]"
                                           attrs="{'invisible':['|', ('is_vehicle', '=', False), ('fuel_type_id', '=', False)],
                                                   'required':[('is_vehicle', '=', True),('fuel_type_id', '!=', False)],
                                                   'readonly':[('state', '!=', 'draft')]}"/>
                                    <field name="destination_location_id" domain="[('company_id', '=', company_id)]"
                                           attrs="{'invisible':['|', ('is_vehicle', '=', False), ('fuel_type_id', '=', False)],
                                                   'required':[('is_vehicle', '=', True),('fuel_type_id', '!=', False)],
                                                   'readonly':[('state', '!=', 'draft')]}"/>
                                    <field name="bd_overhead_id" attrs="{'invisible':['|', ('budgeting_period', '=', 'project'), ('fuel_type_id', '=', False)],
                                                   'required':[('budgeting_period', 'in', ['monthly', 'custom']),('fuel_type_id', '!=', False)],
                                                   'readonly':[('state', '!=', 'draft')]}"/>
                                    <field name="cs_overhead_id" attrs="{'invisible':['|', ('budgeting_period', '!=', 'project'), ('fuel_type_id', '=', False)],
                                                   'required':[('budgeting_period', '=', 'project'),('fuel_type_id', '!=', False)],
                                                   'readonly':[('state', '!=', 'draft')]}"/>
                                    <label for="fuel_qty" string="Fuel Available Quantity" attrs="{'invisible':[('fuel_type_id', '=', False)]}"/>
                                    <div attrs="{'invisible':[('fuel_type_id', '=', False)]}">
                                        <field name="fuel_qty"/>
                                        <field name="uom_id"/>
                                    </div>
                                    <field name="analytic_tag_ids" widget="many2many_tags"/>
                                    <field name="department_type" invisible="1"/>
                                </group>
                                <group>
                                    <field name="start_date" required="1" force_save="1"
                                        attrs="{'readonly':[('state', '!=', 'draft')]}"/>  
                                    <field name="end_date" required="1" force_save="1"
                                        attrs="{'readonly':[('state', '!=', 'draft')]}"/>  
                                    <field name="actual_start_date" force_save="1" readonly="1"/>   
                                    <field name="actual_end_date"  force_save="1" readonly="1"/> 
                                    <field name="duration" widget="asset_duration" readonly="1" force_save="1"/>  
                                    <field name="is_vehicle" invisible="1"/>
                                    <field name="branch_id" force_save="1" readonly="1"/>
                                    <field name="company_id" force_save="1" readonly="1"/>
                                    <field name="create_uid" string='Created By'/>
                                    <field name="create_date" string='Creation Date'/>
                                    <field name="project_asset_ids" invisible="1"/>
                                    <field name="budgeting_period" invisible="1"/>
                                    <field name="asset_allocation_option" invisible="1"/>
                                    <field name="is_asset_allocation_approval_matrix" invisible="1"/>
                                    <field name="approvers_ids" widget="many2many_tags" invisible="1"/>
                                    <field name="approved_user_ids" widget="many2many_tags" invisible="1" readonly="0"/>
                                    <field name="is_approver" invisible="1"/>
                                    <field name="approved_user_text" invisible="1"/>
                                    <field name="approved_user" invisible="1" readonly="0"/>
                                    <field name="employee_id" invisible="1"/>
                                    <field name="feedback_parent" invisible="1"/>
                                </group>
                            </group>
                        </div>
                        <notebook>
                            <page name="asset_usage" string="Asset Usage" attrs="{'invisible':[('is_vehicle', '=', True)]}">
                                <field name="time_ids" widget="one2many_list" attrs="{'readonly':[('state', 'in', ['draft', 'done'])]}">
                                    <tree editable="bottom" delete="0">
                                        <control>
                                            <create name="add_product_control" string="Add Time"/>
                                        </control>
                                        <field name="operator_id"/>
                                        <field name="date_start" required="1"/>
                                        <field name="date_end" required="1"/>
                                        <field name="duration" widget="asset_duration" readonly="1" force_save="1"/>
                                        <field name="distance" force_save="1" invisible="1"/>
                                        <field name="fuel_type_id" readonly="1" invisible="1"/>
                                        <field name="fuel_qty" invisible="1"/>
        <!--                                <field name="temp_fuel_qty" invisible="1"/>-->
                                        <field name="fuel_log" attrs="{'column_invisible':[('parent.is_vehicle', '=', False)]}"/>
                                        <field name="odometer" attrs="{'column_invisible':[('parent.is_vehicle', '=', False)]}"/>
                                        <field name="unit" attrs="{'column_invisible':[('parent.is_vehicle', '=', False)]}"/>
                                        <field name="hour_meter" widget="float_time" force_save="1"/>
                                        <field name="is_new" invisible="1"/>
                                        <button name="delete_timestamp" confirm="Deleting this data will also delete any related data, are you sure?" type="object" class="btn fa fa-trash-o oe_edit_only"/>
                                    </tree>
                                </field>
                            </page>
                            <page name="vehicle_usage" string="Vehicle Usage" attrs="{'invisible':['|',('is_vehicle', '=', False),('asset_id', '=', False)]}">
                                <field name="time_ids" widget="one2many_list" attrs="{'readonly':[('state', 'in', ['draft', 'done'])]}">
                                    <tree editable="bottom" delete="0">
                                        <control>
                                            <create name="add_product_control" string="Add Time"/>
                                        </control>
                                        <field name="operator_id"/>
                                        <field name="date_start" required="1"/>
                                        <field name="date_end" required="1"/>
                                        <field name="duration" widget="asset_duration" readonly="1" force_save="1"/>
                                        <field name="distance" force_save="1" invisible="1"/>
                                        <field name="fuel_type_id" readonly="1" invisible="1"/>
                                        <field name="fuel_qty" invisible="1"/>
        <!--                                <field name="temp_fuel_qty" invisible="1"/>-->
                                        <field name="fuel_log" attrs="{'column_invisible':[('parent.is_vehicle', '=', False)]}"/>
                                        <field name="odometer" attrs="{'column_invisible':[('parent.is_vehicle', '=', False)]}"/>
                                        <field name="unit" attrs="{'column_invisible':[('parent.is_vehicle', '=', False)]}"/>
                                        <field name="hour_meter" widget="float_time" force_save="1"/>
                                        <field name="is_new" invisible="1"/>
                                        <button name="delete_timestamp" confirm="Deleting this data will also delete any related data, are you sure?" type="object" class="btn fa fa-trash-o oe_edit_only"/>
                                    </tree>
                                </field>
                            </page>
                            <page name="approval_matrix" string="Approval Matrix Line" attrs="{'invisible':[('is_asset_allocation_approval_matrix','!=',True)]}">
                                <group>
                                    <group>
                                        <field name="approving_matrix_asset_allocation_id" readonly="1"/>
                                    </group>
                                </group>
                                <field name="asset_allocation_user_ids">
                                    <tree editable="bottom" create="false" delete="false">
                                        <field name="name"/>
                                        <field name="user_ids" widget="many2many_tags" readonly="1" force_save="1"/>
                                        <field name="matrix_user_ids" widget="many2many_tags" invisible="1" readonly="1" force_save="1"/>
                                        <field name="is_delegation_mail_sent" invisible="1"/>
                                        <field name="minimum_approver" readonly="1" force_save="1"/>
                                        <field name="timestamp" invisible="1" force_save="1"/>
                                        <field name="approver_state" invisible="1" force_save="1"/>
                                        <field name="approval_status" readonly="1" force_save="1"/>
                                        <field name="approved_time" readonly="1" force_save="1"/>
                                        <field name="is_approve" invisible="1"/>
                                        <field name="approved_employee_ids" invisible="1"/>
                                        <field name="feedback" readonly="1" force_save="1"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>    
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="activity_ids"/>
                        <field name="message_ids"/>
                    </div>
                </form>
            </field>
        </record>

        <record id="view_asset_allocation_menu_tree" model="ir.ui.view">
            <field name="name">allocation.asset.tree</field>
            <field name="model">allocation.asset</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="display_name" string="Asset Allocation Name"/>
                    <field name="project" string="Project"/>
                    <field name="job_order" string="Job Order"/>
                    <field name="start_date" string="Scheduled Start Date"/>
                    <field name="end_date" string="Scheduled End Date"/>
                    <field name="state" string="Status"/>
                    <field name="state" string="Status"
                            widget="badge"
                            decoration-secondary="state == 'draft'"
                            decoration-success="state == 'done'"
                            decoration-info="state == 'in_progress'"
                            />
                </tree>
            </field>
        </record>

        <record id="view_asset_allocation_line_menu_tree" model="ir.ui.view">
            <field name="name">allocation.asset.line.tree</field>
            <field name="model">allocation.asset.line</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="allocation_id" string="Asset Allocation"/>
                    <field name="project" string="Project"/>
                    <field name="asset_allocation_option" invisible="1"/>
                    <field name="job_order" string="Job Order"/>
                    <field name="asset_id" string="Asset"/>
                    <field name="serial_number" string="Serial Number"/>
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="duration" string="Duration" widget="asset_duration"/>
                    <field name="state" string="Status"
                            widget="badge"
                            decoration-secondary="state == 'draft'"
                            decoration-warning3="state == 'to_approve'"
                            decoration-success2="state == 'approved'"
                            decoration-danger="state == 'rejected'"
                            decoration-success="state == 'done'"
                            />
                </tree>
            </field>
        </record>

        <record id="view_asset_allocation_line_menu_calendar" model="ir.ui.view">
            <field name="name">allocation.asset.line.calendar</field>
            <field name="model">allocation.asset.line</field>
            <field name="arch" type="xml">
                <calendar date_start="start_date" date_stop="end_date">
                    <field name="name" />
                </calendar>
            </field>
        </record>

        <record id="asset_allocation_action_const" model="ir.actions.act_window">
            <field name="name">Asset Allocation</field>
            <field name="res_model">allocation.asset</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('department_type', '=', 'project')]</field>
            <field name="context">{'default_department_type': 'project'}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No Asset Allocation found, Let's create one!
                </p>
            </field>
        </record>

        <record id="asset_allocation_line_action_const" model="ir.actions.act_window">
            <field name="name">Asset Allocation Line</field>
            <field name="res_model">allocation.asset.line</field>
            <field name="view_mode">tree,form,calendar</field>
           <field name="domain">[('department_type', '=', 'project')]</field>
           <field name="context">{'default_department_type': 'project'}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No Asset Allocation Line found, Let's create one!
                </p>
            </field>
        </record>

        <menuitem id = "asset_allocation_sub_menu_asset"
