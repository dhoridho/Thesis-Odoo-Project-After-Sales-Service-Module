<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="job_estimate_report_form" model="ir.ui.view">
        <field name="name">job.estimate.report</field>
        <field name="model">job.estimate.report</field>
        <field name="inherit_id" ref="equip3_construction_sales_operation.job_estimate_report_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='print_level_option']" position="before">
                <field name="contract_category" invisible="1"/>
            </xpath>
            <xpath expr="//group/group[1]" position="attributes">
                <attribute name="attrs">{'invisible': [('contract_category','=','var')]}</attribute>
            </xpath>
        </field>
    </record>

</odoo>