<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_order_report_wizard" model="ir.ui.view">
        <field name="name">sale.order.report.wizard.view.form</field>
        <field name="model">construction.sale.order.report.wizard</field>
        <field name="inherit_id" ref="equip3_construction_sales_operation.model_name_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='print_level_option']" position="before">
                <field name="contract_category" invisible="1"/>
            </xpath>
            <xpath expr="//group/group[1]" position="attributes">
                <attribute name="attrs">{'invisible': [('contract_category','=','var')]}</attribute>
            </xpath>
        </field>
    </record>

</odoo>