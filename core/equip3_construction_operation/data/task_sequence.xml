<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="task_sequence" model="ir.sequence">
            <field name="name">Task Sequence</field>
            <field name="code">project.task.sequence</field>
            <field name="prefix">TSK/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="progress_history_sequence" model="ir.sequence">
            <field name="name">Progress History Sequence</field>
            <field name="code">progress.history.sequence</field>
            <field name="prefix">PRH/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>