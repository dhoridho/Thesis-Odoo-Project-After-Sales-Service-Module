<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="budget_change_request_sequence" model="ir.sequence">
            <field name="name">Budget Change Request Sequence</field>
            <field name="code">budget.change.request.sequence</field>
            <field name="prefix">BCR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>
        <record id="project_budget_transfer_sequence" model="ir.sequence">
            <field name="name">Project Budget Transfer Seqeunce</field>
            <field name="code">project.budget.transfer.sequence</field>
            <field name="prefix">PBT/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>
        <record id="budget_transfer_sequence" model="ir.sequence">
            <field name="name">Budget Transfer Seqeunce</field>
            <field name="code">budget.transfer.sequence</field>
            <field name="prefix">MBT/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>
        <record id="change_allocation_sequence" model="ir.sequence">
            <field name="name">Change Allocation Seqeunce</field>
            <field name="code">change.allocation.sequence</field>
            <field name="prefix">CAB/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>