<?xml version="1.0" encoding="utf-8"?>
<odoo>     
    <record id="other_account_voucher_multi_company_rule" model="ir.rule">
        <field name="name">Other Income Rule</field>
        <field name="model_id" ref="aos_account_voucher.model_account_voucher"/>
        <field name="domain_force">[
                ('company_id', 'in', company_ids)
        ]</field>
    </record>

    <record id="account_operation_move_branch_rule" model="ir.rule">
            <field name="name">Journal Entries Multi-Branch</field>
            <field name="model_id" ref="model_account_move"/>
            <field name="domain_force">[
                '|', 
                    ('branch_id', '=', False), 
                    ('branch_id', 'in', branch_ids)
            ]</field>
        </record>

    
    <record id="account_operation_receipt_voucher_branch_rule" model="ir.rule">
            <field name="name">Receipt Voucher Multi-Branch</field>
            <field name="model_id" ref="model_receipt_voucher"/>
            <field name="domain_force">[
                '|', 
                    ('branch_id', '=', False), 
                    ('branch_id', 'in', branch_ids)
            ]</field>
        </record>

    <record id="account_multipayment_multicomp_rule" model="ir.rule">
        <field name="name">Account Multipayment Multi-Company</field>
        <field name="model_id" ref="model_account_multipayment"/>
        <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
    </record>

    <record id="payment_voucher_multibranch_rule" model="ir.rule">
        <field name="name">Payment Voucher Multi-Branch</field>
        <field name="model_id" ref="model_payment_voucher"/>
        <field name="domain_force">['|',('branch_id','=',False),('branch_id', 'in', branch_ids)]</field>
    </record>

    
