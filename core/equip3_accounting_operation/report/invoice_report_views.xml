<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="report_invoice_printout_editor" model="ir.actions.report">
        <field name="name">Invoice PrintOut Editor</field>
        <field name="model">invoice.printout.editor</field>
        <field name="report_type">qweb-pdf</field>
        <field name="print_report_name">
            'Custom Print - %s' % (object.name)</field>
        <field name="report_name">equip3_accounting_operation.exclusive_invoice_report_printout_editor</field>
        <field name="report_file">equip3_accounting_operation.exclusive_invoice_report_printout_editor</field>
    </record>
   
    <record id="paperformat_invoice_printout_editor_potrait" model="report.paperformat">
        <field name="name">Potrait Printout Editor</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">30</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">35</field>
        <field name="dpi">80</field>
    </record>

