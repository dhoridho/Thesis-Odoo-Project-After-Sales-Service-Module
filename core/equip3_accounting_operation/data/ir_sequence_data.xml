<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
        <!-- Sequences for Customer multipayment -->
        <record id="seq_customer_multipayment" model="ir.sequence">
            <field name="name">Customer Multi Receipts</field>
            <field name="code">customer.account.multipayment</field>
            <field name="prefix">CMP/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Vendor multipayment -->
        <record id="seq_vendor_multipayment" model="ir.sequence">
            <field name="name">Vendor Multi Payments</field>
            <field name="code">vendor.account.multipayment</field>
            <field name="prefix">VMP/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="transfer_reference_value_sequence" model="ir.sequence">
            <field name="name">Account Internal Transfer </field>
            <field name="code">account.internal.transfer</field>
            <field name="prefix">IBT/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="payment_voucher_sequence" model="ir.sequence">
            <field name="name">Payment voucher </field>
            <field name="code">payment.voucher.code</field>
            <field name="prefix">PVO/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="receipt_voucher_sequence" model="ir.sequence">
            <field name="name">Receipt Voucher </field>
            <field name="code">receipt.voucher.code</field>
            <field name="prefix">RVO/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="account_voucher_oin_sequence" model="ir.sequence">
            <field name="name">Other Income</field>
            <field name="code">seq.account.voucher.oin</field>
            <field name="prefix">EIN/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="account_voucher_oex_sequence" model="ir.sequence">
            <field name="name">Other Expense</field>
            <field name="code">seq.account.voucher.oex</field>
            <field name="prefix">EEX/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- from equip3_accounting_giro -->
        <record id="receipt_giro" model="ir.sequence">
            <field name="name">Receipt Giro</field>
            <field name="code">receipt.giro</field>
            <field name="prefix">RG/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="payment_giro" model="ir.sequence">
            <field name="name">Payment Giro</field>
            <field name="code">payment.giro</field>
            <field name="prefix">PG/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Debt Collection -->
        <record id="debt_collection" model="ir.sequence">
            <field name="name">Debt Collection</field>
            <field name="code">debt.collection</field>
            <field name="prefix">DCF/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
