<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_move_global_wizard_form_view" model="ir.ui.view">
        <field name="name">stock_move_global_wizard_form_view</field>
        <field name="model">sh.stock.move.global.check</field>
        <field name="inherit_id" ref="equip3_inventory_qc.stock_move_global_wizard_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='recheck_max_number']" position="after">
                <field name="is_for_mrp" invisible="1"/>
                <field name="plan_id" invisible="1"/>
                <field name="production_id" invisible="1"/>
                <field name="workorder_id" invisible="1"/>
                <field name="consumption_id" invisible="1"/>
                <field name="mrp_quality_check_id" invisible="1"/>
            </xpath>
        </field>
    </record>

</odoo>