<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="seq_assembly_production_record" model="ir.sequence">
            <field name="name">Assembly Production Record</field>
            <field name="code">assembly.production.record</field>
            <field name="prefix">SAR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_disassembly_production_record" model="ir.sequence">
            <field name="name">Disassembly Production Record</field>
            <field name="code">disassembly.production.record</field>
            <field name="prefix">SDR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

    </data>
</odoo>
