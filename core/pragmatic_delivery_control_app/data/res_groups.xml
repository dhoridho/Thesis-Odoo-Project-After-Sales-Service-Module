<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
		<!-- Delivery Control Access -->
         <record model="ir.module.category" id="module_category_delivery_control_app">
            <field name="name">Delivery Control App</field>
            <field name="sequence">11</field>
        </record>
        <record id="group_delivery_control_app_user" model="res.groups">
            <field name="category_id" ref="module_category_delivery_control_app"/>
            <field name="name">User</field>
        </record>
        <record id="group_delivery_control_app_manager" model="res.groups">
            <field name="category_id" ref="module_category_delivery_control_app"/>
            <field name="name">Manager</field>
            <field name="implied_ids" eval="[(4, ref('group_delivery_control_app_user'))]"/>
        </record>

         <record id="google_maps_api_key" model="ir.config_parameter">
            <field name="key">google.api_key_geocode</field>
            <field name="value">Your API Key</field>
        </record>

