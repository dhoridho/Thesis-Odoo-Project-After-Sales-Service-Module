<odoo>
    <data>
        <record id="course_stage_1" model="training.stages">
            <field name="sequence">1</field>
            <field name="name">New</field>
            <field name="is_default_stages">True</field>
        </record>

        <record id="course_stage_7" model="training.stages">
            <field name="sequence">2</field>
            <field name="name">To Approve</field>
            <field name="is_default_stages">True</field>
        </record>

        <record id="course_stage_2" model="training.stages">
            <field name="sequence">3</field>
            <field name="name">Approved</field>
            <field name="is_default_stages">True</field>
        </record>

        <record id="course_stage_3" model="training.stages">
            <field name="sequence">4</field>
            <field name="name">On Progress</field>
            <field name="is_default_stages">True</field>
        </record>

        <record id="course_stage_4" model="training.stages">
            <field name="sequence">5</field>
            <field name="name">Completed</field>
            <field name="is_default_stages">True</field>
        </record>

        <record id="course_stage_5" model="training.stages">
            <field name="sequence">6</field>
            <field name="name">Rejected</field>
            <field name="fold">True</field>
            <field name="is_default_stages">True</field>
        </record>

        <record id="course_stage_6" model="training.stages">
            <field name="sequence">7</field>
            <field name="name">Cancelled</field>
            <field name="fold">True</field>
            <field name="is_default_stages">True</field>
        </record>
    </data>
</odoo>