<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="sequence_training_request" model="ir.sequence">
            <field name="name">Training Request Sequence</field>
            <field name="code">training.request</field>
            <field name="prefix">TRQ/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_training_histories" model="ir.sequence">
            <field name="name">Training Histories Sequence</field>
            <field name="code">training.histories</field>
            <field name="prefix">TRH/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_training_conduct" model="ir.sequence">
            <field name="name">Training Conduct Sequence</field>
            <field name="code">training.conduct</field>
            <field name="prefix">TRC/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_training_cancellation" model="ir.sequence">
            <field name="name">Training Cancellation</field>
            <field name="code">hr.training.cancellation</field>
            <field name="prefix">TRC/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_training_conduct_cancellation" model="ir.sequence">
            <field name="name">Training Conduct Cancellation</field>
            <field name="code">hr.training.conduct.cancellation</field>
            <field name="prefix">TRN/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>