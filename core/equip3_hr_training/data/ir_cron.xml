<odoo>
      <data noupdate="1">

        <record id="hr_training_stage_move" model="ir.cron">
            <field name="name">Hr training stage move</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 22:00:00')" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_training_conduct"/>
            <field name="state">code</field>
            <field name="doall" eval="True"/>
            <field name="code">model.ir_cron_move_stage()</field>
        </record>

          <!-- Training Scheduler for Auto Follow Up Approver-->
          <record forcecreate="True" id="ir_cron_training_auto_follow_up_approver" model="ir.cron">
              <field name="name">Auto Follow Up Training Approver</field>
              <field name="model_id" ref="model_training_request"/>
              <field name="state">code</field>
              <field name="code">model.get_auto_follow_up_approver()</field>
              <field eval="False" name="active"/>
              <field name="user_id" ref="base.user_root"/>
              <field name="interval_number">1</field>
              <field name="interval_type">hours</field>
              <field name="numbercall">-1</field>
              <field eval="False" name="doall"/>
          </record>
          <!-- Training Cancel Scheduler for Auto Follow Up Approver-->
          <record forcecreate="True" id="ir_cron_training_cancellation_auto_follow_up_approver" model="ir.cron">
              <field name="name">Auto Follow Up Training Cancellation Approver</field>
              <field name="model_id" ref="model_hr_training_cancellation"/>
              <field name="state">code</field>
              <field name="code">model.get_auto_follow_up_approver()</field>
              <field eval="False" name="active"/>
              <field name="user_id" ref="base.user_root"/>
              <field name="interval_number">1</field>
              <field name="interval_type">hours</field>
              <field name="numbercall">-1</field>
              <field eval="False" name="doall"/>
          </record>
          <!-- Training Conduct Scheduler for Auto Follow Up Approver-->
          <record forcecreate="True" id="ir_cron_training_conduct_auto_follow_up_approver" model="ir.cron">
              <field name="name">Auto Follow Up Training Conduct Approver</field>
              <field name="model_id" ref="model_training_conduct"/>
              <field name="state">code</field>
              <field name="code">model.get_auto_follow_up_approver()</field>
              <field eval="False" name="active"/>
              <field name="user_id" ref="base.user_root"/>
              <field name="interval_number">1</field>
              <field name="interval_type">hours</field>
              <field name="numbercall">-1</field>
              <field eval="False" name="doall"/>
          </record>
          <!-- Training Conduct Scheduler for Auto Follow Up Approver-->
          <record forcecreate="True" id="ir_cron_training_conduct_cancellation_auto_follow_up_approver" model="ir.cron">
              <field name="name">Auto Follow Up Training Conduct Cancellation Approver</field>
              <field name="model_id" ref="model_hr_training_conduct_cancellation"/>
              <field name="state">code</field>
              <field name="code">model.get_auto_follow_up_approver()</field>
              <field eval="False" name="active"/>
              <field name="user_id" ref="base.user_root"/>
              <field name="interval_number">1</field>
              <field name="interval_type">hours</field>
              <field name="numbercall">-1</field>
              <field eval="False" name="doall"/>
          </record>
      </data>


</odoo>