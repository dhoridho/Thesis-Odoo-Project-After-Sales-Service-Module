<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="budget_change_sequence" model="ir.sequence">
        <field name="name">Budget Change</field>
        <field name="code">budget.change.req</field>
        <field name="prefix">BCR/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
    </record>

    <record id="purchase_change_sequence" model="ir.sequence">
        <field name="name">Purchase Change Request</field>
        <field name="code">purchase.change.request.seq</field>
        <field name="prefix">PCR/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
    </record>

    <record id="budget_transfer_sequence" model="ir.sequence">
        <field name="name">Budget Transfer Sequence</field>
        <field name="code">budget.transfer.seq</field>
        <field name="prefix">BTR/%(y)s/%(month)s/%(day)s/</field>
        <field name="padding">3</field>
    </record>

</odoo>
