                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" 
                                                style="width: 57px;left: 248px;top: 103px;">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" 
                                                style="width: 57px;left: 248px;top: 178px;">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector" 
                                                style="width: 80px;left: 208px;top: 64px; transform:rotate(+90deg)">
                                                    <div class="nd_connector_line"></div>
                                                </div>
                                                <div class="nd_connector" 
                                                style="width: 80px;left: 208px;top: 139px; transform:rotate(+90deg)">
                                                    <div class="nd_connector_line"></div>
                                                </div>
                                                <button name="equip3_accounting_taxoperation.action_invoice_tax_to_pay"  
                                                    type="action"
                                                    style=" left: 542px;top: 0px;"
                                                    class="btn btn-flow btn-default menu-button-asset">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/customer-taxes-to-pay.svg"/>
                                                    </div>
                                                    <div class="button-text">Customer Taxes to Pay</div>
                                                </button>
                                                <button name="equip3_accounting_operation.action_account_internal_transfer"  
                                                    type="action"
                                                    style=" left: 542px;top: 75px;"
                                                    class="btn btn-flow btn-default menu-button-asset">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/bank-transfer.svg"/>
                                                    </div>
                                                    <div class="button-text">Internal Cash/Bank Transfer</div>
                                                </button>
                                                <button name="equip3_accounting_deposit.action_customer_deposit"  
                                                    type="action"
                                                    style=" left: 542px;top: 150px;"
                                                    class="btn btn-flow btn-default menu-button-asset">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/customer-deposit.svg"/>
                                                    </div>
                                                    <div class="button-text">Customer Deposit</div>
                                                </button>
                                                <button name="equip3_accounting_multicurrency.currency_invoice_revaluation_wizard_action"  
                                                    type="action"
                                                    style=" left: 820px;top: 0px;"
                                                    class="btn btn-flow btn-default menu-button-asset">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/ap-ar-revaluation.svg"/>
                                                    </div>
                                                    <div class="button-text">AP/AR Revaluation</div>
                                                </button>
                                                <button name="aos_account_voucher.action_receipt_voucher_list_aos_voucher"  
                                                    type="action"
                                                    style=" left: 820px;top: 75px;"
                                                    class="btn btn-flow btn-default menu-button-asset">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/other-income.svg"/>
                                                    </div>
                                                    <div class="button-text">Other Income</div>
                                                </button>
                                                
