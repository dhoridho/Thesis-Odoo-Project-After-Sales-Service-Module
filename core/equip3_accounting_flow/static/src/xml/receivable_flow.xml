                                                    <div class="button-text">Recurring Invoice</div>
                                                </button>
                                                <button name="account.action_account_payments"  
                                                    type="action"
                                                    style=" left: 300px;top: 0px;"
                                                    class="btn btn-flow btn-default menu-button-asset">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/receipt.svg"/>
                                                    </div>
                                                    <div class="button-text">Receipt</div>
                                                </button>
                                                <button name="equip3_accounting_operation.action_account_multipayment_customer"  
                                                    type="action"
                                                    style=" left: 300px;top: 75px;"
