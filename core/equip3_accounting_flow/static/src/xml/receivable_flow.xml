                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/customer-multi-receipt.svg"/>
                                                    </div>
                                                    <div class="button-text">Customer Multi Receipt</div>
                                                </button>
                                                <button name="equip3_accounting_operation.action_receipt_giro"  
                                                    type="action"
                                                    style=" left: 300px;top: 150px;"
                                                    class="btn btn-flow btn-default menu-button-asset">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/receipt-giro.svg"/>
                                                    </div>
                                                    <div class="button-text">Receipt Giro</div>
                                                </button>
