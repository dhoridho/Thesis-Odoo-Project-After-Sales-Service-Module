<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <!-- Client Action -->
    <t t-name="accounting_flow_receivable_action">
        <div class="accounting_flow_action">
            <div class="accounting_flow_action_content d-flex flex-column"> 
                <div class="content-header">
                    <div class="d-flex p-3 w-100">
                        <span>Finance Account Receivable Flow</span>
                    </div>
                </div>
                <div class="content-body">
                    <div class="container">
                        <div class="row h-100">
                            <div class="accordion">
                                <div class="accordion-item accordion-master-data">
                                    <h2 class="accordion-header" data-target="collapseMasterData">
                                        <button class="accordion-button" type="button">
                                        Master Data
                                        </button>
                                    </h2>
                                    <div id="collapseMasterData" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="account.res_partner_action_customer" 
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-vendors">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/customers.svg"/>
                                                    </div>
                                                    <div class="button-text">Customers</div>
                                                </button>
                                                <button name="base.action_currency_form" 
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-currency">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/currency.svg"/>
                                                    </div>
                                                    <div class="button-text">Currency</div>
                                                </button>
                                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item accordion-accounting-flow">
                                    <h2 class="accordion-header" data-target="collapseTrxFlow">
                                        <button class="accordion-button" type="button">
                                        Account Receivable Transaction Flow
                                        </button>
                                    </h2>
                                    <div id="collapseTrxFlow" class="accordion-collapse show" style="height: 230px;">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">
                                                <button name="account.action_move_out_invoice_type" 
                                                    type="action"
                                                    style=" top: 0px;"
                                                    class="btn btn-flow btn-default menu-button-bills">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/invoice.svg"/>
                                                    </div>
                                                    <div class="button-text">Invoice</div>
                                                </button>
                                                <button name="account.action_move_out_refund_type"  
                                                    type="action"
                                                    style=" top: 75px;"
                                                    class="btn btn-flow btn-default menu-button-asset-progress">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_accounting_flow/static/src/img/accounting/finance-account-receivable/customer-credit-note.svg"/>
