#. module: equip3_hr_expense_extend
#: model:mail.template,body_html:equip3_hr_expense_extend.email_template_application_for_expense_approval
msgid ""
"\n"
"            <p>Dear ${ctx['approver_name']|safe},\n"
"            <br/>\n"
"            <p>\n"
"            </br>\n"
"                Based on the Expense Request ${object.seq_name}. You have "
"approval pending that need your decision to approve or reject the expense "
"request from ${object.employee_id.name}.\n"
"            </p>\n"
"            <br/>\n"
"            <p>\n"
"                ${object.employee_id.name} has requested Expense for "
"${object.name}.\n"
"            </p>\n"
"            <br/>\n"
"            <table cellspacing=\"0\" cellpadding=\"0\"\n"
"                   style=\"width:900px;border-collapse:collapse;background:"
"inherit;color:inherit;border:1px solid black;\">\n"
"                <thead style=\"border:1px solid black;\">\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Expense Date</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Description</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Analytic Account</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Taxes</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Total</th>\n"
"                </thead>\n"
"                <tbody style=\"border:1px solid black;\">\n"
"                    % for line in object.expense_line_ids:\n"
"                    <tr style=\"border:1px solid black;\">\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${line.date}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${line.name}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${line.analytic_account_id.name}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">\n"
"                        % for tax in line.tax_ids:\n"
"                            ${tax.name} \n"
"                        % endfor\n"
"                        </td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${format_amount(line.total_amount, object.currency_id)}</td>\n"
"                    </tr>\n"
"                    % endfor\n"
"                </tbody>\n"
"            </table>\n"
"            <br/>\n"
"            <p>\n"
"                You can approve or reject the Expense request by clicking "
"this button:\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"            <a href=\"${ctx['url']|safe}\" style=\"padding: 8px 12px; font-"
"size: 12px; color: #FFFFFF; text-decoration: none !important; font-weight: "
"400; background-color: #875A7B; border: 0px solid #875A7B; border-"
"radius:3px\">View Expense</a>\n"
"            <br/>\n"
"            <br/>\n"
"            <p>\n"
"                Thank you.\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"        "
msgstr ""
"\n"
"            <p>Dear ${ctx['approver_name']|safe},\n"
"            <br/>\n"
"            <p>\n"
"            </br>\n"
"                Berdasarkan Permintaan Pengeluaran ${object.seq_name}. Anda "
"memiliki persetujuan yang tertunda yang membutuhkan keputusan Anda untuk "
"menyetujui atau menolak permintaan pengeluaran dari ${object.employee_id."
"name}.\n"
"            </p>\n"
"            <br/>\n"
"            <p>\n"
"                ${object.employee_id.name} telah meminta Pengeluaran untuk "
"${object.name}.\n"
"            </p>\n"
"            <br/>\n"
"            <table cellspacing=\"0\" cellpadding=\"0\"\n"
"                   style=\"width:900px;border-collapse:collapse;background:"
"inherit;color:inherit;border:1px solid black;\">\n"
"                <thead style=\"border:1px solid black;\">\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Tanggal Pengeluaran</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Deskripsi</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Akun Analitik</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Pajak</th>\n"
"                    <th class=\"text-center\" style=\"border:1px solid "
"black;\">Total</th>\n"
"                </thead>\n"
"                <tbody style=\"border:1px solid black;\">\n"
"                    % for line in object.expense_line_ids:\n"
"                    <tr style=\"border:1px solid black;\">\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${line.date}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${line.name}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${line.analytic_account_id.name}</td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">\n"
"                        % for tax in line.tax_ids:\n"
"                            ${tax.name} \n"
"                        % endfor\n"
"                        </td>\n"
"                        <td style=\"border:1px solid black;text-align:"
"center;\">${format_amount(line.total_amount, object.currency_id)}</td>\n"
"                    </tr>\n"
"                    % endfor\n"
"                </tbody>\n"
"            </table>\n"
"            <br/>\n"
"            <p>\n"
"                Anda dapat menyetujui atau menolak permintaan Pengeluaran "
"dengan mengklik tombol ini:\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"            <a href=\"${ctx['url']|safe}\" style=\"padding: 8px 12px; font-"
"size: 12px; color: #FFFFFF; text-decoration: none !important; font-weight: "
"400; background-color: #875A7B; border: 0px solid #875A7B; border-"
"radius:3px\">Lihat Pengeluaran</a>\n"
"            <br/>\n"
"            <br/>\n"
"            <p>\n"
"                Terima Kasih.\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"        "

#. module: equip3_hr_expense_extend
#: model:mail.template,body_html:equip3_hr_expense_extend.email_template_approval_expense_request
msgid ""
"\n"
"            <p>Dear ${object.employee_id.name},</p>\n"
"            <br/>\n"
"            <p>\n"
"                We have received your request for Expense request ${object."
"seq_name}.\n"
"            </p>\n"
"            <br/>\n"
"            <p>\n"
"                This notification serves as confirmation that your expense "
"request has been approved.\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"            <p>\n"
"                Thank you.\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"        "
msgstr ""
"\n"
"            <p>Dear ${object.employee_id.name},</p>\n"
"            <br/>\n"
"            <p>\n"
"                Kami telah menerima permintaan Anda untuk permintaan Biaya "
"${object.seq_name}.\n"
"            </p>\n"
"            <br/>\n"
"            <p>\n"
"                Notifikasi ini berfungsi sebagai konfirmasi bahwa "
"permintaan pengeluaran Anda telah disetujui.\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"            <p>\n"
"                Terima Kasih.\n"
"            </p>\n"
"            <br/>\n"
"            <br/>\n"
"        "

