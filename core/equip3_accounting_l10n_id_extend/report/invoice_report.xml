<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_invoice_discount_inherit" inherit_id="account.report_invoice_document">
            <xpath expr="//tr[@class='border-black o_total']" position="before">
                <tr class="o_subtotal">
                    <td>ppn</td>
                    <td class="text-right">
                        <span t-field="o.total_ppn"/>
                    </td>
                </tr>
                <tr class="o_subtotal">
                    <td>pph</td>
                    <td class="text-right">
                        <span t-field="o.total_pph"/>
                    </td>
                </tr>
            </xpath>
    </template>
</odoo>