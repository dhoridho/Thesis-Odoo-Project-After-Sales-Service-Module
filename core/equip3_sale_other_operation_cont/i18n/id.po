msgid ""
"\n"
"\t\t\t\t<div style=\"margin: 0px; padding: 0px;\">\n"
"\t\t\t\t\t<p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"\t\t\t\t\t\tDate: ${ctx.get('date')}\n"
"\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\tDear ${ctx.get('approver_name')}\n"
"\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\tBased on the Blanket Order Approval Request ${object.name}. You "
"have approval pending that need\n"
"your decision to approve or reject the Blanket Order Approval request from "
"${object.user_id.partner_id.name}.\n"
