"\t\t\t\t\t\t\t<a href=${ctx.get('url')} style=\"padding: 8px 12px; font-"
"size: 12px; color: #FFFFFF; text-decoration: none !important; font-weight: "
"400; background-color: #875A7B; border: 0px solid #875A7B; border-"
"radius:3px\">\n"
"\t\t\t\t\t\t\t\t${object.name}\n"
"\t\t\t\t\t\t\t</a>\n"
"\t\t\t\t\t\t</div>\n"
"\t\t\t\t\t\t<br/>\n"
