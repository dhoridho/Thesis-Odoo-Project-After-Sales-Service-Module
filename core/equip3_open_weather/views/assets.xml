<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <template id="equip3_open_weather_assets_backend" name="equip3_open_weather assets_backend" inherit_id="web.assets_backend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/equip3_open_weather/static/src/scss/open_weather.scss"/>
        </xpath>
        
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/equip3_open_weather/static/src/js/open_weather.js"></script>
        </xpath>
    </template>

</odoo>