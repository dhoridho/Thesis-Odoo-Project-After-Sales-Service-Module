#. module: ss_whatsapp_connector
#: model:mail.template,body_html:ss_whatsapp_connector.whatsapp_invoice_template
msgid ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px;\">\n"
"        <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                Dear\n"
"                % if object.partner_id.parent_id:\n"
"                    ${object.partner_id.name} (${object.partner_id.parent_id.name}),\n"
"                % else:\n"
"                    ${object.partner_id.name},\n"
"                % endif\n"
"                <br /><br />\n"
"                Here is your\n"
"                % if object.name:\n"
"                    invoice ${object.name}\n"
"                % else:\n"
"                    invoice\n"
"                %endif\n"
"                % if object.invoice_origin:\n"
"                    (with reference: ${object.invoice_origin})\n"
"                % endif\n"
"                with amount ${format_amount(object.amount_total, object.currency_id)}\n"
"                from ${object.company_id.name}.\n"
"                % if object.payment_state == 'paid':\n"
"                    This invoice is already paid.\n"
"                % else:\n"
"                    Please remit payment at your earliest convenience.\n"
"                % endif\n"
"                <br />\n"
"                The invoice date and time is \"${object.invoice_date}\"\n"
"                <br />\n"
"                Do not hesitate to contact us if you have any questions.\n"
"                % if object.invoice_user_id.signature:\n"
"                    <br />\n"
"                    ${object.invoice_user_id.signature | safe}\n"
"                % endif\n"
"                </p>\n"
"        \n"
"        "
msgstr ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px;\">\n"
"        <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                Yang Terhormat\n"
"                % if object.partner_id.parent_id:\n"
"                    ${object.partner_id.name} (${object.partner_id.parent_id.name}),\n"
"                % else:\n"
"                    ${object.partner_id.name},\n"
"                % endif\n"
"                <br /><br />\n"
"                Ini milikmu\n"
"                % if object.name:\n"
"                    invoice ${object.name}\n"
"                % else:\n"
"                    invoice\n"
"                %endif\n"
"                % if object.invoice_origin:\n"
"                    (with reference: ${object.invoice_origin})\n"
"                % endif\n"
"                dengan total ${format_amount(object.amount_total, object.currency_id)}\n"
"                dari ${object.company_id.name}.\n"
"                % if object.payment_state == 'paid':\n"
"                    faktur telah dibayar.\n"
"                % else:\n"
"                    Harap kirimkan pembayaran sesegera mungkin.\n"
"                % endif\n"
"                <br />\n"
"                Tanggal dan waktu faktur adalah \"${object.invoice_date}\"\n"
"                <br />\n"
"                Jangan ragu untuk menghubungi kami jika Anda memiliki pertanyaan.\n"
"                % if object.invoice_user_id.signature:\n"
"                    <br />\n"
"                    ${object.invoice_user_id.signature | safe}\n"
"                % endif\n"
"                </p>\n"
"        \n"
"        "

#. module: ss_whatsapp_connector
#: model:mail.template,body_html:ss_whatsapp_connector.whatsapp_sales_template
msgid ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px;\">\n"
"        <p> Hello ${object.partner_id.name or ''},</p>\n"
"        Your order Number ${object.name} with amount ${format_amount(object.amount_total, object.currency_id)} is Confirmed.\n"
"        <br>\n"
"        Your order date and time is \"${object.date_order}\"\n"
"        <br>\n"
"        If you have any questions, please do not hesitate to contact us.\n"
"        <br>\n"
"        \n"
"        "
msgstr ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; ukuran font: 12px;\">\n"
"         <p> Halo ${object.partner_id.name or ''},</p>\n"
"         Nomor pesanan Anda ${object.name} dengan jumlah ${format_amount(object.amount_total, object.currency_id)} Dikonfirmasi.\n"
"         <br>\n"
"         Tanggal dan waktu pesanan Anda adalah \"${object.date_order}\"\n"
"         <br>\n"
"         Jika Anda memiliki pertanyaan, jangan ragu untuk menghubungi kami.\n"
"         <br>\n"
"         \n"
"         "

