<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="career_transition_configuration_action">
        <div class="career_transition_flow_action">
            <div class="career_transition_flow_action_content d-flex flex-column">
                <div class="content-header">
                    <div class="d-flex p-3 w-100">
                        <span>Career Transition Flow</span>
                    </div>
                </div>
                <div class="content-body">
                    <div class="container">
                        <div class="row h-100">
                            <div class="accordion">
                                <div class="accordion-item accordion-master-data">
                                    <h2 class="accordion-header" data-target="collapseMasterData">
                                        <button class="accordion-button" type="button">
                                        Master Data
                                        </button>
                                    </h2>
                                    <div id="collapseMasterData" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content clearfix">
                                                <button name="equip3_hr_career_transition.career_transition_type_act_window"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-customer">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/career-transition-type.svg"/>
                                                    </div>
                                                    <div class="button-text">Career Transition Types</div>
                                                </button>
                                                <button name="equip3_hr_career_transition.career_transition_category_act_window"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-customer">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/career-transition-category.svg"/>
                                                    </div>
                                                    <div class="button-text">Career Transition Category</div>
                                                </button>
                                                <button name="equip3_hr_career_transition.career_transition_approval_matrix_act_window"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-customer">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/career-transition-approval-matrix.svg"/>
                                                    </div>
                                                    <div class="button-text">Career Transition Approval Matrix</div>
                                                </button>
                                                <button name="equip3_hr_career_transition.hr_career_transition_letter_act_window"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-customer">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/career-transition-letter.svg"/>
                                                    </div>
                                                    <div class="button-text">Career Transition Letter</div>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="accordion-item accordion-career-transition-flow">
                                    <h2 class="accordion-header" data-target="collapseCareerTransitionFlow">
                                        <button class="accordion-button" type="button">
                                        Career Transition Flow
                                        </button>
                                    </h2>
                                    <div id="collapseCareerTransitionFlow" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">
                                                <button name="equip3_hr_career_transition.hr_career_transition_request_act_window"
                                                    type="action"
                                                    style="top: 100px;"
                                                    class="btn btn-flow btn-default menu-button-transition-request">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/career-transition-request.svg"/>
                                                    </div>
                                                    <div class="button-text">Career Transition Request</div>
                                                </button>
                                                <button name="equip3_hr_career_transition.hr_career_transition_to_approve_act_window"
                                                    type="action"
                                                    style=" left: 350px; top: 100px;"
                                                    class="btn btn-flow btn-default menu-button-transition-request-to-approve">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/career-transition-approval.svg"/>
                                                    </div>
                                                    <div class="button-text">Career Transition to Approve</div>
                                                </button>
                                                <button name="hr_contract.action_hr_contract"
                                                    type="action"
                                                    style=" left: 717px; top: 100px;"
                                                    class="btn btn-flow btn-default menu-button-contract">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/contract.svg"/>
                                                    </div>
                                                    <div class="button-text">Contracts</div>
                                                </button>
                                                <div class="nd_connector connector_line_right_mwc">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                                <div class="nd_connector connector_line_right_cio">
                                                    <div class="nd_connector_line"></div>
                                                    <div class="nd_connector_arrow_right"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="accordion-item accordion-reporting">
                                    <h2 class="accordion-header" data-target="collapseReporting">
                                        <button name="reporting" class="accordion-button" type="button">
                                        Reporting
                                        </button>
                                    </h2>
                                    <div id="collapseReporting" class="accordion-collapse show">
                                        <div class="accordion-body">
                                            <div class="accordion-content width-node">

                                                <button name="equip3_hr_career_transition.action_career_transition_view_pivot"
                                                    type="action"
                                                    class="btn btn-flow btn-default menu-button-rfm-analysis">
                                                    <div class="button-img">
                                                        <img class="img-fluid" src="/equip3_hr_career_transition/static/src/img/career-transition-analysis.svg"/>
                                                    </div>
                                                    <div class="button-text">Career Transition Analysis</div>
                                                </button>
                                                <div class="d-flex" style="width: 100%; gap:15px; ">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>