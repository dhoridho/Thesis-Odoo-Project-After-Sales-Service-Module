<?xml version="1.0" encoding="UTF-8"?>
<odoo>


    <template id="assets" name="Assets POS" inherit_id="point_of_sale.assets">
        <xpath expr="//script[@src='/point_of_sale/static/lib/html2canvas.js']" position="before">
            <link rel="stylesheet" type="text/css" href="/web/static/lib/select2/select2.css"/>
            <link rel="stylesheet" type="text/css" href="/web/static/lib/select2-bootstrap-css/select2-bootstrap.css"/>
            <script type="text/javascript" src="/web/static/lib/select2/select2.js"></script>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Payment/PaymentScreenPaymentLines.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Payment/PaymentScreen.js"/>
        </xpath>
    </template>

    <template id="assets_pos" name="Assets POS" inherit_id="equip3_pos_masterdata.assets_pos">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/equip3_pos_membership/static/src/css/Loyalty.css"/>
            <link rel="stylesheet" href="/equip3_pos_membership/static/src/css/custom.css"/>
        </xpath>
        
        <xpath expr="//script[@src='/equip3_pos_masterdata/static/src/js/Screens/ProductScreen/ProductScreen.js']" position="before">
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/RedeemRewardButton.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/RedeemProductButton.js"/>
        </xpath>

        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Core/loadModels.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Core/BigData.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Core/Models.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Core/Database.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Core/Order.js"/>

            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/ChromeWidgets/SyncModels.js"/>
            
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Popups/ReChargePointPopup.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Popups/MemberDepositPopups.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Popups/MemberDepositAddPopups.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Popups/MemberDepositPaymentPopups.js"/>


            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/FeatureButtons.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/ProductScreen.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/CustomerButton.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/MemberDepositButton.js"/>


            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/MemberDeposit/MemberDepositCreate.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/MemberDeposit/MemberDepositDetail.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/MemberDeposit/MemberDepositRow.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/MemberDeposit/MemberDepositList.js"/>


            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/Cart/ClientInformation.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/Cart/OrderWidget.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/ProductScreen/Cart/PointsSummary.js"/>

            <!-- <script type="text/javascript" src="/equip3_pos_membership/static/src/js/CustomerFacingScreen/CustomerFacingScreen.js"/> -->
            
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Client/ClientListScreen.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Client/ClientLine.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Client/RetailClientLine.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Payment/PaymentLoyaltyRewardButton.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Payment/PaymentScreenStatus.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/Screens/Payment/DepositPaymentButton.js"/>
            
        </xpath>
    </template>

    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/equip3_pos_membership/static/src/css/backend.css"/>
        </xpath>
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/equip3_custom_selection_widgets.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/equip3_generate_barcode_widgets.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/equip3_member_card_preview_widgets.js"/>
            <script type="text/javascript" src="/equip3_pos_membership/static/src/js/equip3_count_product_redeem.js"/>
            
        </xpath>
    </template>
    
</odoo>