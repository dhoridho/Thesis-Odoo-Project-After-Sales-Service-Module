<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	
	<record id="view_pos_session_form_inherit" model="ir.ui.view">
        <field name="name">pos.session.form.view.inherit</field>
        <field name="model">pos.session</field>
        <field name="inherit_id" ref="equip3_pos_general.view_pos_session_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='last_login_time']" position="after">
                <field name="scheduler_invoice_finish_date" readonly="1"/>
            </xpath>
        </field>
    </record>

</odoo>