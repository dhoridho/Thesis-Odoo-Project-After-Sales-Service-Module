<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="POSMembRetailClientLine" t-inherit="equip3_pos_masterdata.RetailClientLine" t-inherit-mode="extension" owl="1">
        <xpath expr="//td[hasclass('row-body-parent_id')]" position="replace"/>
        <xpath expr="//td[hasclass('row-body-type')]" position="replace"/>
        <xpath expr="//td[hasclass('row-body-company_type')]" position="replace"/>
        <xpath expr="//td[hasclass('row-body-balance')]" position="replace"/>
        <xpath expr="//td[hasclass('row-body-pos_total_amount')]" position="replace"/>

        <xpath expr="//td[hasclass('row-body-barcode')]" position="after">
            <td class="row-body-pos_loyalty_point">
                <t t-esc="env.pos.format_currency_no_symbol(partner.pos_loyalty_point)"/>
 <!--                <i t-on-click.stop="reChargePoints"
                   t-if="env.pos.config.pos_loyalty_ids"
                   class="oe_link_icon fa fa-edit oe_icon line-lot-icon"
                   aria-label="ReCharge points of Customer"
                   role="img"
                   title="ReCharge points of Customer"
                /> -->
            </td>
        </xpath> 
    </t>
</templates>