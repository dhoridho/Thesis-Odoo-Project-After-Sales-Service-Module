<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="receipt_template_overview_report" inherit_id="equip3_pos_masterdata.receipt_template_overview_report">
        <xpath expr="//div[hasclass('receipt_template_c_content_bottom')]" position="after">
                <div t-if="receipt_template.is_receipt_member_info">
                    <p style="    margin: 1px 0px;white-space: nowrap; overflow: hidden; text-overflow: unset;">--------------------------------------------------------------------------------------</p>
                    <p  style="    margin: 1px 0px;"><b>Loyalty Member :  XXXXXX </b></p>
                    <p  style="    margin: 1px 0px;" >Opening Points : XXXXXX </p>
                    <p  style="    margin: 1px 0px;">Plus Points : XXXXXX</p>
                    <p  style="    margin: 1px 0px;">Redeem Points : XXXXXX </p>
                    <p  style="    margin: 1px 0px;" >Current Points : XXXXXX</p>
                </div>
        </xpath>
    </template>
</odoo>
