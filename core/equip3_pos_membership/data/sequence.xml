<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_pos_member_barcode" model="ir.sequence">
            <field name="name">POS Member Barcode</field>
            <field name="code">pos.member.barcode</field>
            <field name="prefix"></field>
            <field name="padding">6</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_pos_generate_efaktur" model="ir.sequence">
            <field name="name">POS Generate E-Faktur</field>
            <field name="code">pos.generate.efaktur</field>
            <field name="prefix">GEF/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_pos_update_member_point" model="ir.sequence">
            <field name="name">POS Update Member Point</field>
            <field name="code">pos.loyalty.update.point</field>
            <field name="prefix">UMP/%(year)s/%(month)s/%(day)s/</field>
            <field name="range_reset">yearly</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

    </data>
</odoo>
