<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="seq_generate_pos_voucher" model="ir.sequence">
            <field name="name">Generate POS Voucher</field>
            <field name="code">generate.pos.voucher</field>
            <field name="prefix">GVC/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_pos_session" model="ir.sequence">
            <field name="name">POS Voucher</field>
            <field name="code">pos.voucher</field>
            <field name="prefix">Voucher/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="pos_session_order_sequence" model="ir.sequence">
            <field name="name">POS Voucher</field>
            <field name="code">pos.session.order.sequence</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
        </record>
        
        <record id="seq_generate_pos_coupon_program" model="ir.sequence">
            <field name="name">Generate POS Coupon Program</field>
            <field name="code">pos.coupon.program</field>
            <field name="prefix">GCP/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False" />
        </record>

    </data>
</odoo>
