<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

    <record model="ir.module.category" id="module_category_sales_point_of_sale">
        <field name="name">Point of Sale</field>
        <field name="sequence">1</field>
        <field name="parent_id" eval="False"/>
    </record>

    <record model="ir.module.category" id="module_category_pos_invoices">
        <field name="name">POS Related Invoices</field>
        <field name="sequence">1</field>
        <field name="parent_id" ref="base.module_category_accounting"/>
    </record>

    <record id="group_pos_account_staff" model="res.groups">
        <field name="name">Accountant Staff</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_pos_invoices"/>
       <field name="implied_ids" eval="[(4, ref('account.group_account_invoice'))]"/>
    </record>

    <record id="group_pos_account_manager" model="res.groups">
        <field name="name">Accountant Manager</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_pos_invoices"/>
        <field name="implied_ids" eval="[(4, ref('account.group_account_invoice'))]"/>
    </record>

    <record id="group_pos_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_sales_point_of_sale"/>
        <field name="implied_ids" eval="[(4, ref('sales_team.group_sale_salesman')),(4, ref('account.group_account_readonly'))]"/>
    </record>

    <record id="group_pos_cooker" model="res.groups">
        <field name="name">Cooker</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_sales_point_of_sale"/>
        <field name="implied_ids" eval="[(4, ref('equip3_pos_masterdata.group_pos_user')),(4, ref('stock.group_stock_user'))]"/>
    </record>

    <record id="group_pos_waiter" model="res.groups">
        <field name="name">Waiter</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_sales_point_of_sale"/>
        <field name="implied_ids" eval="[(4, ref('equip3_pos_masterdata.group_pos_user')),(4, ref('stock.group_stock_user'))]"/>
    </record>

    <record id="group_pos_cashier" model="res.groups">
        <field name="name">Cashier</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_sales_point_of_sale"/>
        <field name="implied_ids" eval="[(4, ref('equip3_pos_masterdata.group_pos_user')),(4, ref('stock.group_stock_user'))]"/>
    </record>

    <record id="group_pos_staff" model="res.groups">
        <field name="name">Staff</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_sales_point_of_sale"/>
       <field name="implied_ids" eval="[(4, ref('equip3_pos_masterdata.group_pos_user')),(4, ref('stock.group_stock_user'))]"/>
    </record>

    <record id="group_pos_supervisor" model="res.groups">
        <field name="name">Supervisor</field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_sales_point_of_sale"/>
        <field name="implied_ids" eval="[(4, ref('equip3_pos_masterdata.group_pos_user')),(4, ref('stock.group_stock_user')),(4, ref('ks_dashboard_ninja.ks_dashboard_ninja_group_manager'))]"/>
    </record>

    <record id="group_pos_manager" model="res.groups">
        <field name="name">Administrator </field>
        <field name="category_id" ref="equip3_pos_masterdata.module_category_sales_point_of_sale"/>
        <field name="implied_ids" eval="[(4, ref('equip3_pos_masterdata.group_pos_user')),(4, ref('stock.group_stock_user')),(4, ref('equip3_pos_masterdata.group_pos_supervisor')),(4, ref('sales_team.group_sale_manager')),(4, ref('account.group_account_manager'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

    </data>

</odoo>
