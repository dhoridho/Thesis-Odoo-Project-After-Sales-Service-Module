<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="KitchenProcessingTimes" owl="1">
        <span class="product-name">
            <span>
            <!-- <span t-att-class="{warning_box: warningWaitingTime}"> -->
            	<t t-esc="getProcessingTime"/>
        	</span>
        </span>
    </t>

    <t t-name="KitchenProcessingTimesBackward" owl="1">
        <button class="priority btn" t-att-class="{'bg-danger': redWaitingtime}">
            <i class="fa fa-clock-o" style="margin-right: 5px;"></i> 
            <span class="product-name">
                <t t-esc="getProcessingTime"/>
            </span>
        </button>
    </t>

