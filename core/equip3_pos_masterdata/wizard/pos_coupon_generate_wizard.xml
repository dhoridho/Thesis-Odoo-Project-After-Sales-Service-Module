<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pos_coupon_generate_wizard_view_form" model="ir.ui.view">
        <field name="name">pos.coupon.generate.wizard</field>
        <field name="model">pos.coupon.generate.wizard</field>
        <field name="arch" type="xml">
            <form string="Generate Vouchers">
                <group>
                    <group>
                        <field name="no_of_coupon" required="1"/>
                        <field name="coupon_program_id" invisible="1"/>
                    </group>
                </group>
                <footer>
                    <button name="action_confirm" string="Generate" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>