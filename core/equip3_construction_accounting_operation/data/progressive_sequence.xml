<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="progressive_project_claim_sequence" model="ir.sequence">
            <field name="name">Progressive Project Claim Sequence</field>
            <field name="code">progressive.project.claim.sequence</field>
            <field name="prefix">PPC/INV/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="progressive_project_bill_sequence" model="ir.sequence">
            <field name="name">Progressive Project Bill Sequence</field>
            <field name="code">progressive.project.bill.sequence</field>
            <field name="prefix">PPC/BILL/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="claim_request_sequence" model="ir.sequence">
            <field name="name">Claim Request Sequence</field>
            <field name="code">claim.request.sequence</field>
            <field name="prefix">CR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="bill_request_sequence" model="ir.sequence">
            <field name="name">Bill Request Sequence</field>
            <field name="code">bill.request.sequence</field>
            <field name="prefix">CR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="jurnal_entries_claim_request_sequence" model="ir.sequence">
            <field name="name">Jurnal Entry Claim Request Sequence</field>
            <field name="code">jurnal.claim.request.sequence</field>
            <field name="prefix">CRJ/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
