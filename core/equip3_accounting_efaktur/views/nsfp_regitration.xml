<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="nsfp_registration_view_tree" model="ir.ui.view">
            <field name="name">nsfp.registration.tree</field>
            <field name="model">nsfp.registration</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="record_id" readonly="1" force_save="1"  string="Pajak Express ID"/>                            
                    <field name="company_id" required="1" options="{'no_create': True, 'no_open': True}"/>
                    <field name="fiscal_year" required="1" />                            
                    <field name="start" string="Start Number" placeholder="xxx-xx-xxxxxxxx"  widget="mask" data-inputmask="'mask': '999-99-99999999'" required="1"/>                            
                    <field name="end" string="End Number" placeholder="xxx-xx-xxxxxxxx"  widget="mask" data-inputmask="'mask': '999-99-99999999'" required="1"/>
                    <field name="registration_date" string="Registration Date" readonly="1" force_save="1"/>
                    <field name="last_number" string="Last Number" readonly="1" force_save="1" data-inputmask="'mask': '999-99-99999999'"/>
                    <field name="is_use" string="Used" readonly="1" force_save="1"/>
                   
                </tree>
            </field>
        </record>

        

        <record id="action_nsfp_registration" model="ir.actions.act_window">
            <field name="name">NSFP Registration</field>
            <field name="res_model">nsfp.registration</field>
            <field name="view_mode">tree</field>
            <field name="context"></field>
            <field name="domain"></field>            
        </record>

        <menuitem 
        id="menu_action_nsfp_registration"  
        action="action_nsfp_registration" 
        name="NSFP Registration"
        parent="equip3_accounting_efaktur.menu_efaktur"
        groups="equip3_accounting_efaktur.group_is_pajak_express_integration"
        />

        

</odoo>