<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'aos_base_account'), ('name', '=', 'group_down_payment_accrue')]"/>
        </function>
        <value eval="{'noupdate': False}" />
    </function>

    <data noupdate="0">
        <record model="res.groups" id="group_is_pajak_express_integration">
            <field name="name">PJAP Integration</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>

        <record id="aos_base_account.group_down_payment_accrue" model="res.groups">
            <field name="name">Accrued Down Payment</field>
            <field name="category_id" ref="base.module_category_hidden"/>
            <field name="comment">Handle Accrued Down Payment</field>
        </record>
    </data>
</odoo>