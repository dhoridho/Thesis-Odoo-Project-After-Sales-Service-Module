<odoo>
    <data>
        <record id="account_tax_digunggung_wizard_form" model="ir.ui.view">
            <field name="name">account.tax.digunggung.form</field>
	    <field name="model">account.tax.digunggung</field>
            <field name="arch" type="xml">
            	<form>
                    <group colspan='4'>
                        <group>
                            <field name="tax_report" readonly="1" />
                            <field name="start_date_digunggung"/>
                        </group>
                        <group>
                            <field name="partner_id" domain="[('faktur_pajak_gabungan','=',True)]" />
                            <field name="end_date_digunggung"/>
                            <field name="company_id" />
                        </group>
                    </group>
                    <footer>
                        <button string="Confirm" name="confirm" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="ppn_keluaran_digunggung_wizard_action" model="ir.actions.act_window">
            <field name="name">PPN Keluaran Digunggung</field>
            <field name="res_model">account.tax.digunggung</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="account_tax_digunggung_wizard_form"/>
            <field name="target">new</field>
            <field name="context">{'default_tax_report': 'keluaran'}</field>
        </record>

        <menuitem action="ppn_keluaran_digunggung_wizard_action" id="menu_ppn_keluaran_digunggung_wizard_action" sequence="11" parent="equip3_accounting_efaktur.menu_efaktur"/>
        
        <record id="ppn_masukkan_digunggung_wizard_action" model="ir.actions.act_window">
            <field name="name">PPN Masukkan Digunggung</field>
            <field name="res_model">account.tax.digunggung</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="account_tax_digunggung_wizard_form"/>
            <field name="context">{'default_tax_report': 'masukkan'}</field>
            <field name="target">new</field>
        </record>

        <menuitem action="ppn_masukkan_digunggung_wizard_action" id="menu_ppn_masukkan_digunggung_wizard_action" sequence="12" parent="equip3_accounting_efaktur.menu_efaktur"/>

    </data>
</odoo>