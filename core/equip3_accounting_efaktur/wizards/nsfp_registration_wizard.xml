<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_nsfp_registration_wizard" model="ir.ui.view">
            <field name="name">nsfp.registration.wizard.tree</field>
            <field name="model">nsfp.registration.wizard</field>
            <field name="arch" type="xml">
                <form >
                    <sheet>

                        <group name="nsfp_registration" string="NSFP Registration">
                            <field name="fiscal_year" />                            
                            <field name="start" string="Start Number" placeholder="xxx-xx-xxxxxxxx"  widget="mask" data-inputmask="'mask': '999-99-99999999'"/>                            
                            <field name="end" string="End Number" placeholder="xxx-xx-xxxxxxxx"  widget="mask" data-inputmask="'mask': '999-99-99999999'"/>
                            
                        </group>
                    </sheet>
                    <footer>
                        <button string="Save" name="save" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
                    
                </form>

            </field>
        </record>

        


        

</odoo>