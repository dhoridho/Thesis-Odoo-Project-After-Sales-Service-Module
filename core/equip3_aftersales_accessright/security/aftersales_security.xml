<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record model="ir.module.category" id="cateagory_aftersales_root">
            <field name="name">Aftersales</field>
            <field name="sequence">100</field>
        </record>
    
        <record id="module_category_aftersales" model="ir.module.category">
            <field name="name">Aftersales</field>
            <field name="sequence">1</field>
            <field name="parent_id" ref="cateagory_aftersales_root"/>
        </record>

        <record id="group_aftersales_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_aftersales"/>
        </record>

        <record id="group_aftersales_administration" model="res.groups">
            <field name="name">Administrator</field>
            <field name="category_id" ref="module_category_aftersales"/>
            <field name="implied_ids" eval="[(4, ref('group_aftersales_user'))]"/>
        </record>
        
    </data>
    

</odoo>
