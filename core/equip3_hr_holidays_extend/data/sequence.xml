<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_leave_cancelation" model="ir.sequence">
            <field name="name">Leave Cancelation Sequence</field>
            <field name="code">hr.leave.cancelation</field>
            <field name="prefix">LCC/%(range_y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hr_leave" model="ir.sequence">
            <field name="name">Leave Sequence</field>
            <field name="code">hr.leave</field>
            <field name="prefix">LRQ/%(range_y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hr_leave_allocation" model="ir.sequence">
            <field name="name">Leave Allocation Sequence</field>
            <field name="code">hr.leave.allocation</field>
            <field name="prefix">LAQ/%(range_y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
