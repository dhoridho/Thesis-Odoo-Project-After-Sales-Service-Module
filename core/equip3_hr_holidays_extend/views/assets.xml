<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="equip3_hr_holidays_extend_restrict_button" name="Hide Archive Buttons" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/equip3_hr_holidays_extend/static/src/js/restrict_button.js"></script>
        </xpath>
    </template>
    <template id="hr_leave_flow" inherit_id="web.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/equip3_hr_holidays_extend/static/src/css/hr_leave_flow.css"/>
            <script type="text/javascript" src="/equip3_hr_holidays_extend/static/src/js/leave_flow.js"/>
        </xpath>
    </template>
</odoo>