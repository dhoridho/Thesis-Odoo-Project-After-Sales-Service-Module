<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_plan_task_check_list_tree" model="ir.ui.view">
        <field name="name">view.plan.task.check.list.tree</field>
        <field name="model">plan.task.check.list</field>
        <field name="arch" type="xml">
            <tree string="Plan Task Check List">
                <field name="equipment_id"/>
                <field name="maintenance_wo_id" string="Work Order"/>
                <field name="maintenance_ro_id" string="Repair Order"/>
                <field name="task"/>
                <field name="vehicle_parts_ids" widget="many2many_tags"/>
            </tree>
        </field>
    </record>

</odoo>