<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="equip3_manuf_reports_assets_backend" name="equip3_manuf_reports assets" inherit_id="web.assets_backend">
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/kanban_gantt_controller.js"></script>
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/list_view_report.js"></script>
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/manuf_flow.js"></script>
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/ks_gantt_production.js"></script>
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/ks_gantt_workorder.js"></script>
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/cost_of_goods_manufactured.js"></script>
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/moving_production_cost_report.js"></script>
                <script type="text/javascript" src="/equip3_manuf_reports/static/src/js/moving_plan_cost_report.js"></script>
            </xpath>

            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/equip3_manuf_reports/static/src/css/manuf_flow.css"/>
                <link rel="stylesheet" type="text/scss" href="/equip3_manuf_reports/static/src/scss/cost_of_goods_manufactured.scss"/>
                <link rel="stylesheet" type="text/scss" href="/equip3_manuf_reports/static/src/scss/moving_production_cost_report.scss"/>
                <link rel="stylesheet" type="text/scss" href="/equip3_manuf_reports/static/src/scss/moving_plan_cost_report.scss"/>
                <link rel="stylesheet" type="text/scss" href="/equip3_manuf_reports/static/src/scss/ks_gantt_workorder.scss"/>
            </xpath>
        </template>

        <template id="report_assets_pdf" inherit_id="web.report_assets_pdf">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/equip3_manuf_reports/static/src/scss/cost_of_goods_manufactured.scss"/>
            </xpath>
        </template>
    </data>
</odoo>