#. module: equip3_accounting_cash_advance_extd
#: model:mail.template,body_html:equip3_accounting_cash_advance_extd.email_template_cash_advance_approval_matrix
msgid ""
"\n"
"            <p>Date ${ctx.get('date')},</p>\n"
"            <p>Subject : Application for Cash Advance Approval,</p>\n"
"            <p>Dear ${ctx.get('approver_name')},</p>\n"
"            <p>You have approval pending that needs your decision to approve or reject the cash advance.</p>\n"
"            <p>${object.request_partner_id.name} has requested a ${ctx.get('amount_cash_advance')} ${ctx.get('currency_id_cash_advance')}.\n"
"            </p>\n"
"            <p>You can approve or reject the cash advance by clicking this button:</p>\n"
"            <div style = \"margin: 16px;\">\n"
"                <a href=${ctx.get('url')} style = \"padding: 5px 10px; font-size: 12px; line-height: 18px;\n"
"                 color: #FFFFFF; border-color:#875A7B; text-decoration: none; display: inline-block; \n"
"                 margin-bottom: 0px; font-weight: 400; text-align: center; vertical-align: middle; \n"
"                 cursor: pointer; white-space: nowrap; background-image: none; background-color: #875A7B;\n"
"                 border: 1px solid #875A7B; border-radius:3px\">View Cash Advance</a></div>\n"
"            <p>Thank you,</p>\n"
"            <p>This is an automatically generated email, please do not reply.</p>\n"
"            "
msgstr ""

#. module: equip3_accounting_cash_advance_extd
#: model:mail.template,body_html:equip3_accounting_cash_advance_extd.email_template_cash_advance_submitter_approval_matrix
msgid ""
"\n"
"            <p>Date ${ctx.get('date')},</p>\n"
"            <p>Subject : Approval of Cash Advance</p>\n"
"            <p>Dear ${object.request_partner_id.name}</p>\n"
"            <p>We have received a response to your request for a Cash Advance on ${ctx.get('create_date')}.\n"
"            </p>\n"
"            <p>This notification serves as confirmation that your cash advance has been approved.\n"
"            </p>\n"
"            <p>Thank you,</p>\n"
"            <p>This is an automatically generated email, please do not reply.</p>\n"
"            "
msgstr ""

#. module: equip3_accounting_cash_advance_extd
#: model:mail.template,body_html:equip3_accounting_cash_advance_extd.email_template_cash_advance_rejected_matrix
msgid ""
"\n"
"            <p>Date ${ctx.get('date')},</p>\n"
"            <p>Subject : Rejection of Cash Advance</p>\n"
"            <p>Dear ${object.request_partner_id.name}</p>\n"
"            <p>We have received a response to your request for a Cash Advance on ${ctx.get('create_date')}.\n"
"            </p>\n"
"            <p>This notification serves as confirmation that your cash advance has been rejected.\n"
"            </p>\n"
"            <p>Here is the feedback from ${ctx.get('rejected_name')}</p>\n"
"            <p>${ctx.get('feedback')}</p>\n"
"            <p>Thank you,</p>\n"
"            <p>This is an automatically generated email, please do not reply.</p>\n"
"            "
msgstr ""

