<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="vendor_payment_request_sequence" model="ir.sequence">
            <field name="name">Vendor Payment sequence</field>
            <field name="code">vendor.payment.id.sequence</field>
            <field name="prefix">POPR/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <record id="increment_five" model="ir.sequence">
        <field name="name">Seq Stock Move</field>
        <field name="code">increment.five.stock.move</field>
        <field name="company_id" eval="False"/>
        <field eval="5" name="number_next"/>
        <field eval="5" name="number_increment"/>
    </record>
</odoo>