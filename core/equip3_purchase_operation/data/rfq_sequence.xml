<odoo>
     <data noupdate="1">
          <record id="purchase_rfq_new_seqs" model="ir.sequence">
               <field name="name">Request for Quotation</field>
               <field name="code">purchase.order.seqs.rfq</field>
               <field name="prefix">RFQ/%(y)s/%(month)s/%(day)s/</field>
               <field name="padding">3</field>
          </record>

         <record id="purchase_rfq_new_seqs_goods" model="ir.sequence">
             <field name="name">Request for Quotation</field>
             <field name="code">purchase.order.seqs.rfq.goods</field>
             <field name="prefix">RFQ/G/%(y)s/%(month)s/%(day)s/</field>
             <field name="padding">3</field>
         </record>

         <record id="purchase_rfq_new_seqs_services" model="ir.sequence">
             <field name="name">Request for Quotation</field>
             <field name="code">purchase.order.seqs.rfq.services</field>
             <field name="prefix">RFQ/S/%(y)s/%(month)s/%(day)s/</field>
             <field name="padding">3</field>
         </record>

          <record id="purchase_order_new_seqs" model="ir.sequence">
               <field name="name">Purchase Order</field>
               <field name="code">purchase.order.seqs</field>
               <field name="prefix">PO/%(y)s/%(month)s/%(day)s/</field>
               <field name="padding">3</field>
               <field name="company_id" eval="False"/>
          </record>

         <record id="purchase_order_new_seqs_goods" model="ir.sequence">
             <field name="name">Purchase Order</field>
             <field name="code">purchase.order.seqs.goods</field>
             <field name="prefix">PO/G/%(y)s/%(month)s/%(day)s/</field>
             <field name="padding">3</field>
             <field name="company_id" eval="False"/>
         </record>

         <record id="purchase_order_new_seqs_services" model="ir.sequence">
             <field name="name">Purchase Order</field>
             <field name="code">purchase.order.seqs.services</field>
             <field name="prefix">PO/S/%(y)s/%(month)s/%(day)s/</field>
             <field name="padding">3</field>
             <field name="company_id" eval="False"/>
         </record>

		 <record id="purchase_req_new_seqs" model="ir.sequence">
			 <field name="name">Purchase Request</field>
			 <field name="code">purchase.req.seqs</field>
			 <field name="prefix">PR/%(y)s/%(month)s/%(day)s/</field>
			 <field name="padding">3</field>
			 <field name="company_id" eval="False"/>
		 </record>

         <record id="purchase_req_new_seqs_goods" model="ir.sequence">
             <field name="name">Purchase Request</field>
             <field name="code">purchase.req.seqs.goods</field>
             <field name="prefix">PR/G/%(y)s/%(month)s/%(day)s/</field>
             <field name="padding">3</field>
             <field name="company_id" eval="False"/>
         </record>

         <record id="purchase_req_new_seqs_services" model="ir.sequence">
             <field name="name">Purchase Request</field>
             <field name="code">purchase.req.seqs.services</field>
             <field name="prefix">PR/S/%(y)s/%(month)s/%(day)s/</field>
             <field name="padding">3</field>
             <field name="company_id" eval="False"/>
         </record>

         <record id="service_work_order_sequence" model="ir.sequence">
             <field name="name">Service Work Order</field>
             <field name="code">service.work.order</field>
             <field name="prefix">SWO/%(y)s/%(month)s/%(day)s/</field>
             <field name="padding">3</field>
             <field name="company_id" eval="False"/>
         </record>
     </data>
</odoo>