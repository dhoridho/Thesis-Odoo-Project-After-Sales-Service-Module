<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-extend="purchase.PurchaseDashboard">
        <t t-jquery=".o_dashboard_action:eq(1)" t-operation="replace">
            <td class="o_main o_dashboard_action" title="All Waiting RFQs" name="purchase.purchase_action_dashboard" context='{"search_default_waiting_for_approve": true, "search_default_rfq_approved": true}'>
                <a href="#"><t t-esc="values['all_waiting']"/><br/>Waiting</a>
            </td>
        </t>
        <t t-jquery=".o_dashboard_action:eq(4)" t-operation="replace">
            <td class="o_main o_dashboard_action" title="My Waiting RFQs" name="purchase.purchase_action_dashboard" context='{"search_default_waiting_for_approve": true, "search_default_rfq_approved": true, "search_default_my_purchases": true}'>
                <a href="#"><t t-esc="values['my_waiting']"/></a>
            </td>
        </t>
    </t>
</templates>