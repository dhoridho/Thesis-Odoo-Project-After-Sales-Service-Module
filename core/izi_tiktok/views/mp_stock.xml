<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="izi_tiktok.search_mp_stock" model="ir.ui.view">
            <field name="name">Marketplace Stock Search View</field>
            <field name="model">mp.stock</field>
            <field name="inherit_id" ref="izi_marketplace.search_mp_stock"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='filter_per_mp']" position="inside">
                    <filter string="Tiktok Shop" name="filter_to_tiktok" domain="[('marketplace','=', 'tiktok')]"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
