<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="izi_tiktok.action_window_tts_shop" model="ir.actions.act_window">
            <field name="name">Shop</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">mp.tiktok.shop</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="izi_tiktok.action_window_tts_logistic" model="ir.actions.act_window">
            <field name="name">Logistic</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">mp.tiktok.logistic</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="izi_tiktok.action_window_mp_stock" model="ir.actions.act_window">
              <field name="name">Marketplace Stock</field>
              <field name="type">ir.actions.act_window</field>
              <field name="res_model">mp.stock</field>
              <field name="view_mode">tree</field>
              <field name="domain">[('marketplace', '=', 'tiktok')]</field>
              <field name="context">{
                'create': False,
                'delete': False,
                'default_marketplace': 'tiktok',
                "search_default_group_by_mp_account": 1,
              }</field>
        </record>
        <record id="izi_tiktok.action_window_tts_category" model="ir.actions.act_window">
          <field name="name">Product Category</field>
          <field name="type">ir.actions.act_window</field>
          <field name="res_model">mp.tiktok.product.category</field>
          <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>