<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- agreement.renewal.line tree view -->
        <record id="agreement_renewal_line_view_tree" model="ir.ui.view">
            <field name="name">agreement.renewal.line.view.tree</field>
            <field name="model">agreement.renewal.line</field>
            <field name="arch" type="xml">
                <tree string="Renewal Contract Line" delete="false">
                    <field name="agreement_id" widget="many2onebutton"/>
                    <field name="agreement_stage_id"/>
                    <field name="last_contract" readonly="1"/>
                    <field name="desc"/>
                </tree>
            </field>
        </record>
    </data>
    
</odoo>