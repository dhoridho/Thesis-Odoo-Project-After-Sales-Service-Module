<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="OnlineOrderButton" owl="1">
        <div class="ticket-button online-order-feature-button" 
            t-att-class="{'oe_hidden': !env.pos.config.online_outlet_id }"  
            t-on-click="onClick">
            <span>Online Orders</span>
        </div>
    </t>

    <t t-name="OnlineOrderHeaderButtons" t-inherit="equip3_pos_masterdata.RightHeaderButtons" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('right-header-button')]" position="inside">
            <OnlineOrderButton/>
        </xpath>
    </t> 

</templates>
