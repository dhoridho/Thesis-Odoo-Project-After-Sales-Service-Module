<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="OloReceiptScreen" t-inherit="point_of_sale.ReceiptScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('next')]" position="after">
            <div class="button button-next-online-order" t-on-click.stop="nextOnlineOrder" t-att-class="{ highlight: !locked }" 
                style="display: none;">
                Next <i class="fa fa-angle-double-right"></i>
            </div>
        </xpath>
    </t>

</templates>
