<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_pos_pos_form_inherit" model="ir.ui.view">
        <field name="name">view.pos.pos.form.inherit</field>
        <field name="model">pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='extra']" position="inside">
                <group string="Online Outlet">
                    <group name="online_outlet_1">
                        <field name="is_online_outlet" readonly="1"/>
                        <field name="oloutlet_order_id" readonly="1"/>
                        <field name="oloutlet_order_from" readonly="1"/>
                        <field name="oloutlet_order_type" readonly="1"/>
                        <field name="oloutlet_order_info" readonly="1"/>
                    </group>
                </group> 
            </xpath>
        </field>
    </record>

</odoo>