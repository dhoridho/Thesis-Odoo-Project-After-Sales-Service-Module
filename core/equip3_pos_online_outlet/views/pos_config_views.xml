<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="pos_config_retail_view_form_inherit" model="ir.ui.view">
        <field name="name">pos.config.retail</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="equip3_pos_general.pos_config_retail"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Online Outlets" groups="point_of_sale.group_pos_manager">
                    <group>
                        <group>
                            <field name="online_outlet_id"/>
                        </group>
                    </group>
                </page>
            </xpath>
       	</field>
    </record>
</odoo>