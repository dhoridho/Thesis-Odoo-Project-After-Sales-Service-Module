<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_pos_top_selling_wizard" model="ir.ui.view">
            <field name="name">pos.change.cashier.wizard.form</field>
            <field name="model">pos.cashier.change.wizard</field>
            <field name="arch" type="xml">
                <form string="Cashier Change Report">
                    <group>
                  
                        <group>
                            <field name="start_datetime" required="1"/>
                        </group>
                        <group>
                            <field name="end_datetime" required="1"/>
                        </group>
                
                    </group>
                    <group>
                        <field name="pos_branch_id" required="1"/>
                    </group>
                              
                    <footer>
                        <button name="generate_report" string="Print" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
            </field>
        </record> 
        <record id="action_report_cashier_change" model="ir.actions.act_window">
            <field name="name">Cashier Change Report</field>
            <field name="res_model">pos.cashier.change.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <!-- <menuitem id="menu_report_cashier_change" name="Cashier Change Report Wizard" action="action_report_cashier_change" parent="menu_report_parent" sequence="7"/> -->
    </data>
</odoo>

