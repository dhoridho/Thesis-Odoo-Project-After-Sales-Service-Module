<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="assets_backend" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/cashier_change_report_controller.js"></script>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/analytic_dashboard.js"></script>
                <link rel="stylesheet" href="/equip3_pos_report/static/src/css/apd.css" />
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/pivot_controller.js"></script>
            </xpath>

        </template>

        <template id="pos_reports_assets" name="POS Reports Assets" inherit_id="point_of_sale.assets">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/equip3_pos_report/static/src/css/reports.css" />

                <script type="text/javascript" src="/equip3_pos_report/static/src/js/models.js"/>

                <!-- =============== Audit Summary =============== -->
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/AuditReport/ReportLocationButtonWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/AuditReport/PopupLocationWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/AuditReport/LocationReceiptScreen.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/AuditReport/LocationReceipt.js"/>
                
                <!-- =============== Category Summary =============== -->
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/CategorySummary/ReportCategoryButtonWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/CategorySummary/PopupCategoryWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/CategorySummary/CategoryReceiptWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/CategorySummary/XMLPosCategorySummaryReceipt.js"/>
                
                <!-- =============== Order Summary =============== -->
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/OrderSummary/ReportOrderButtonWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/OrderSummary/PopupOrderWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/OrderSummary/OrderReceiptWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/OrderSummary/XMLPosOrderSummaryReceipt.js"/>

                <!-- =============== Payment Summary =============== -->
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/PaymentReport/ReportPaymentButtonWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/PaymentReport/PaymentSummaryPopup.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/PaymentReport/PaymentReceiptWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/PaymentReport/XMLPosPaymentSummaryReceipt.js"/>

                <!-- =============== Product Summary =============== -->
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/ReportProductButton/ReportProductButtonWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/ReportProductButton/PopupProductWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/ReportProductButton/ProductReceiptWidget.js"/>
                <script type="text/javascript" src="/equip3_pos_report/static/src/js/ReportProductButton/XMLPosProductSummaryReceipt.js"/>
                
            </xpath>
        </template>


        <template id="assets_pos" inherit_id="pos_hr.assets">
            <xpath expr="//script[@src='/pos_hr/static/src/js/useSelectEmployee.js']" position="replace">
               <script type="text/javascript" src="/equip3_pos_report/static/src/js/selectemployee.js"></script>
            </xpath>
        </template>
        
        <menuitem id="menu_report_parent"
            name="Other Reports"
            parent="point_of_sale.menu_point_root"
            sequence="20"/>
    
    </data>
</odoo>

