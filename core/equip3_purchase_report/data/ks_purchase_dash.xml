        <record id="ks_purchase_item_1" model="ks_dashboard_ninja.item">
            <field name="name">Request for Quotations</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_domain">[["state","in",("draft", "sent","to approve","waiting_for_approve", "rfq_approved")],["dp", "=", False]]</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_icon" type="base64" file="equip3_purchase_report/static/description/images/dashboard/request_for_quotation.png"/>
            <field name="ks_default_icon">False</field>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_dashboard_item_theme">blue</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#357cb8,0.99</field>
            <field name="ks_button_color">#ffffff,0.99</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_purchase_item_2" model="ks_dashboard_ninja.item">
            <field name="name">Purchase Orders</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_domain">[["state","=","purchase"],["invoice_status", "!=", 'invoiced'],["dp", "=", False]]</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_icon" type="base64" file="equip3_purchase_report/static/description/images/dashboard/purchase_order.png"/>
            <field name="ks_default_icon">False</field>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_dashboard_item_theme">blue</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#f3994b,0.99</field>
            <field name="ks_button_color">#000000,0.99</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record id="ks_purchase_item_3" model="ks_dashboard_ninja.item">
            <field name="name">Purchase Requests</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('purchase_request.model_purchase_request')"/>
            <field name="ks_domain">[["state","=","purchase_request"],["purchase_req_state","in",("pending","in_progress")]]</field>
            <field name="ks_date_filter_field" eval="ref('purchase_request.field_purchase_request__create_date')"/>
            <field name="ks_icon" type="base64" file="equip3_purchase_report/static/description/images/dashboard/purchase_request.png"/>
            <field name="ks_default_icon">False</field>
            <field name="ks_icon_select">Custom</field>
            <field name="ks_dashboard_item_theme">yellow</field>
            <field name="ks_background_color">#ffffff,0.99</field>
            <field name="ks_font_color">#000000,0.99</field>
            <field name="ks_default_icon_color">#44bf49,0.99</field>
            <field name="ks_button_color">#000000,0.99</field>
            <field name="ks_layout">layout6</field>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_update_items_data">15000</field>
            <field name="ks_company_id" eval="0"/>
