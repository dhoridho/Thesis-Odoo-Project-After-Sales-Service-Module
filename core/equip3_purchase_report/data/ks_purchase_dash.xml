        <record id="ks_purchase_item_5" model="ks_dashboard_ninja.item">
            <field name="name">Direct Purchases</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_domain">[["state","in",("draft","sent","waiting_for_approve","rfq_approved","purchase")],["dp","=",True]]</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_icon" type="base64" file="equip3_purchase_report/static/description/images/dashboard/direct_order.png"/>
