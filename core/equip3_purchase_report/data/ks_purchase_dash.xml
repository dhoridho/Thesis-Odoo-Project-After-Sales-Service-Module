        <record id="ks_purchase_item_6" model="ks_dashboard_ninja.item">
            <field name="name">Blanket Orders</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('purchase_requisition.model_purchase_requisition')"/>
            <field name="ks_domain">[["state","=","blanket_order"],["bo_state","in",("pending_order","ongoing")]]</field>
            <field name="ks_date_filter_field" eval="ref('purchase_requisition.field_purchase_requisition__create_date')"/>
            <field name="ks_icon" type="base64" file="equip3_purchase_report/static/description/images/dashboard/blanket_order.png"/>
