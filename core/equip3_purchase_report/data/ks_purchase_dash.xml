        <record id="ks_purchase_item_7" model="ks_dashboard_ninja.item">
            <field name="name">Top Vendors</field>
            <field name="ks_dashboard_item_type">ks_bar_chart</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_chart_groupby_type">relational_type</field>
            <field name="ks_chart_measure_field"
                   eval="[(6, 0, [ref('purchase.field_purchase_order__amount_total'),ref('purchase.field_purchase_order__amount_tax'),ref('purchase.field_purchase_order__amount_untaxed')])]"/>
            <field name="ks_chart_relation_groupby" eval="ref('purchase.field_purchase_order__partner_id')"/>
            <field name="ks_chart_item_color">cool</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_auto_update_type">ks_live_update</field>
            <field name="ks_show_live_pop_up">1</field>
            <field name="ks_company_id" eval="0"/>
        </record>

        <record id="ks_purchase_item_8" model="ks_dashboard_ninja.item">
            <field name="name">Top 5 Products Purchased</field>
            <field name="ks_dashboard_item_type">ks_pie_chart</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order_line')"/>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_chart_groupby_type">relational_type</field>
            <field name="ks_chart_measure_field" eval="[(6, 0, [ref('purchase.field_purchase_order_line__product_uom_qty')])]"/>
            <field name="ks_chart_relation_groupby" eval="ref('purchase.field_purchase_order_line__product_id')"/>
            <field name="ks_chart_item_color">neon</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order_line__create_date')"/>
            <field name="ks_record_data_limit">5</field>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record id="ks_purchase_item_9" model="ks_dashboard_ninja.item">
            <field name="name">Last 5 Month Purchased</field>
            <field name="ks_dashboard_item_type">ks_bar_chart</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_chart_groupby_type">date_type</field>
            <field name="ks_chart_measure_field"
                   eval="[(6, 0, [ref('purchase.field_purchase_order__amount_total'),ref('purchase.field_purchase_order__amount_tax'),ref('purchase.field_purchase_order__amount_untaxed')])]"/>
            <field name="ks_chart_relation_groupby" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_chart_date_groupby">month</field>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_sort_by_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_record_data_limit">5</field>
            <field name="ks_chart_item_color">warm</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record id="ks_purchase_item_10" model="ks_dashboard_ninja.item">
            <field name="name">Top 10 Purchase Orders</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_list_view_fields"
                   eval="[(6, 0, [ref('purchase.field_purchase_order__name'),ref('purchase.field_purchase_order__amount_total'),ref('purchase.field_purchase_order__create_date'),ref('purchase.field_purchase_order__partner_id')])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_sort_by_field" eval="ref('purchase.field_purchase_order__amount_total')"/>
            <field name="ks_record_data_limit">10</field>
            <field name="ks_list_view_layout">layout_1</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record id="ks_purchase_item_11" model="ks_dashboard_ninja.item">
            <field name="name">Polar Area</field>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_chart_groupby_type">relational_type</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_chart_measure_field" eval="[(6, 0, [ref('purchase.field_purchase_order__amount_untaxed')])]"/>
            <field name="ks_chart_relation_groupby" eval="ref('purchase.field_purchase_order__amount_total')"/>
            <field name="ks_chart_relation_sub_groupby" eval="ref('purchase.field_purchase_order__amount_total')"/>
            <field name="ks_domain">[["id","&lt;",10]]</field>
            <field name="ks_chart_item_color">cool</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_dashboard_item_type">ks_polarArea_chart</field>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record id="ks_purchase_item_12" model="ks_dashboard_ninja.item">
            <field name="name">Purchase Order Amount Total</field>
            <field name="ks_chart_data_count_type">sum</field>
            <field name="ks_chart_groupby_type">relational_type</field>
            <field name="ks_model_id" eval="ref('purchase.model_purchase_order')"/>
            <field name="ks_chart_measure_field"
                   eval="[(6, 0, [ref('purchase.field_purchase_order__amount_total'),ref('purchase.field_purchase_order__amount_untaxed')])]"/>
            <field name="ks_chart_relation_groupby" eval="ref('purchase.field_purchase_order__amount_total')"/>
            <field name="ks_domain">[["id","&lt;",11]]</field>
            <field name="ks_chart_item_color">default</field>
            <field name="ks_date_filter_field" eval="ref('purchase.field_purchase_order__create_date')"/>
            <field name="ks_dashboard_item_type">ks_horizontalBar_chart</field>
            <field name="ks_company_id" eval="0"/>
            <!--            <field name="ks_actions" eval="ref('purchase.field_purchase_order__quotations')"/>-->
        </record>
        <record id="ks_purchase_item_13" model="ks_dashboard_ninja.item">
            <field name="name">Pending Payments</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_model_id" eval="ref('account.model_account_move')"/>
            <field name="ks_list_view_fields"
                   eval="[(6, 0, [ref('account.field_account_move__name'),ref('account.field_account_move__partner_id'),ref('account.field_account_move__invoice_date_due'),ref('account.field_account_move__company_id'),ref('account.field_account_move__amount_total_signed')])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_domain">[["payment_state","=","not_paid"],["move_type", "=", "in_invoice"]]</field>
            <field name="ks_date_filter_field" eval="ref('account.field_account_move__create_date')"/>
            <field name="ks_sort_by_field" eval="ref('account.field_account_move__invoice_date_due')"/>
            <field name="ks_list_view_layout">layout_2</field>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record id="ks_purchase_item_14" model="ks_dashboard_ninja.item">
            <field name="name">Pending Delivery</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_model_id" eval="ref('stock.model_stock_picking')"/>
            <field name="ks_list_view_fields"
                   eval="[(6, 0, [ref('stock.field_stock_picking__name'),ref('stock.field_stock_picking__partner_id'),ref('stock.field_stock_picking__scheduled_date'),ref('stock.field_stock_picking__origin'),ref('stock.field_stock_picking__company_id')])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_domain">[["picking_type_id","=",1],["state","not in",("done","cancel")]]</field>
            <field name="ks_date_filter_field" eval="ref('stock.field_stock_picking__create_date')"/>
            <field name="ks_sort_by_field" eval="ref('stock.field_stock_picking__scheduled_date')"/>
            <field name="ks_list_view_layout">layout_4</field>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record id="ks_purchase_item_15" model="ks_dashboard_ninja.item">
            <field name="name">Vendor Evaluation</field>
            <field name="ks_dashboard_item_type">ks_list_view</field>
            <field name="ks_model_id" eval="ref('equip3_purchase_report.model_vendor_evaluation')"/>
            <field name="ks_list_view_fields"
                   eval="[(6, 0, [ref('equip3_purchase_report.field_vendor_evaluation__vendor_res'),ref('equip3_purchase_report.field_vendor_evaluation__on_time_rate_res'),ref('equip3_purchase_report.field_vendor_evaluation__fulfillment_res'),ref('equip3_purchase_report.field_vendor_evaluation__final_point_res')])]"/>
            <field name="ks_sort_by_order">DESC</field>
            <field name="ks_sort_by_field" eval="ref('equip3_purchase_report.field_vendor_evaluation__final_point_res')"/>
            <field name="ks_list_view_layout">layout_1</field>
            <field name="ks_company_id" eval="0"/>
        </record>
        <record model="ks_dashboard_ninja.board" id="purchase_dashboards_new">
            <field name="name">Purchase Dashboard</field>
            <field name="ks_dashboard_menu_name">Purchase Dashboard</field>
            <field name="ks_dashboard_top_menu_id" ref="purchase.menu_purchase_root"/>
            <field name="ks_dashboard_default_template" ref="equip3_purchase_report.ks_purchase_dash"/>
            <field name="ks_dashboard_active" eval="True"/>
            <field name="ks_dashboard_group_access" eval="False"/>
            <field name="ks_dashboard_menu_sequence" eval="0"/>
        </record>
    </data>

</odoo>