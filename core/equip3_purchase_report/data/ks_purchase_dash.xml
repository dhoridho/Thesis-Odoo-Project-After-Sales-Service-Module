        <record id="ks_purchase_item_4" model="ks_dashboard_ninja.item">
            <field name="name">Purchase Tenders</field>
            <field name="ks_dashboard_item_type">ks_tile</field>
            <field name="ks_record_count_type">count</field>
            <field name="ks_model_id" eval="ref('sh_po_tender_management.model_purchase_agreement')"/>
