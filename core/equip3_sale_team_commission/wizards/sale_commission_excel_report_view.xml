<odoo>
        <record id="view_sale_commission_excel_report" model="ir.ui.view">
            <field name="name">sale.commission.excel.report.wizard</field>
            <field name="model">sale.commission.excel.report</field>
            <field name="arch" type="xml">
                <form string="Excel Report">
                    <group>
                        <field name="excel_file" invisible="True" />
                        <field name="file_name" filename="excel_file" invisible="True" />
                        <button name="download_report" string="Sales Commission Report" type="object" class="oe_highlight" />
                    </group>
                    <footer>
                        <button special="cancel" string="Cancel" class="oe_highlight" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_sale_commission_excel_report" model="ir.actions.act_window">
            <field name="name">Sale Commission Excel Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sale.commission.excel.report</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref='view_sale_commission_excel_report'/>
            <field name="target">new</field>
        </record>

</odoo>