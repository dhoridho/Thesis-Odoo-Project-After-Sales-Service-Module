<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="angcrane_employee_report" model="ir.actions.report">
        <field name="name">FORM IR21</field>
        <field name="model">wiz.hr.employee.report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sg_ir21.report_form_ir21</field>
    </record>

    <record id="form_ir21_report_paper_format" model="report.paperformat">
        <field name="name">Form IR21 Report</field>
        <field name="default" eval="True" />
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="margin_top">3</field>
        <field name="margin_bottom">10</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">45</field>
        <field name="dpi">90</field>
        <field name="orientation">Portrait</field>
        <field name="report_ids"
            eval="[(4, ref('sg_ir21.angcrane_employee_report'))]"></field>
    </record>

</odoo>