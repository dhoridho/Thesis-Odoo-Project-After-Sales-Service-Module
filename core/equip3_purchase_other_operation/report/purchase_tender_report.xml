<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="report_purchase_tender_landscap" model="ir.actions.report">
		<field name="name">Purchase Tender</field>
		<field name="model">print.purchase.tender.report</field>
		<field name="report_type">qweb-pdf</field>
		<field name="report_name">equip3_purchase_other_operation.exclusive_report_purchase_tender</field>
		<field name="report_file">equip3_purchase_other_operation.exclusive_report_purchase_tender</field>
		<field name="binding_model_id" ref="model_print_purchase_tender_report"/>
		<field name="binding_type">report</field>
	</record>

	<record id="action_report_analyze_quotations" model="ir.actions.report">
        <field name="name">Analyze Quotations</field>
        <field name="model">purchase.agreement</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">equip3_purchase_other_operation.report_analyze_quot</field>
        <field name="report_file">equip3_purchase_other_operation.report_analyze_quot</field>
        <field name="binding_model_id" ref="model_purchase_agreement" />
        <field name="binding_type">report</field>
    </record>

    <record id="sh_po_tender_management.action_report_analyze_quotations" model="ir.actions.report">
        <field name="binding_model_id" eval="False"/>
    </record>

    <record id="paperformat_purchase_tender_landscap" model="report.paperformat">
        <field name="name">Purchase Tender</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">297</field>
        <field name="page_width">210</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">35</field>
        <field name="margin_bottom">23</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">40</field>
        <field name="dpi">90</field>
    </record>

    <record id="report_purchase_tender_landscap" model="ir.actions.report">
        <field name="paperformat_id" ref="equip3_purchase_other_operation.paperformat_purchase_tender_landscap"/>
    </record>
</odoo>