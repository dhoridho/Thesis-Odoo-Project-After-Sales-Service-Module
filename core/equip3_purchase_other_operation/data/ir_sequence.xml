<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="sh_purchase_agreement_seq2" model="ir.sequence">
		<field name="name">Purchase Agreement Sequence</field>
		<field name="code">purchase.agreement.seqs</field>
		<field name="prefix">TE/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>
	<record id="sh_open_purchase_agreement_seq2" model="ir.sequence">
		<field name="name">Open Purchase Agreement Sequence</field>
		<field name="code">purchase.agreement.seqs.open</field>
		<field name="prefix">OTE/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
	</record>
	<record id="seq_blanket_orders" model="ir.sequence">
		<field name="name">Blanket Order</field>
		<field name="code">purchase.requisition.blanket.order.new</field>
		<field name="prefix">BO/%(y)s/%(month)s/%(day)s/</field>
		<field name="padding">3</field>
		<field name="company_id" eval="False"></field>
	</record>

	<record id="sh_purchase_agreement_seq3" model="ir.sequence">
		<field name="name">Purchase Agreement Sequence</field>
