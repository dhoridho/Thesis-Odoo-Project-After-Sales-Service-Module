<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="report.client_action">
        <t t-jquery=".o_report_iframe" t-operation="replace">
            <iframe class="o_report_iframe" allowfullscreen="true"/>
        </t>
    </t>
    <t t-extend="report.client_action.ControlButtons">
        <t t-jquery=".o_report_buttons" t-operation="replace">
            <div class="o_report_buttons">
                <button type="button" class="btn btn-primary o_report_print" title="Download">Download</button>
                <button type="button" class="btn btn-secondary o_report_direct" title="Print">Print</button>
            </div>
        </t>
    </t>
</templates>