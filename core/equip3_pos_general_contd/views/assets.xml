<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend"  inherit_id="equip3_pos_general.assets_backend">
        <xpath expr="//script[@src='/equip3_pos_general/static/src/js/form_controller.js']" position="after">
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/form_controller.js"/>
        </xpath>
    </template>


	<template id="assets" inherit_id="point_of_sale.assets">
		<xpath expr="." position="inside">
			<link rel="stylesheet" href="/equip3_pos_general_contd/static/src/css/main.css"/>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Core/Order.js"/>

			<script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/PopUps/MyCustomMessagePopup.js"></script>
			<script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/PopUps/ProductExchangePopup.js"></script>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/PopUps/ImportFilePopup.js"></script>
            
			<script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Screens/PosOrder/PosOrderDetail.js"/>
			<script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Screens/PaymentScreen/PaymentScreen.js"/>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/pos_order.js"/>
		</xpath>
	</template>
	
    <template id="assets_index" name="Index POS" inherit_id="equip3_pos_masterdata.assets_index">
		<xpath expr="//script[@src='/equip3_pos_masterdata/static/src/libs/indexed_db.js']" position="after">
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/libs/IndexedDBContd.js"/>
        </xpath>
    </template>

    <template id="inherit_pos_general_assets" inherit_id="equip3_pos_general.custom_pos">
        <xpath expr="//script[@src='/equip3_pos_general/static/src/js/Core/BigData.js']" position="after">
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Core/Database.js"/>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Core/BigData.js"/>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Core/Models.js"/>

            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Screens/OrderHistoryLocal/OrderHistoryLocalDetail.js"/>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Screens/OrderHistoryLocal/OrderHistoryLocalRow.js"/>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Screens/OrderHistoryLocal/OrderHistoryLocalList.js"/>
            <script type="text/javascript" src="/equip3_pos_general_contd/static/src/js/Screens/ProductScreen/OrderHistoryLocalButton.js"/>
        </xpath>
	</template>

</odoo>

