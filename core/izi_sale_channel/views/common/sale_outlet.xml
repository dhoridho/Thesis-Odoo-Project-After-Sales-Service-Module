<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
  
  	<record id="izi_sale_channel.tree_sale_outlet" model="ir.ui.view">
		<field name="name">Sale Outlet Tree View</field>
		<field name="model">sale.outlet</field>
		<field name="arch" type="xml">
			<tree string="Sale Outlet">
				<field name="name"/>
				<field name="warehouse_id"/>              
			</tree>
		</field>
	</record>

	<record id="izi_sale_channel.form_sale_outlet"
		model="ir.ui.view">
		<field name="name">Sale Outlet Form View</field>
    	<field name="model">sale.outlet</field>
		<field name="arch" type="xml">
			<form string="Sale Outlet">
				<sheet>
					<div class="oe_title">
						<label for="name" class="oe_edit_only" />
						<h1>
							<field name="name" />
						</h1>
					</div>
					<group>
						<field name="warehouse_id"/>
						<field name="partner_id"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>

  </data>
</odoo>