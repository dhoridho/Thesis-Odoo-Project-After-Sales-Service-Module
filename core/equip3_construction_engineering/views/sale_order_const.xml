                                <field name="amount_line"  readonly="1" force_save="1" optional="hide" widget='monetary' options="{'currency_field': 'currency_id'}" sum="Total Contract"/>
                                <field name="cascaded" force_save="1" invisible="1"/>
                                <field name="is_child" readonly="1" force_save="1" attrs="{'column_invisible': [('parent.hide_cascade', '=', False)]}"/>
                                <field name="parent_manuf_line" force_save="1" invisible="1"/>
                                <field name="state_line" invisible = "1"/>
                                <field name="currency_id" invisible="1"/>
                                <field name="company_id" invisible="1"/>
                            </tree>
                        </field>
                        <group attrs="{'readonly': [('state', '=', 'block')]}">
                            <group name="adjustment" string="Adjustment" attrs="{'invisible': [('adjustment_type','!=', 'global')]}">
                                <group>
                                    <label for="adjustment_method_global" string="Method" attrs="{'invisible': [('adjustment_type','!=','global')]}"/>
                                    <div>
                                        <field name="adjustment_method_global" class="oe_inline" attrs="{'invisible': [('adjustment_type','!=','global')]}"/>
                                    </div>
                                    <label for="adjustment_amount_global" string="Amount" attrs="{'invisible': [('adjustment_type','!=','global')]}"/>
                                    <div>
                                        <field name="adjustment_amount_global" class="oe_inline" attrs="{'invisible': [('adjustment_type','!=','global')]}"/>
                                        <label for="adjustment_amount_global" string="%%" attrs="{'invisible': [('adjustment_method_global', '!=', 'per')]}" class="oe_inline"/>
                                    </div>
                                </group>
                                <group invisible="1">
                                    <field name="scope_adjustment_ids" widget="one2many_list" nolabel="1" class="o_scope_section" invisible="1">
                                        <tree editable="bottom" create="false" delete="false">
                                            <field name="sr_no" readonly="1" />
                                            <field name="order_id" string="Number" invisible = "1"/>
                                            <field name="project_scope" readonly="1" force_save="1"/>
                                            <field name="subtotal_scope" readonly="1" force_save="1" invisible = "1"/>
                                            <field name="adjustment_type" invisible = "1"/>
                                            <field name="adjustment_method_scope" string="Method"/>
                                            <field name="adjustment_amount_scope" string="Amount"/>
                                            <field name="adjustment_subtotal_scope" string="Subtotal"/>
                                        </tree>
                                    </field>
                                </group>
                                <group invisible="1">
                                    <field name="section_adjustment_ids" widget="one2many_list" nolabel="1" class="o_scope_section" invisible="1">
                                        <tree editable="bottom" create="false" delete="false">
                                            <field name="sr_no" readonly="1"  />
                                            <field name="order_id" string="Number" invisible = "1"/>
                                            <field name="project_scope" readonly="1" force_save="1"/>
                                            <field name="section_name" readonly="1" force_save="1" invisible="1"/>
                                            <field name="section" readonly="1" force_save="1"/>
                                            <field name="subtotal_section" readonly="1" force_save="1" invisible = "1"/>
                                            <field name="adjustment_type" invisible = "1"/>
                                            <field name="adjustment_method_section" string="Method"/>
                                            <field name="adjustment_amount_section" string="Amount"/>
                                            <field name="adjustment_subtotal_section" string="Subtotal"/>
                                        </tree>
                                    </field>
                                </group>
                                <group>
                                </group>
                            </group>    
                            <group name="discount" string="Discount" attrs="{'invisible': [('discount_type','!=', 'global')]}">
                                <group>
                                    <label for="discount_method_global" string="Method" attrs="{'invisible': [('discount_type','!=','global')]}"/>
                                    <div>
                                        <field name="discount_method_global" class="oe_inline" attrs="{'invisible': [('discount_type','!=','global')]}"/>
                                    </div>
                                    <label for="discount_amount_global" string="Amount" attrs="{'invisible': [('discount_type','!=','global')]}"/>
                                    <div>
                                        <field name="discount_amount_global" class="oe_inline" attrs="{'invisible': [('discount_type','!=','global')]}"/>
                                        <label for="discount_amount_global" string="%%" attrs="{'invisible': [('discount_method_global', '!=', 'per')]}" class="oe_inline"/>
