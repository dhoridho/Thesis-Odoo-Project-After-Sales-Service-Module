                                            <field name="sr_no" readonly="1" />
                                            <field name="order_id" string="Number" invisible = "1"/>
                                            <field name="project_scope" readonly="1" force_save="1"/>
                                            <field name="section_name" readonly="1" force_save="1" invisible="1"/>
                                            <field name="section" readonly="1" force_save="1"/>
                                            <field name="subtotal_section" readonly="1" force_save="1" invisible = "1"/>
                                            <field name="adjustment_amount"  readonly="1" force_save="1" invisible = "1"/>
                                            <field name="discount_type" invisible = "1"/>
                                            <field name="discount_method_section" string="Method"/>
                                            <field name="discount_amount_section" string="Amount"/>
                                            <field name="discount_subtotal_section" string="Subtotal"/>
                                        </tree>
                                    </field>
                                </group>
                                <group>
                                </group>
                            </group>
