                                <field name="sale_cons_id" string="Number" invisible = "1"/>
                                <field name="sr_no" readonly="1" />
                                <field name="active" invisible="1"/>
                                <field name="project_id" invisible="1"/>
                                <field name="customer_id" invisible="1"/>
                                <field name="user_id" invisible="1" widget="many2many_tags"/>
                                <field name="job_references" widget="many2many_tags" invisible="1"/>
                                <field name="job_reference" invisible="1"/> 
                                <field name='project_scope' readonly="1" force_save="1"/>
                                <field name='section' readonly="1" force_save="1" />
                                <field name="variable_ref" readonly="1" force_save="1" optional="hide"/>
                                <field name="final_finish_good_id" readonly="1" force_save="1" attrs="{'column_invisible': [('parent.hide_cascade', '=', False)]}"/>
                                <field name='finish_good_id' readonly="1" force_save="1"/>
                                <field name='bom_id' readonly="1" force_save="1"/>
                                <field name='quantity' readonly="1" force_save="1"/>
                                <field name='uom_id' readonly="1" force_save="1"/>
                                <field name="adjustment_type" invisible="1"/>
                                <field name="adjustment_method_manuf" attrs="{'column_invisible': [('parent.adjustment_type','!=','manuf')]}"/>
                                <field name="adjustment_amount_manuf" attrs="{'column_invisible': [('parent.adjustment_type','!=','manuf')]}"/>
                                <field name="adjustment_subtotal_manuf" invisible = "1"/>
                                <field name="manuf_adjustment" readonly="1" force_save="1" optional="hide" widget='monetary' options="{'currency_field': 'currency_id'}" sum="Total Adjustment"/>
                                <field name="discount_type" invisible = "1"/>
                                <field name="discount_method_manuf" attrs="{'column_invisible': [('parent.discount_type','!=','manuf')]}"/>
                                <field name="discount_amount_manuf" attrs="{'column_invisible': [('parent.discount_type','!=','manuf')]}"/>
                                <field name="discount_subtotal_manuf" invisible = "1"/>
                                <field name="manuf_discount" readonly="1" force_save="1" optional="hide" widget='monetary' options="{'currency_field': 'currency_id'}" sum="Total Discount"/>
