                                <field name="total_labour" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                                <field name="total_overhead" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                                <field name="total_asset" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                                <field name="total_subcon" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                                <div class="oe_subtotal_footer_separator oe_inline o_td_label">
                                    <label for="amount_untaxed" />
                                </div>
                                <field name="amount_untaxed" nolabel="1" class="oe_subtotal_footer_separator" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                                <field name="adjustment_sub" widget='monetary' options="{'currency_field': 'currency_id'}" string="Adjustment (+)"/>
                                <field name="discount_sub" widget="monetary" options="{'currency_field': 'currency_id'}"  string="Discount (-)"/> 
                                <div class="oe_subtotal_footer_separator oe_inline o_td_label">
                                    <label for="contract_amount" />
                                </div>
                                <field name="contract_amount" nolabel="1" class="oe_subtotal_footer_separator" widget='monetary'/>
                                <field name="amount_tax" widget='monetary'/>
                                <div class="oe_subtotal_footer_separator oe_inline o_td_label">
                                    <label for="amount_total" />
                                </div>
                                <field name="amount_total" nolabel="1" class="oe_subtotal_footer_separator" widget='monetary'/>
                            </group>
                            <group name="note_group">
                                <div>
                                    <label for="terms_conditions_id" string="Terms and Conditions"/>
