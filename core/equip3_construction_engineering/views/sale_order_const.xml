                                            <field name="discount_type" invisible = "1"/>
                                            <field name="discount_method_scope" string="Method"/>
                                            <field name="discount_amount_scope" string="Amount"/>
                                            <field name="discount_subtotal_scope" string="Subtotal"/>
                                        </tree>
                                    </field>
                                </group>
                                <group invisible="1">
                                    <field name="section_discount_ids" widget="one2many_list" nolabel="1" class="o_scope_section" invisible="1">
